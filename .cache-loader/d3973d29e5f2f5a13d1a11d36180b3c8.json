tory from '@src/lib/browserHistory';\n\nimport { Partial, Account, AccountType, AccountPreset } from '@src/types';\n\nimport './AddPresetIMAP.scss';\n\nexport interface Props {\n  name: string;\n  logo: string;\n  passwordUrl: string;\n  type: AccountType;\n  preset?: AccountPreset;\n}\n\nexport interface StateProps {\n  showHeader: boolean;\n}\n\nexport interface DispatchProps {\n  addAccount(account: Partial<Account>): void;\n}\n\nexport interface State {\n  email: string;\n  password: string;\n  showPrompt: boolean;\n}\n\nexport default class AddPresetIMAP extends React.PureComponent<\n  Props & StateProps & DispatchProps, State\n> {\n  constructor() {\n    super();\n\n    this.state = {\n      email: '',\n      password: '',\n      showPrompt: true,\n    };\n  }\n\n  handleEmailChange = (e: React.ChangeEvent<HTMLInputElement>) => {\n    this.setState({ email: e.currentTarget.value });\n  }\n\n  handlePasswordChange = (e: React.ChangeEvent<HTMLInputElement>) => {\n    this.setState({ password: e.currentTarget.value });\n  }\n\n  handleBackPress = () => {\n    browserHistory.goBack();\n  }\n\n  handleDismissPrompt = () => {\n    this.setState({ showPrompt: false });\n  }\n\n  handleSubmit = (e: React.FormEvent<HTMLFormElement>) => {\n    e.preventDefault();\n    e.stopPropagation();\n\n    this.props.addAccount({\n      email: this.state.email,\n      password: this.state.password,\n      type: this.props.type,\n      preset: this.props.preset,\n    });\n  }\n\n  handleCreatePassword = () => {\n    window.open(this.props.passwordUrl, '_blank');\n  }\n\n  renderPrompt() {\n    return (\n      <div className=\"add-preset-imap\">\n        {this.props.showHeader && <NavigationHeader\n          title={`Add ${this.props.name}`}\n          onBack={this.handleBackPress}\n        />}\n        <div className=\"header\">\n          <img src={this.props.logo} />\n        </div>\n        <div className=\"prompt\">\n          <h1>Create an App Password for {this.props.name}</h1>\n          <p>\n            You'll have to turn on 2-factor authentication &amp; create an app-specific\n                password in order to connect your {this.props.name} account to Polymail.\n                Click below to set it up!\n              </p>\n          <p>\n            <button className=\"create-password\" onClick={this.handleCreatePassword}>\n              Create Password\n            </button>\n          </p>\n          <p>\n            <button className=\"already-done\" onClick={this.handleDismissPrompt}>\n              I've already done this\n            </button>\n          </p>\n        </div>\n      </div>\n    );\n  }\n\n  renderForm() {\n    return (\n      <div className=\"add-preset-imap\">\n        {this.props.showHeader && <NavigationHeader\n          title={`Add ${this.props.name}`}\n          onBack={this.handleBackPress}\n        />}\n        <div className=\"header\">\n          <img src={this.props.logo} />\n        </div>\n        <form onSubmit={this.handleSubmit}>\n          <div className=\"field\">\n            <TextInput\n              name=\"email\"\n              description=\"Email\"\n              placeholder=\"Email\"\n              value={this.state.email}\n              type=\"email\"\n              onChange={this.handleEmailChange}\n              autoComplete=\"email\"\n              autofocus\n              required\n            />\n          </div>\n          <div className=\"field\">\n            <TextInput\n              name=\"password\"\n              description=\"Password\"\n              placeholder=\"Password\"\n              value={this.state.password}\n              type=\"password\"\n              onChange={this.handlePasswordChange}\n              required\n            />\n          </div>\n          <div className=\"submit\">\n            <button>Add Account</button>\n          </div>\n        </form>\n      </div>\n    );\n  }\n\n  render() {\n    if (this.state.showPrompt) {\n      return this.renderPrompt();\n    }\n\n    return this.renderForm();\n  }\n}\n"]}]}