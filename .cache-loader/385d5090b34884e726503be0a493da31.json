c/components/Popover/Popover';\nimport './MessageOptionsPopover.scss';\n\ninterface Props {\n  options: string[];\n  onOptionSelect: (e: React.SyntheticEvent<HTMLButtonElement>) => void;\n  onHide: () => void;\n}\n\nclass MessageOptionsPopover extends React.PureComponent<Props> {\n  private popover: Popover;\n\n  show = () => {\n    this.popover.show();\n  }\n\n  hide = () => {\n    this.popover.hide();\n  }\n\n  handleRef = (ref: Popover) => {\n    this.popover = ref;\n  }\n\n  render() {\n    const { options, onOptionSelect, onHide } = this.props;\n\n    return (\n      <Popover\n        ref={this.handleRef}\n        className=\"message-options-popover\"\n        onHide={onHide}\n      >\n        {options.map((option: string, index: number) => (\n          <button\n            onClick={onOptionSelect}\n            key={index}\n            data-option={option}\n            className=\"cell border-bottom\"\n          >\n            {option}\n          </button>\n        ))}\n      </Popover>\n    );\n  }\n}\n\nexport default MessageOptionsPopover;\n"]}]}