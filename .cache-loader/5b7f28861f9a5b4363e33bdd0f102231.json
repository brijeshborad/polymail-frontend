MAAM,EAAC,uBAAuB,EAAE,EAAE,MAAM,EAAE,IAAI,CAAC,IAAI,EAAE,GAAI;YACxE,oBAAC,SAAS,IAAC,SAAS,EAAC,MAAM,GAAE,CACzB,CACF,CACP,CAAC;AACJ,CAAC,CAAC;AAEF,eAAe,IAAI,CAAC","sourcesContent":["import React from 'react';\nimport classnames from 'classnames';\nimport moment from 'moment';\n\nimport { ContactNote, TeamMember, Me } from '@src/types';\n\nimport ThreeDots from '@src/components/Icons/ThreeDots.svg';\nimport './Note.scss';\n\nexport interface Props {\n  note: ContactNote;\n  teamMember?: TeamMember | Me;\n  className?: string;\n}\n\nconst Note = (props: Props) => {\n  const { note, teamMember } = props;\n  const time = moment(note.updated).calendar(undefined, {\n    sameDay: '[Today at] h:mm A',\n    nextDay: '[Tomorrow at] h:mm A',\n    nextWeek: 'dddd [at] h:mm A',\n    lastDay: '[Yesterday at] h:mm A',\n    lastWeek: '[Last] dddd [at] h:mm A',\n    sameElse: 'MMM D [at] h:mm A',\n  });\n\n  return (\n    <div className={classnames('note', props.className)}>\n      {teamMember && <img src={teamMember.photoUrl} />}\n      <div className=\"main\">\n        <div className=\"heading\">\n          <span className=\"name\">\n            {teamMember ? `${teamMember.firstName} ${teamMember.lastName}` : 'Team Member'}\n          </span>\n          <span className=\"time\">{time}</span>\n        </div>\n        <div className=\"body\" dangerouslySetInnerHTML={{ __html: note.body }} />\n        <ThreeDots className=\"icon\"/>\n      </div>\n    </div>\n  );\n};\n\nexport default Note;\n"]}]}