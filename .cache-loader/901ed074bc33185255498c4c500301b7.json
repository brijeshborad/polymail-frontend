CP,UAAU,CAAC,OAAO,EAAE,aAAa,EAAE;oBACjC,SAAS,WAAA;oBACT,gBAAgB,EAAE,aAAa;iBAChC,CAAC,EAEJ,OAAO,EAAE,IAAI,CAAC,cAAc;;gBAG3B,eAAe,IAAI,CAClB,oBAAC,UAAU,IAAC,SAAS,EAAC,YAAY,GAAG,CACtC,CACG,CACP;YACA,CAAC,SAAS,IAAI,CAAC,aAAa,IAAI,CAC/B,6BACE,SAAS,EAAE,UAAU,CAAC,qBAAqB,EAAE,EAAE,MAAM,EAAE,WAAW,EAAE,SAAS,WAAA,EAAE,CAAC,EAChF,OAAO,EAAE,IAAI,CAAC,UAAU;gBAE1B,6BACE,KAAK,EAAE;wBACL,QAAQ,EAAE,KAAK;qBAChB,EACD,GAAG,EAAE,SAAS,CAAC,YAAY,EAC3B,MAAM,EAAE,IAAI,CAAC,UAAU,GACvB;gBACF,6BAAK,SAAS,EAAE,UAAU,CAAC,WAAW,EAAE,EAAE,MAAM,EAAE,WAAW,EAAE,CAAC;oBAC9D,oBAAC,IAAI,OAAG,CACJ,CACF,CACL,CACG,CACP,CAAC;IACJ,CAAC;IACH,gBAAC;AAAD,CAAC,AAjFD,CAAwB,KAAK,CAAC,aAAa,GAiF1C;AAED,eAAe,SAAS,CAAC","sourcesContent":["import React from 'react';\nimport Play from '@src/components/Icons/Play.svg';\nimport Disclosure from '@src/components/Icons/Disclosure.svg';\nimport classnames from 'classnames';\nimport './DemoVideoPreview.scss';\nimport { DemoVideoMeta } from '@src/types';\n\nexport interface DispatchProps {\n  openDemoVideo: (d: DemoVideoMeta) => void;\n}\n\nexport interface Props {\n  demoVideo: DemoVideoMeta;\n  width: number;\n  labelDisclosure?: boolean;\n  labelPosition?: string;\n  hideThumbnail?: boolean;\n  hideHeader?: boolean;\n  collapsed?: boolean;\n}\n\nexport interface State {\n  imageLoaded: boolean;\n  collapsed?: boolean;\n}\n\nclass DemoVideo extends React.PureComponent<DispatchProps & Props, State> {\n  constructor(props: Props & DispatchProps) {\n    super(props);\n    this.state = {\n      imageLoaded: false,\n      collapsed: props.collapsed || props.hideThumbnail || false,\n    };\n  }\n\n  // Handle toggling collapse, or activating if not showing thumbnail\n  handleCollapse = () => {\n    const { collapsed } = this.state;\n    const { hideThumbnail } = this.props;\n    if (hideThumbnail) {\n      this.handleOpen();\n      return;\n    }\n\n    this.setState({\n      collapsed: !collapsed,\n    });\n  }\n\n  handleOpen = () => {\n    const { openDemoVideo, demoVideo } = this.props;\n    openDemoVideo(demoVideo);\n  }\n\n  handleLoad = () => {\n    this.setState({\n      imageLoaded: true,\n    });\n  }\n\n  render() {\n    const {\n      demoVideo, width, hideHeader, labelPosition,\n      labelDisclosure, hideThumbnail,\n     } = this.props;\n    const { imageLoaded, collapsed } = this.state;\n\n    return (\n      <div\n        className=\"demo-video-preview\"\n      >\n        {!hideHeader && (\n          <div\n            className={\n              classnames('label', labelPosition, {\n                collapsed,\n                'hide-thumbnail': hideThumbnail,\n              })\n            }\n            onClick={this.handleCollapse}\n          >\n            LEARN MORE\n            {labelDisclosure && (\n              <Disclosure className=\"disclosure\" />\n            )}\n          </div>\n        )}\n        {!collapsed && !hideThumbnail && (\n          <div\n            className={classnames('thumbnail-container', { loaded: imageLoaded, collapsed })}\n            onClick={this.handleOpen}\n          >\n          <img\n            style={{\n              maxWidth: width,\n            }}\n            src={demoVideo.thumbnailUrl}\n            onLoad={this.handleLoad}\n          />\n          <div className={classnames('play-icon', { loaded: imageLoaded })}>\n            <Play />\n          </div>\n        </div>\n        )}\n      </div>\n    );\n  }\n}\n\nexport default DemoVideo;\n"]}]}