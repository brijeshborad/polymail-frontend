ce<T> {\n  return {\n    status: ResourceStatus.Loading,\n  };\n}\n\nexport function resourceRefreshing<T>(data: T): Resource<T> {\n  return {\n    data,\n    status: ResourceStatus.Refreshing,\n  };\n}\n\nexport function resourceOk<T>(data: T): Resource<T> {\n  return {\n    data,\n    status: ResourceStatus.Ok,\n  };\n}\n\nexport function resourceFailed<T>(error: Error, data?: T): Resource<T> {\n  return {\n    error,\n    status: ResourceStatus.Failure,\n    data,\n  };\n}\n\nexport function selectResourceLoading<T>(resource?: Resource<T>): boolean {\n  if (!resource) {\n    return false;\n  }\n  if (resource.status === ResourceStatus.Ok || resource.status === ResourceStatus.Failure) {\n    return false;\n  }\n  return true;\n}\n\nexport function selectResourceError<T>(resource?: Resource<T>): Error|undefined {\n  if (resource && resource.status === ResourceStatus.Failure && resource.error) {\n    return resource.error;\n  }\n  return;\n}\n"]}]}