cesContent":["import React from 'react';\nimport { truncate, truncatedProgress } from '@src/lib/attachment';\n\nimport Close from '@src/components/Icons/Close.svg';\n\nimport './AttachmentCell.scss';\nimport ToolTip from '@src/components/Base/Tooltip';\nimport { ApiError } from '@src/api/base';\nimport { map } from 'lodash';\nimport Loader from '@src/components/Loader/Loader';\n\ninterface Props {\n  id: string;\n  filename: string;\n  progress: number;\n  iconUrl: string;\n  length: number;\n  fileUrl?: string;\n\n  indeterminateUploading?: boolean;\n  onDelete?: (id: string) => void;\n  error?: Error;\n  isMeta?: boolean;\n}\n\ninterface State {\n  showErrorTip: boolean;\n}\n\nclass AttachmentCell extends React.PureComponent<Props, State> {\n  constructor() {\n    super();\n    this.state = { showErrorTip: false };\n  }\n\n  removeAttachment = (e: React.MouseEvent<HTMLButtonElement>) => {\n    e.preventDefault();\n    if (this.props.onDelete) {\n      this.props.onDelete(this.props.id);\n    }\n  }\n\n  // Calculate width and progress\n  renderProgress = () => {\n    if (this.props.progress === 1) {\n      return;\n    }\n\n    const progressCompleted = this.props.progress;\n    const progressStyle = `${progressCompleted * 100}%`;\n    return (\n      <div\n        style={{ width: progressStyle }}\n        className=\"progress\"\n      />\n    );\n  }\n\n  renderSize = () => {\n    if (this.props.progress === 1) {\n      return truncate(this.props.length);\n    }\n\n    return truncatedProgress(this.props.length, this.props.progress);\n  }\n\n  handleErrorEnter = () => {\n    if (!this.props.error) {\n      return;\n    }\n    this.setState({ showErrorTip: true });\n  }\n\n  handleErrorLeave = () => {\n    if (!this.props.error) {\n      return;\n    }\n    this.setState({ showErrorTip: false });\n  }\n\n  // Handle attachment download\n  handleAttachmentClick = () => {\n    const { fileUrl } = this.props;\n    if (fileUrl) {\n      window.open(fileUrl, '_blank');\n    }\n  }\n\n  renderName = () => {\n    const { isMeta } = this.props;\n    if (isMeta) {\n      return (\n        <a\n          className=\"name\"\n          href={this.props.fileUrl}\n          download={this.props.filename}\n        >\n          {this.props.filename}\n        </a>\n      );\n    }\n\n    return (\n      <div\n        className=\"name\"\n        onClick={this.handleAttachmentClick}\n      >\n        {this.props.filename}\n      </div>\n    );\n  }\n\n  render() {\n    const error = this.props.error;\n    let errorString = error && error.message;\n    if (error && (error as ApiError).errors) {\n      errorString = map((error as ApiError).errors, e => e).join(' ');\n    }\n\n    return (\n      <div className=\"compose-attachment-cell-container\">\n        {this.renderProgress()}\n        <div\n          className=\"compose-attachment-cell\"\n          onMouseEnter={this.handleErrorEnter}\n          onMouseLeave={this.handleErrorLeave}\n        >\n          {this.props.error &&\n            <span className=\"error-span\">⚠️</span>\n          }\n          <img\n            src={this.props.iconUrl}\n            className=\"preview\"\n          />\n          {this.renderName()}\n          {this.props.indeterminateUploading &&\n            <Loader />\n          }\n          <div className=\"size\">\n            {this.renderSize()}\n          </div>\n          {this.state.showErrorTip &&\n            <ToolTip\n              className=\"tooltip\"\n              bottom=\"40px\"\n              left=\"10px\"\n              copy={errorString || ''}\n              multiline\n              anchorBottom\n            />\n          }\n          <button\n            onClick={this.removeAttachment}\n            className=\"remove-button\"\n          >\n            <Close height=\"10px\" width=\"10px\" />\n          </button>\n        </div>\n      </div>\n    );\n  }\n}\n\nexport default AttachmentCell;\n"]}]}