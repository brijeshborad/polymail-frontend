H,CAGG;IACL,UAAU,EAAE,UAAC,KAAa,IAAK,OAAA,QAAQ,CAAC,UAAU,CAAC,KAAK,CAAC,CAAC,EAA3B,CAA2B;IAC1D,oBAAoB,EAAE,UAAC,SAAkB,IAAK,OAAA,QAAQ,CAAC,oBAAoB,CAAC;QAC1E,IAAI,EAAE,QAAQ,CAAC,IAAI;QACnB,SAAS,WAAA;KACV,CAAC,CAAC,EAH2C,CAG3C;IACH,aAAa,EAAE,UAAA,OAAO,IAAI,OAAA,QAAQ,CAAC,aAAa,CAAC,OAAO,CAAC,CAAC,EAAhC,CAAgC;CAC3D,CAAC,EAd6F,CAc7F,CAAC;AAEH,eAAe,OAAO,CACpB,eAAe,EAAE,kBAAkB,CACpC,CAAC,YAAY,CAAC,CAAC","sourcesContent":["import RightSidebar, { DispatchProps, StateProps, Props } from './RightSidebar';\nimport { Dispatch } from 'redux';\n\nimport { connect } from 'react-redux';\nimport { StoreState } from '@src/types';\n\nimport { selectContact, contactGet, contactUpdate } from '@src/ducks/contacts';\nimport { rightSidebarTabSelect, rightSidebarCollapse } from '@src/ducks/rightSidebar';\nimport { draftCreateConfiguration } from '@src/ducks/drafts';\nimport { Configuration } from '@src/lib/draft';\nimport { RightSidebarTab, RightSidebarSubTab } from '@src/types/rightSidebar';\nimport { selectHubspotEnabled, selectSalesforceEnabled } from '@src/ducks/integrations';\n\nconst mapStateToProps = (state: StoreState, ownProps: Props): StateProps => {\n  const hubspotEnabled = selectHubspotEnabled(state);\n  const salesforceEnabled = selectSalesforceEnabled(state);\n  return {\n    contact: state.selectedContact && selectContact(state, state.selectedContact),\n    contactGroups: state.contactGroups,\n    email: state.selectedContact,\n    collapsed: state.rightSidebar.collapsed[ownProps.page],\n    selectedTab: state.rightSidebar.tab,\n    hotkeyActions: state.hotkeyActions,\n    hideHubspot: !hubspotEnabled,\n    hideSalesforce: !salesforceEnabled,\n  };\n};\n\nconst mapDispatchToProps = (dispatch: Dispatch<StoreState>, ownProps: Props): DispatchProps => ({\n  selectRightSidebarTab: (tab: RightSidebarTab, subTab?: RightSidebarSubTab) =>\n    dispatch(rightSidebarTabSelect({ rsbTab: tab, rsbSubTab: subTab })),\n  createDraft: (configuration: Configuration, recipients: string[]) =>\n    dispatch(draftCreateConfiguration({\n      configuration,\n      recipients,\n    })),\n  getContact: (email: string) => dispatch(contactGet(email)),\n  collapseRightSidebar: (collapsed: boolean) => dispatch(rightSidebarCollapse({\n    page: ownProps.page,\n    collapsed,\n  })),\n  updateContact: contact => dispatch(contactUpdate(contact)),\n});\n\nexport default connect<StateProps, DispatchProps, Props>(\n  mapStateToProps, mapDispatchToProps,\n)(RightSidebar);\n"]}]}