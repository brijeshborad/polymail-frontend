 MiddlewareAPI } from 'redux';\n\nexport const updateTheme = (store: MiddlewareAPI<StoreState>, override?: ThemeSetting) => {\n  const isDarkMode = window.matchMedia('(prefers-color-scheme: dark)').matches;\n  const windowsTheme = (isElectronWindows || IS_WINDOWS) ? ' theme-windows' : '';\n  const themeSetting = override ? override : store.getState().theme.data;\n\n  switch (themeSetting) {\n    case ThemeSetting.Dark:\n      document.body.className = 'theme-dark' + windowsTheme;\n      document.body.setAttribute('theme', 'theme-dark');\n      break;\n    case ThemeSetting.Auto:\n      const theme = isDarkMode ? 'theme-dark' + windowsTheme : 'theme-light' + windowsTheme;\n      document.body.className = theme;\n      document.body.setAttribute('theme', theme);\n      break;\n    case ThemeSetting.Light:\n    default:\n      document.body.className = 'theme-light' + windowsTheme;\n      document.body.setAttribute('theme', 'theme-light');\n  }\n};\n"]}]}