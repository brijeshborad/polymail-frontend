AAK,SAAS,EAAC,SAAS,IAAE,IAAI,IAAI,IAAI,CAAC,IAAI,CAAC,WAAW,EAAE,CAAO;gBAChE,6BAAK,SAAS,EAAC,MAAM,EAAC,OAAO,EAAE,IAAI,CAAC,qBAAqB;oBACvD,oBAAC,SAAS,OAAG,CACT,CACC;YACT,oBAAC,qBAAqB,IAAC,GAAG,EAAE,IAAI,CAAC,uBAAuB,GAAI;YAC3D,SAAS,IAAI,KAAK;iBAChB,MAAM,CAAC,UAAA,CAAC,IAAI,OAAA,CAAC,CAAC,CAAC,MAAM,EAAT,CAAS,CAAC;iBACtB,GAAG,CAAC,UAAA,CAAC,IAAI,OAAA,CACR,oBAAC,WAAW,IACV,GAAG,EAAE,CAAC,CAAC,GAAG,EACV,IAAI,EAAE,CAAC,CAAC,IAAI,EACZ,KAAK,EAAE,CAAC,CAAC,KAAK,EACd,SAAS,EAAE,CAAC,CAAC,GAAG,CAAC,WAAW,EAAE,EAC9B,IAAI,EAAE,CAAC,CAAC,IAAI,EACZ,QAAQ,EAAE,WAAW,CAAC,WAAW,EAAE,KAAK,CAAC,CAAC,GAAG,CAAC,WAAW,EAAE,EAC3D,IAAI,EAAE,CAAC,CAAC,IAAI,EACZ,OAAO,EAAE,CAAC,CAAC,OAAO,EAClB,YAAY,EAAE,CAAC,CAAC,YAAY,EAC5B,iBAAiB,EAAE,CAAC,CAAC,iBAAiB,GACtC,CACH,EAbS,CAaT,CAAC;YACH,IAAI,CAAC,KAAK,CAAC,WAAW;gBACrB,oBAAC,iBAAiB,IAChB,GAAG,EAAE,IAAI,CAAC,SAAS,EACnB,IAAI,EAAE,IAAI,EACV,MAAM,EAAE,IAAI,CAAC,qBAAqB,GAClC,CAEA,CACP,CAAC;IACJ,CAAC;IACH,kBAAC;AAAD,CAAC,AAtOD,CAA0B,KAAK,CAAC,aAAa,GAsO5C;AAED,eAAe,WAAW,CAAC","sourcesContent":["import React from 'react';\nimport ReactDOM from 'react-dom';\nimport classnames from 'classnames';\nimport Mentions from '@src/components/Icons/Mentions.svg';\nimport Plus from '@src/components/Icons/Plus.svg';\nimport Template from '@src/components/Icons/Template.svg';\nimport Calendar from '@src/components/Icons/Calendar.svg';\nimport Unsubscribe from '@src/components/Icons/Unsubscribe.svg';\nimport Disclosure from '@src/components/Icons/Disclosure.svg';\nimport ThreeDots from '@src/components/Icons/ThreeDots.svg';\nimport Share from '@src/components/Icons/Share.svg';\nimport { Team, OnboardingType, Features, Onboarding } from '@src/types';\nimport { SectionItem } from '../Item/SectionItem';\nimport InviteTeamPopover from './InviteTeamPopover';\nimport InviteTeamPopoverComponent from './InviteTeamPopover/InviteTeamPopover';\nimport TeamDisclosurePopover from './TeamDisclosurePopover/TeamDisclosurePopover';\nimport { NavbarTab } from '@src/types/leftSidebar';\n\nimport './TeamSection.scss';\nimport { checkNextHotkeyChange, HotkeyAction } from '@src/lib/hotkeys';\nimport { AmplitudeEvents, logAmplitudeEvent } from '@src/lib/amplitude-analytics';\n\nexport interface StateProps {\n  collapsed: boolean;\n  team?: Team;\n  selectedTab: NavbarTab;\n  features: Features;\n  mentionsCount?: number;\n  userMentionsCount?: number;\n\n  hotkeyActions?: { [key: string]: number };\n  showMentionsOnboarding?: boolean;\n  showTeamInviteOnboarding?: boolean;\n  showUnsubscriberOnboarding?: boolean;\n  onboarding: Onboarding;\n}\n\nexport interface DispatchProps {\n  collapse: (collapsedState: boolean) => void;\n  updateOnboarding: (type: OnboardingType) => void;\n}\n\ninterface State {\n  showPopover: boolean;\n}\n\nclass TeamSection extends React.PureComponent<StateProps & DispatchProps, State> {\n  popover: InviteTeamPopoverComponent;\n  disclosurePopover: TeamDisclosurePopover;\n\n  constructor() {\n    super();\n    this.state = {\n      showPopover: false,\n    };\n  }\n\n  componentDidUpdate(oldProps: StateProps & DispatchProps) {\n    const oldActions = oldProps.hotkeyActions || {};\n    const newActions = this.props.hotkeyActions || {};\n    this.handleHotkeyActionsChange(oldActions, newActions);\n  }\n\n  handleHotkeyActionsChange = (\n    oldActions: { [action: string]: number },\n    newActions: { [action: string]: number },\n  ) => {\n    // Handle CC toggle\n    if (checkNextHotkeyChange(HotkeyAction.InviteTeamMembers, newActions, oldActions)) {\n      const teamElement = ReactDOM.findDOMNode(this);\n      const top = teamElement && (teamElement as Element).getBoundingClientRect().top + 100 || 0;\n      this.handleShowPopover(top);\n    }\n  }\n\n  toggleCollapse = (e: React.SyntheticEvent<HTMLButtonElement>) => {\n    const { collapse, collapsed } = this.props;\n\n    e.preventDefault();\n    e.stopPropagation();\n\n    collapse(!collapsed);\n  }\n\n  handleRef = (ref: any) => {\n    this.popover = ref && ref.getWrappedInstance();\n  }\n\n  handleShowPopoverClick = (e: React.MouseEvent<HTMLElement>) => {\n    const target = e.currentTarget;\n    const boundingBox = target && target.getBoundingClientRect && target.getBoundingClientRect();\n\n    if (boundingBox) {\n      this.handleShowPopover(boundingBox.bottom);\n    }\n    logAmplitudeEvent(AmplitudeEvents.ViewInvitePeople);\n  }\n\n  handleShowPopover = (top: number) => {\n    this.setState({ showPopover: true }, () => {\n      this.popover.show(top);\n    });\n  }\n\n  handleTeamPopoverHide = () => {\n    const { showTeamInviteOnboarding, updateOnboarding } = this.props;\n    // Dismiss team invite onboarding if clicked\n    if (showTeamInviteOnboarding) {\n      updateOnboarding(OnboardingType.InviteTeamMembers);\n    }\n\n    this.setState({ showPopover: false });\n  }\n\n  handleMentionsClick = () => {\n    const { showMentionsOnboarding, updateOnboarding } = this.props;\n    if (showMentionsOnboarding) {\n      updateOnboarding(OnboardingType.MentionsTab);\n    }\n  }\n\n  handleDisclosurePopover = (ref: TeamDisclosurePopover) => {\n    this.disclosurePopover = ref;\n  }\n\n  showDisclosurePopover = (e: React.MouseEvent<HTMLElement>) => {\n    const target = e.currentTarget;\n    const boundingBox = target && target.getBoundingClientRect && target.getBoundingClientRect();\n\n    if (boundingBox) {\n      e.stopPropagation();\n      e.preventDefault();\n      this.disclosurePopover.show(boundingBox.bottom);\n    }\n  }\n\n  // TODO(SHIN): Performance Hotfix: Identify better conditional showing of team popover\n  render() {\n    const {\n      collapsed,\n      team,\n      selectedTab,\n      mentionsCount,\n      onboarding,\n      showTeamInviteOnboarding,\n      features,\n      showMentionsOnboarding,\n      showUnsubscriberOnboarding,\n    } = this.props;\n\n    interface ItemFormat {\n      key: string;\n      name: string;\n      link?: string;\n      onClick: any;\n      icon: JSX.Element;\n      hidden: boolean;\n      count: number;\n      isOnboarding: boolean|undefined;\n      notificationCount: boolean;\n    }\n\n    // TOOD(SHIN): Implement OnboardingGem for Shared by Me\n    let items: ItemFormat[] = [\n      {\n        key: 'sharedThreads',\n        name: 'Shared By Me',\n        link: '/sharedThreads',\n        onClick: this.handleMentionsClick,\n        icon: <Share />,\n        hidden: false,\n        count: 0,\n        isOnboarding: !onboarding.sharedThreadsTabGem && onboarding.steps?.saveAsTemplate,\n        notificationCount: true,\n      },\n      {\n        key: 'templates',\n        name: 'Templates',\n        icon: <Template />,\n        onClick: undefined,\n        hidden: false,\n        link: '/templates',\n        count: 0,\n        isOnboarding: !onboarding.templatesTabGem && onboarding.steps?.messageTemplates,\n        notificationCount: false,\n      },\n      {\n        key: 'calendar',\n        name: 'Meeting Times',\n        icon: <Calendar />,\n        onClick: undefined,\n        hidden: false,\n        link: '/calendar',\n        count: 0,\n        isOnboarding: !onboarding.meetingTimesTabGem && onboarding.steps?.comment,\n        notificationCount: false,\n      },\n      {\n        key: 'unsubscriber',\n        name: 'Unsubscriber',\n        icon: <Unsubscribe />,\n        onClick: undefined,\n        hidden: false,\n        link: '/unsubscriber',\n        count: 0,\n        isOnboarding: showUnsubscriberOnboarding,\n        notificationCount: false,\n      },\n      {\n        key: 'referral',\n        name: 'Invite People',\n        icon: <Plus />,\n        onClick: this.handleShowPopoverClick,\n        hidden: false,\n        link: undefined,\n        count: 0,\n        isOnboarding: showTeamInviteOnboarding,\n        notificationCount: false,\n      },\n    ];\n\n    // Add Mentions if comments is enabled\n    if (features.comments) {\n      const commentItem: ItemFormat[] = [{\n        key: 'mentions',\n        name: 'Mentions',\n        link: '/mentions',\n        icon: <Mentions />,\n        onClick: this.handleMentionsClick,\n        hidden: false,\n        count: mentionsCount || 0,\n        isOnboarding: showMentionsOnboarding,\n        notificationCount: true,\n      }];\n\n      items = commentItem.concat(items);\n    }\n\n    return (\n      <div className=\"team-section\">\n        <button className={classnames('header', { collapsed })} onClick={this.toggleCollapse}>\n          <div className=\"icon\">\n            <Disclosure />\n          </div>\n          <div className=\"heading\">{team && team.name.toUpperCase()}</div>\n          <div className=\"more\" onClick={this.showDisclosurePopover}>\n            <ThreeDots />\n          </div>\n        </button>\n        <TeamDisclosurePopover ref={this.handleDisclosurePopover} />\n        {collapsed && items\n          .filter(i => !i.hidden)\n          .map(i => (\n            <SectionItem\n              key={i.key}\n              name={i.name}\n              count={i.count}\n              className={i.key.toLowerCase()}\n              icon={i.icon}\n              selected={selectedTab.toLowerCase() === i.key.toLowerCase()}\n              link={i.link}\n              onClick={i.onClick}\n              isOnboarding={i.isOnboarding}\n              notificationCount={i.notificationCount}\n            />\n          ))}\n        {this.state.showPopover &&\n          <InviteTeamPopover\n            ref={this.handleRef}\n            team={team}\n            onHide={this.handleTeamPopoverHide}\n          />\n        }\n      </div>\n    );\n  }\n}\n\nexport default TeamSection;\n"]}]}