kBAAkB,CACpC,CAAC,UAAU,CAAC,CAAC","sourcesContent":["import { connect, Dispatch } from 'react-redux';\n\nimport { StoreState } from '@src/types';\nimport Onboarding, { StateProps, DispatchProps } from './Onboarding';\nimport {\n  preferencesUpdate, selectRsbOnboarding,\n  selectShownOnboardingSections, selectAllOnboardingSteps,\n} from '@src/ducks/me';\nimport { selectDraft, selectDraftForThread } from '@src/ducks/drafts';\n\nconst mapStateToProps = (state: StoreState): StateProps => ({\n  accounts: state.accounts,\n  features: state.features,\n  preferences: state.preferences,\n  onboarding: state.onboarding,\n  currentStep: selectRsbOnboarding(state),\n  isDraft: state.selectedDraft\n    ? !!selectDraft(state, state.selectedDraft).data\n    : state.selectedThread && !!selectDraftForThread(state, state.selectedThread.id).data,\n  displayedSections: selectShownOnboardingSections(state),\n  allSteps: selectAllOnboardingSteps(state),\n  muteSounds: state.muteSounds,\n});\n\nconst mapDispatchToProps = (dispatch: Dispatch<StoreState>): DispatchProps => ({\n  updatePreferences: preferences => dispatch(preferencesUpdate(preferences)),\n  dispatchAction: action => dispatch(action),\n});\n\nexport default connect<StateProps, DispatchProps, {}>(\n  mapStateToProps, mapDispatchToProps,\n)(Onboarding);\n"]}]}