I,CAAC,SAAS,IAAI,CAAC,QAAQ,IAAI,kCAClC,SAAS,EAAC,OAAO,EACjB,IAAI,EAAE,aAAa,EACnB,GAAG,EAAE,IAAI,CAAC,SAAS,EACnB,KAAK,EAAE,OAAO,EACd,QAAQ,EAAE,IAAI,CAAC,iBAAiB,EAChC,MAAM,EAAE,IAAI,CAAC,UAAU,EACvB,SAAS,EAAE,IAAI,CAAC,aAAa,EAC7B,WAAW,EAAE,WAAW,GACxB,CACE,CACP,CAAC;IACJ,CAAC;IACH,oBAAC;AAAD,CAAC,AA9HD,CAA4B,KAAK,CAAC,aAAa,GA8H9C;AAED,eAAe,aAAa,CAAC","sourcesContent":["import React from 'react';\nimport classnames from 'classnames';\n\nimport Edit from '@src/components/Icons/Edit.svg';\n\nimport './EditableInput.scss';\nimport { KeyCode } from '@src/types';\nimport { isPrimaryModifierKey } from '@src/lib/hotkeys';\n\ninterface Props {\n  className: string;\n  text: string;\n  onSet: (property: string, text: string) => void;\n  numberOfLines?: number;\n  placeholder?: string;\n  property: string;\n  onBlur?: () => void;\n  readOnly?: boolean;\n  showIfEmpty?: boolean;\n  onTab?: (e: React.KeyboardEvent<HTMLElement>) => void;\n  onChange?: (property: string, text: string) => void;\n}\n\ninterface State {\n  edit: boolean;\n  newText: string;\n}\n\nclass EditableInput extends React.PureComponent<Props, State> {\n  private input: any;\n\n  constructor() {\n    super();\n\n    this.state = {\n      edit: false,\n      newText: '',\n    };\n  }\n\n  UNSAFE_componentWillReceiveProps(nextProps: Props) {\n    if (this.props.text !== nextProps.text && this.state.newText !== nextProps.text) {\n      this.setState({ newText: nextProps.text });\n    }\n  }\n\n  componentDidMount() {\n    this.setState({ newText: this.props.text });\n  }\n\n  edit = () => {\n    this.setState({ edit: true }, () => {\n      this.input.selectionStart = this.input.selectionEnd = this.input.value.length;\n      this.input.focus();\n    });\n  }\n\n  handleInputChange = () => {\n    this.setState({ newText: this.input.value }, () => {\n      if (this.props.onChange) {\n        this.props.onChange(this.props.property, this.state.newText);\n      }\n    });\n  }\n\n  handleKeyDown = (e: any) => {\n    const { onTab, className } = this.props;\n    switch (e.keyCode) {\n      case KeyCode.Enter:\n        // Do not capture if hitting enter on event note and meta key not enabled\n        if (className === 'event-notes' && !(isPrimaryModifierKey(e))) {\n          return;\n        }\n\n        this.set();\n        break;\n      case KeyCode.Tab:\n        if (onTab) {\n          e.preventDefault();\n          e.stopPropagation();\n          e.persist();\n          onTab(e);\n        }\n        break;\n      default:\n        break;\n    }\n  }\n\n  set = () => {\n    const { property } = this.props;\n    const { newText } = this.state;\n\n    this.setState({ edit: false });\n    this.props.onSet(property, newText);\n  }\n\n  handleBlur = () => {\n    const { onBlur } = this.props;\n\n    if (onBlur) {\n      onBlur();\n    }\n\n    this.set();\n  }\n\n  handleRef = (ref: any) => {\n    this.input = ref;\n  }\n\n  render() {\n    const { className, text, numberOfLines, placeholder, readOnly, showIfEmpty } = this.props;\n    const { edit, newText } = this.state;\n    const showInput = !numberOfLines || numberOfLines < 2;\n    const showPlaceholder = text.length === 0;\n\n    return (\n      <div\n        className={classnames('editable-input', className, {\n          edit,\n          'always-show': showIfEmpty,\n          placeholder: showPlaceholder,\n          'read-only': readOnly,\n        })}\n      >\n        <button className=\"text\" onClick={readOnly ? undefined : this.edit}>\n          <span className=\"copy\">{showPlaceholder ? placeholder : text}</span>\n          <Edit className=\"icon\"/>\n        </button>\n        {edit && showInput && !readOnly && <input\n          dir=\"auto\"\n          className=\"input\"\n          type=\"text\"\n          ref={this.handleRef}\n          value={newText}\n          onChange={this.handleInputChange}\n          onBlur={this.handleBlur}\n          onKeyDown={this.handleKeyDown}\n          placeholder={placeholder}\n        />}\n        {edit && !showInput && !readOnly && <textarea\n          className=\"input\"\n          rows={numberOfLines}\n          ref={this.handleRef}\n          value={newText}\n          onChange={this.handleInputChange}\n          onBlur={this.handleBlur}\n          onKeyDown={this.handleKeyDown}\n          placeholder={placeholder}\n        />}\n      </div>\n    );\n  }\n}\n\nexport default EditableInput;\n"]}]}