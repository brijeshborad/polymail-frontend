B,gCACE,OAAO,EAAE,KAAI,CAAC,mBAAmB,gBACrB,OAAO,EACnB,SAAS,EAAC,MAAM,EAChB,GAAG,EAAE,OAAO,IAEX,OAAO,CACD,CACV,EATuB,CASvB,CAAC,CACM,CACX,CAAC;IACJ,CAAC;IACH,mBAAC;AAAD,CAAC,AApDD,CAA2B,KAAK,CAAC,aAAa,GAoD7C;AAED,eAAe,YAAY,CAAC","sourcesContent":["import React from 'react';\n\nimport Popover from '@src/components/Popover/Popover';\n\nimport './CalendarMenu.scss';\n\nexport interface Props {\n  options: CalendarOption[];\n  onSelect: (key: CalendarOption) => void;\n  onHide: () => void;\n}\n\nexport const enum CalendarOption {\n  CalendarInvite = 'Send Calendar Invite',\n  SchedulingUrl = 'Share Available Meeting Times',\n}\n\nclass CalendarMenu extends React.PureComponent<Props> {\n  private popover: Popover;\n\n  componentDidMount() {\n    this.popover.show();\n  }\n\n  handleRef = (ref: Popover) => {\n    this.popover = ref;\n  }\n\n  handleSettingSelect = (e: React.SyntheticEvent<HTMLElement>) => {\n    const { onSelect, onHide } = this.props;\n    const title = e.currentTarget.dataset && e.currentTarget.dataset.title;\n\n    if (title) {\n      switch (title) {\n        case CalendarOption.CalendarInvite:\n          onSelect(CalendarOption.CalendarInvite);\n          break;\n        case CalendarOption.SchedulingUrl:\n          onSelect(CalendarOption.SchedulingUrl);\n          break;\n        default:\n          break;\n      }\n    }\n\n    onHide();\n  }\n\n  render() {\n    const { options } = this.props;\n    return (\n      <Popover\n        ref={this.handleRef}\n        className=\"calendar-menu\"\n        onHide={this.props.onHide}\n      >\n        {options.map(setting => (\n          <button\n            onClick={this.handleSettingSelect}\n            data-title={setting}\n            className=\"cell\"\n            key={setting}\n          >\n            {setting}\n          </button>\n        ))}\n      </Popover>\n    );\n  }\n}\n\nexport default CalendarMenu;\n"]}]}