ACrB;QAED,IAAI,IAAI,CAAC,KAAK,YAAY,WAAW,EAAE;YACrC,IAAI,CAAC,KAAK,CAAC,KAAK,EAAE,CAAC;SACpB;IACH,CAAC;IA2BD,mCAAM,GAAN;QACE,OAAO,CACL,oBAAC,OAAO,IACN,GAAG,EAAE,IAAI,CAAC,gBAAgB,EAC1B,SAAS,EAAC,sBAAsB,EAChC,MAAM,EAAE,IAAI,CAAC,YAAY,EACzB,MAAM,EAAE,IAAI,CAAC,UAAU,EACvB,OAAO,QACP,UAAU;YAEV,6BAAK,SAAS,EAAC,SAAS;gBACtB,6BAAK,SAAS,EAAC,OAAO;;oBAEpB,gCAAQ,OAAO,EAAE,IAAI,CAAC,YAAY;wBAChC,oBAAC,KAAK,IAAC,KAAK,EAAC,MAAM,EAAC,MAAM,EAAC,MAAM,GAAG,CAC7B,CACL;gBACN,8BAAM,SAAS,EAAC,MAAM,EAAC,QAAQ,EAAE,IAAI,CAAC,YAAY;oBAChD,6BAAK,SAAS,EAAC,SAAS,qFAElB;oBACN,6BAAK,SAAS,EAAC,OAAO;wBACpB,+BACE,WAAW,EAAC,mBAAmB,EAC/B,GAAG,EAAE,IAAI,CAAC,cAAc,GACxB,CACE;oBACN,6BAAK,SAAS,EAAC,gBAAgB;wBAC7B,gCACE,SAAS,EAAC,gBAAgB,EAC1B,OAAO,EAAE,IAAI,CAAC,cAAc,EAC5B,IAAI,EAAC,QAAQ,eAGN,CACL,CACD,CACH,CACE,CACX,CAAC;IACJ,CAAC;IACH,yBAAC;AAAD,CAAC,AAxFD,CAAiC,KAAK,CAAC,aAAa,GAwFnD;AAED,eAAe,kBAAkB,CAAC","sourcesContent":["import React from 'react';\n\nimport './PhoneNumberPopover.scss';\nimport Popover from '@src/components/Popover/Popover';\nimport Close from '@src/components/Icons/Close.svg';\n\nexport interface Props {\n  onContinue: (phone: string) => void;\n  onCancel: () => void;\n}\n\nclass PhoneNumberPopover extends React.PureComponent<Props> {\n  popover: Popover;\n  input: HTMLInputElement;\n\n  handlePopoverRef = (ref: Popover) => {\n    this.popover = ref;\n  }\n\n  handleInputRef = (ref: HTMLInputElement) => {\n    this.input = ref;\n  }\n\n  componentDidMount() {\n    if (this.popover) {\n      this.popover.show();\n    }\n\n    if (this.input instanceof HTMLElement) {\n      this.input.focus();\n    }\n  }\n\n  handleShow = () => {\n    if (this.input instanceof HTMLInputElement) {\n      this.input.focus();\n    }\n  }\n\n  handleCancel = () => {\n    const { onCancel } = this.props;\n    onCancel();\n  }\n\n  handleSubmit = (e: React.FormEvent<any>): boolean => {\n    e.preventDefault();\n    e.stopPropagation();\n    this.handleContinue();\n    return false;\n  }\n\n  handleContinue = () => {\n    const { onContinue } = this.props;\n    if (onContinue) {\n      onContinue(this.input.value);\n    }\n  }\n\n  render() {\n    return (\n      <Popover\n        ref={this.handlePopoverRef}\n        className=\"phone-number-popover\"\n        onHide={this.handleCancel}\n        onShow={this.handleShow}\n        overlay\n        persistent\n      >\n        <div className=\"confirm\">\n          <div className=\"title\">\n            Enter your phone number\n            <button onClick={this.handleCancel}>\n              <Close width=\"10px\" height=\"10px\" />\n            </button>\n          </div>\n          <form className=\"body\" onSubmit={this.handleSubmit}>\n            <div className=\"message\">\n              Please enter the phone number that we should send your authentication code to.\n            </div>\n            <div className=\"input\">\n              <input\n                placeholder=\"+1 (123) 456-7890\"\n                ref={this.handleInputRef}\n              />\n            </div>\n            <div className=\"action-buttons\">\n              <button\n                className=\"confirm-button\"\n                onClick={this.handleContinue}\n                type=\"submit\"\n              >\n                Continue\n              </button>\n            </div>\n          </form>\n        </div>\n      </Popover>\n    );\n  }\n}\n\nexport default PhoneNumberPopover;\n"]}]}