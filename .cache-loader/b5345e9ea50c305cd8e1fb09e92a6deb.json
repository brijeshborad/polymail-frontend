ACH,CACX,CAAC;IACJ,CAAC;IACH,cAAC;AAAD,CAAC,AAtED,CAAsB,KAAK,CAAC,aAAa,GAsExC;AAED,eAAe,OAAO,CAAC","sourcesContent":["import React from 'react';\nimport classnames from 'classnames';\nimport Popover from '@src/components/Popover/Popover';\nimport './Tooltip.scss';\n\ninterface Props {\n  className?: string;\n  top?: string;\n  left?: string;\n  bottom?: string;\n  right?: string;\n  copy: string;\n  ref?: (ref: Popover) => void;\n  delay?: number;\n  anchorBottom?: boolean;\n  hoverChild?: JSX.Element;\n  multiline?: boolean;\n  maxWidth?: string;\n}\n\nclass ToolTip extends React.PureComponent<Props> {\n  popover: Popover;\n  timeout: NodeJS.Timer;\n\n  constructor() {\n    super();\n  }\n\n  componentDidMount() {\n    const { delay } = this.props;\n    if (!delay) {\n      this.popover.show();\n      return;\n    }\n\n    this.timeout = global.setTimeout(\n      () => {\n        if (this.popover) {\n          this.popover.show();\n        }\n      },\n      delay,\n    );\n  }\n\n  componentWillUnmount() {\n    if (this.timeout) {\n      clearTimeout(this.timeout);\n    }\n  }\n\n  handleRef = (ref: Popover) => {\n    this.popover = ref;\n  }\n\n  render() {\n    const {\n      top,\n      left,\n      copy,\n      className,\n      bottom,\n      right,\n      anchorBottom,\n      hoverChild,\n      multiline,\n      maxWidth,\n    } = this.props;\n    return (\n      <Popover\n        className={classnames('tooltip', className, {\n          'anchor-bottom': !!anchorBottom,\n          multiline,\n        })}\n        style={{\n          top,\n          left,\n          bottom,\n          right,\n          maxWidth,\n        }}\n        ref={this.handleRef}\n        fadesOut\n        autoFocusOff\n      >\n        {!hoverChild && <span>{copy}</span>}\n        {hoverChild}\n      </Popover>\n    );\n  }\n}\n\nexport default ToolTip;\n"]}]}