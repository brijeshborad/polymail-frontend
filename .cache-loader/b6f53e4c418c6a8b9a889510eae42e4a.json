AU,IAAI,UAAU,EAAE;gBAC1B,CAAC,YAAY,IAAI,CAChB,6BAAK,SAAS,EAAC,gBAAgB;oBAC7B;wBACE,oBAAC,cAAc,IACb,SAAS,EAAC,QAAG,GACb;wBACF,oBAAC,cAAc,IACb,SAAS,EAAC,QAAG,GACb;wBACF,gDAAwB,CACpB;oBACN;wBACE,oBAAC,cAAc,IACb,SAAS,EAAC,QAAG,GACb;wBACF,8CAAsB,CAClB;oBACN;wBACE,oBAAC,cAAc,IACb,SAAS,EAAC,cAAS,GACnB;wBACF,+CAAuB,CACnB,CACF,CACP;gBACA,YAAY,IAAI,YAAY,EAAE,CAC3B,CACE,CACX,CAAC;IACJ,CAAC;IACH,qBAAC;AAAD,CAAC,AAvYD,CAA6B,KAAK,CAAC,aAAa,GAuY/C;AAED,eAAe,cAAc,CAAC","sourcesContent":["import React from 'react';\nimport classnames from 'classnames';\nimport Popover from '@src/components/Popover/Popover';\n\nimport { KeyCode, CommandControlType, OnboardingType } from '@src/types';\nimport { getHotkey, HotkeyAction } from '@src/lib/hotkeys';\n\nimport './ControlPopover.scss';\nimport KeyboardSymbol from '@src/components/Base/KeyboardSymbol/KeyboardSymbol';\nimport { typeaheadRegex } from '@src/lib/typeahead';\nimport ManageButton from './ManageButton/ManageButton';\nimport { AmplitudeEvents, logAmplitudeEvent } from '@src/lib/amplitude-analytics';\n\ninterface Props {\n  icon?: JSX.Element;\n  heading?: string;\n  cancelToggle?: JSX.Element;\n  options: Option[];\n  placeholder: string;\n  className?: string;\n  content?: JSX.Element;\n  onHide: () => void;\n  type: CommandControlType;\n  showOnMount?: boolean;\n  hideInput?: boolean;\n\n  onSelect?: (option: any, index: number) => void;\n  onShow?: () => void;\n  onInputChange?: (value: string) => void;\n  onInputSubmit?: (value: string) => void;\n  onSelectedIndexChange?: (index: number) => void;\n  renderPreview?: () => JSX.Element;\n  renderFooter?: () => JSX.Element;\n  renderMain?: () => JSX.Element;\n  style?: any;\n  allowManage?: boolean;\n\n  // Helper props when passing in a custom input field\n  input?: JSX.Element;\n  customInputValue?: string;\n\n  rsbOnboarding?: OnboardingType;\n}\n\nexport interface Option {\n  title: string;\n  desc?: any;\n  icon?: JSX.Element;\n  customDescRenderer?: any;\n}\n\ninterface State {\n  value: string;\n  selectedIndex: number;\n  show: boolean;\n}\n\n// TODO(SHIN): Update hotkey mechanism. Currently listens to document.body (capture)\n// This gets events before they bubble up and ignores if the popover is not showing\nclass ControlPopover extends React.PureComponent<Props, State> {\n  private popover: Popover;\n  private input?: HTMLInputElement;\n  private optionsDiv?: HTMLDivElement;\n  private timeout: NodeJS.Timer;\n  private ref: HTMLDivElement;\n\n  constructor(props: Props) {\n    super();\n\n    this.state = {\n      value: '',\n      selectedIndex: 0,\n      show: props.showOnMount || false,\n    };\n  }\n\n  componentDidMount() {\n    if (this.props.showOnMount) {\n      this.show();\n    }\n  }\n\n  componentWillUnmount() {\n    if (this.timeout) {\n      clearTimeout(this.timeout);\n    }\n  }\n\n  UNSAFE_componentWillReceiveProps(nextProps: Props) {\n    if (this.props.options !== nextProps.options) {\n      this.setState({ selectedIndex: 0 });\n      return;\n    }\n  }\n\n  componentDidUpdate(prevProps: Props, prevState: State) {\n    const { onSelectedIndexChange } = this.props;\n\n    if (prevState.selectedIndex !== this.state.selectedIndex && onSelectedIndexChange) {\n      onSelectedIndexChange(this.state.selectedIndex);\n      const preview = this.ref && this.ref.getElementsByClassName('preview')[0];\n\n      if (preview) {\n        preview.scrollTop = 0;\n      }\n    }\n\n    // Helps maintain selection\n    const { selectedIndex } = this.state;\n    if (selectedIndex && selectedIndex !== prevState.selectedIndex && this.optionsDiv) {\n      const element = this.optionsDiv.querySelector(`#option-${selectedIndex}`);\n      if (element) {\n        const cellBox = element.getBoundingClientRect();\n        const cellTop = cellBox.height * (selectedIndex);\n        const cellBottom = cellBox.height + (cellBox.height * selectedIndex);\n\n        const listBox = this.optionsDiv.getBoundingClientRect();\n        const listTop = this.optionsDiv.scrollTop;\n        const listBottom = listBox.height + this.optionsDiv.scrollTop;\n\n        if (cellTop < listTop) {\n          this.optionsDiv.scrollTop = cellTop;\n          return;\n        }\n\n        if (cellBottom > listBottom) {\n          this.optionsDiv.scrollTop = cellBottom - listBox.height;\n          return;\n        }\n      }\n    }\n  }\n\n  // Uses Global Event listener on document.body, on capture instead of bubble.\n  handleKeyDown = (e: React.KeyboardEvent<HTMLElement>) => {\n    const { selectedIndex, show } = this.state;\n    const { options, onSelect } = this.props;\n\n    let handled = false;\n\n    if (!show) {\n      return;\n    }\n\n    // If popover is showing do not propogate\n    e.stopPropagation();\n\n    const hotkey = getHotkey(e);\n    if (hotkey && hotkey.name === HotkeyAction.ToggleCommandControl) {\n      this.toggle();\n      e.preventDefault();\n      e.stopPropagation();\n      return;\n    }\n\n    switch (e.keyCode) {\n      case KeyCode.Up:\n        handled = true;\n        let index = selectedIndex;\n        if (selectedIndex === 0) {\n          index = options.length;\n        }\n        this.setState({ selectedIndex: index - 1 });\n        break;\n      case KeyCode.Down:\n        handled = true;\n        this.setState({ selectedIndex: (selectedIndex + 1) % options.length });\n        break;\n      case KeyCode.Esc:\n        handled = true;\n        this.hide();\n        break;\n      case KeyCode.Enter:\n        if (onSelect) {\n          handled = true;\n          this.handleSelect();\n          this.hide();\n        }\n        break;\n      default:\n        break;\n    }\n\n    if (handled) {\n      e.stopPropagation();\n      e.preventDefault();\n    }\n  }\n\n  handleOptionsRef = (ref: HTMLDivElement) => {\n    this.optionsDiv = ref;\n  }\n\n  handleInputChange = (e: React.FormEvent<HTMLInputElement>) => {\n    const { onInputChange } = this.props;\n    this.setState(\n      {\n        value: e.currentTarget.value,\n        selectedIndex: 0,\n      },\n      () => {\n        if (onInputChange) {\n          onInputChange(this.state.value);\n        }\n      },\n    );\n  }\n\n  handleSubmit = (e: React.FormEvent<HTMLElement>) => {\n    e.preventDefault();\n    if (this.props.onInputSubmit) {\n      this.props.onInputSubmit(this.state.value);\n    }\n    this.setState({ value: '' });\n    if (this.input) {\n      this.input.value = '';\n    }\n  }\n\n  hide = () => {\n    this.setState({ show: false }, () => {\n      this.popover.hide();\n    });\n  }\n\n  show = () => {\n    this.setState({ show: true }, () => {\n      this.timeout = global.setTimeout(\n        () => {\n          if (this.input) {\n            this.input.focus();\n          }\n\n          if (this.props.onShow) {\n            this.props.onShow();\n          }\n        },\n        0,\n      );\n    });\n    this.popover.show();\n  }\n\n  toggle = () => {\n    const { show } = this.state;\n\n    if (show) {\n      this.hide();\n    } else {\n      this.show();\n    }\n  }\n\n  handleInputRef = (ref: HTMLInputElement) => {\n    this.input = ref;\n  }\n\n  handlePopoverRef = (ref: Popover) => {\n    this.popover = ref;\n  }\n\n  handleOptionMouseEnter = (e: React.MouseEvent<HTMLElement>) => {\n    const index = e.currentTarget.id && e.currentTarget.id.replace('option-', '');\n\n    if (index) {\n      this.setState({ selectedIndex: parseInt(index, 10) });\n    }\n  }\n\n  handleSelect = () => {\n    const { options, onSelect, rsbOnboarding } = this.props;\n    const { selectedIndex } = this.state;\n\n    const selectedOptionTitle = options[selectedIndex].title;\n    if (selectedOptionTitle === 'Share Available Meeting Times'\n        && rsbOnboarding === OnboardingType.Scheduling)  {\n      logAmplitudeEvent(AmplitudeEvents.TourStepTwelve);\n    }\n\n    if (onSelect) {\n      onSelect(selectedOptionTitle, selectedIndex);\n      this.hide();\n    }\n  }\n\n  // Renders options when highlighting\n  renderOption = (option: Option, index: number) => {\n    const { customInputValue } = this.props;\n    const { selectedIndex } = this.state;\n    const stateValue = this.state.value;\n\n    const value = customInputValue || stateValue;\n\n    const regex = typeaheadRegex(value, true);\n    const match = option.title.match(regex);\n    let matches;\n\n    if (value.length && match && match.length > 0) {\n      const titleLowercase = option.title.toLowerCase();\n      const valueLowercase = value.toLowerCase();\n      let start = 0;\n      let end = titleLowercase.indexOf(valueLowercase[0]);\n      matches = [\n        option.title.slice(start, end),\n        <strong key={`${index}-strong`}>{option.title[end]}</strong>,\n      ];\n\n      for (let i = 1; i < value.length; i += 1) {\n        start = end + 1;\n        end = titleLowercase.indexOf(valueLowercase[i], start);\n        matches.push(option.title.slice(start, end));\n        matches.push(<strong>{option.title[end]}</strong>);\n      }\n\n      if (end !== option.title.length - 1) {\n        matches.push(option.title.slice(end + 1));\n      }\n    }\n\n    return (\n      <button\n        id={`option-${index}`}\n        className={classnames('option', {\n          selected: selectedIndex === index,\n        })}\n        key={`${index}-option`}\n        onMouseEnter={this.handleOptionMouseEnter}\n        onClick={this.handleSelect}\n      >\n        {!!option.icon && <div className=\"icon\" key={`${index}-icon`}>{option.icon}</div>}\n        <div className=\"title\" key={`${index}-title`}>{matches || option.title}</div>\n        <div className=\"desc\" key={`${index}-desc`}>\n          {option.customDescRenderer ? option.customDescRenderer(option.desc) : option.desc}\n        </div>\n      </button>\n    );\n  }\n\n  handleRef = (ref: HTMLDivElement) => {\n    this.ref = ref;\n  }\n\n  render() {\n    const {\n      icon,\n      heading,\n      options,\n      content,\n      className,\n      placeholder,\n      input,\n      onHide,\n      renderPreview,\n      renderFooter,\n      renderMain,\n      hideInput,\n      style,\n      cancelToggle,\n      allowManage,\n      type,\n    } = this.props;\n    const { value } = this.state;\n\n    return (\n      <Popover\n        ref={this.handlePopoverRef}\n        className={classnames('control-popover', className)}\n        onKeyDown={this.handleKeyDown}\n        onHide={onHide}\n        style={style}\n        fadesOut\n        overlay\n      >\n        <div className=\"container\" ref={this.handleRef}>\n          {!!heading && (\n            <div className=\"control-header\">\n              {icon &&\n                icon\n              }\n              <div className=\"title\">\n                {heading}\n              </div>\n              {cancelToggle || null}\n              {allowManage && (\n                <ManageButton\n                  type={type}\n                  onClick={this.hide}\n                />\n              )}\n            </div>\n          )}\n          {content}\n          {!hideInput && <div className=\"input-container\">\n            {!input && <form\n              onSubmit={this.handleSubmit}\n            >\n              <input\n                dir=\"auto\"\n                className=\"control-input\"\n                placeholder={placeholder}\n                ref={this.handleInputRef}\n                onChange={this.handleInputChange}\n                value={value}\n                spellCheck={false}\n              />\n            </form>}\n            {input}\n          </div>}\n          {!renderMain && <div className=\"main\">\n            <div\n              className=\"options\"\n              ref={this.handleOptionsRef}\n            >\n              {options.map((option, i) => this.renderOption(option, i))}\n            </div>\n            {!!renderPreview && <div className=\"preview\">\n              {renderPreview()}\n            </div>}\n          </div>}\n          {renderMain && renderMain()}\n          {!renderFooter && (\n            <div className=\"control-footer\">\n              <div>\n                <KeyboardSymbol\n                  character=\"⬆\"\n                />\n                <KeyboardSymbol\n                  character=\"⬇\"\n                />\n                <span>to navigate</span>\n              </div>\n              <div>\n                <KeyboardSymbol\n                  character=\"↵\"\n                />\n                <span>to select</span>\n              </div>\n              <div>\n                <KeyboardSymbol\n                  character=\"⎋ (esc)\"\n                />\n                <span>to dismiss</span>\n              </div>\n            </div>\n          )}\n          {renderFooter && renderFooter()}\n        </div>\n      </Popover>\n    );\n  }\n}\n\nexport default ControlPopover;\n"]}]}