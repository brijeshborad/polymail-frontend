,CAAC,MAAM,CAAC,CAAC,CAAC,MAAM,CAAC,IAAI,CAAC;YACrE,OAAO,CACL,gCACE,SAAS,EAAE,UAAU,CACnB,2BAA2B,EAC3B,EAAE,QAAQ,EAAE,aAAa,KAAK,KAAK,EAAE,CACtC,EACD,GAAG,EAAE,UAAU,gBACH,KAAK,EACjB,OAAO,EAAE,WAAW,EACpB,YAAY,EAAE,gBAAgB;gBAE9B,8BAAM,SAAS,EAAC,SAAS,IAAE,UAAU,CAAC,CAAC,CAAC,CAAC,WAAW,EAAE,GAAG,UAAU,CAAC,KAAK,CAAC,CAAC,CAAC,CAAQ,CAC7E,CACV,CAAC;QACJ,CAAC,CAAC,CACE,CACF,CACP,CAAC;AACJ,CAAC,CAAC;AAEF,eAAe,SAAS,CAAC","sourcesContent":["import React from 'react';\nimport classnames from 'classnames';\nimport { ThreadBox, List, MailboxType } from '@src/types';\n\nexport interface Props {\n  onSuggestionMouseEnter: (component: string, index: number) => void;\n  onSelect: (index: number) => void;\n  options: Array<List|ThreadBox|MailboxType>;\n  selectedIndex?: number;\n}\n\nconst BoxSearch: React.SFC<Props> = ({\n  onSuggestionMouseEnter, onSelect, options, selectedIndex,\n}) => {\n  const handleClick = (e: React.SyntheticEvent<HTMLButtonElement>) => {\n    const index = e.currentTarget.dataset.index;\n\n    if (index) {\n      onSelect(parseInt(index, 10));\n    }\n  };\n  const handleMouseEnter = (e: React.SyntheticEvent<HTMLButtonElement>) => {\n    const index = e.currentTarget.dataset.index;\n\n    if (index) {\n      onSuggestionMouseEnter('contact', parseInt(index, 10));\n    }\n  };\n\n  if (!options.length) {\n    return <div />;\n  }\n\n  return (\n    <div className=\"box-search suggestion-category\">\n      <div className=\"heading\">\n        <span>Labels</span>\n        <span className=\"separator\" />\n      </div>\n      <div className=\"suggestions\">\n        {options.map((option, index) => {\n          const optionName = typeof option === 'string' ? option : option.name;\n          return (\n            <button\n              className={classnames(\n                'box-suggestion suggestion',\n                { selected: selectedIndex === index },\n              )}\n              key={optionName}\n              data-index={index}\n              onClick={handleClick}\n              onMouseEnter={handleMouseEnter}\n            >\n              <span className=\"heading\">{optionName[0].toUpperCase() + optionName.slice(1)}</span>\n            </button>\n          );\n        })}\n      </div>\n    </div>\n  );\n};\n\nexport default BoxSearch;\n"]}]}