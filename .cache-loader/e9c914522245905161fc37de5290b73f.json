wserHistory';\nimport NavigationHeader from '@src/routes/Settings/components/NavigationHeader';\nimport AsanaLogo from '@src/assets/Asana.png';\nimport asanaScreenShot from './assets/asana-screenshot@3x.png';\nimport asanaScreenShot2 from './assets/asana-screenshot2@3x.png';\nimport { Team, Integration, IntegrationStatus } from '@src/types';\n\nimport './Asana.scss';\nimport DropdownCell from '@src/routes/Settings/components/DropdownCell';\nimport { AsanaWorkspace } from '@src/types/asana';\n\nexport interface StateProps {\n  asanaIntegration?: Integration;\n  team: Team;\n  refreshIntegrations: boolean;\n  workspaces: AsanaWorkspace[];\n}\n\nexport interface DispatchProps {\n  loginAsana: (team: string) => void;\n  logoutAsana: () => void;\n  listIntegrations: () => void;\n  updateAsana: (workspace: string) => void;\n  listWorkspaces: () => void;\n}\n\nclass Asana extends React.PureComponent<StateProps & DispatchProps> {\n  componentDidMount() {\n    const { listWorkspaces } = this.props;\n\n    listWorkspaces();\n  }\n\n  handleOnBack = () => {\n    browserHistory.replace('/settings/integrations');\n  }\n\n  UNSAFE_componentWillReceiveProps(nextProps: StateProps & DispatchProps) {\n    if (nextProps.refreshIntegrations && !this.props.refreshIntegrations) {\n      nextProps.listIntegrations();\n    }\n  }\n\n  handleInstall = () => {\n    const { loginAsana, team } = this.props;\n\n    if (team) {\n      loginAsana(team.id);\n    }\n  }\n\n  handleUninstall = () => {\n    this.props.logoutAsana();\n  }\n\n  handleWorkspaceSelect = (value: string, index: number, id?: string) => {\n    const { updateAsana, workspaces, asanaIntegration } = this.props;\n    const selectedWorkspace = asanaIntegration && asanaIntegration.asanaWorkspace;\n\n    if (!selectedWorkspace) {\n      return;\n    }\n\n    if (workspaces[index] && workspaces[index].id !== selectedWorkspace) {\n      updateAsana(workspaces[index].id);\n    }\n  }\n\n  render() {\n    const { asanaIntegration, workspaces } = this.props;\n    const lineTwo = 'Provide your team with context by ' +\n      'linking new Asana tasks to the email conversation they originated from.';\n    const lineThree = 'Display Asana tasks and their statuses '\n      + 'within the email conversation they originated from';\n    const workspaceNames = workspaces.map(w => w.name);\n    const selectedWorkspace = asanaIntegration && asanaIntegration.asanaWorkspace;\n\n    return (\n      <div className=\"settings-panel asana-integration\">\n        <NavigationHeader\n          title=\"Integrations\"\n          onBack={this.handleOnBack}\n          contextButton={<span className=\"context\">Asana</span>}\n        />\n        <div className=\"header\">\n          <div className=\"logo\">\n            <img src={AsanaLogo} />\n          </div>\n          <h1>Asana</h1>\n          <p className=\"description\">Create and view Asana tasks from conversations in Polymail</p>\n          {!asanaIntegration &&\n            <button className=\"install\" onClick={this.handleInstall}>Install</button>}\n          {asanaIntegration && asanaIntegration.status !== IntegrationStatus.Ok &&\n            <button className=\"install\" onClick={this.handleInstall}>Reauthorize</button>}\n          {asanaIntegration && asanaIntegration.status === IntegrationStatus.Ok &&\n            <div className=\"enabled\">ENABLED</div>}\n          {asanaIntegration && asanaIntegration.status === IntegrationStatus.Ok &&\n            <button className=\"uninstall\" onClick={this.handleUninstall}>Uninstall</button>}\n        </div>\n        {selectedWorkspace && <div className=\"workspace\">\n          <DropdownCell\n            title=\"Asana Workspace\"\n            subtitle=\"The Asana workspace that Polymail will connect to.\"\n            onSelect={this.handleWorkspaceSelect}\n            values={workspaceNames || []}\n            startIndex={workspaces.findIndex(w => w.id === selectedWorkspace)}\n          />\n        </div>}\n        <div className=\"integration-details\">\n          <p className=\"details\">With the Asana Integration, you can:</p>\n          <ul>\n            <li>Create new Asana tasks from email conversations.</li>\n            <li>{lineTwo}</li>\n            <li>{lineThree}</li>\n          </ul>\n          <div className=\"screenshot-container\">\n            <img src={asanaScreenShot} className=\"screenshot\" />\n            <img src={asanaScreenShot2} className=\"screenshot\" />\n          </div>\n        </div>\n      </div>\n    );\n  }\n}\n\nexport default Asana;\n"]}]}