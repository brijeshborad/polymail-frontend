dleClick = (e: React.SyntheticEvent<HTMLButtonElement>) => {\n    const index = e.currentTarget.dataset.index;\n\n    if (index) {\n      onSelect(parseInt(index, 10));\n    }\n  };\n  const handleMouseEnter = (e: React.SyntheticEvent<HTMLButtonElement>) => {\n    const index = e.currentTarget.dataset.index;\n\n    if (index) {\n      onSuggestionMouseEnter('default', parseInt(index, 10));\n    }\n  };\n\n  return (\n    <div className=\"default-search\">\n      <div className=\"suggestion-category\">\n        <div className=\"heading\">\n          <span>Narrow your search</span>\n          <span className=\"separator\" />\n        </div>\n        <div className=\"suggestions\">\n          {!!options.modifiers && options.modifiers.map((o, index) => (\n            <button\n              className={classnames(\n                'list-suggestion suggestion',\n                { selected: selectedIndex === index },\n              )}\n              onClick={handleClick}\n              onMouseEnter={handleMouseEnter}\n              key={index}\n              data-index={index}\n            >\n              <span className=\"heading\">{o.heading}</span>\n              <span className=\"description\">{o.list}</span>\n            </button>\n          ))}\n        </div>\n      </div>\n      <div className=\"suggestion-category\">\n        <div className=\"heading\">\n          <span>Contacts</span>\n          <span className=\"separator\" />\n        </div>\n        <div className=\"suggestions\">\n          {!!options.contacts && options.contacts.map((o, index) => (\n            <button\n              className={classnames(\n                'list-suggestion suggestion',\n                { selected: selectedIndex === index + options.modifiers.length },\n              )}\n              onClick={handleClick}\n              onMouseEnter={handleMouseEnter}\n              key={index}\n              data-index={index + options.modifiers.length}\n            >\n              <span className=\"heading\">{o.heading}</span>\n              <span className=\"description\">{o.list}</span>\n            </button>\n          ))}\n        </div>\n      </div>\n      <div className=\"suggestion-category\">\n        <div className=\"heading\">\n          <span>Date</span>\n          <span className=\"separator\" />\n        </div>\n        <div className=\"suggestions\">\n          {!!options.dates && options.dates.map((o, index) => (\n            <button\n              className={classnames(\n                'list-suggestion suggestion',\n                { selected: selectedIndex ===\n                  index + options.modifiers.length + options.contacts.length },\n              )}\n              onClick={handleClick}\n              onMouseEnter={handleMouseEnter}\n              key={index}\n              data-index={index + options.modifiers.length + options.contacts.length}\n            >\n              <span className=\"heading\">{o.heading}</span>\n              <span className=\"description\">{o.list}</span>\n            </button>\n          ))}\n        </div>\n      </div>\n    </div>\n  );\n};\n\nexport default DefaultSearch;\n"]}]}