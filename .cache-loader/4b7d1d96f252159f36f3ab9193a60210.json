AC,KAAK,CAAC,eAAe,KAAK,CAAC,EAC1C,SAAS,EAAE,0BAA0B,CAAC,UAAU,EAAE,CAAC,CAAC,GACpD,CACH,EAV2B,CAU3B,CAAC,CACE,CACP,EAdK,CAcL,CACM,CACL,CACP,CAAC;IACJ,CAAC;IACH,aAAC;AAAD,CAAC,AA9CD,CAAqB,KAAK,CAAC,aAAa,GA8CvC;AAED,eAAe,MAAM,CAAC","sourcesContent":["import React from 'react';\nimport { Spring } from 'react-spring';\nimport OnboardingLegendSection from './components/OnboardingLegendSection';\nimport {\n  OnboardingSection, onboardingSectionCompleted,\n  sectionForOnboardingType,\n} from '@src/lib/onboarding';\nimport { OnboardingType, Onboarding } from '@src/types';\n\nexport interface Props {\n  onboarding: Onboarding;\n  currentStep?: OnboardingType;\n  displayedSections: OnboardingSection[];\n  onOnboardingStepClick?: (type: OnboardingType) => void;\n}\n\nexport interface State {\n  expandedSection: OnboardingSection;\n}\n\nclass Legend extends React.PureComponent<Props, State> {\n  constructor(props: Props) {\n    super();\n    const { currentStep } = props;\n    this.state = {\n      expandedSection: currentStep\n        ? sectionForOnboardingType(currentStep)\n        : OnboardingSection.NavigatingPolymail,\n    };\n  }\n\n  toggleSection = (s: OnboardingSection) => {\n    this.setState({ expandedSection: s });\n  }\n\n  render() {\n    const { onOnboardingStepClick, onboarding, displayedSections } = this.props;\n    return (\n      <div className=\"onboarding-legend\">\n        <Spring\n          from={{\n            opacity: 0,\n          }}\n          to={{\n            opacity: 1,\n          }}\n        >\n          {p => (\n            <div className=\"container\">\n              {displayedSections.map(s => (\n                <OnboardingLegendSection\n                  key={s}\n                  styles={p}\n                  section={s}\n                  onClick={this.toggleSection}\n                  onStepClick={onOnboardingStepClick}\n                  expanded={this.state.expandedSection === s}\n                  completed={onboardingSectionCompleted(onboarding, s)}\n                />\n              ))}\n            </div>\n          )}\n        </Spring>\n      </div>\n    );\n  }\n}\n\nexport default Legend;\n"]}]}