cesTabHeader.scss';\n\ninterface Props {\n  isMobile?: boolean;\n  toggleSidebar: () => void;\n}\n\n// Header View at top of LSB\nconst PreferencesTabHeader = (props: Props) => {\n  const { isMobile, toggleSidebar } = props;\n\n  return (\n    <div className=\"preferences-tab-header\">\n        {!isMobile ?  (\n        <Link to={'/'}>\n          <div className={classNames('back-button', { desktop: !isMobile })}>\n            <button>\n              <RightArrow />\n            </button>\n            BACK\n          </div>\n        </Link>\n        ) : (\n          <div className=\"close-button\" onClick={toggleSidebar}>\n            <span><Close /></span>\n          </div>\n        )}\n      <div className=\"title\">\n        Settings\n      </div>\n    </div>\n  );\n};\n\nexport default PreferencesTabHeader;\n"]}]}