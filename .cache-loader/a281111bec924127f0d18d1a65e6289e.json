pe, Onboarding } from '@src/types';\n\nexport enum OnboardingSection {\n  NavigatingPolymail = 'Navigating Polymail',\n  ManagingYourEmail = 'Managing Emails',\n  Composing = 'Send Emails like a Pro',\n  PowerUser = 'Become a Power User',\n  ClaimCredit = 'Claim Your $10 Credit',\n  DownloadNativeApps = 'Download Native Apps',\n  Unknown = 'unknown',\n}\n\nexport const randomizedReinforcement = (int: number): string => {\n  const phrases = [\n    'Great job!',\n    'Doing great!',\n    'Amazing!',\n    'Perfect!',\n    'Nice job!',\n    'You\\'re a natural!',\n    'Awesome!',\n    'Excellent!',\n    'Brilliant!',\n    'Marvelous!',\n    'Fantastic Job!',\n  ];\n\n  return phrases[int] || phrases[int % phrases.length];\n};\n\nexport const hotkeyForOnboarding = (type: OnboardingType): HotkeyAction | undefined => {\n  switch (type) {\n    case OnboardingType.NextThread:\n      return HotkeyAction.NextThread;\n    case OnboardingType.PreviousThread:\n      return HotkeyAction.PreviousThread;\n    case OnboardingType.Archive:\n      return HotkeyAction.Archive;\n    case OnboardingType.Undo:\n      return HotkeyAction.Undo;\n    case OnboardingType.ReadLater:\n      return HotkeyAction.ReadLater;\n    case OnboardingType.Block:\n      return HotkeyAction.BlockSender;\n    case OnboardingType.CommandLater:\n      return HotkeyAction.ToggleCommandControl;\n    case OnboardingType.SendMessage:\n      return HotkeyAction.Send;\n    case OnboardingType.SendLater:\n      return HotkeyAction.SendLater;\n    case OnboardingType.FollowUpReminder:\n      return HotkeyAction.FollowUpReminder;\n    case OnboardingType.SaveAsTemplate:\n      return HotkeyAction.ToggleCommandControl;\n    case OnboardingType.MessageTemplates:\n      return HotkeyAction.MessageTemplates;\n    case OnboardingType.ShareThread:\n      return HotkeyAction.ToggleCommandControl;\n    case OnboardingType.Comment:\n      return HotkeyAction.Comment;\n    case OnboardingType.ActivityFeed:\n      return HotkeyAction.ToggleCommandControl;\n    case OnboardingType.Scheduling:\n      return HotkeyAction.ToggleCommandControl;\n    default:\n      return undefined;\n  }\n};\n\n// Returns corresponding section for Onboarding Type\nexport const sectionForOnboardingType = (type: OnboardingType): OnboardingSection => {\n  switch (type) {\n    case OnboardingType.NextThread:\n    case OnboardingType.PreviousThread:\n      return OnboardingSection.NavigatingPolymail;\n    case OnboardingType.Archive:\n    case OnboardingType.ReadLater:\n    case OnboardingType.Undo:\n      return OnboardingSection.ManagingYourEmail;\n    case OnboardingType.SendMessage:\n    case OnboardingType.SendLater:\n    case OnboardingType.FollowUpReminder:\n    case OnboardingType.MessageTemplates:\n    case OnboardingType.SaveAsTemplate:\n      return OnboardingSection.Composing;\n    case OnboardingType.ShareThread:\n    case OnboardingType.Comment:\n    case OnboardingType.Scheduling:\n      return OnboardingSection.PowerUser;\n    case OnboardingType.ClaimCredit:\n      return OnboardingSection.ClaimCredit;\n    case OnboardingType.DownloadNativeApps:\n      return OnboardingSection.DownloadNativeApps;\n    default:\n      return OnboardingSection.Unknown;\n  }\n};\n\n// Returns corresponding types for Onboarding Section\nexport const onboardingTypesForSection = (section: OnboardingSection): OnboardingType[] => {\n  switch (section) {\n    case OnboardingSection.NavigatingPolymail:\n      return [\n        OnboardingType.NextThread,\n      ];\n    case OnboardingSection.ManagingYourEmail:\n      return [\n        OnboardingType.Archive,\n        OnboardingType.Undo,\n        OnboardingType.ReadLater,\n      ];\n    case OnboardingSection.Composing:\n      return [\n        OnboardingType.SendMessage,\n        OnboardingType.SendLater,\n        OnboardingType.FollowUpReminder,\n        OnboardingType.MessageTemplates,\n        OnboardingType.SaveAsTemplate,\n      ];\n    case OnboardingSection.PowerUser:\n      return [\n        OnboardingType.ShareThread,\n        OnboardingType.Comment,\n        OnboardingType.Scheduling,\n      ];\n    case OnboardingSection.DownloadNativeApps:\n      return [\n        OnboardingType.DownloadNativeApps,\n      ];\n    case OnboardingSection.ClaimCredit:\n      return [\n        OnboardingType.ClaimCredit,\n      ];\n    case OnboardingSection.Unknown:\n    default:\n      return [];\n  }\n};\n\nexport const titleForOnboardingStep = (h: OnboardingType): string => {\n  let c = 'You Are Unstoppable! ðŸ‘';\n  if (h) {\n    switch (h) {\n      case OnboardingType.Archive:\n        c = 'Marking things as done';\n        break;\n      case OnboardingType.ReadLater:\n        c = 'Snoozing Emails';\n        break;\n      case OnboardingType.NextThread:\n        c = 'Moving between Threads';\n        break;\n      case OnboardingType.Undo:\n        c = 'Undoing Actions';\n        break;\n      case OnboardingType.Block:\n        c = 'Blocking Senders';\n        break;\n      case OnboardingType.CommandLater:\n        c = 'The Command Center';\n        break;\n\n      // Draft handlers\n      case OnboardingType.SendLater:\n        c = 'Scheduling Emails';\n        break;\n      case OnboardingType.FollowUpReminder:\n        c = 'Never miss a Follow up!';\n        break;\n      case OnboardingType.MessageTemplates:\n        c = 'Message Templates';\n        break;\n      case OnboardingType.SendMessage:\n        c = 'Sending Your First Message';\n        break;\n      case OnboardingType.SaveAsTemplate:\n        c = 'Save as Template';\n        break;\n      case OnboardingType.Complete:\n        c = 'Getting Started!';\n        break;\n      case OnboardingType.ShareThread:\n        c = 'Sharing Conversations';\n        break;\n\n      case OnboardingType.Comment:\n        c = 'Adding Comments';\n        break;\n\n      case OnboardingType.ActivityFeed:\n        c = 'Activity Feed';\n        break;\n\n      case OnboardingType.Scheduling:\n        c = 'Sharing Meeting Times';\n        break;\n\n      case OnboardingType.DownloadNativeApps:\n        c = 'Get the Polymail Desktop App';\n        break;\n\n      case OnboardingType.ClaimCredit:\n        c = 'Claim your $10 Credit';\n        break;\n      default:\n        c = '';\n        break;\n    }\n  }\n  return c;\n};\n\nexport const onboardingStepCompleted = (onboarding: Onboarding, type: OnboardingType): boolean => {\n  if (!onboarding.steps) {\n    return false;\n  }\n\n  switch (type) {\n    case OnboardingType.NextThread:\n      return onboarding.steps.nextThread;\n    case OnboardingType.PreviousThread:\n      return onboarding.steps.previousThread;\n    case OnboardingType.Archive:\n      return onboarding.steps.archive;\n    case OnboardingType.ReadLater:\n      return onboarding.steps.readLater;\n    case OnboardingType.Undo:\n      return onboarding.steps.undo;\n    case OnboardingType.SaveAsTemplate:\n      return onboarding.steps.saveAsTemplate;\n    case OnboardingType.SendLater:\n      return onboarding.steps.sendLater;\n    case OnboardingType.FollowUpReminder:\n      return onboarding.steps.followUpReminder;\n    case OnboardingType.MessageTemplates:\n      return onboarding.steps.messageTemplates;\n    case OnboardingType.ShareThread:\n      return onboarding.steps.shareThread;\n    case OnboardingType.Comment:\n      return onboarding.steps.comment;\n    case OnboardingType.Scheduling:\n      return onboarding.steps.scheduling;\n    case OnboardingType.ClaimCredit:\n      return onboarding.claimedCredit;\n    case OnboardingType.SendMessage:\n      return onboarding.sendMessage;\n    case OnboardingType.DownloadNativeApps:\n      return onboarding.downloadNativeApps;\n    default:\n      return false;\n  }\n};\n\nexport const onboardingSectionCompleted = (onboarding: Onboarding, section: OnboardingSection) => {\n  if (!onboarding.steps) { return false; }\n\n  switch (section) {\n    case OnboardingSection.NavigatingPolymail:\n      const { nextThread } = onboarding.steps;\n      return nextThread;\n    case OnboardingSection.ManagingYourEmail:\n      const { archive, readLater, undo } = onboarding.steps;\n      return archive && readLater && undo;\n    case OnboardingSection.Composing:\n      const { sendMessage } = onboarding;\n      const { sendLater, followUpReminder, messageTemplates } = onboarding.steps;\n      return sendMessage && sendLater && followUpReminder && messageTemplates;\n    case OnboardingSection.PowerUser:\n      const { shareThread, comment, scheduling } = onboarding.steps;\n      return shareThread && comment && scheduling;\n    case OnboardingSection.DownloadNativeApps:\n      const { downloadNativeApps } = onboarding;\n      return downloadNativeApps;\n    case OnboardingSection.ClaimCredit:\n      const { claimedCredit } = onboarding;\n      return claimedCredit;\n    default:\n      return false;\n  }\n};\n\nexport const onboardingRequiresThread = (type: OnboardingType) => {\n  switch (type) {\n    case OnboardingType.Archive:\n    case OnboardingType.ReadLater:\n    case OnboardingType.ShareThread:\n    case OnboardingType.Comment:\n      return true;\n    default:\n      return false;\n  }\n};\n\nexport const onboardingRequiresDraft = (type: OnboardingType) => {\n  switch (type) {\n    case OnboardingType.SendMessage:\n    case OnboardingType.SendLater:\n    case OnboardingType.MessageTemplates:\n    case OnboardingType.SaveAsTemplate:\n    case OnboardingType.FollowUpReminder:\n      return true;\n    default:\n      return false;\n  }\n};\n"]}]}