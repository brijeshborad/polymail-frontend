EAAC,OAAO,IACnB,gBAAgB,CAAC,SAAS,CACvB;oBACN,6BAAK,SAAS,EAAC,YAAY;wBACxB,aAAa;;wBAAK,WAAW,CAC1B;oBACN,6BAAK,SAAS,EAAC,MAAM,IAClB,KAAK,IAAI,MAAM,CAAC,KAAK,CAAC,SAAS,CAAC,CAAC,MAAM,CAAC,wBAAwB,CAAC,CAC9D;oBACN,6BAAK,SAAS,EAAC,WAAW;wBACvB,YAAY;;wBAAK,QAAQ,IAAI,QAAQ,CAAC,OAAO,CAAC,GAAG,EAAE,GAAG,CAAC,CACpD;oBACL,CAAC,CAAC,gBAAgB,CAAC,QAAQ;wBAC1B,6BAAK,SAAS,EAAC,UAAU;4BACvB,6BAAK,SAAS,EAAC,MAAM;gCACnB,oBAAC,QAAQ,OAAG,CACR;4BACN,6BAAK,SAAS,EAAC,MAAM,IAClB,gBAAgB,CAAC,QAAQ,CACtB,CACF;oBAEP,KAAK,IAAI,KAAK,CAAC,SAAS,CAAC,MAAM;wBAC9B,6BAAK,SAAS,EAAC,WAAW;4BACxB,6BAAK,SAAS,EAAC,MAAM;gCACnB,oBAAC,YAAY,OAAG,CACZ;4BACN,6BAAK,SAAS,EAAC,MAAM,IAClB,iBAAiB,CAAC,KAAK,CAAC,SAAS,CAAC,CAC/B,CACF,CAEJ;gBACL,CAAC,gBAAgB,CAAC,YAAY,IAAI,CACjC,6BAAK,SAAS,EAAC,UAAU;oBACvB,6BAAK,SAAS,EAAC,MAAM;wBACnB,2BAAG,IAAI,EAAC,6CAA6C,EAAC,MAAM,EAAC,QAAQ;4BACnE,6BAAK,GAAG,EAAE,gBAAgB,GAAI,CAC5B,CACA;oBACN,6BAAK,SAAS,EAAC,MAAM;wBACnB,6BAAK,SAAS,EAAC,OAAO;;4BAEtB,8BAAM,SAAS,EAAC,QAAQ,IAAE,WAAW,CAAQ,CACvC;wBACN,6BAAK,SAAS,EAAC,MAAM;4BACnB,2BAAG,IAAI,EAAC,6CAA6C,0DAEjD,CACA,CACF,CACF,CACP,CACG,CACF,CACP,CAAC;IACJ,CAAC;IACH,6BAAC;AAAD,CAAC,AA7ED,CAAqC,KAAK,CAAC,aAAa,GA6EvD;AAED,eAAe,sBAAsB,CAAC","sourcesContent":["import React from 'react';\nimport moment from 'moment-timezone';\nimport { SchedulingPublic, TimeSlot } from '@src/types/scheduling';\nimport { CalendarEvent } from '@src/types';\nimport RoundCheckmark from '@src/components/Icons/RoundCheckmark.svg';\nimport ContactGroup from '@src/components/Icons/ContactGroup.svg';\nimport Location from '@src/components/Icons/Location.svg';\nimport logoNotification from '@src/components/Icons/LogoNotification@3x.png';\n\nimport './SchedulingConfirmation.scss';\nimport { attendeesToString } from '@src/lib/scheduling';\n\nexport interface Props {\n  schedulingPublic: SchedulingPublic;\n  timeSlot: TimeSlot;\n  event?: CalendarEvent;\n}\n\nclass SchedulingConfirmation extends React.PureComponent<Props> {\n  render() {\n    const { schedulingPublic, timeSlot, event } = this.props;\n    const startTimeCopy = moment(timeSlot.startTime).format('h:mm');\n    const endTimeCopy = moment(timeSlot.endTime).format('h:mm a');\n    const timeZone = moment.tz.guess();\n    const abbreviation = timeZone && moment.tz(timeZone).format('z');\n\n    return (\n      <div className=\"scheduling-confirmation\">\n        <div className=\"container\">\n          <div className=\"header\">\n            <div className=\"icon\">\n              <RoundCheckmark height=\"20px\" width=\"20px\" />\n            </div>\n            <div className=\"copy\">\n              Your meeting with {schedulingPublic.organizerName} has been confirmed!\n          </div>\n          </div>\n          <div className=\"widget\">\n            <div className=\"title\">\n              {schedulingPublic.eventName}\n            </div>\n            <div className=\"event-time\">\n              {startTimeCopy} - {endTimeCopy}\n            </div>\n            <div className=\"date\">\n              {event && moment(event.startTime).format('dddd[,] MMMM D[,] YYYY')}\n            </div>\n            <div className=\"time-zone\">\n              {abbreviation} - {timeZone && timeZone.replace('_', ' ')}\n            </div>\n            {!!schedulingPublic.location &&\n              <div className=\"location\">\n                <div className=\"icon\">\n                  <Location />\n                </div>\n                <div className=\"text\">\n                  {schedulingPublic.location}\n                </div>\n              </div>\n            }\n            {event && event.attendees.length &&\n              <div className=\"attendees\">\n                <div className=\"icon\">\n                  <ContactGroup />\n                </div>\n                <div className=\"text\">\n                  {attendeesToString(event.attendees)}\n                </div>\n              </div>\n            }\n          </div>\n          {!schedulingPublic.hideBranding && (\n            <div className=\"branding\">\n              <div className=\"logo\">\n                <a href=\"https://polymail.io?utm_campaign=scheduling\" target=\"_blank\">\n                  <img src={logoNotification} />\n                </a>\n              </div>\n              <div className=\"copy\">\n                <div className=\"title\">\n                  Get your own calendar page with\n                <span className=\"bolded\">{' Polymail'}</span>\n                </div>\n                <div className=\"link\">\n                  <a href=\"https://polymail.io?utm_campaign=scheduling\">\n                    Try Polymail for free and schedule meetings faster.\n                  </a>\n                </div>\n              </div>\n            </div>\n          )}\n        </div>\n      </div>\n    );\n  }\n}\n\nexport default SchedulingConfirmation;\n"]}]}