AC,MAAM,GAAG,EAC7C,OAAO,EAAC,eAAe,EACvB,OAAO,EAAE,OAAO,EAChB,WAAW,EAAE,YAAU,OAAO,CAAC,MAAM,yBAAsB,EAC3D,MAAM,EAAE,IAAI,CAAC,KAAK,CAAC,MAAM,EACzB,aAAa,EAAE,IAAI,CAAC,iBAAiB,EACrC,qBAAqB,EAAE,IAAI,CAAC,yBAAyB,EACrD,QAAQ,EAAE,IAAI,CAAC,YAAY,EAC3B,aAAa,EAAE,IAAI,CAAC,aAAa,EACjC,WAAW,QACX,WAAW,SACX,CACH,CAAC;IACJ,CAAC;IACH,sBAAC;AAAD,CAAC,AArHD,CAA8B,KAAK,CAAC,aAAa,GAqHhD;AAED,eAAe,eAAe,CAAC","sourcesContent":["import React from 'react';\n\nimport ControlPopover, { Option } from '../components/ControlPopover';\n\nimport Calendar from '@src/components/Icons/Calendar.svg';\n\nimport './SchedulingPages.scss';\nimport { CommandControlType, OnboardingType } from '@src/types';\nimport { sortMatches } from '@src/lib/matcher';\nimport { typeaheadRegex } from '@src/lib/typeahead';\nimport { Scheduling } from '@src/types/scheduling';\nimport SchedulingChooserCell from '@src/routes/Calendar/components/ManageSchedulingChooser/components/SchedulingChooserCell';\nimport browserHistory from '@src/lib/browserHistory';\n\nexport interface StateProps {\n  isDefault?: boolean;\n  schedulings: Scheduling[];\n  rsbOnboarding?: OnboardingType;\n}\n\nexport interface Props {\n  onHide: () => void;\n}\n\nexport interface DispatchProps {\n  copySchedulingUrl: (scheduling: Scheduling) => void;\n}\n\ninterface State {\n  options: Option[];\n  selectedScheduling?: Scheduling;\n}\n\nclass SchedulingPages extends React.PureComponent<Props & StateProps & DispatchProps, State> {\n  constructor(props: Props & StateProps & DispatchProps) {\n    super();\n\n    this.state = {\n      options: [],\n      selectedScheduling: props.schedulings.length ? props.schedulings[0] : undefined,\n    };\n  }\n\n  componentDidMount() {\n    const { schedulings, onHide } = this.props;\n    if (!schedulings.length) {\n      browserHistory.push('/calendar');\n      onHide();\n      return;\n    }\n\n    this.setState({\n      options: this.schedulingTypeahead('').map(template => ({\n        title: template.name,\n      })),\n    });\n  }\n\n  schedulingTypeahead = (partial: string) => {\n    const { schedulings } = this.props;\n\n    // Return sorted by created date if no query\n    if (partial === '') {\n      return schedulings;\n    }\n\n    // Return sorted by match if query\n    const query = partial.trim().toLowerCase();\n    const regex = typeaheadRegex(query);\n\n    const matches = this.props.schedulings.filter((template) => {\n      if (template.name.toLowerCase().match(regex)) {\n        return true;\n      }\n\n      return false;\n    });\n\n    return sortMatches(matches, query, 'name');\n  }\n\n  handleInputChange = (value: string) => {\n    const { schedulings } = this.props;\n    const newOptions = this.schedulingTypeahead(value)\n      .map(s => ({ title: s.name }));\n    const selectedScheduling = !!schedulings.length && !!newOptions.length &&\n      schedulings.filter(\n        s => s.name === newOptions[0].title,\n      )[0];\n    this.setState({\n      options: newOptions,\n      selectedScheduling: selectedScheduling || undefined,\n    });\n  }\n\n  handleSelectedIndexChange = (index: number) => {\n    const { schedulings } = this.props;\n\n    this.setState({ selectedScheduling: schedulings.filter(\n      s => s.name === this.state.options[index].title,\n    )[0] });\n  }\n\n  handleSelect = (option: any, index: number) => {\n    const { schedulings, copySchedulingUrl } = this.props;\n    const { options } = this.state;\n    const scheduling = schedulings.filter(\n      s => s.name === options[index].title,\n    )[0];\n    if (scheduling) {\n      copySchedulingUrl(scheduling);\n    }\n  }\n\n  renderPreview = () => {\n    const { selectedScheduling } = this.state;\n\n    if (selectedScheduling) {\n      return (\n        <SchedulingChooserCell\n          scheduling={selectedScheduling}\n          disableInteraction\n        />\n      );\n    }\n\n    return <div />;\n  }\n\n  render() {\n    const { options } = this.state;\n\n    return (\n      <ControlPopover\n        className=\"scheduling-popover\"\n        type={CommandControlType.SchedulingPages}\n        icon={<Calendar width=\"21px\" height=\"18px\" />}\n        heading=\"Meeting Times\"\n        options={options}\n        placeholder={`search ${options.length} scheduling pages...`}\n        onHide={this.props.onHide}\n        onInputChange={this.handleInputChange}\n        onSelectedIndexChange={this.handleSelectedIndexChange}\n        onSelect={this.handleSelect}\n        renderPreview={this.renderPreview}\n        allowManage\n        showOnMount\n      />\n    );\n  }\n}\n\nexport default SchedulingPages;\n"]}]}