,IAAI,OAAO,KAAK,SAAS,EAAE;gBACzB,OAAO;aACR;YAED,wBAAwB;YACxB,IAAM,OAAO,gBAAQ,OAAO,CAAE,CAAC;YAE/B,qEAAqE;YACrE,IAAI,QAAQ,KAAK,UAAU,EAAE;gBAC3B,IAAM,KAAK,GAAG,KAAK,CAAC,KAAK,CAAC,GAAG,CAAC,CAAC;gBAE/B,gBAAgB;gBAChB,IAAI,KAAK,CAAC,MAAM,GAAG,CAAC,EAAE;oBACpB,OAAO,CAAC,SAAS,GAAG,KAAK,CAAC,CAAC,CAAC,CAAC;iBAC9B;gBAED,IAAI,KAAK,CAAC,MAAM,GAAG,CAAC,EAAE;oBACpB,IAAM,QAAQ,GAAG,KAAK,CAAC,KAAK,CAAC,MAAM,GAAG,CAAC,CAAC,CAAC;oBACzC,IAAI,QAAQ,CAAC,MAAM,GAAG,CAAC,EAAE;wBACvB,OAAO,CAAC,UAAU,GAAG,QAAQ,CAAC;qBAC/B;iBACF;aACF;YAED,kCAAkC;YAClC,aAAa,uBACR,OAAO,GACP,QAAQ,EACX,CAAC;YAEH,KAAI,CAAC,QAAQ,CAAC,QAAQ,CAAC,CAAC;QAC1B,CAAC,CAAA;QAED,eAAS,GAAG,UAAC,GAAyB;YACpC,KAAI,CAAC,OAAO,GAAG,GAAG,CAAC;QACrB,CAAC,CAAA;QAED,mBAAa,GAAG,UAAC,CAAoC;YAC7C,IAAA,gBAAqC,EAAnC,4BAAW,EAAE,oBAAsB,CAAC;YAC5C,IAAI,OAAO,EAAE;gBACX,IAAM,WAAW,GACf,OAAO,CAAC,QAAQ,KAAK,EAAE,CAAC,CAAC,CAAI,OAAO,CAAC,QAAQ,UAAK,OAAO,CAAC,KAAK,MAAG,CAAC,CAAC,CAAC,OAAO,CAAC,KAAK,CAAC;gBACrF,CAAC,CAAC,cAAc,EAAE,CAAC;gBACnB,CAAC,CAAC,eAAe,EAAE,CAAC;gBACpB,WAAW,CAAC,aAAa,CAAC,MAAM,EAAE,CAAC,WAAW,CAAC,CAAC,CAAC;aAClD;QACH,CAAC,CAAA;QAxGC,KAAI,CAAC,KAAK,GAAG;YACX,QAAQ,EAAE,EAAE;YACZ,GAAG,EAAE,EAAE;YACP,QAAQ,EAAE,EAAE;YACZ,YAAY,EAAE,EAAE;YAChB,QAAQ,EAAE,EAAE;YACZ,KAAK,EAAE,EAAE;SACV,CAAC;;IACJ,CAAC;IAED,uCAAiB,GAAjB;QACU,IAAA,4BAAO,CAAgB;QAE/B,IAAI,OAAO,EAAE;YACX,IAAI,CAAC,QAAQ,CAAC;gBACZ,QAAQ,EAAE,OAAO,CAAC,QAAQ,CAAC,MAAM;oBAC/B,CAAC,CAAC,OAAO,CAAC,QAAQ;oBAClB,CAAC,CAAC,UAAU,CAAC,OAAO,CAAC,KAAK,CAAC,KAAK,CAAC,GAAG,CAAC,CAAC,CAAC,CAAC,CAAC;gBAC3C,GAAG,EAAE,OAAO,CAAC,GAAG;gBAChB,QAAQ,EAAE,OAAO,CAAC,iBAAiB;gBACnC,YAAY,EAAE,OAAO,CAAC,YAAY;gBAClC,QAAQ,EAAE,OAAO,CAAC,QAAQ;gBAC1B,KAAK,EAAE,OAAO,CAAC,YAAY,CAAC,MAAM,CAAC,CAAC,CAAC,OAAO,CAAC,YAAY,CAAC,CAAC,CAAC,CAAC,MAAM,CAAC,CAAC,CAAC,EAAE;aACzE,CAAC,CAAC;SACJ;IACH,CAAC;IAED,sDAAgC,GAAhC,UAAiC,SAAgB;QACvC,IAAA,2BAAO,CAAe;QAE9B,IAAI,OAAO,IAAI,CAAC,OAAO,CAAC,IAAI,CAAC,KAAK,CAAC,OAAO,EAAE,OAAO,CAAC,EAAE;YACpD,IAAI,CAAC,QAAQ,CAAC;gBACZ,QAAQ,EAAE,OAAO,CAAC,QAAQ,CAAC,MAAM;oBAC/B,CAAC,CAAC,OAAO,CAAC,QA