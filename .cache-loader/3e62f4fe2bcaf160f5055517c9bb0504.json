B;gCACP,oBAAC,aAAa,IACZ,SAAS,EAAC,UAAU,EACpB,IAAI,EAAE,QAAQ,EACd,QAAQ,EAAC,UAAU,EACnB,KAAK,EAAE,IAAI,CAAC,SAAS,EACrB,WAAW,EAAC,WAAW,EACvB,GAAG,EAAC,UAAU,EACd,QAAQ,EAAE,IAAI,GACd,CACE;4BACN,6BAAK,SAAS,EAAE,UAAU,CAAC,EAAE,KAAK,EAAE,YAAY,CAAC,MAAM,KAAK,CAAC,EAAE,CAAC;gCAC9D,8BAAM,SAAS,EAAC,cAAc;oCAC5B,oBAAC,OAAO,IAAC,SAAS,EAAC,MAAM,GAAG,CACvB;gCACP,oBAAC,aAAa,IACZ,SAAS,EAAC,cAAc,EACxB,IAAI,EAAE,YAAY,EAClB,QAAQ,EAAC,cAAc,EACvB,KAAK,EAAE,IAAI,CAAC,SAAS,EACrB,WAAW,EAAC,aAAa,EACzB,GAAG,EAAC,SAAS,EACb,QAAQ,EAAE,IAAI,GACd,CACE;4BACN,6BAAK,SAAS,EAAE,UAAU,CAAC,EAAE,KAAK,EAAE,QAAQ,CAAC,MAAM,KAAK,CAAC,EAAE,CAAC;gCAC1D,8BAAM,SAAS,EAAC,cAAc;oCAC5B,oBAAC,QAAQ,IAAC,SAAS,EAAC,MAAM,GAAG,CACxB;gCACP,oBAAC,aAAa,IACZ,SAAS,EAAC,UAAU,EACpB,IAAI,EAAE,QAAQ,EACd,QAAQ,EAAC,UAAU,EACnB,KAAK,EAAE,IAAI,CAAC,SAAS,EACrB,WAAW,EAAC,cAAc,EAC1B,QAAQ,EAAE,IAAI,GACd,CACE;4BACN,6BAAK,SAAS,EAAE,UAAU,CAAC,EAAE,KAAK,EAAE,KAAK,CAAC,MAAM,KAAK,CAAC,EAAE,CAAC;gCACvD,8BAAM,SAAS,EAAC,cAAc;oCAC5B,oBAAC,KAAK,IAAC,SAAS,EAAC,MAAM,GAAG,CACrB;gCACP,oBAAC,aAAa,IACZ,SAAS,EAAC,OAAO,EACjB,IAAI,EAAE,KAAK,EACX,QAAQ,EAAC,OAAO,EAChB,KAAK,EAAE,IAAI,CAAC,SAAS,EACrB,WAAW,EAAC,kBAAkB,EAC9B,QAAQ,EAAE,IAAI,GACd,CACE,CACF;wBACN,6BAAK,SAAS,EAAC,kBAAkB;4BAC9B,iBAAiB,CAAC,MAAM,GAAG,CAAC,IAAI,yCAAe;4BAChD,6BAAK,SAAS,EAAC,QAAQ;gCACpB,KAAK,IAAI,gCAAQ,OAAO,EAAE,IAAI,CAAC,gBAAgB,qBAAyB;gCACzE,oBAAC,oBAAoB,IACnB,aAAa,EAAE,iBAAiB,CAAC,GAAG,CAAC,UAAA,CAAC,IAAI,OAAA,CAAC,EAAD,CAAC,CAAC,EAC5C,aAAa,EAAE,IAAI,CAAC,iBAAiB,EACrC,GAAG,EAAE,IAAI,CAAC,SAAS,GACnB;gCACD,iBAAiB,CAAC,GAAG,CAAC,UAAA,KAAK,IAAI,OAAA,CAC9B,2BAAG,GAAG,EAAE,KAAK,CAAC,EAAE;oCACb,KAAK,CAAC,IAAI;oCACX;wCACE,oBAAC,KAAK,IAAC,KAAK,EAAC,GAAG,EAAC,MAAM,EAAC,GAAG,EAAC,SAAS,EAAC,MAAM,GAAG,CACxC,CACP,CACL,EAP+B,CAO/B,CAAC,CACE,CACF,CACF,CACF;gBACL,OAAO,CAAC,OAAO,IAAI,oBAAC,WAAW,IAAC,OAAO,EAAE,OAAO,CAAC,OAAO,GAAI;gBAC7D,oBAAC,0BAA0B,IAAC,OAAO,EAAE,OAAO,GAAI,CAC5C,CACP,CACG,CACP,CAAC;IACJ,CAAC;IACH,kBAAC;AAAD,CAAC,AArPD,CAA0B,KAAK,CAAC,aAAa,GAqP5C;AAED,6EAA6E;AAC7E,mDAAmD;AAEnD,eAAe,WAAW,CAAC","sourcesContent":["import React from 'react';\nimport classnames from 'classnames';\nimport { capitalize, isEqual } from 'lodash';\n\nimport { Contact, ContactGroup } from '@src/types';\n\nimport './ContactInfo.scss';\n\nimport ContactGroupsPopover from './components/ContactGroupsPopover';\nimport SocialIcon, { IconType } from './components/SocialIcon';\nimport EditableInput from '@src/components/Form/EditableInput/EditableInput';\nimport EmptyState from './components/EmptyState';\nimport CompanyInfo from './components/CompanyInfo';\nimport ContactInteractions from './components/ContactInteractions';\n\nimport Compose from '@src/components/Icons/Compose.svg';\nimport Work from './Icons/Work.svg';\nimport Company from './Icons/Company.svg';\nimport Phone from './Icons/Phone.svg';\nimport Location from '@src/components/Icons/Location.svg';\nimport Close from '@src/components/Icons/Close.svg';\nimport { Configuration } from '@src/lib/draft';\n\n// TODO(SHIN): Re-enable when implementing contact share\n// import Lock from '@src/components/Icons/Lock.svg';\n\ninterface Props {\n  contact?: Contact;\n  contactGroups: { [id: string]: ContactGroup };\n  createDraft: (configuration: Configuration, recipients: string[]) => void;\n  updateContact: (contact: Contact) => void;\n}\n\ninterface State {\n  fullName: string;\n  bio: string;\n  position: string;\n  organization: string;\n  location: string;\n  phone: string;\n}\n\nclass ContactInfo extends React.PureComponent<Props, State> {\n  private popover: ContactGroupsPopover;\n\n  constructor() {\n    super();\n\n    this.state = {\n      fullName: '',\n      bio: '',\n      position: '',\n      organization: '',\n      location: '',\n      phone: '',\n    };\n  }\n\n  componentDidMount() {\n    const { contact } = this.props;\n\n    if (contact) {\n      this.setState({\n        fullName: contact.fullName.length\n          ? contact.fullName\n          : capitalize(contact.email.split('@')[0]),\n        bio: contact.bio,\n        position: contact.organizationTitle,\n        organization: contact.organization,\n        location: contact.location,\n        phone: contact.phoneNumbers.length ? contact.phoneNumbers[0].number : '',\n      });\n    }\n  }\n\n  UNSAFE_componentWillReceiveProps(nextProps: Props) {\n    const { contact } = nextProps;\n\n    if (contact && !isEqual(this.props.contact, contact)) {\n      this.setState({\n        fullName: contact.fullName.length\n          ? contact.fullName\n          : capitalize(contact.email.split('@')[0]),\n        bio: contact.bio,\n        position: contact.organizationTitle,\n        organization: contact.organization,\n        location: contact.location,\n        phone: contact.phoneNumbers.length ? contact.phoneNumbers[0].number : '',\n      });\n    }\n  }\n\n  handleGroupSelect = (groupId: string) => {\n    this.popover.toggle();\n  }\n\n  handleAddToGroup = () => {\n    this.popover.show();\n  }\n\n  handleSet = (property: string, value: string) => {\n    const { updateContact, contact } = this.props;\n    const newState = {};\n    newState[property] = value;\n\n    // Do nothing if contact has not been defined\n    if (contact === undefined) {\n      return;\n    }\n\n    // Set Contact to Update\n    const updated = { ...contact };\n\n    // If Fullname was changed, then change additional values for contact\n    if (property === 'fullName') {\n      const words = value.split(' ');\n\n      // Start Initial\n      if (words.length > 0) {\n        updated.givenName = words[0];\n      }\n\n      if (words.length > 1) {\n        const lastWord = words[words.length - 1];\n        if (lastWord.length > 0) {\n          updated.familyName = lastWord;\n        }\n      }\n    }\n\n    // Update contact with all changes\n    updateContact({\n      ...updated,\n      ...newState,\n    });\n\n    this.setState(newState);\n  }\n\n  handleRef = (ref: ContactGroupsPopover) => {\n    this.popover = ref;\n  }\n\n  handleCompose = (e: React.SyntheticEvent<HTMLElement>) => {\n    const { createDraft, contact } = this.props;\n    if (contact) {\n      const emailString =\n        contact.fullName !== '' ? `${contact.fullName} <${contact.email}>` : contact.email;\n      e.preventDefault();\n      e.stopPropagation();\n      createDraft(Configuration.MailTo, [emailString]);\n    }\n  }\n\n  render() {\n    const { contact, contactGroups } = this.props;\n    const { fullName, bio, position, organization, location, phone } = this.state;\n    let contactGroupNames: Array<{ id: string; name: string }> = [];\n    const ContactInteractionsConnect = ContactInteractions as any;\n\n    if (contact) {\n      // TODO(Jeff): Make sure proper contact group are passed down to this component.\n      contactGroupNames = contact.groups.map((id) => {\n        return { id, name: contactGroups[id] ? contactGroups[id].name : id };\n      });\n    }\n\n    return (\n      <div className=\"contact-info\">\n        {!contact && <EmptyState />}\n        {contact && (\n          <div className=\"main\">\n            <div className=\"contact-desc\">\n              <div className=\"action-bar\">\n                <button className=\"compose\" onClick={this.handleCompose}>\n                  <Compose height=\"15\" width=\"15\" className=\"icon\" />\n                </button>\n              </div>\n              <div className=\"contact\">\n                <div className=\"main\">\n                  <img className=\"profile-pic\" src={contact.photoUrl} />\n                  <EditableInput\n                    className=\"full-name\"\n                    text={fullName}\n                    property=\"fullName\"\n                    onSet={this.handleSet}\n                    placeholder=\"Add full name\"\n                  />\n                </div>\n                <div className=\"social\">\n                  {contact.socialMedia.map(social => (\n                    <SocialIcon social={social} key={social.network} type={IconType.Button} />\n                  ))}\n                </div>\n                <div className=\"desc\">\n                  <EditableInput\n                    className=\"bio\"\n                    text={bio}\n                    property=\"bio\"\n                    onSet={this.handleSet}\n                    numberOfLines={4}\n                    placeholder=\"Add description\"\n                    readOnly={true}\n                  />\n                  <div className={classnames({ empty: position.length === 0 })}>\n                    <span className=\"icon-wrapper\">\n                      <Work className=\"icon\" />\n                    </span>\n                    <EditableInput\n                      className=\"position\"\n                      text={position}\n                      property=\"position\"\n                      onSet={this.handleSet}\n                      placeholder=\"Add title\"\n                      key=\"position\"\n                      readOnly={true}\n                    />\n                  </div>\n                  <div className={classnames({ empty: organization.length === 0 })}>\n                    <span className=\"icon-wrapper\">\n                      <Company className=\"icon\" />\n                    </span>\n                    <EditableInput\n                      className=\"organization\"\n                      text={organization}\n                      property=\"organization\"\n                      onSet={this.handleSet}\n                      placeholder=\"Add company\"\n                      key=\"company\"\n                      readOnly={true}\n                    />\n                  </div>\n                  <div className={classnames({ empty: location.length === 0 })}>\n                    <span className=\"icon-wrapper\">\n                      <Location className=\"icon\" />\n                    </span>\n                    <EditableInput\n                      className=\"location\"\n                      text={location}\n                      property=\"location\"\n                      onSet={this.handleSet}\n                      placeholder=\"Add location\"\n                      readOnly={true}\n                    />\n                  </div>\n                  <div className={classnames({ empty: phone.length === 0 })}>\n                    <span className=\"icon-wrapper\">\n                      <Phone className=\"icon\" />\n                    </span>\n                    <EditableInput\n                      className=\"phone\"\n                      text={phone}\n                      property=\"phone\"\n                      onSet={this.handleSet}\n                      placeholder=\"Add phone number\"\n                      readOnly={true}\n                    />\n                  </div>\n                </div>\n                <div className=\"groups-container\">\n                  {contactGroupNames.length > 0 && <h5>Groups</h5>}\n                  <div className=\"groups\">\n                    {false && <button onClick={this.handleAddToGroup}>+ Add to Group</button>}\n                    <ContactGroupsPopover\n                      contactGroups={contactGroupNames.map(g => g)}\n                      onGroupSelect={this.handleGroupSelect}\n                      ref={this.handleRef}\n                    />\n                    {contactGroupNames.map(group => (\n                      <a key={group.id}>\n                        {group.name}\n                        <button>\n                          <Close width=\"8\" height=\"8\" className=\"icon\" />\n                        </button>\n                      </a>\n                    ))}\n                  </div>\n                </div>\n              </div>\n            </div>\n            {contact.company && <CompanyInfo company={contact.company} />}\n            <ContactInteractionsConnect contact={contact} />\n          </div>\n        )}\n      </div>\n    );\n  }\n}\n\n// TODO(SHIN): Beta - Add to top of action-bar when bringing in contact share\n// {!(!!contact.team) && <button><Lock /></button>}\n\nexport default ContactInfo;\n"]}]}