AAI,EAAE,4BAAW,EAAE,oCAAe,EAAE,sBAAQ,EAAE,UAAuB,CAAC;IAC9E,IAAM,IAAI,GAAG,kBAAkB,CAAC,WAAW,CAAC,CAAC;IAC7C,IAAM,KAAK,GAAG,uBAAuB,CAAC,WAAW,CAAC,CAAC;IAEnD,IAAM,WAAW,GAAG;QAClB,cAAc,CAAC,IAAI,CAAC,gBAAc,EAAI,CAAC,CAAC;IAC1C,CAAC,CAAC;IAEF,OAAO,CACL,6BACE,SAAS,EAAE,UAAU,CAAC,yBAAyB,EAAE;YAC/C,QAAQ,UAAA;YACR,qBAAqB,EAAE,CAAC,CAAC,kBAAkB;SAC5C,CAAC,EACF,OAAO,EAAE,WAAW;QAEpB,6BAAK,SAAS,EAAC,iBAAiB;YAC9B,6BAAK,SAAS,EAAE,UAAU,CAAC,gBAAgB,EAAE,EAAE,OAAO,EAAE,CAAC,QAAQ,EAAE,CAAC,IAChE,CAAC,QAAQ,CAAC,CAAC,CAAC,SAAS,CAAC,CAAC,CAAC,UAAU,CAChC,CACF;QACN,6BAAK,SAAS,EAAC,SAAS;YACtB,6BAAK,SAAS,EAAC,KAAK;gBAClB,6BAAK,SAAS,EAAC,MAAM,IAClB,IAAI,CACD;gBACN,6BAAK,SAAS,EAAC,OAAO,IAChB,eAAe,UAAO,CACtB,CACF;YACN,6BAAK,SAAS,EAAC,MAAM,IAClB,IAAI,CACD;YACN,6BAAK,SAAS,EAAC,aAAa,IACzB,KAAK,CACF,CACF,CACF,CACP,CAAC;AACJ,CAAC,CAAC;AAEF,eAAe,qBAAqB,CAAC","sourcesContent":["import React from 'react';\nimport { Scheduling, TimeWindow } from '@src/types/scheduling';\nimport moment from 'moment';\nimport './SchedulingChooserCell.scss';\nimport browserHistory from '@src/lib/browserHistory';\nimport classnames from 'classnames';\n\nexport interface Props {\n  scheduling: Scheduling;\n  selected?: boolean;\n  disableInteraction?: boolean;\n}\n\nconst timeWindowsToDates = (windows: TimeWindow[]) => {\n  const days = windows.map(w => moment.weekdaysShort(w.dayOfWeek)).join(', ');\n  return days;\n};\n\nconst timeWindowsToTimeString = (windows: TimeWindow[]) => {\n  const timeWindow = windows[0];\n  if (!timeWindow) {\n    return '';\n  }\n\n  const start = moment().startOf('day')\n    .add(timeWindow.afterSecondsSinceMidnight, 's').format('hA');\n  const end = moment().startOf('day')\n    .add(timeWindow.beforeSecondsSinceMidnight, 's').format('hA');\n\n  return `${start} - ${end}`;\n};\n\nconst SchedulingChooserCell = (props: Props) => {\n  const { selected, disableInteraction } = props;\n  const { name, timeWindows, durationMinutes, disabled, id } = props.scheduling;\n  const days = timeWindowsToDates(timeWindows);\n  const hours = timeWindowsToTimeString(timeWindows);\n\n  const handleClick = () => {\n    browserHistory.push(`/calendars/${id}`);\n  };\n\n  return (\n    <div\n      className={classnames('scheduling-chooser-cell', {\n        selected,\n        'disable-interaction': !!disableInteraction,\n      })}\n      onClick={handleClick}\n    >\n      <div className=\"enabled-section\">\n        <div className={classnames('enabled-button', { enabled: !disabled })}>\n          { !disabled ? 'ENABLED' : 'DISABLED' }\n        </div>\n      </div>\n      <div className=\"content\">\n        <div className=\"top\">\n          <div className=\"name\">\n            {name}\n          </div>\n          <div className=\"right\">\n            {`${durationMinutes} mins`}\n          </div>\n        </div>\n        <div className=\"days\">\n          {days}\n        </div>\n        <div className=\"time-window\">\n          {hours}\n        </div>\n      </div>\n    </div>\n  );\n};\n\nexport default SchedulingChooserCell;\n"]}]}