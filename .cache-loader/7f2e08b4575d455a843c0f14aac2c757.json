arClick: (calendar: string) => void;\n  hiddenCalendars: string[];\n}\n\nconst CalendarList: React.SFC<Props> = ({ calendars, onCalendarClick, hiddenCalendars }) => {\n  return (\n    <div className=\"calendar-list\">\n      <h4>Calendars</h4>\n      <div className=\"calendars\">\n        {calendars.map((c) => {\n          const handleCalendarClick = () => {\n            onCalendarClick(c.id);\n          };\n          const isHidden = hiddenCalendars.indexOf(c.id) !== -1;\n          return (\n            <button className=\"calendar-button\" onClick={handleCalendarClick} key={c.id}>\n              <div className=\"color\" style={{ backgroundColor: c.backgroundColor }}>\n                {isHidden && <div className=\"white-circle\" />}\n              </div>\n              <span>{c.name}</span>\n            </button>\n          );\n        })}\n      </div>\n    </div>\n  );\n};\n\nexport default CalendarList;\n"]}]}