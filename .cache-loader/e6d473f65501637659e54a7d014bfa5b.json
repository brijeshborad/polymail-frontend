IAAI,CAAC,gBAAgB,EAC/B,IAAI,EAAC,MAAM,EACX,SAAS,EAAE,IAAI,GACf;gBACF,+BAAO,SAAS,EAAC,eAAe,EAAC,IAAI,EAAC,QAAQ,EAAC,KAAK,EAAC,QAAQ,GAAG,CAC3D;YACP,6BAAK,SAAS,EAAC,oBAAoB;gBACjC,gCAAQ,SAAS,EAAC,UAAU,EAAC,OAAO,EAAE,IAAI,CAAC,KAAK,CAAC,MAAM,eAAmB,CACtE,CACF,CACP,CAAC;IACJ,CAAC;IACH,oBAAC;AAAD,CAAC,AA9DD,CAA4B,KAAK,CAAC,aAAa,GA8D9C;AAED,eAAe,aAAa,CAAC","sourcesContent":["import React from 'react';\n\nimport TextInput from '@src/components/Form/TextInput';\n\nimport './TwoFactorAuth.scss';\n\ninterface State {\n  code: string;\n}\n\nexport interface Props {\n  validateCode: (code: string) => void;\n  resendCode: () => void;\n  logout: () => void;\n}\n\nclass TwoFactorAuth extends React.PureComponent<Props, State> {\n  constructor() {\n    super();\n    this.state = {\n      code: '',\n    };\n  }\n\n  handleCodeChange = (e: React.FormEvent<HTMLInputElement>) => {\n    this.setState({ code: e.currentTarget.value });\n  }\n\n  handleSubmit = (e: React.FormEvent<HTMLFormElement>) => {\n    if (e) {\n      e.preventDefault();\n    }\n\n    this.props.validateCode(this.state.code);\n  }\n\n  handleResend = () => {\n    this.props.resendCode();\n  }\n\n  render() {\n    return (\n      <div className=\"tfa-login\">\n        <div className=\"prompt\">\n          <div className=\"title\">\n            Two-Factor Authentication\n          </div>\n          <div className=\"description\">\n            {'You have two factor authentication set up, '\n            + 'please enter the 6 digit code sent to your mobile phone.'}\n          </div>\n          <div className=\"resend-container\">\n            {`Didn't receive a code? `}\n            <button\n              className=\"resend-code\"\n              onClick={this.handleResend}\n            >\n              Send Code Again\n            </button>\n          </div>\n        </div>\n        <form onSubmit={this.handleSubmit}>\n          <TextInput\n            description=\"Verification Code\"\n            placeholder=\"your verification code\"\n            value={this.state.code}\n            onChange={this.handleCodeChange}\n            type=\"code\"\n            autofocus={true}\n          />\n          <input className=\"submit-button\" type=\"submit\" value=\"Verify\" />\n        </form>\n        <div className=\"sign-out-container\">\n          <button className=\"sign-out\" onClick={this.props.logout}>Sign Out</button>\n        </div>\n      </div>\n    );\n  }\n}\n\nexport default TwoFactorAuth;\n"]}]}