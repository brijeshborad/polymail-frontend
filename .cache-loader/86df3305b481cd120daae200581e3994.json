esourceLoading(state.salesforce.summaries[state.selectedContact]) || false,\n  error: state.selectedContact &&\n    selectResourceError(state.salesforce.summaries[state.selectedContact]) || undefined,\n  salesforce: state.selectedContact && state.salesforce.summaries[state.selectedContact] ?\n    state.salesforce.summaries[state.selectedContact].data : undefined,\n  tasks: state.selectedContact && selectSalesforceTasks(state, state.selectedContact) || [],\n  email: state.selectedContact,\n  contact: state.selectedContact && selectContact(state, state.selectedContact),\n  schemas: state.salesforce.schemas,\n  pendingSummary: state.selectedContact &&\n    state.salesforce.pendingSummaries[state.selectedContact] || undefined,\n});\n\nconst mapDispatchToProps = (dispatch: Dispatch<StoreState>): DispatchProps => ({\n  createSummary: meta => dispatch(salesforceSummaryCreate(meta)),\n  convertToContact: (email: string) => dispatch(salesforceLeadConvert(email)),\n  updateField: (meta: SalesforceSummaryUpdateMeta) => dispatch(salesforceSummaryUpdate(meta)),\n  createTask: (meta: SalesforceTaskCreateMeta) => dispatch(salesforceTaskCreate(meta)),\n  getSchemas: type => dispatch(salesforceSchemaGet(type)),\n  updatePending: meta => dispatch(salesforcePendingUpdate(meta)),\n  clearPending: meta => dispatch(salesforcePendingClear(meta)),\n  fetchSummary: email => dispatch(salesforceSummaryFetch(email)),\n});\n\nexport default connect<StateProps, DispatchProps, {}>(\n  mapStateToProps, mapDispatchToProps,\n)(Salesforce);\n"]}]}