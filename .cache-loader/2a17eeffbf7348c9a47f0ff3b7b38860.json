n    <div className=\"multi-dropdown-cell\">\n      <div className=\"heading\">\n        <h4 className=\"title\">{title}</h4>\n        {subtitle && <h5 className=\"subtitle\">{subtitle}</h5>}\n      </div>\n      <div className=\"dropdowns\">\n        {dropdowns.map((dropdown: DropdownProps, index: number) => {\n          return (\n            <Dropdown\n              key={index}\n              values={dropdown.values}\n              onSelect={dropdown.onSelect}\n              description={dropdown.description}\n              id={dropdown.id}\n              startIndex={dropdown.startIndex}\n            />\n          );\n        })}\n      </div>\n    </div>\n  );\n};\n\nexport default ToggleCell;\n"]}]}