;YACN,6BAAK,SAAS,EAAC,QAAQ;gBACrB,6BAAK,SAAS,EAAC,SAAS;oBACtB,2BAAG,IAAI,EAAE,IAAI,CAAC,GAAG,EAAE,MAAM,EAAC,QAAQ,EAAC,GAAG,EAAC,qBAAqB,IACzD,aAAa,CAAC,SAAS,EAAE,IAAI,CAAC,MAAM,CAAC,CACpC,CACA,CACF;YACN,2BAAG,SAAS,EAAC,aAAa;gBACxB,oBAAC,OAAO,IAAC,UAAU,EAAE,EAAE,MAAM,EAAE,QAAQ,EAAE,GAAG,EAAE,qBAAqB,EAAE,IAClE,aAAa,CAAC,aAAa,EAAE,IAAI,CAAC,MAAM,CAAC,CAClC,CACR,CACA,CACP,CAAC;IACJ,CAAC,CAAC,CAAC;AACL,CAAC;AAMD,IAAM,aAAa,GAAqB,UAAC,EAAY;QAAV,sBAAQ;IAAO,OAAA,CACxD,6BAAK,SAAS,EAAC,sCAAsC;QAClD,QAAQ,CAAC,MAAM,KAAK,CAAC,CAAC,CAAC,CAAC,CACvB,6BAAK,SAAS,EAAC,OAAO,kBAEhB,CACP,CAAC,CAAC,CAAC,IAAI;QACP,KAAK,CAAC,QAAQ,CAAC,CACZ,CACP;AATyD,CASzD,CAAC;AAEF,eAAe,aAAa,CAAC","sourcesContent":["import React from 'react';\nimport Linkify from 'react-linkify';\n\nimport { getFieldValue, getIcon, getCreatedDate } from './TasksTable';\n\nimport { SObject, SalesforceTaskSubtype } from '@src/types/salesforce';\n\nimport './ActivityTable.scss';\n\nfunction getCopy(subtype: string) {\n  switch (subtype) {\n    case SalesforceTaskSubtype.Task:\n      return 'had a task';\n    case SalesforceTaskSubtype.Phone:\n      return 'logged a call';\n    case SalesforceTaskSubtype.Email:\n      return 'emailed';\n    default:\n      // don't want to display unknown types\n      return '';\n  }\n}\n\nfunction cells(activity: SObject[]) {\n  return activity.map((item) =>  {\n    const subtype = getFieldValue('TaskSubtype', item.fields) as string;\n\n    return (\n      <div key={item.href} className=\"salesforce-components-activity-table-cell\">\n        <div className=\"meta\">\n          <div className=\"icon\">{getIcon(subtype)}</div>\n          <div className=\"owner\">{getFieldValue('OwnerId', item.fields)}</div>\n          <div className=\"type\">{getCopy(subtype)}</div>\n          <div className=\"date\">{getCreatedDate(item.fields)}</div>\n        </div>\n        <div className=\"header\">\n          <div className=\"subject\">\n            <a href={item.url} target=\"_blank\" rel=\"noopener noreferrer\">\n              {getFieldValue('Subject', item.fields)}\n            </a>\n          </div>\n        </div>\n        <p className=\"description\">\n          <Linkify properties={{ target: '_blank', rel: 'noopener noreferrer' }}>\n            {getFieldValue('Description', item.fields)}\n          </Linkify>\n        </p>\n      </div>\n    );\n  });\n}\n\ninterface Props {\n  activity: SObject[];\n}\n\nconst ActivityTable: React.SFC<Props> = ({ activity }) => (\n  <div className=\"salesforce-components-activity-table\">\n    {activity.length === 0 ? (\n      <div className=\"empty\">\n        No Activity\n      </div>\n    ) : null}\n    {cells(activity)}\n  </div>\n);\n\nexport default ActivityTable;\n"]}]}