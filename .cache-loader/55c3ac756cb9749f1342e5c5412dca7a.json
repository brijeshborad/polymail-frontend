{ selectUnsubscribeLink } from '@src/ducks/messageBodies';\nimport { selectSharedThread } from '@src/ducks/sharedThreads';\nimport { selectCalendarSchedulingUrl } from '@src/ducks/scheduling';\nimport { withRouter } from 'react-router';\nimport { selectBlockFilter } from '@src/ducks/filters';\n\nconst mapStateToProps = (state: StoreState): Partial<Props> => {\n  const locationString = location.pathname && location.pathname.toLowerCase();\n  const locationSplit = locationString && locationString.split('/');\n  const isUserSharedThreads = locationSplit && locationSplit.indexOf('sharedthreads') !== -1;\n  const sharedThread = state.selectedSharedThread && state.selectedSharedThread &&\n    selectSharedThread(state, state.selectedSharedThread);\n  return ({\n    selectedThread: state.selectedThread,\n    draft: state.selectedDraft\n      ? selectDraft(state, state.selectedDraft).data\n      : state.selectedThread && selectDraftForThread(state, state.selectedThread.id).data,\n    hideActions: !state.me.data || !sharedThread || sharedThread.user !== state.me.data.id,\n    rightSidebarCollapsed: state.rightSidebar.collapsed.threads,\n    rightSidebarTab: state.rightSidebar.tab,\n    preferences: state.preferences,\n    unsubscribeLink: state.selectedThread && selectUnsubscribeLink(state, state.selectedThread.id),\n    commandControlType: state.commandControlType,\n    schedulingUrl: selectCalendarSchedulingUrl(state),\n    userSharedThreads: isUserSharedThreads || false,\n    showSharedThreadsOnboarding: Object.keys(state.sharedThreads).length === 0,\n    sharedThreadsLoaded: state.sharedThreadsStatus === ResourceStatus.Ok,\n    blockFilter: state.selectedThread && selectBlockFilter(state, state.selectedThread.id),\n    hotkeyActions: state.hotkeyActions,\n  });\n};\n\nconst mapDispatchToProps = (dispatch: Dispatch<StoreState>): DispatchProps => ({\n  dispatchAction: (action: Action<any, any>) => dispatch(action),\n  selectContact: (email: string) => dispatch(contactSelect(email)),\n  submitCommitMeta: commit => dispatch(threadCommitsSubmit(commit)),\n  collapseRightSidebar: (collapsed: boolean) => dispatch(rightSidebarCollapse({\n    collapsed,\n    page: Page.Threads,\n  })),\n  updateHotkey: hotkeyAction => dispatch(hotkeyUpdate(hotkeyAction)),\n});\n\nexport default withRouter(connect(mapStateToProps, mapDispatchToProps)(Mentions));\n"]}]}