,EAAC,MAAM,EAAC,MAAM,EAAC,MAAM,GAAG;IACnD,QAAQ,EAAE,oBAAC,QAAQ,IAAC,KAAK,EAAC,MAAM,EAAC,MAAM,EAAC,MAAM,GAAG;IACjD,MAAM,EAAE,oBAAC,KAAK,IAAC,KAAK,EAAC,MAAM,EAAC,MAAM,EAAC,MAAM,GAAG;CAC7C,CAAC;AAaF,MAAM,CAAC,IAAM,WAAW,GAAG,UAAC,KAAY;IAEpC,IAAA,iBAAI,EAAE,yBAAQ,EAAE,+BAAW,EAAE,uCAAe,EAC5C,mCAAa,EAAE,qCAAc,EAAE,2BAAS,CAChC;IACV,IAAM,iBAAiB,GAAG,CAAC,OAAO,EAAE,SAAS,EAAE,WAAW,EAAE,UAAU,EAAE,QAAQ,CAAC,CAAC;IAClF,IAAM,UAAU,GAAG,UAAC,CAAoC;QACtD,CAAC,CAAC,cAAc,EAAE,CAAC;QACnB,CAAC,CAAC,eAAe,EAAE,CAAC;QAEpB,aAAa,CAAC,CAAC,cAAc,CAAC,CAAC;IACjC,CAAC,CAAC;IAEF,OAAO,CACL,6BAAK,SAAS,EAAE,UAAU,CAAC,UAAU,EAAE,EAAE,iBAAiB,EAAE,cAAc,EAAE,CAAC;QAC1E,CAAC,SAAS,IAAI,CACb,oBAAC,UAAU,IACT,QAAQ,EAAE,QAAQ,EAClB,eAAe,EAAE,eAAe,GAChC,CACH;QACA,IAAI;aACF,MAAM,CAAC,UAAA,CAAC,IAAI,OAAA,iBAAiB,CAAC,OAAO,CAAC,CAAC,CAAC,GAAG,CAAC,KAAK,CAAC,CAAC;YAClD,CAAC,iBAAiB,CAAC,OAAO,CAAC,CAAC,CAAC,GAAG,CAAC,GAAG,CAAC,CAAC,IAAI,CAAC,CAAC,CAAC,CAAC,KAAK,CAAC,EADzC,CACyC,CAAC;aACtD,GAAG,CAAC,UAAC,SAAS;YACL,IAAA,qBAAI,EAAE,uBAAK,EAAE,mBAAG,EAAE,+BAAS,CAAe;YAClD,IAAI,IAAI,GAAG,WAAS,eAAe,CAAC,EAAE,SAAI,GAAK,CAAC;YAEhD,IAAI,GAAG,CAAC,UAAU,CAAC,WAAW,CAAC,EAAE;gBAC/B,IAAI,GAAG,6BAA2B,GAAG,CAAC,KAAK,CAAC,GAAG,CAAC,OAAO,CAAC,GAAG,CAAC,GAAG,CAAC,CAAG,CAAC;aACrE;iBAAM,IAAI,GAAG,KAAK,OAAO,EAAE;gBAC1B,IAAI,GAAG,WAAS,eAAe,CAAC,EAAE,WAAQ,CAAC;aAC5C;YAED,OAAO,oBAAC,WAAW,IACjB,GAAG,EAAE,GAAG,EACR,IAAI,EAAE,IAAI,EACV,KAAK,EAAE,KAAK,EACZ,SAAS,EAAE,GAAG,CAAC,UAAU,CAAC,WAAW,CAAC,CAAC,CAAC,CAAC,SAAS,CAAC,CAAC,CAAC,GAAG,EACxD,QAAQ,EAAE,WAAW,KAAK,GAAG,EAC7B,IAAI,EAAE,KAAK,CAAC,GAAG,CAAC,EAChB,IAAI,EAAE,IAAI,EACV,eAAe,EAAE,GAAG,KAAK,OAAO,CAAC,CAAC,CAAC,UAAU,CAAC,CAAC,CAAC,SAAS,EACzD,SAAS,EAAE,SAAS,GACpB,CAAC;QACL,CAAC,CAAC,CACA,CACP,CAAC;AACJ,CAAC,CAAC","sourcesContent":["import React from 'react';\nimport classnames from 'classnames';\n\nimport MailHeader from './MailHeader/MailHeader';\nimport { SectionItem } from './Item/SectionItem';\nimport { Account, Tab } from '@src/types';\n\nimport Inbox from '@src/components/Icons/Inbox.svg';\nimport Archive from '@src/components/Icons/Archive.svg';\nimport Starred from '@src/components/Icons/Starred.svg';\nimport ReadLater from '@src/components/Icons/Clock.svg';\nimport Sent from '@src/components/Icons/Sent.svg';\nimport SendLater from '@src/components/Icons/SendLater.svg';\nimport FollowUp from '@src/components/Icons/FollowUp.svg';\nimport Draft from '@src/components/Icons/Draft.svg';\nimport Trash from '@src/components/Icons/Trash.svg';\n\nimport './MailSection.scss';\n\nconst ICONS = {\n  inbox: <Inbox width=\"14px\" height=\"13px\"/>,\n  archive: <Archive width=\"14px\" height=\"14px\" />,\n  trash: <Trash width=\"14px\" height=\"14px\" />,\n  starred: <Starred width=\"14px\" height=\"14px\" />,\n  later: <ReadLater width=\"14px\" height=\"14px\" />,\n  sent: <Sent width=\"14px\" height=\"12px\" />,\n  sendLater: <SendLater width=\"18px\" height=\"14px\" />,\n  followUp: <FollowUp width=\"14px\" height=\"15px\" />,\n  drafts: <Draft width=\"14px\" height=\"14px\" />,\n};\n\nexport interface Props {\n  title: string;\n  accounts: Account[];\n  selectedTab: string;\n  tabs: Tab[];\n  selectedAccount: Account;\n  collapseTrash: (collapse: boolean) => void;\n  trashCollapsed: boolean;\n  collapsed: boolean;\n}\n\nexport const MailSection = (props: Props) => {\n  const {\n    tabs, accounts, selectedTab, selectedAccount,\n    collapseTrash, trashCollapsed, collapsed,\n  } = props;\n  const conditionallyShow = ['later', 'starred', 'sendLater', 'followUp', 'outbox'];\n  const trashClick = (e: React.SyntheticEvent<HTMLElement>) => {\n    e.preventDefault();\n    e.stopPropagation();\n\n    collapseTrash(!trashCollapsed);\n  };\n\n  return (\n    <div className={classnames('mail-tab', { 'trash-collapsed': trashCollapsed })}>\n      {!collapsed && (\n        <MailHeader\n          accounts={accounts}\n          selectedAccount={selectedAccount}\n        />\n      )}\n      {tabs\n        .filter(t => conditionallyShow.indexOf(t.key) === -1 ||\n          (conditionallyShow.indexOf(t.key) > -1 && !!t.count))\n        .map((itemProps) => {\n          const { name, count, key, errorCopy } = itemProps;\n          let link = `/mail/${selectedAccount.id}/${key}`;\n\n          if (key.startsWith('accounts-')) {\n            link = `/mail/all/inbox/account-${key.slice(key.indexOf('-') + 1)}`;\n          } else if (key === 'inbox') {\n            link = `/mail/${selectedAccount.id}/inbox`;\n          }\n\n          return <SectionItem\n            key={key}\n            name={name}\n            count={count}\n            className={key.startsWith('accounts-') ? 'account' : key}\n            selected={selectedTab === key}\n            icon={ICONS[key]}\n            link={link}\n            disclosureClick={key === 'trash' ? trashClick : undefined}\n            errorCopy={errorCopy}\n          />;\n        })}\n    </div>\n  );\n};\n"]}]}