\nimport Archive from '@src/components/Icons/Archive.svg';\nimport Trash from '@src/components/Icons/Trash.svg';\nimport Starred from '@src/components/Icons/Starred.svg';\nimport Clock from '@src/components/Icons/Clock.svg';\nimport Label from '@src/components/Icons/Label.svg';\nimport UnreadAction from '../../Icons/UnreadAction.svg';\nimport Unsubscribe from '@src/components/Icons/Unsubscribe.svg';\nimport Bell from '@src/components/Icons/Bell.svg';\nimport HoverTip from '@src/components/Base/HoverTip';\nimport { HotkeyAction } from '@src/lib/hotkeys';\n\ninterface ThreadDetailHeaderItemProps {\n  box: ThreadBox | FilterBox | string;\n  onClick: (box: ThreadBox | FilterBox | string) => void;\n  thread?: Thread;\n  isBlocked?: boolean;\n}\nconst ICONS = {\n  inbox: <Inbox />,\n  archive: <Archive />,\n  trash: <Trash />,\n  starred: <Starred />,\n  later: <Clock />,\n  list: <Label />,\n  unread: <UnreadAction />,\n  'Block Sender': <Unsubscribe />,\n  'Mute Conversation': <Bell />,\n};\n\nconst ThreadDetailHeaderItem = (props: ThreadDetailHeaderItemProps) => {\n  const { thread, box, isBlocked } = props;\n  const onClick = (e: React.MouseEvent<HTMLButtonElement>) => {\n    props.onClick(props.box);\n  };\n\n  let action: HotkeyAction|undefined;\n  const meSent = thread && thread.sent;\n\n  // Identify selection state\n  let selected = false;\n  if (thread) {\n    switch (box) {\n      case FilterBox.Unread:\n        selected = !thread.read;\n        action = HotkeyAction.ToggleUnread;\n        break;\n      case FilterBox.Starred:\n        selected = thread.starred;\n        action = HotkeyAction.ToggleStar;\n        break;\n      case ThreadBox.Archive:\n        selected = thread.box === box;\n        action = HotkeyAction.Archive;\n        break;\n      case ThreadBox.Trash:\n        selected = thread.box === box;\n        action = HotkeyAction.Trash;\n        break;\n      case ThreadBox.Later:\n        selected = thread.box === box;\n        action = HotkeyAction.ReadLater;\n        break;\n      case ThreadBox.Inbox:\n        selected = thread.box === box;\n        action = meSent ? HotkeyAction.MoveSent : HotkeyAction.Inbox;\n        break;\n      case ThreadBox.List:\n        selected = thread.box === box;\n        action = HotkeyAction.Label;\n        break;\n      case 'Block Sender':\n        selected = !!isBlocked;\n        action = isBlocked ? HotkeyAction.UnblockSender : HotkeyAction.BlockSender;\n        break;\n      case 'Mute Conversation':\n        selected = thread.muted || false;\n        action = thread.muted ? HotkeyAction.UnmuteConversation : HotkeyAction.MuteConversation;\n        break;\n      default:\n        selected = thread.box === box;\n        break;\n    }\n  }\n\n  return (\n    <HoverTip\n      bottom=\"-30px\"\n      left=\"5px\"\n      copy={action || props.box}\n      hotkeyAction={action}\n    >\n      <button\n        className={classnames('circular-button', { selected })}\n        onClick={onClick}\n      >\n        {ICONS[props.box]}\n      </button>\n    </HoverTip>\n  );\n};\n\nexport default ThreadDetailHeaderItem;\n"]}]}