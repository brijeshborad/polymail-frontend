m 'react-redux';\nimport { StoreState, AttendeeStatus } from '@src/types';\nimport CalendarInviteBar, { StateProps, DispatchProps, Props } from './CalendarInviteBar';\nimport { calendarEventRespond } from '@src/ducks/calendars';\n\nconst mapStateToProps = (state: StoreState, ownProps: Props): StateProps => {\n  const { account } = ownProps.thread;\n  const threadAccount = state.accounts.find(a => a.id === account);\n  let status;\n\n  if (threadAccount && state.selectedCalendarEvent) {\n    const { email } = threadAccount;\n    const attendee = state.selectedCalendarEvent.attendees.find(a => a.email === email);\n    status = attendee && attendee.status;\n  }\n\n  return {\n    selectedCalendarEvent: state.selectedCalendarEvent,\n    status,\n  };\n};\n\nconst mapDispatchToProps = (dispatch: Dispatch<StoreState>): DispatchProps => ({\n  respond: (id: string, status: AttendeeStatus) =>\n    dispatch(calendarEventRespond({ id, status })),\n});\n\nexport default connect(mapStateToProps, mapDispatchToProps)(CalendarInviteBar);\n"]}]}