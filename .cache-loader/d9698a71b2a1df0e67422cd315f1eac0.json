,KAAK,SAAG;oBAC3D,oBAAC,KAAK,IAAC,IAAI,EAAC,aAAa,EAAC,SAAS,EAAE,SAAS,EAAE,KAAK,SAAG;oBACvD,QAAQ,IAAI,CACX,oBAAC,QAAQ,IAAC,IAAI,EAAC,kBAAkB,EAAC,EAAE,EAAC,GAAG,GAAG,CAC5C;oBACD,oBAAC,KAAK,IAAC,IAAI,EAAC,cAAc,EAAC,SAAS,EAAE,gBAAgB,EAAE,KAAK,SAAG;oBAChE,oBAAC,KAAK,IAAC,IAAI,EAAC,GAAG,EAAC,SAAS,EAAE,IAAI,CAAC,eAAe,EAAE,KAAK,SAAG,CAClD,CACL;YACL,OAAO,IAAI,6BAAK,SAAS,EAAC,KAAK;gBAC9B,8EAAsD;gBACtD,2BAAG,IAAI,EAAC,mCAAmC,EAAC,MAAM,EAAC,QAAQ,sBAAoB;gBAC/E,0CAAkB;gBAClB,2BAAG,IAAI,EAAC,+BAA+B,EAAC,MAAM,EAAC,QAAQ,wBAAsB;gBAC7E,sCAAc,CACV;YACN,oBAAC,KAAK,OAAG,CACL,CACP,CAAC;IACJ,CAAC;IACH,YAAC;AAAD,CAAC,AA1FD,CAAoB,KAAK,CAAC,aAAa,GA0FtC;AAED,6DAA6D;AAC7D,8DAA8D;AAC9D,oEAAoE;AACpE,kEAAkE;AAElE,eAAe,KAAK,CAAC","sourcesContent":["import React from 'react';\nimport { RouteComponentProps, Route, Switch, Redirect, RouteProps } from 'react-router';\nimport { Account, OAuthType } from '@src/types';\n\nimport LoginPane from './components/LoginPane';\n\n// TODO(SHIN): Beta - Re-enable when signup-flow is introduced\nimport SignupPane from './components/SignupPane';\nimport ConnectProvider from '@src/components/Form/ConnectProvider';\nimport ChoosePassword from './components/ChoosePassword';\n// import ForgotPassword from './components/ForgotPassword';\n// import ResetPassword from './components/ResetPassword';\n\nimport logo from './Icons/PolymailLogo@3x.png';\nimport darkLogo from '@src/routes/Login/Icons/PolymailLogoDarkMode@3x.png';\n\nimport './Login.scss';\nimport AddFastmail from '@src/components/Providers/AddFastmail/AddFastmail';\nimport AddiCloud from '@src/components/Providers/AddiCloud/AddiCloud';\nimport AddIMAP from '@src/components/Providers/AddIMAP';\nimport AddYahoo from '@src/components/Providers/AddYahoo/AddYahoo';\nimport Toast from '@src/components/Base/Toast';\nimport { LoginType } from '@src/api/auth';\nimport TeamSignupPane from '@src/routes/Login/components/TeamSignupPane';\nimport UserProfile from '@src/routes/Login/components/UserProfile';\nimport TwoFactorAuth from './components/TwoFactorAuth';\nimport browserHistory from '@src/lib/browserHistory';\nimport MagicLink from './components/MagicLink';\nimport MagicLinkSuccess from './components/MagicLinkSuccess';\nimport { AuthOAuthLoginMeta } from '@src/ducks/auth';\nimport ChoosePlan from './components/ChoosePlan';\n\nexport interface Props {\n  loggedIn: boolean;\n  loading: boolean;\n  accounts: Account[];\n  needsTfa: boolean;\n}\n\nexport interface DispatchProps {\n  login: (email: string, password: string, type: LoginType) => void;\n  resetPassword: (email: string) => void;\n  setToast: (error: Error) => void;\n  loginOAuth: (meta: AuthOAuthLoginMeta) => void;\n  setUnsubscriberSignup: (m: boolean) => void;\n  getLoginToken: () => void;\n}\n\ninterface State {\n  email: string;\n  password: string;\n}\n\nclass Login extends React.PureComponent<Props & DispatchProps & RouteComponentProps<any>, State> {\n  constructor() {\n    super();\n    this.state = {\n      email: '',\n      password: '',\n    };\n  }\n\n  componentDidMount() {\n    const { setUnsubscriberSignup } = this.props;\n    const search = new URLSearchParams(browserHistory.location.search);\n\n    const unsubscriberSignup = !!search.get('unsubscriber');\n\n    if (unsubscriberSignup) {\n      setUnsubscriberSignup(true);\n    }\n  }\n\n  handleLoginOauth = (type: OAuthType) => {\n    const { nonce, payload } = this.props.match.params;\n    this.props.loginOAuth({\n      type,\n      teamInviteNonce: nonce,\n      teamInvitePayload: payload,\n    });\n  }\n\n  renderLoginPane = (props: RouteProps) => {\n    return (\n        <LoginPane\n          login={this.props.login}\n          resetPassword={this.props.resetPassword}\n          setToast={this.props.setToast}\n          loading={this.props.loading}\n          loginOAuth={this.handleLoginOauth}\n          getLoginToken={this.props.getLoginToken}\n        />\n    );\n  }\n\n  render() {\n    const { match, loggedIn } = this.props;\n    const showTos = match.url === '/login/signup' || match.url === '/login/choosePassword';\n    const isDarkMode = document.body.getAttribute('theme') === 'theme-dark';\n    return (\n      <div className=\"login\">\n        <div className=\"login-header\">\n          {!isDarkMode && (\n            <a href=\"/\"><img src={logo} /></a>\n          )}\n          {isDarkMode && (\n            <a href=\"/\"><img src={darkLogo} /></a>\n          )}\n        </div>\n        <div className=\"login-container\">\n          <Switch>\n            <Route path=\"/login/team/:nonce/:payload\" component={this.renderLoginPane} exact />\n            <Route path=\"/login\" component={this.renderLoginPane} exact />\n            <Route path=\"/login/signup\" component={SignupPane} exact />\n            <Route path=\"/login/userProfile\" component={UserProfile} exact />\n            <Route path=\"/login/choosePlan\" component={ChoosePlan} exact />\n            <Route path=\"/login/signup/team/:nonce/:payload\" component={TeamSignupPane} exact />\n            <Route path=\"/login/connectProvider\" component={ConnectProvider} exact />\n            <Route path=\"/login/connectProvider/imap\" component={AddIMAP} exact />\n            <Route path=\"/login/connectProvider/yahoo\" component={AddYahoo} exact />\n            <Route path=\"/login/connectProvider/fastmail\" component={AddFastmail} exact />\n            <Route path=\"/login/connectProvider/icloud\" component={AddiCloud} exact />\n            <Route path=\"/login/choosePassword\" component={ChoosePassword} exact />\n            <Route path=\"/login/tfa\" component={TwoFactorAuth} exact />\n            <Route path=\"/redirect/*\" component={MagicLink} exact />\n            {loggedIn && (\n              <Redirect from=\"/redirect/magic*\" to=\"/\" />\n            )}\n            <Route path=\"/auth/magic*\" component={MagicLinkSuccess} exact />\n            <Route path=\"/\" component={this.renderLoginPane} exact />\n          </Switch>\n        </div>\n        {showTos && <div className=\"tos\">\n          <span>By creating an account, you agree to our </span>\n          <a href=\"https://polymail.io/legal/privacy\" target=\"_blank\">Privacy Policy </a>\n          <span> and </span>\n          <a href=\"https://polymail.io/legal/tos\" target=\"_blank\"> Terms of Service</a>\n          <span>.</span>\n        </div>}\n        <Toast />\n      </div>\n    );\n  }\n}\n\n// TODO(SHIN): Beta - re-enable when signup flow is available\n// <Route path=\"/login/userProfile\" component={UserProfile} />\n// <Route path=\"/login/forgotPassword\" component={ForgotPassword} />\n// <Route path=\"/login/resetPassword\" component={ResetPassword} />\n\nexport default Login;\n"]}]}