ate, meta) => {\n    const currentValue = state.hotkeyActions[meta] || 0;\n    const nextValue = currentValue + 1;\n    return update(createNesting(state, meta), {\n      hotkeyActions: {\n        [meta]: { $set: nextValue },\n      },\n    });\n  },\n});\n\n// Toggles hotkey calls.\nexport const {\n  type: HOTKEY_SUGGEST,\n  action: hotkeySuggest,\n  reducer: hotkeySuggestReducer,\n} = create<HotkeyAction|string|ConfirmationError, null>('HOTKEY_SUGGEST', { });\n\nexport default [\n  hotkeyUpdateReducer,\n  hotkeySuggestReducer,\n];\n"]}]}