B,EAChC,IAAI,EAAC,OAAO,EACZ,YAAY,EAAC,OAAO,GACpB;wBACF,oBAAC,SAAS,IACR,WAAW,EAAC,mBAAmB,EAC/B,WAAW,EAAC,6CAA6C,EACzD,KAAK,EAAE,IAAI,CAAC,KAAK,CAAC,QAAQ,EAC1B,QAAQ,EAAE,IAAI,CAAC,oBAAoB,EACnC,IAAI,EAAC,UAAU,EACf,YAAY,EAAC,cAAc,GAC3B,CACE,CACF;gBACN,gCACE,SAAS,EAAC,eAAe,EACzB,IAAI,EAAC,QAAQ,EACb,QAAQ,EAAE,OAAO;oBAEhB,OAAO,IAAI,oBAAC,MAAM,OAAG;+BAEf;gBACT,6BAAK,SAAS,EAAC,eAAe;;oBAE5B,oBAAC,IAAI,IAAC,EAAE,EAAC,QAAQ,kBAAmB,CAChC,CACD,CACH,CACP,CAAC;IACJ,CAAC;IACH,iBAAC;AAAD,CAAC,AAxJD,CAAyB,KAAK,CAAC,aAAa,GAwJ3C;AAED,eAAe,UAAU,CAAC","sourcesContent":["import React from 'react';\nimport { Link } from 'react-router-dom';\n\nimport TextInput from '@src/components/Form/TextInput';\nimport Google from '@src/components/Icons/Google.svg';\nimport Microsoft from '@src/components/Icons/Microsoft.svg';\n\nimport './SignupPane.scss';\nimport { isEmail } from '@src/lib/search';\nimport { MeCreateMeta } from '@src/ducks/me';\nimport { OAuthType } from '@src/types';\nimport Loader from '@src/components/Loader/Loader';\nimport { recordFBEvent } from '@src/lib/analytics';\nimport browserHistory from '@src/lib/browserHistory';\n\nexport interface DispatchProps {\n  signup: (user: MeCreateMeta) => void;\n  setToast: (e: Error) => void;\n  loginOAuth: (type: OAuthType) => void;\n}\n\nexport interface StateProps {\n  loading: boolean;\n  unsubscriber: boolean;\n}\n\ninterface State {\n  email: string;\n  password: string;\n}\n\nclass SignupPane extends React.PureComponent<DispatchProps & StateProps, State> {\n  constructor() {\n    super();\n    this.state = {\n      email: '',\n      password: '',\n    };\n  }\n\n  componentDidMount() {\n    const search = new URLSearchParams(browserHistory.location.search);\n    const unsubscriberSignup = !!search.get('unsubscriber');\n\n    if (!unsubscriberSignup) {\n      recordFBEvent('trackCustom', 'SignupViewed');\n    }\n  }\n\n  handleEmailChange = (e: React.FormEvent<HTMLInputElement>) => {\n    this.setState({ email: e.currentTarget.value });\n  }\n\n  handlePasswordChange = (e: React.FormEvent<HTMLInputElement>) => {\n    this.setState({ password: e.currentTarget.value });\n  }\n\n  handleContinue = (e: React.FormEvent<HTMLFormElement>) => {\n    e.preventDefault();\n\n    const { setToast, signup } = this.props;\n    const { email, password } = this.state;\n\n    if (!isEmail(email)) {\n      setToast(new Error('Please choose a valid email'));\n      return;\n    }\n\n    if (password.length < 8) {\n      setToast(new Error('Please choose a password that is at least 8 characters'));\n      return;\n    }\n\n    if (!password.match(/\\W|_/)) {\n      setToast(new Error('Passwords must contain at least 1 special character.'));\n      return;\n    }\n\n    if (email && password) {\n      signup({\n        email,\n        password,\n      });\n    }\n  }\n\n  handleGoogleLogin = (e: React.MouseEvent<HTMLButtonElement>) => {\n    e.preventDefault();\n    e.stopPropagation();\n\n    this.props.loginOAuth(OAuthType.Google);\n  }\n\n  handleMicrosoftLogin = (e: React.MouseEvent<HTMLButtonElement>) => {\n    e.preventDefault();\n    e.stopPropagation();\n\n    this.props.loginOAuth(OAuthType.Microsoft);\n  }\n\n  render() {\n    const { loading, unsubscriber } = this.props;\n\n    let titleCopy = 'Start your free 7 day-trial!';\n    if (unsubscriber) {\n      titleCopy = 'Get Started with Unsubcriber!';\n    }\n\n    return (\n      <div className=\"signup-pane\">\n        <form className=\"user-fields-wrapper\" onSubmit={this.handleContinue}>\n          <div className=\"signup-prompt\">\n            <div className=\"title\">\n              {titleCopy}\n            </div>\n          </div>\n          <div className=\"oauth\">\n            <button\n              className=\"google\"\n              onClick={this.handleGoogleLogin}\n              type=\"button\"\n            >\n              <div className=\"icon\">\n                <Google width=\"18px\" height=\"18px\" />\n              </div>\n              <div className=\"text\">\n                Sign in with Google\n              </div>\n            </button>\n            <button\n              className=\"microsoft\"\n              onClick={this.handleMicrosoftLogin}\n              type=\"button\"\n            >\n              <div className=\"icon\">\n                <Microsoft width=\"18px\" height=\"18px\" />\n              </div>\n              <div className=\"text\">\n                Sign in with Microsoft\n              </div>\n            </button>\n          </div>\n          <div className=\"separator-container\">\n            <div className=\"separator\" />\n            <div className=\"text\">or</div>\n            <div className=\"separator\" />\n          </div>\n          <div className=\"user-fields\">\n            <div>\n              <TextInput\n                description=\"Work email\"\n                placeholder=\"Email\"\n                value={this.state.email}\n                onChange={this.handleEmailChange}\n                type=\"email\"\n                autoComplete=\"email\"\n              />\n              <TextInput\n                description=\"Choose a password\"\n                placeholder=\"8 characters or more, 1+ special characters\"\n                value={this.state.password}\n                onChange={this.handlePasswordChange}\n                type=\"password\"\n                autoComplete=\"new-password\"\n              />\n            </div>\n          </div>\n          <button\n            className=\"signup-button\"\n            type=\"submit\"\n            disabled={loading}\n          >\n            {loading && <Loader />}\n            Continue\n          </button>\n          <div className=\"sign-up-label\">\n            Already have a Polymail account?\n            <Link to=\"/login\"> Login here</Link>\n          </div>\n        </form>\n      </div>\n    );\n  }\n}\n\nexport default SignupPane;\n"]}]}