AAY,EAC/B,MAAM,EAAE,IAAI,CAAC,cAAc,GAC3B,CACE,CAEJ,CACP,CAAC;IACJ,CAAC;IACH,eAAC;AAAD,CAAC,AAnHD,CAA8B,KAAK,CAAC,aAAa,GAmHhD","sourcesContent":["import React from 'react';\nimport classnames from 'classnames';\nimport { Link } from 'react-router-dom';\nimport ListMenu from './ListMenu';\n\nimport Label from '@src/components/Icons/Label.svg';\nimport Disclosure from '@src/components/Icons/Disclosure.svg';\nimport ThreeDots from '@src/components/Icons/ThreeDots.svg';\n\nimport './ListItem.scss';\nimport { List } from '@src/types';\nimport { MAX_LIST_COUNT } from '@src/lib/models/mailCount';\n\ninterface Props {\n  list?: List;\n  name: string;\n  count: number;\n  expanded: boolean;\n  expandable: boolean;\n  indent: number;\n  onExpand: (key: string) => void;\n  selected: boolean;\n  link: string;\n  listId?: string;\n  editable?: boolean;\n\n  onDelete: (list: List) => void;\n  onUpdate: (oldList: List, newName: string) => void;\n  onCreate: (name: string) => void;\n}\n\ninterface State {\n  showMenu: boolean;\n}\n\nexport class ListItem extends React.PureComponent<Props, State> {\n  private menu: ListMenu;\n\n  constructor() {\n    super();\n\n    this.state = { showMenu: false };\n  }\n\n  // Abstract click event to handle state better\n  expandItem = (e: React.SyntheticEvent<any>) => {\n    e.preventDefault();\n    e.stopPropagation();\n    if (this.props.listId) {\n      this.props.onExpand(this.props.listId);\n    }\n  }\n\n  toggleMenu = (e: React.MouseEvent<HTMLElement>) => {\n    e.stopPropagation();\n    e.preventDefault();\n\n    this.menu.toggle();\n    this.setState({ showMenu: !this.state.showMenu });\n  }\n\n  handleMenuHide = () => {\n    this.setState({ showMenu: false });\n  }\n\n  handleRef = (ref: ListMenu) => {\n    this.menu = ref;\n  }\n\n  handleCreate = (name: string) => {\n    if (this.props.list) {\n      const newName = this.props.list.name + '/' + name;\n      this.props.onCreate(newName);\n    }\n  }\n\n  handleUpdate = (name: string) => {\n    if (this.props.list) {\n      this.props.onUpdate(this.props.list, name);\n    }\n  }\n\n  handleDelete = () => {\n    if (this.props.list) {\n      this.props.onDelete(this.props.list);\n    }\n  }\n\n  render() {\n    // Expandable Button for item\n    const { expanded, expandable, selected, indent, count, link, editable, list } = this.props;\n    const { showMenu } = this.state;\n    const expandableButton = (\n      <div className=\"left-disclosure-area\">\n        <button onClick={this.expandItem}>\n          <Disclosure\n            className={classnames('left-sidebar-left-disclosure-button', { expanded })}\n          />\n        </button>\n      </div>\n    );\n\n    const name = this.props.name;\n    return (\n      <div\n        key={list && list.id + `${selected}`}\n        className={classnames('list-item', {\n          selected,\n          menu: showMenu,\n        })}\n      >\n        <Link\n          to={link}\n          className={\n            classnames('left-sidebar-list-item', {\n              empty: count === 0,\n              expandable,\n            })\n          }\n        >\n          <div className=\"spacer\" style={{ paddingLeft: indent * 16 }}/>\n          {expandable && expandableButton}\n          <div className=\"icon\">\n            <Label height=\"12px\" width=\"12px\" />\n          </div>\n          <div\n            className=\"middle\"\n          >\n            {name}\n          </div>\n          <div className=\"count\">{count >= MAX_LIST_COUNT ? `${MAX_LIST_COUNT}+` : count}</div>\n        </Link>\n        {editable &&\n          <div className=\"menu\">\n            <button className=\"context-button\" onClick={this.toggleMenu}>\n              <ThreeDots />\n            </button>\n            <ListMenu\n              name={name}\n              ref={this.handleRef}\n              onCreateList={this.handleCreate}\n              onUpdateList={this.handleUpdate}\n              onDeleteList={this.handleDelete}\n              onHide={this.handleMenuHide}\n            />\n          </div>\n        }\n      </div>\n    );\n  }\n}\n"]}]}