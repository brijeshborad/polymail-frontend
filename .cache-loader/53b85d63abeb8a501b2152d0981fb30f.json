ort { toastSet } from '@src/ducks/toasts';\nimport { selectUserIsAdmin } from '@src/ducks/me';\nimport { teamUpdate } from '@src/ducks/team';\nimport { accountUpdate } from '@src/ducks/accounts';\nimport { leftSidebarCollapse } from '@src/ducks/leftSidebar';\n\nconst mapStateToProps = (state: StoreState): StateProps => {\n  const billingSubscription = state.billing.resource && state.billing.resource.data &&\n    state.billing.resource.data.subscription || undefined;\n\n  return {\n    team: state.team.data,\n    teamInvites: state.teamInvites.data,\n    teamMembers: state.teamMembers,\n    isAdmin: selectUserIsAdmin(state),\n    getContact: (email: string) => selectContact(state, email),\n    accounts: state.accounts.filter(a => a.type !== AccountType.Alias),\n    teamAccount: state.accounts.find(a => !!a.team),\n    billingSubscription,\n    typeaheadContacts: state.typeaheadContacts,\n    isMobile: state.isMobile,\n    collapsedMobile: state.leftSidebar.collapsed,\n  };\n};\n\nconst mapDispatchToProps = (dispatch: Dispatch<StoreState>): DispatchProps => ({\n  listTeamInvite: () => dispatch(teamInviteList(null)),\n  fetchTeamMembers: () => dispatch(teamMemberFetch(null)),\n  addTeamAdmin: user => dispatch(teamAdminAdd(user)),\n  removeTeamAdmin: user => dispatch(teamAdminRemove(user)),\n  resendTeamInvite: invite => dispatch(teamInviteResend(invite)),\n  setToast: error => dispatch(toastSet(error)),\n  inviteTeamMember: email => dispatch(teamMemberInvite(email)),\n  updateTeam: meta => dispatch(teamUpdate(meta)),\n  updateAccount: account => dispatch(accountUpdate(account)),\n  removeTeamMember: user => dispatch(teamMemberRemove(user)),\n  contactTypeahead: partial => dispatch(contactTypeahead(partial)),\n  clearContactTypeahead: () => dispatch(contactTypeaheadClear()),\n  removeTeamInvite: invite => dispatch(teamInviteRemove(invite)),\n  collapseLsb: collapse => dispatch(leftSidebarCollapse(collapse)),\n});\n\nexport default connect<StateProps, DispatchProps>(mapStateToProps, mapDispatchToProps)(Team);\n"]}]}