AK,oBAAoB,CAAC,QAAQ,IAAI,6BAAK,SAAS,EAAC,UAAU,2CAE/E;YACL,cAAc,CAAC,MAAM,KAAK,oBAAoB,CAAC,QAAQ,IAAI,6BAAK,SAAS,EAAC,WAAW,uEAEhF;YACL,cAAc,CAAC,MAAM,KAAK,oBAAoB,CAAC,OAAO,IAAI,CAAC,MAAM;gBAChE,gCAAQ,SAAS,EAAC,eAAe,EAAC,OAAO,EAAE,IAAI,CAAC,MAAM,aAE7C;YACV,cAAc,CAAC,MAAM,KAAK,oBAAoB,CAAC,OAAO,IAAI,MAAM;gBAC/D,6BAAK,SAAS,EAAC,QAAQ,cAEjB,CACJ,CACP,CAAC;IACJ,CAAC;IACH,yBAAC;AAAD,CAAC,AA3CD,CAAiC,KAAK,CAAC,aAAa,GA2CnD;AAED,eAAe,kBAAkB,CAAC","sourcesContent":["import React from 'react';\nimport { ReferralInvite, ReferralInviteStatus } from '@src/types';\nimport './ReferralInviteCell.scss';\nimport { generatePhotoUrl } from '@src/lib/contact';\n\nexport interface Props {\n  referralInvite: ReferralInvite;\n}\n\nexport interface DispatchProps {\n  resendInvite: (id: string) => void;\n}\n\ninterface State {\n  resent: boolean;\n}\n\nclass ReferralInviteCell extends React.PureComponent<DispatchProps & Props, State> {\n  constructor(props: DispatchProps & Props) {\n    super(props);\n\n    this.state = { resent: false };\n  }\n\n  resend = () => {\n    const { referralInvite, resendInvite } = this.props;\n\n    resendInvite(referralInvite.id);\n    this.setState({ resent: true });\n  }\n\n  render() {\n    const { referralInvite } = this.props;\n    const { resent } = this.state;\n    const photoUrl = generatePhotoUrl(referralInvite.email);\n\n    return (\n      <div className=\"referral-invite-cell\">\n        <img className=\"photo\" src={photoUrl} />\n        <div className=\"info\">\n          <h6 className=\"full-name\" />\n          <div className=\"email\">{referralInvite.email}</div>\n        </div>\n        {referralInvite.status === ReferralInviteStatus.Redeemed && <div className=\"redeemed\">\n          You received credit for this invite!\n        </div>}\n        {referralInvite.status === ReferralInviteStatus.SignedUp && <div className=\"signed-up\">\n          Signed Up.  Next, have your friend send a message with Polymail.\n        </div>}\n        {referralInvite.status === ReferralInviteStatus.Pending && !resent &&\n          <button className=\"resend-button\" onClick={this.resend}>\n            Resend\n          </button>}\n        {referralInvite.status === ReferralInviteStatus.Pending && resent &&\n          <div className=\"resent\">\n            Resent!\n          </div>}\n      </div>\n    );\n  }\n}\n\nexport default ReferralInviteCell;\n"]}]}