B,MAAM,CAAC,IAAI,CAAC,GAAG,EAAE,QAAQ,CAAC,CAAC;IAC7B,CAAC,CAAC;IAEF,IAAM,SAAS,GAAG,IAAI,CAAC,SAAS,CAAC;IACjC,IAAM,IAAI,GAAG,CAAC,IAAI,KAAK,QAAQ,CAAC,IAAI,IAAI,IAAI,CAAC,IAAI,CAAC,IAAI,IAAI,CAAC,UAAU,CAAC;IAEtE,OAAO,CACL,gCACE,SAAS,EAAE,UAAU,CAAC,SAAS,EAAE,IAAI,EAAE,SAAS,CAAC,EACjD,OAAO,EAAE,WAAW;QAEpB,oBAAC,IAAI,OAAG,CACD,CACV,CAAC;AACJ,CAAC,CAAC;AAEF,eAAe,UAAU,CAAC","sourcesContent":["import React from 'react';\nimport classnames from 'classnames';\n\nimport { Social } from '@src/types';\n\nimport './SocialIcon.scss';\n\nimport Twitter from './Icons/Twitter.svg';\nimport Facebook from './Icons/Facebook.svg';\nimport LinkedIn from './Icons/LinkedIn.svg';\nimport AngelList from './Icons/AngelList.svg';\n\nimport FacebookIcon from './Icons/FacebookIcon.svg';\n\nconst SOCIAL_PROPERTIES = {\n  twitter: {\n    buttonIcon: Twitter,\n    className: 'tt-s',\n  },\n  facebook: {\n    buttonIcon: Facebook,\n    className: 'fb-s',\n    icon: FacebookIcon,\n  },\n  linkedin: {\n    buttonIcon: LinkedIn,\n    className: 'linkedIn',\n  },\n  angellist: {\n    buttonIcon: AngelList,\n    className: 'angelList',\n  },\n};\n\ninterface Props {\n  social: Social;\n  type: IconType;\n}\n\nexport const enum IconType {\n  Icon = 'icon',\n  Button = 'button',\n}\n\nconst SocialIcon = (props: Props) => {\n  const { social, type } = props;\n  const { network, url } = social;\n  const item = SOCIAL_PROPERTIES[network];\n\n  if (!item) {\n    return <span />;\n  }\n\n  const handleClick = (e: React.MouseEvent<HTMLElement>) => {\n    e.stopPropagation();\n    e.preventDefault();\n\n    window.open(url, '_blank');\n  };\n\n  const className = item.className;\n  const Icon = (type === IconType.Icon && item.icon) || item.buttonIcon;\n\n  return (\n    <button\n      className={classnames('ss-icon', type, className)}\n      onClick={handleClick}\n    >\n      <Icon />\n    </button>\n  );\n};\n\nexport default SocialIcon;\n"]}]}