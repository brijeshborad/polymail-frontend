AAa;gBACb,2BAAG,SAAS,EAAC,aAAa,gCAA8B;gBACvD,CAAC,eAAe;oBACf,gCAAQ,SAAS,EAAC,SAAS,EAAC,OAAO,EAAE,IAAI,CAAC,aAAa,cAAkB;gBAC1E,eAAe,IAAI,eAAe,CAAC,MAAM,KAAK,iBAAiB,CAAC,EAAE;oBACjE,gCAAQ,SAAS,EAAC,SAAS,EAAC,OAAO,EAAE,IAAI,CAAC,aAAa,kBAAsB;gBAC9E,eAAe,IAAI,eAAe,CAAC,MAAM,KAAK,iBAAiB,CAAC,EAAE;oBACjE,6BAAK,SAAS,EAAC,SAAS,cAAc;gBACvC,eAAe,IAAI,eAAe,CAAC,MAAM,KAAK,iBAAiB,CAAC,EAAE;oBACjE,gCAAQ,SAAS,EAAC,WAAW,EAAC,OAAO,EAAE,IAAI,CAAC,eAAe,gBAAoB,CAC7E;YACN,6BAAK,SAAS,EAAC,qBAAqB;gBAClC,2BAAG,SAAS,EAAC,SAAS,0CAAwC;gBAC9D;oBACE,iHAAuF;oBACvF;;wBACE,+DAAkC,CAC/B,CACF,CAID,CACF,CACP,CAAC;IACJ,CAAC;IACH,WAAC;AAAD,CAAC,AA7DD,CAAmB,KAAK,CAAC,aAAa,GA6DrC;AAED,eAAe,IAAI,CAAC","sourcesContent":["import React from 'react';\nimport browserHistory from '@src/lib/browserHistory';\nimport NavigationHeader from '@src/routes/Settings/components/NavigationHeader';\nimport ZoomLogo from '@src/components/Icons/Zoom.svg';\nimport { Team, Integration, IntegrationStatus } from '@src/types';\n\nimport './Zoom.scss';\n\nexport interface StateProps {\n  zoomIntegration?: Integration;\n  team: Team;\n  refreshIntegrations: boolean;\n}\n\nexport interface DispatchProps {\n  loginZoom: (team: string) => void;\n  logoutZoom: () => void;\n  listIntegrations: () => void;\n}\n\nclass Zoom extends React.PureComponent<StateProps & DispatchProps> {\n  handleOnBack = () => {\n    browserHistory.replace('/settings/integrations');\n  }\n\n  UNSAFE_componentWillReceiveProps(nextProps: StateProps & DispatchProps) {\n    if (nextProps.refreshIntegrations && !this.props.refreshIntegrations) {\n      nextProps.listIntegrations();\n    }\n  }\n\n  handleInstall = () => {\n    const { loginZoom, team } = this.props;\n\n    if (team) {\n      loginZoom(team.id);\n    }\n  }\n\n  handleUninstall = () => {\n    this.props.logoutZoom();\n  }\n\n  render() {\n    const { zoomIntegration } = this.props;\n\n    return (\n      <div className=\"settings-panel zoom-integration\">\n        <NavigationHeader\n          title=\"Integrations\"\n          onBack={this.handleOnBack}\n          contextButton={<span className=\"context\">Zoom</span>}\n        />\n        <div className=\"header\">\n          <div className=\"logo\"><ZoomLogo width=\"85px\" height=\"85px\" /></div>\n          <h1>Zoom</h1>\n          <p className=\"description\">Connect your Zoom Account</p>\n          {!zoomIntegration &&\n            <button className=\"install\" onClick={this.handleInstall}>Install</button>}\n          {zoomIntegration && zoomIntegration.status !== IntegrationStatus.Ok &&\n            <button className=\"install\" onClick={this.handleInstall}>Reauthorize</button>}\n          {zoomIntegration && zoomIntegration.status === IntegrationStatus.Ok &&\n            <div className=\"enabled\">ENABLED</div>}\n          {zoomIntegration && zoomIntegration.status === IntegrationStatus.Ok &&\n            <button className=\"uninstall\" onClick={this.handleUninstall}>Uninstall</button>}\n        </div>\n        <div className=\"integration-details\">\n          <p className=\"details\">With the Zoom Integration, you can:</p>\n          <ul>\n            <li>Generate Zoom meeting urls when someone books a meeting on your calendar page.</li>\n            <li>Create a Zoom meeting url directly from the Command Center, with the command&nbsp;\n              <b>\"Zoom: Create Meeting Link\"</b>\n            </li>\n          </ul>\n          {/* <div className=\"screenshot-container\">\n            <img src={salesforceScreenshot} className=\"screenshot\" />\n          </div> */}\n        </div>\n      </div>\n    );\n  }\n}\n\nexport default Zoom;\n"]}]}