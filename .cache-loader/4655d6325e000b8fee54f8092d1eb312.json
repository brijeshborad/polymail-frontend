ed = sequenceStageRecipient.updated ?\n      new Date(sequenceStageRecipient.updated) : new Date();\n    const scheduled = sequenceRecipient.scheduled ?\n      new Date(sequenceRecipient.scheduled) : new Date();\n\n    return {\n      ...sequenceStageRecipient,\n      email: sequenceRecipient.email,\n      fullName: `${sequenceRecipient.givenName} ${sequenceRecipient.familyName}`,\n      paused: sequenceRecipient.paused,\n      sequenceStatus: sequenceRecipient.status,\n      willSend: scheduled,\n\n      created,\n      updated,\n    };\n  };\n"]}]}