, List, Mailbox, MailCount, LeftSidebarCount, Features,\n} from '@src/types';\n\nimport './Campaigns.scss';\n\nimport LeftSidebar from '@src/components/LeftSidebar';\nimport UpgradePrompt from '@src/components/Base/UpgradePrompt/UpgradePrompt';\nimport Toast from '@src/components/Base/Toast';\n\nexport interface Props {\n  contact: Contact;\n  contactGroups: { [id: string]: ContactGroup; };\n  accounts: Account[];\n  lists: List[];\n  selectedMailbox: Mailbox;\n  mailCount: MailCount;\n  leftSidebarCount: LeftSidebarCount;\n  features: Features;\n}\n\nexport interface DispatchProps {\n  fetchData: () => Action;\n}\n\nclass Campaigns extends React.PureComponent<Props & DispatchProps> {\n  render() {\n    const {\n      accounts,\n      lists,\n      selectedMailbox,\n      mailCount,\n      leftSidebarCount,\n      features,\n    } = this.props;\n    const LeftSidebarConnect = LeftSidebar as any;\n\n    return (\n      <div className={classnames('campaigns', { 'should-upgrade': !features.campaigns })}>\n        <LeftSidebarConnect\n          accounts={accounts}\n          lists={lists}\n          selectedMailbox={selectedMailbox}\n          mailCount={mailCount}\n          leftSidebarCount={leftSidebarCount}\n        />\n        <div className=\"main-container\">\n          <iframe\n            className=\"main\"\n            src={`${process.env.APPS_URL}/v1/campaigns?disableIntercom=true`}\n          />\n          {!features.campaigns && <div className=\"upgrade-container\">\n            <UpgradePrompt\n              title=\"Campaigns\"\n              description=\"To send personalized email campaigns, upgrade to a paid plan.\"\n            />\n          </div>}\n        </div>\n        <Toast />\n      </div>\n    );\n  }\n}\n\nexport default Campaigns;\n"]}]}