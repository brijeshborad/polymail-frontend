C,kBAAkB,EAAE;YAC9D,IAAM,oBAAkB,GAAG,UAAU,CAAC,OAAO,CAAC,KAAK,EAAE,EAAE,CAAC,CAAC;YACzD,IAAM,WAAW,GAAG,MAAM,CAAC,IAAI,CAAC,YAAY,CAAC;iBAC1C,MAAM,CAAC,UAAC,GAAG;gBACV,IAAM,aAAa,GAAG,YAAY,CAAC,GAAG,CAAC,IAAI,YAAY,CAAC,GAAG,CAAC,CAAC,OAAO,CAAC,KAAK,EAAE,EAAE,CAAC,CAAC;gBAChF,OAAO,aAAa,KAAK,oBAAkB,CAAC;YAC9C,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;YAER,IAAI,CAAC,QAAQ,CAAC;gBACZ,KAAK,EAAE,WAAW,IAAI,SAAS;aAChC,CAAC,CAAC;SACJ;IACH,CAAC;IAOD,2BAAM,GAAN;QACE,OAAO,CACL,gCACE,QAAQ,EAAE,IAAI,CAAC,QAAQ,EACvB,SAAS,EAAC,qBAAqB,EAC/B,KAAK,EAAE,IAAI,CAAC,KAAK,CAAC,KAAK,EACvB,QAAQ,EAAE,CAAC,CAAC,IAEV,MAAM,CAAC,IAAI,CAAC,YAAY,CAAC,CAAC,GAAG,CAAC,UAAA,IAAI;YAClC,OAAA,gCAAQ,GAAG,EAAE,IAAI,EAAE,KAAK,EAAE,IAAI,IAC3B,IAAI,CACE;QAFT,CAES,CACV,CAEM,CACV,CAAC;IACJ,CAAC;IACH,iBAAC;AAAD,CAAC,AA9DD,CAAyB,KAAK,CAAC,aAAa,GA8D3C;AAED,eAAe,UAAU,CAAC","sourcesContent":["import React from 'react';\n\nimport { WebSafeFonts } from '@src/lib/fonts/WebSafeFonts';\n\ninterface Props {\n  selectedFontFamily?: string;\n  onSelect: (value: string) => void;\n}\ninterface State {\n  value: string;\n}\n\nclass FontSelect extends React.PureComponent<Props, State> {\n  constructor() {\n    super();\n    this.state = {\n      value: 'Default',\n    };\n  }\n\n  componentDidMount() {\n    const fontFamily = this.props.selectedFontFamily;\n    if (fontFamily) {\n      const strippedFontFamily = fontFamily.replace(/\\W/g, '');\n      const matchedFont = Object.keys(WebSafeFonts)\n        .filter((key) => {\n          const strippedValue = WebSafeFonts[key] && WebSafeFonts[key].replace(/\\W/g, '');\n          return strippedValue === strippedFontFamily;\n        })[0];\n\n      this.setState({\n        value: matchedFont || 'Default',\n      });\n    }\n  }\n\n  UNSAFE_componentWillReceiveProps(nextProps: Props) {\n    const fontFamily = nextProps.selectedFontFamily;\n    if (fontFamily && fontFamily !== this.props.selectedFontFamily) {\n      const strippedFontFamily = fontFamily.replace(/\\W/g, '');\n      const matchedFont = Object.keys(WebSafeFonts)\n        .filter((key) => {\n          const strippedValue = WebSafeFonts[key] && WebSafeFonts[key].replace(/\\W/g, '');\n          return strippedValue === strippedFontFamily;\n        })[0];\n\n      this.setState({\n        value: matchedFont || 'Default',\n      });\n    }\n  }\n\n  onChange = (e: React.FormEvent<HTMLSelectElement>) => {\n    this.setState({ value: e.currentTarget.value });\n    this.props.onSelect(WebSafeFonts[e.currentTarget.value] || '');\n  }\n\n  render() {\n    return (\n      <select\n        onChange={this.onChange}\n        className=\"compose-font-select\"\n        value={this.state.value}\n        tabIndex={-1}\n      >\n        { Object.keys(WebSafeFonts).map(font =>\n          <option key={font} value={font}>\n            {font}\n          </option>,\n        )\n        }\n      </select>\n    );\n  }\n}\n\nexport default FontSelect;\n"]}]}