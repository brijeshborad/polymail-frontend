e?: Date;\n  isMobile?: boolean;\n}\n\nconst NotificationEnableWarning: React.SFC<Props> = ({ onClose, expirationDate, isMobile }) => {\n  const daysLeft = trialDaysLeft(expirationDate);\n  return (\n    <div className={classnames('banner trial-expiring', isMobile && 'mobile')}>\n      <div className=\"main\">\n        <span>\n          {`${daysLeft} ${daysLeft > 1 ? 'days' : 'day'} left in your Polymail trial. `}\n          <Link to=\"/settings/billing\">Select a plan</Link>\n        </span>\n      </div>\n      <span className=\"close-button-container\" onClick={onClose}>\n        <Close className=\"close-button\" />\n      </span>\n    </div>\n  );\n};\n\nexport default NotificationEnableWarning;\n"]}]}