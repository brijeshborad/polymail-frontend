CL;wBACF,oBAAC,KAAK,IACJ,IAAI,EAAC,mCAAmC,EACxC,SAAS,EAAE,cAAc,EACzB,GAAG,EAAC,mCAAmC,EACvC,KAAK,SACL;wBACF,oBAAC,KAAK,IACJ,IAAI,EAAC,iCAAiC,EACtC,SAAS,EAAE,cAAc,EACzB,GAAG,EAAC,iCAAiC,EACrC,KAAK,SACL;wBACF,oBAAC,KAAK,IACJ,IAAI,EAAC,wCAAwC,EAC7C,SAAS,EAAE,mBAAmB,EAC9B,GAAG,EAAC,wCAAwC,EAC5C,KAAK,SACL;wBACF,oBAAC,KAAK,IACJ,IAAI,EAAC,mDAAmD,EACxD,SAAS,EAAE,mBAAmB,EAC9B,GAAG,EAAC,mDAAmD,EACvD,KAAK,SACL,CACK,CACL;gBACL,CAAC,QAAQ,CAAC,SAAS,IAAI,aAAa,KAAK,WAAW;oBACnD,6BAAK,SAAS,EAAC,mBAAmB;wBAChC,oBAAC,aAAa,IACZ,KAAK,EAAC,WAAW,EACjB,WAAW,EAAE,yCAAyC;gCACpD,6CAA6C,GAC/C,CACE;gBACP,CAAC,QAAQ,CAAC,SAAS,IAAI,aAAa,KAAK,WAAW;oBACnD,6BAAK,SAAS,EAAC,mBAAmB;wBAChC,oBAAC,aAAa,IACZ,KAAK,EAAC,WAAW,EACjB,WAAW,EAAE,oBAAoB;gCAC/B,6CAA6C,GAC/C,CACE,CACJ;YACN,oBAAC,cAAc,IAAC,OAAO,EAAE,kBAAkB,CAAC,MAAM,GAAI;YACtD,oBAAC,KAAK,OAAG,CACL,CACP,CAAC;IACJ,CAAC;IACH,gBAAC;AAAD,CAAC,AAxJD,CAAwB,KAAK,CAAC,aAAa,GAwJ1C;AAED,eAAe,SAAS,CAAC","sourcesContent":["import React from 'react';\nimport classnames from 'classnames';\nimport { Switch, Route, RouteComponentProps } from 'react-router';\nimport { Features, CommandControlType, Action, MailboxType } from '@src/types';\nimport LeftSidebar from '@src/components/LeftSidebar';\n\nimport './Sequences.scss';\nimport SequenceList from './routes/SequenceList';\nimport SequenceDetail from './routes/SequenceDetail';\nimport SequenceStageDetail from './routes/SequenceStageDetail';\nimport Campaigns from './routes/Campaigns/Campaigns';\nimport { recordEvent, UserEvents } from '@src/lib/analytics';\nimport Toast from '@src/components/Base/Toast';\nimport UpgradePrompt from '@src/components/Base/UpgradePrompt/UpgradePrompt';\nimport CommandControl from '@src/components/CommandControl';\nimport { getHotkey, HotkeyAction } from '@src/lib/hotkeys';\nimport { commandControlShow, commandControlHide } from '@src/ducks/commandControl';\nimport browserHistory from '@src/lib/browserHistory';\nimport ContactSupport from '@src/components/ContactSupport';\n\nexport interface StateProps {\n  sequenceCount?: number;\n  features: Features;\n  selectedTitle: string;\n}\n\nexport interface DispatchProps {\n  dispatchAction: (action: Action<any, any>) => void;\n}\n\nclass Sequences extends React.PureComponent<StateProps & RouteComponentProps<any> & DispatchProps> {\n  componentDidMount() {\n    const { dispatchAction } = this.props;\n    recordEvent(UserEvents.SequencesOpened);\n    dispatchAction(commandControlHide(undefined));\n  }\n\n  handleKeyDown = (e: React.KeyboardEvent<HTMLDivElement>) => {\n    const {\n      dispatchAction,\n    } = this.props;\n    let handled = false;\n\n    const activeElement = document.activeElement;\n    const ignoreInput = activeElement &&\n    (\n      ['INPUT', 'TEXTAREA'].indexOf(activeElement.tagName) !== -1\n      || (activeElement as HTMLElement).isContentEditable\n    );\n\n    if (!!ignoreInput) {\n      return;\n    }\n\n    const hotkey = getHotkey(e);\n    switch (hotkey && hotkey.name) {\n      case HotkeyAction.NavigationCenter:\n        this.props.dispatchAction(commandControlShow(CommandControlType.Navigation));\n        handled = true;\n        break;\n      // Navigation\n      case HotkeyAction.NavigateInbox:\n      case HotkeyAction.NavigateStarred:\n      case HotkeyAction.NavigateReadLater:\n      case HotkeyAction.NavigateArchive:\n      case HotkeyAction.NavigateTrash:\n      case HotkeyAction.NavigateFollowUp:\n      case HotkeyAction.NavigateSent:\n      case HotkeyAction.NavigateSendLater:\n      case HotkeyAction.NavigateDraft:\n        if (hotkey && hotkey.route) {\n          const newMailbox = { type: MailboxType.Any };\n          browserHistory.push(hotkey.route(newMailbox));\n          handled = true;\n        }\n        break;\n      case HotkeyAction.ToggleCommandControl:\n        dispatchAction(commandControlShow(CommandControlType.Thread));\n        handled = true;\n        break;\n      default:\n        break;\n    }\n\n    if (handled) {\n      e.preventDefault();\n      e.stopPropagation();\n    }\n  }\n  render() {\n    const { sequenceCount, features, selectedTitle } = this.props;\n    const LeftSidebarConnect = LeftSidebar as any;\n\n    return (\n      <div\n        className={classnames('sequences', {\n          'should-upgrade':selectedTitle !== 'Campaigns' ?\n            !features.sequences :\n            !features.campaigns,\n        })}\n        onKeyDown={this.handleKeyDown}\n      >\n        <ContactSupport />\n        <LeftSidebarConnect\n          accounts={[]}\n          lists={[]}\n          count={sequenceCount}\n          selectedTitle={selectedTitle}\n        />\n        <div\n          className=\"main-container\"\n          tabIndex={-1}\n        >\n          <div className=\"main\">\n            <Switch>\n              <Route path=\"/sequences\" component={SequenceList} key=\"/sequences\" exact />\n              <Route\n                path=\"/sequences/archive\"\n                component={SequenceList}\n                key=\"/sequences/archive\"\n                exact\n              />\n              <Route\n                path=\"/sequences/campaigns\"\n                component={Campaigns}\n                key=\"/sequences/campaigns\"\n                exact\n              />\n              <Route\n                path=\"/sequences/:sequenceId\"\n                component={SequenceDetail}\n                key=\"/sequences/:sequenceId\"\n                exact\n              />\n              <Route\n                path=\"/sequences/:sequenceId/recipients\"\n                component={SequenceDetail}\n                key=\"/sequences/:sequenceId/recipients\"\n                exact\n              />\n              <Route\n                path=\"/sequences/:sequenceId/settings\"\n                component={SequenceDetail}\n                key=\"/sequences/:sequenceId/settings\"\n                exact\n              />\n              <Route\n                path=\"/sequences/:sequenceId/stages/:stageId\"\n                component={SequenceStageDetail}\n                key=\"/sequences/:sequenceId/stages/:stageId\"\n                exact\n              />\n              <Route\n                path=\"/sequences/:sequenceId/stages/:stageId/recipients\"\n                component={SequenceStageDetail}\n                key=\"/sequences/:sequenceId/stages/:stageId/recipients\"\n                exact\n              />\n            </Switch>\n          </div>\n          {!features.sequences && selectedTitle !== 'Campaigns' &&\n            <div className=\"upgrade-container\">\n              <UpgradePrompt\n                title=\"Sequences\"\n                description={'To send multi-stage outreach campaigns,' +\n                  ' upgrade to Polymail Premium or Enterprise.'}\n              />\n            </div>}\n          {!features.campaigns && selectedTitle === 'Campaigns' &&\n            <div className=\"upgrade-container\">\n              <UpgradePrompt\n                title=\"Campaigns\"\n                description={'To send campaigns,' +\n                  ' upgrade to Polymail Premium or Enterprise.'}\n              />\n            </div>}\n        </div>\n        <CommandControl default={CommandControlType.Thread }/>\n        <Toast />\n      </div>\n    );\n  }\n}\n\nexport default Sequences;\n"]}]}