AI,CAAC,CAAC,CAAC,EACnG,UAAU,CAAC,EAAE,CAAC,gBAAgB,CAAC,MAAM,CAAC,IAAI,EAAE,IAAI,CAAC,CAAC,EAClD,UAAU,CAAC,EAAE,CAAC,eAAe,CAAC,IAAI,CAAC,CAAC,CACrC,EAJc,CAId,CAAC;aACD,KAAK,CAAC,UAAA,CAAC,IAAI,OAAA,UAAU,CAAC,EAAE,CAAC,QAAQ,CAAC,CAAC,CAAC,CAAC,EAA1B,CAA0B,CAAC;IAPzC,CAOyC,CAC1C;AAZH,CAYG,CAAC;AAER,IAAM,oBAAoB,GACtB,UAAC,OAAO,EAAE,KAAK,EAAE,IAAI;IACrB,OAAA,OAAO,CAAC,MAAM,CAAC,kBAAkB,CAAC;SAC/B,MAAM,CAAC,UAAA,MAAM,IAAI,OAAA,MAAM,CAAC,MAAM,KAAK,YAAY,CAAC,KAAK;QACpD,CAAC,CAAC,KAAK,CAAC,QAAQ,EAAE,CAAC,IAAI,IAAI,CAAC,CAAC,KAAK,CAAC,QAAQ,EAAE,CAAC,IAAI,CAAC,IAAI,EADvC,CACuC,CAAC;SACzD,OAAO,CAAC,UAAC,MAA4B;QACpC,OAAA,IAAI,CAAC,WAAW,CAAC,YAAY,CAAC,KAAK,CAAC,QAAQ,EAAE,CAAC,IAAI,CAAC,IAAK,CAAC,EAAE,EAAE,MAAM,CAAC,IAAI,CAAC;aACvE,GAAG,CAAC,cAAM,OAAA,cAAc,CAAC,IAAI,CAAC,EAApB,CAAoB,CAAC;aAC/B,KAAK,CAAC,UAAA,CAAC,IAAI,OAAA,UAAU,CAAC,EAAE,CAAC,QAAQ,CAAC,CAAC,CAAC,CAAC,EAA1B,CAA0B,CAAC;IAFzC,CAEyC,CAC1C;AAPH,CAOG,CAAC;AAER,IAAM,sBAAsB,GAG1B,UAAC,OAAO,EAAE,KAAK,EAAE,IAAI;IACnB,OAAA,OAAO,CAAC,MAAM,CAAC,oBAAoB,CAAC;SACjC,MAAM,CAAC,UAAA,MAAM,IAAI,OAAA,MAAM,CAAC,MAAM,KAAK,YAAY,CAAC,KAAK,EAApC,CAAoC,CAAC;SACtD,OAAO,CAAC,UAAA,MAAM;QACb,OAAA,IAAI,CAAC,IAAI,CAAC,cAAc,CAAC,MAAM,CAAC,IAAI,CAAC;aAClC,GAAG,CAAC,UAAA,IAAI,IAAI,OAAA,kBAAkB,CAAC,MAAM,CAAC,IAAI,EAAE,IAAI,CAAC,EAArC,CAAqC,CAAC;aAClD,KAAK,CAAC,UAAA,CAAC,IAAI,OAAA,UAAU,CAAC,EAAE,CAAC,kBAAkB,CAAC,MAAM,CAAC,IAAI,EAAE,CAAC,CAAC,CAAC,EAAjD,CAAiD,CAAC;IAFhE,CAEgE,CACjE;AANH,CAMG,CAAC;AAER,IAAM,YAAY,GAAiD,UAAA,OAAO;IACxE,OAAA,OAAO;SACJ,MAAM,CAAC,QAAQ,CAAC;SAChB,GAAG,CAAC,cAAM,OAAA,eAAe,CAAC,IAAI,CAAC,EAArB,CAAqB,CAAC;AAFnC,CAEmC,CAAC;AAEtC,eAAe;IACb,eAAe;IACf,oBAAoB;IACpB,kBAAkB;IAClB,YAAY;IACZ,gBAAgB;IAChB,mBAAmB;IACnB,oBAAoB;IACpB,oBAAoB;IACpB,oBAAoB;IACpB,sBAAsB;CACvB,CAAC","sourcesContent":["import { Epic } from 'redux-observable';\nimport Cache, { Stores } from '@src/lib/db';\nimport {\n  StoreState,\n  Action,\n  ActionStatus,\n  TeamMember,\n  TeamInvite,\n  Team,\n  Onboarding,\n} from '@src/types/index';\nimport { Deps } from '@src/lib/epic';\nimport {\n  TEAM_MEMBER_INVITE,\n  TEAM_MEMBER_FETCH,\n  teamMemberFetch,\n  TEAM_INVITE_LIST,\n  teamInviteList,\n  TEAM_ADMIN_ADD,\n  teamAdminAdd,\n  TEAM_ADMIN_REMOVE,\n  teamAdminRemove,\n  TEAM_INVITE_RESEND,\n  TEAM_MEMBER_REMOVE,\n  TEAM_INVITE_REMOVE,\n  TeamInviteValidateMeta,\n  TEAM_INVITE_VALIDATE,\n  teamInviteValidate,\n  teamMemberRemove,\n} from '@src/ducks/teamMembers';\nimport { DB_READY } from '@src/ducks/db';\nimport { toastSet, ConfirmationError } from '@src/ducks/toasts';\nimport { Observable } from 'rxjs/Observable';\nimport { onboardingPatch, OnboardingPatchMeta } from '@src/ducks/me';\nimport { TeamInviteValidateResponse } from '@src/api/team';\nimport { recordGAEvent, recordFBEvent } from '@src/lib/analytics';\n\nconst fetchTeamMember: Epic<Action<null, TeamMember[]>, StoreState> = action$ =>\n action$\n  .ofType(TEAM_MEMBER_FETCH)\n  .filter(action => action.status === ActionStatus.Start)\n  .flatMap(action => Cache.list(Stores.TeamMembers))\n  .map((members: TeamMember[]) => teamMemberFetch(null, members));\n\nconst inviteTeamMemberEpic: Epic<\n  Action<string|Error|null|OnboardingPatchMeta, null|TeamInvite[]|Onboarding\n>, StoreState, Deps> = (action$, store, deps) =>\n    action$\n      .ofType(TEAM_MEMBER_INVITE)\n      .filter(action => action.status === ActionStatus.Start &&\n        !!store.getState().team && !!store.getState().team.data)\n      .flatMap((action: Action<string, null>) =>\n        deps.teamMembers.invite(store.getState().team.data!.id, action.meta)\n          .flatMap(() => {\n            const actions = [];\n            actions.push(Observable.of(teamInviteList(null)));\n            if (!store.getState().onboarding.inviteTeamMembers) {\n              actions.push(Observable.of(onboardingPatch({\n                skipped: false,\n                type: { inviteTeamMembers: true },\n              })));\n            }\n            return Observable.merge(\n              ...actions,\n            );\n          })\n          .do(() => {\n            recordGAEvent('user', 'invitedTeamMember');\n            recordFBEvent('trackCustom', 'InvitedTeamMember');\n          })\n          .catch(e => Observable.of(toastSet(e))),\n      );\n\nconst listTeamInviteEpic: Epic<Action<null|Error, TeamInvite[]|null>, StoreState, Deps>\n  = (action$, store, deps) =>\n    action$\n      .ofType(TEAM_INVITE_LIST)\n      .filter(action => action.status === ActionStatus.Start &&\n        !!store.getState().team && !!store.getState().team.data)\n      .flatMap(action =>\n        deps.teamMembers.listTeamInvite(store.getState().team.data!.id)\n          .map((invites: TeamInvite[]) => teamInviteList(null, invites))\n          .catch(e => Observable.of(toastSet(e))),\n      );\n\nconst teamAdminAddEpic: Epic<Action<string|Error|null, Team|null>, StoreState, Deps>\n  = (action$, store, deps) =>\n    action$.ofType(TEAM_ADMIN_ADD)\n      .filter(action => action.status === ActionStatus.Start &&\n        !!store.getState().team && !!store.getState().team.data)\n      .flatMap((action: Action<string, Team>) =>\n        deps.teamMembers.createTeamAdmin(store.getState().team.data!.id, action.meta)\n          .map((team: Team) => teamAdminAdd(action.meta, team))\n          .catch(e => Observable.of(toastSet(e))),\n      );\n\nconst teamAdminRemoveEpic: Epic<Action<string|Error|null, Team|null>, StoreState, Deps>\n  = (action$, store, deps) =>\n    action$.ofType(TEAM_ADMIN_REMOVE)\n      .filter(action => action.status === ActionStatus.Start &&\n        !!store.getState().team && !!store.getState().team.data)\n      .flatMap((action: Action<string, Team>) =>\n        deps.teamMembers.deleteTeamAdmin(store.getState().team.data!.id, action.meta)\n          .map((team: Team) => teamAdminRemove(action.meta, team))\n          .catch(e => Observable.of(toastSet(e))),\n      );\n\nconst teamInviteResendEpic: Epic<Action<string|null|Error, null|TeamInvite[]>, StoreState, Deps>\n  = (action$, store, deps) =>\n    action$.ofType(TEAM_INVITE_RESEND)\n      .filter(action => action.status === ActionStatus.Start &&\n        !!store.getState().team && !!store.getState().team.data)\n      .flatMap((action: Action<string, null>) =>\n        deps.teamMembers.resendInvite(store.getState().team.data!.id, action.meta)\n          .map(() => teamInviteList(null))\n          .catch(e => Observable.of(toastSet(e))),\n      );\n\nconst teamMemberDeleteEpic: Epic<Action<string|null|Error, null|TeamMember[]>, StoreState, Deps>\n  = (action$, store, deps) =>\n    action$.ofType(TEAM_MEMBER_REMOVE)\n      .filter(action => action.status === ActionStatus.Start &&\n        !!store.getState().team && !!store.getState().team.data)\n      .flatMap((action: Action<string, null>) =>\n        deps.teamMembers.delete(store.getState().team.data!.id, action.meta)\n          .flatMap(() => Cache.deleteSingle(Stores.TeamMembers, action.meta))\n          .flatMap(() => Observable.merge(\n            Observable.of(toastSet(new ConfirmationError('Your teammate has been successfully removed', true))),\n            Observable.of(teamMemberRemove(action.meta, null)),\n            Observable.of(teamMemberFetch(null)),\n          ))\n          .catch(e => Observable.of(toastSet(e))),\n      );\n\nconst teamInviteDeleteEpic: Epic<Action<string|null|Error, null|TeamInvite[]>, StoreState, Deps>\n  = (action$, store, deps) =>\n    action$.ofType(TEAM_INVITE_REMOVE)\n      .filter(action => action.status === ActionStatus.Start &&\n        !!store.getState().team && !!store.getState().team.data)\n      .flatMap((action: Action<string, null>) =>\n        deps.teamMembers.deleteInvite(store.getState().team.data!.id, action.meta)\n          .map(() => teamInviteList(null))\n          .catch(e => Observable.of(toastSet(e))),\n      );\n\nconst teamInviteValidateEpic: Epic<\n  Action<TeamInviteValidateMeta, TeamInviteValidateResponse>, StoreState, Deps\n> =\n  (action$, store, deps) =>\n    action$.ofType(TEAM_INVITE_VALIDATE)\n      .filter(action => action.status === ActionStatus.Start)\n      .flatMap(action =>\n        deps.team.validateInvite(action.meta)\n          .map(body => teamInviteValidate(action.meta, body))\n          .catch(e => Observable.of(teamInviteValidate(action.meta, e))),\n      );\n\nconst dbReadyFetch: Epic<Action<null, TeamMember[]>, StoreState> = action$ =>\n  action$\n    .ofType(DB_READY)\n    .map(() => teamMemberFetch(null));\n\nexport default [\n  fetchTeamMember,\n  inviteTeamMemberEpic,\n  listTeamInviteEpic,\n  dbReadyFetch,\n  teamAdminAddEpic,\n  teamAdminRemoveEpic,\n  teamInviteResendEpic,\n  teamMemberDeleteEpic,\n  teamInviteDeleteEpic,\n  teamInviteValidateEpic,\n];\n"]}]}