AE,CAAC;QAE1D,6BAAK,SAAS,EAAC,aAAa,IACzB,mBAAmB,CAAC,SAAS,CAAC,CAC3B;QACN,gCACE,SAAS,EAAC,iBAAiB,EAC3B,OAAO,EAAE,WAAW,iBAGb,CACL,CACP,CAAC;AACJ,CAAC,CAAC;AAEF,eAAe,aAAa,CAAC","sourcesContent":["import React from 'react';\nimport classnames from 'classnames';\n\nimport { AwardCellType } from '../AwardCell/AwardCell';\n\nimport './EmptyStatCell.scss';\n\ninterface Props {\n  awardType?: AwardCellType;\n  isCard?: boolean;\n}\n\nconst descriptionForAward = (type: AwardCellType): string|null => {\n  switch (type) {\n    case AwardCellType.Scheduling:\n      return `You haven't booked any meetings using Polymail scheduling. Check the link below to learn how to enable this!`;\n    case AwardCellType.Blocked:\n      return `You didn't block any senders this year. Learn how to easily unsubscribe from senders below!`;\n    case AwardCellType.SharedConversations:\n      return `You haven't shared any conversations this year. Learn how to leave comments and share conversations below!`;\n    default: return null;\n  }\n};\n\nconst EmptyStatCell = (props: Props) => {\n  const {\n    awardType,\n    isCard,\n  } = props;\n  if (!awardType) {\n    return null;\n  }\n\n  const handleClick = (e: React.MouseEvent<any>) => {\n    e.stopPropagation();\n    e.preventDefault();\n\n    switch (awardType) {\n      case AwardCellType.Scheduling:\n        window.open('https://help.polymail.io/en/articles/1788023-calendar-scheduling', '_blank');\n        return;\n      case AwardCellType.Blocked:\n        window.open('https://help.polymail.io/en/articles/3308492-blocking-unwanted-senders-in-polymail', '_blank');\n        return;\n      case AwardCellType.SharedConversations:\n        window.open('https://help.polymail.io/en/articles/2156127-shared-conversations', '_blank');\n        return;\n      default:\n        return;\n    }\n  };\n\n  return (\n    <div\n      className={classnames('empty-stat-cell', { card: isCard })}\n    >\n      <div className=\"description\">\n        {descriptionForAward(awardType)}\n      </div>\n      <button\n        className=\"navigate-button\"\n        onClick={handleClick}\n      >\n        Learn More\n      </button>\n    </div>\n  );\n};\n\nexport default EmptyStatCell;\n"]}]}