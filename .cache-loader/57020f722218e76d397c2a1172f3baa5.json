\nimport { ReferralInvite, ReferralInviteStatus } from '@src/types';\nimport './ReferralInviteList.scss';\nimport ReferralInviteCell from './ReferralInviteCell';\n\nexport interface StateProps {\n  referralInvites: ReferralInvite[];\n}\n\nclass ReferralInviteList extends React.PureComponent<StateProps> {\n  render() {\n    const { referralInvites } = this.props;\n\n    if (!referralInvites.length) {\n      return null;\n    }\n\n    const redeemed = referralInvites\n      .filter(ri => ri.status === ReferralInviteStatus.Redeemed).length;\n\n    return (\n      <div className=\"referral-invite-list\">\n        <div className=\"heading\">\n          <h5>People you've already invited</h5>\n          {!!redeemed && <div className=\"redeemed\">\n            You've earned a total of ${redeemed * 10} in credit\n          </div>}\n        </div>\n        <div className=\"list\">\n          {referralInvites.map(ri => (\n            <ReferralInviteCell referralInvite={ri} key={ri.id} />\n          ))}\n        </div>\n      </div>\n    );\n  }\n}\n\nexport default ReferralInviteList;\n"]}]}