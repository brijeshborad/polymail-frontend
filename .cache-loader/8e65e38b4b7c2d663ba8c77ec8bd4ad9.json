O,CAAC,MAAM;QAClB,wBAAwB,EAAE,oBAAoB,CAAC,KAAK,EAAE,cAAc,CAAC,iBAAiB,CAAC;QACvF,0BAA0B,EAAE,CAAC,KAAK,CAAC,UAAU,CAAC,eAAe;YAC3D,CAAC,eAAe,CAAC,SAAS,GAAG,eAAe,CAAC,WAAW,CAAC,GAAG,IAAI;QAClE,aAAa,EAAE,KAAK,CAAC,aAAa;QAClC,QAAQ,EAAE,KAAK,CAAC,QAAQ;QACxB,UAAU,EAAE,KAAK,CAAC,UAAU;KAC7B,CAAC;AACJ,CAAC,CAAC;AAEF,IAAM,kBAAkB,GAAG,UAAC,QAA8B,IAAoB,OAAA,CAAC;IAC7E,QAAQ,EAAE,UAAC,QAAiB,IAAK,OAAA,QAAQ,CAAC,uBAAuB,CAAC,QAAQ,CAAC,CAAC,EAA3C,CAA2C;IAC5E,gBAAgB,EAAE,UAAA,IAAI;;QAAI,OAAA,QAAQ,CAAC,eAAe,CAAC;YACjD,OAAO,EAAE,KAAK;YACd,IAAI,YAAI,GAAC,IAAI,IAAG,IAAI,KAAE;SAAE,CACvB,CAAC;IAHsB,CAGtB;CACL,CAAC,EAN4E,CAM5E,CAAC;AAEH,eAAe,OAAO,CACpB,eAAe,EACf,kBAAkB,CACnB,CAAC,oBAAoB,CAAC,CAAC","sourcesContent":["import CollapsedTeamSection, { StateProps, DispatchProps } from './CollapsedTeamSection';\nimport { connect, Dispatch } from 'react-redux';\nimport { StoreState, OnboardingType } from '@src/types';\nimport { leftSidebarTeamCollapse } from '@src/ducks/leftSidebar';\nimport { selectOnboardingShow, onboardingPatch } from '@src/ducks/me';\nimport { selectSharedThreads } from '@src/ducks/sharedThreads';\nimport { SharedThreadFilterMode } from '@src/types/sharedThreads';\nimport { selectAllMailboxCount } from '@src/ducks/mailCounts';\n\nconst mapStateToProps = (state: StoreState): StateProps => {\n  // Normal Shared Threads\n  const threads = selectSharedThreads(state, {\n    mode: SharedThreadFilterMode.Unread,\n  });\n\n  const team = state.team.data;\n  const allMailboxCount = selectAllMailboxCount(state);\n\n  return {\n    collapsed: state.leftSidebar.teamCollapsed,\n    team,\n    selectedTab: state.leftSidebar.selectedTab,\n    mentionsCount: threads ? threads.length : 0,\n    showMentionsOnboarding: selectOnboardingShow(state, OnboardingType.MentionsTab) &&\n      !!threads.length,\n    showTeamInviteOnboarding: selectOnboardingShow(state, OnboardingType.InviteTeamMembers),\n    showUnsubscriberOnboarding: !state.onboarding.unsubscriberGem &&\n      (allMailboxCount.inboxRead + allMailboxCount.inboxUnread) > 1000,\n    hotkeyActions: state.hotkeyActions,\n    features: state.features,\n    onboarding: state.onboarding,\n  };\n};\n\nconst mapDispatchToProps = (dispatch: Dispatch<StoreState>): DispatchProps => ({\n  collapse: (collapse: boolean) => dispatch(leftSidebarTeamCollapse(collapse)),\n  updateOnboarding: type => dispatch(onboardingPatch({\n    skipped: false,\n    type: { [type]: true } },\n    )),\n});\n\nexport default connect<StateProps, DispatchProps>(\n  mapStateToProps,\n  mapDispatchToProps,\n)(CollapsedTeamSection);\n"]}]}