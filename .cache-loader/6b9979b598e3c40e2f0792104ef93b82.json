AS,EAAC,gBAAgB;YAC7B,6BAAK,SAAS,EAAC,MAAM;gBAClB,OAAO,CAAC,SAAS;;gBAAQ,OAAO,CAAC,UAAU,CACxC;YACN,6BAAK,SAAS,EAAC,OAAO,IACnB,OAAO,CAAC,KAAK,CACV,CACF,CACO,CAChB,CAAC;AACJ,CAAC,CAAC;AAEF,eAAe,eAAe,CAAC","sourcesContent":["import React from 'react';\nimport { animated } from 'react-spring';\nimport classnames from 'classnames';\n\nimport { StatYearlySummaryTopExchange } from '@src/api/analytics';\n\nimport './ContactCardCell.scss';\n\ninterface Props {\n  styles: any;\n  contact: StatYearlySummaryTopExchange;\n  isSelected?: boolean;\n  onHover: (hoveredContact: string) => void;\n}\n\nexport enum ContactCardCellType {\n  Card = 'card',\n  TextBlock = 'textBlock',\n}\n\nconst ContactCardCell = (props: Props) => {\n  const {\n    styles,\n    contact,\n    isSelected,\n  } = props;\n\n  const handleClick = () => {\n    props.onHover(contact.email);\n  };\n\n  return (\n    <animated.div\n      className={classnames('contact-card-cell', { selected: !!isSelected })}\n      style={{\n        opacity: styles.opacity,\n        transform: styles.t.to({\n          range: [0, 0.25, .5, 0.75, 1],\n          output: [0.8, 1.05, 1.1, 1.05, 1.0],\n        }).to((x: number) => `scale(${x})`),\n      }}\n      onClick={handleClick}\n    >\n      <div className=\"avatar\">\n        {/* tslint:disable-next-line:max-line-length */}\n        <img src={contact.photoUrl} alt=\"\" className=\"\"/>\n      </div>\n      <div className=\"sender-details\">\n        <div className=\"name\">\n          {contact.givenName}&nbsp;{contact.familyName}\n        </div>\n        <div className=\"email\">\n          {contact.email}\n        </div>\n      </div>\n    </animated.div>\n  );\n};\n\nexport default ContactCardCell;\n"]}]}