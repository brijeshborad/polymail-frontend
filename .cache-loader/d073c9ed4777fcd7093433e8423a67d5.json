CAAC,MAAM,GACjB,CACH,CAAC;AACJ,CAAC,CAAC;AAEF,eAAe,cAAc,CAAC","sourcesContent":["import React from 'react';\nimport SendLater from './components/SendLater';\nimport ReadLater from './components/ReadLater';\nimport Thread from './components/Thread';\nimport FollowUp from './components/FollowUp';\nimport Label from './components/Label';\nimport Draft from './components/Draft';\nimport Templates from './components/Templates';\nimport Calendar from './components/Calendar';\nimport System from './components/System';\nimport Navigation from './components/Navigation';\n\nimport { CommandControlType } from '@src/types';\nimport SchedulingPages from './components/SchedulingPages';\n\nexport interface Props {\n  action?: CommandControlType;\n  scheduled?: Date;\n  default: CommandControlType;\n  labels?: string[];\n  potentialLabels?: string[];\n}\n\nexport interface DispatchProps {\n  onHide: () => void;\n}\n\nconst COMPONENTS = {\n  sendlater: SendLater,\n  readlater: ReadLater,\n  thread: Thread,\n  followup: FollowUp,\n  label: Label,\n  draft: Draft,\n  templates: Templates,\n  navigation: Navigation,\n  calendar: Calendar,\n  system: System,\n  schedulingPages: SchedulingPages,\n};\n\nconst CommandControl = (props: Props & DispatchProps) => {\n  let Component = COMPONENTS[props.default];\n  if (props.action && props.action !== props.default) {\n    Component = COMPONENTS[props.action];\n  }\n\n  return (\n    <Component\n      onHide={props.onHide}\n      scheduled={props.scheduled}\n      isDefault={!props.action}\n      labels={props.labels}\n      potentialLabels={props.potentialLabels}\n      key={props.action}\n    />\n  );\n};\n\nexport default CommandControl;\n"]}]}