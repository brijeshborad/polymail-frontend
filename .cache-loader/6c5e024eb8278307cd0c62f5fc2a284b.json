BAC3B,iEAAkC;gBAClC,6BAAK,SAAS,EAAC,OAAO,IACnB,IAAI,CAAC,KAAK,CAAC,GAAG,CAAC,UAAC,IAAI,EAAE,CAAC,IAAK,OAAA,CAC3B,6BAAK,SAAS,EAAC,MAAM,EAAC,GAAG,EAAE,CAAC,CAAC,QAAQ,EAAE;oBACrC,gCAAK,IAAI,CAAC,KAAK,CAAM;oBACrB,6BAAK,SAAS,EAAC,MAAM,IAAE,IAAI,CAAC,IAAI,CAAO,CACnC,CACP,EAL4B,CAK5B,CAAC,CACE;gBACN,2BACE,SAAS,EAAC,sBAAsB,EAChC,IAAI,EAAC,oEAAoE,EACzE,MAAM,EAAC,QAAQ,wDAGb,CACA;YACN,oBAAC,kBAAkB,OAAG,CAClB,CACP,CAAC;IACJ,CAAC;IACH,mBAAC;AAAD,CAAC,AAlED,CAA2B,KAAK,CAAC,aAAa,GAkE7C;AAED,IAAM,eAAe,GAAG,UAAC,KAAiB;IACxC,OAAO;QACL,QAAQ,EAAE,KAAK,CAAC,QAAQ;QACxB,eAAe,EAAE,KAAK,CAAC,WAAW,CAAC,SAAS;KAC7C,CAAC;AACJ,CAAC,CAAC;AAEF,IAAM,kBAAkB,GAAG,UAAC,QAAuB,IAAoB,OAAA,CAAC;IACtE,WAAW,EAAE,UAAA,QAAQ,IAAI,OAAA,QAAQ,CAAC,mBAAmB,CAAC,QAAQ,CAAC,CAAC,EAAvC,CAAuC;CACjE,CAAC,EAFqE,CAErE,CAAC;AAEH,eAAe,OAAO,CACpB,eAAe,EAAE,kBAAkB,CACpC,CAAC,YAAY,CAAC,CAAC","sourcesContent":["import React from 'react';\n\nimport { connect, Dispatch } from 'react-redux';\nimport { StoreState } from '@src/types';\nimport { leftSidebarCollapse } from '@src/ducks/leftSidebar';\n\nimport NavigationHeader from '@src/routes/Settings/components/NavigationHeader';\n\nimport './ReferFriends.scss';\nimport SendInvites from './components/SendInvites';\nimport ReferralInviteList from './components/ReferralInviteList';\nimport { AmplitudeEvents, logAmplitudeEvent } from '@src/lib/amplitude-analytics';\ninterface Step {\n  emoji: string;\n  text: string;\n}\ninterface Props {\n  isMobile?: boolean;\n  collapsedMobile?: boolean;\n}\ninterface DispatchProps {\n  collapseLsb: (collapsed: boolean) => void;\n}\nclass ReferFriends extends React.PureComponent<Props & DispatchProps> {\n  steps: Step[] = [];\n\n  constructor() {\n    super();\n\n    this.steps = [\n      {\n        emoji: '💁🏽',\n        text: 'Share Polymail with your link or over email.',\n      },\n      {\n        emoji: '💌',\n        text: 'Your friend signs up and sends a message using Polymail.',\n      },\n      {\n        emoji: '💸',\n        text: 'You both receive $10 in credit!',\n      },\n    ];\n  }\n\n  componentDidMount() {\n    logAmplitudeEvent(AmplitudeEvents.ViewReferFriends);\n  }\n\n  render() {\n    return (\n      <div className=\"refer-friends\">\n        <NavigationHeader\n          title=\"Refer Friends\"\n          isMobile={this.props.isMobile}\n          collapsedMobile={this.props.collapsedMobile}\n          collapseLsb={this.props.collapseLsb}\n        />\n        <div className=\"heading\">\n          <h1>🎁</h1>\n          <h3>Tell a friend about Polymail, you both get $10!</h3>\n          <div className=\"desc\">\n            Introduce a friend to Polymail. When they send their first message,{' '}\n            you’ll both receive $10 credit to use toward any paid plan!\n          </div>\n        </div>\n        <SendInvites />\n        <div className=\"instructions\">\n          <h4>It’s easy to get started!</h4>\n          <div className=\"steps\">\n            {this.steps.map((step, i) => (\n              <div className=\"step\" key={i.toString()}>\n                <h3>{step.emoji}</h3>\n                <div className=\"desc\">{step.text}</div>\n              </div>\n            ))}\n          </div>\n          <a\n            className=\"help-center-referral\"\n            href=\"http://help.polymail.io/getting-started/polymails-referral-program\"\n            target=\"_blank\"\n          >\n            Learn more about how invitations work on Polymail\n          </a>\n        </div>\n        <ReferralInviteList />\n      </div>\n    );\n  }\n}\n\nconst mapStateToProps = (state: StoreState): Props => {\n  return {\n    isMobile: state.isMobile,\n    collapsedMobile: state.leftSidebar.collapsed,\n  };\n};\n\nconst mapDispatchToProps = (dispatch: Dispatch<any>): DispatchProps => ({\n  collapseLsb: collapse => dispatch(leftSidebarCollapse(collapse)),\n});\n\nexport default connect<Props, DispatchProps, {}>(\n  mapStateToProps, mapDispatchToProps,\n)(ReferFriends);\n"]}]}