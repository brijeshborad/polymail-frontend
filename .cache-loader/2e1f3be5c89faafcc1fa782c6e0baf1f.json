,IAAK,OAAA,CAC9B,gCACE,SAAS,EAAE,UAAU,CACnB,gCAAgC,EAChC,EAAE,QAAQ,EAAE,aAAa,KAAK,KAAK,EAAE,CACtC,EACD,GAAG,EAAE,MAAM,gBACC,KAAK,EACjB,OAAO,EAAE,WAAW,EACpB,YAAY,EAAE,gBAAgB;YAE9B,8BAAM,SAAS,EAAC,SAAS,IAAE,MAAM,CAAC,CAAC,CAAC,CAAC,WAAW,EAAE,GAAG,MAAM,CAAC,KAAK,CAAC,CAAC,CAAC,CAAQ,CACrE,CACV,EAb+B,CAa/B,CAAC,CACE,CACF,CACP,CAAC;AACJ,CAAC,CAAC;AAEF,eAAe,cAAc,CAAC","sourcesContent":["import React from 'react';\nimport classnames from 'classnames';\n\ninterface Props {\n  showIsOptions: boolean;\n  onSelect: (index: number) => void;\n  onSuggestionMouseEnter: (component: string, index: number) => void;\n  options: string[];\n  selectedIndex?: number;\n}\n\nconst ModifierSearch: React.SFC<Props> = ({\n  showIsOptions, onSelect, onSuggestionMouseEnter, selectedIndex, options,\n}) => {\n  const handleClick = (e: React.SyntheticEvent<HTMLButtonElement>) => {\n    const index = e.currentTarget.dataset.index;\n\n    if (index) {\n      onSelect(parseInt(index, 10));\n    }\n  };\n  const handleMouseEnter = (e: React.SyntheticEvent<HTMLButtonElement>) => {\n    const index = e.currentTarget.dataset.index;\n\n    if (index) {\n      onSuggestionMouseEnter('contact', parseInt(index, 10));\n    }\n  };\n\n  if (!options.length) {\n    return <div />;\n  }\n\n  return (\n    <div className=\"modifier-search suggestion-category\">\n      <div className=\"heading\">\n        <span>Modifiers</span>\n        <span className=\"separator\" />\n      </div>\n      <div className=\"suggestions\">\n        {options.map((option, index) => (\n          <button\n            className={classnames(\n              'modifier-suggestion suggestion',\n              { selected: selectedIndex === index },\n            )}\n            key={option}\n            data-index={index}\n            onClick={handleClick}\n            onMouseEnter={handleMouseEnter}\n          >\n            <span className=\"heading\">{option[0].toUpperCase() + option.slice(1)}</span>\n          </button>\n        ))}\n      </div>\n    </div>\n  );\n};\n\nexport default ModifierSearch;\n"]}]}