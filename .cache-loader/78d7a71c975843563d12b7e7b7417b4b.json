CAAC,UAAC,SAAS;gBACL,IAAA,qBAAI,EAAE,uBAAK,EAAE,mBAAG,EAAE,+BAAS,EAAE,6BAAQ,CAAe;gBAC5D,IAAI,IAAI,GAAG,WAAS,eAAe,CAAC,EAAE,SAAI,GAAK,CAAC;gBAEhD,IAAI,GAAG,CAAC,UAAU,CAAC,WAAW,CAAC,EAAE;oBAC/B,IAAI,GAAG,6BAA2B,GAAG,CAAC,KAAK,CAAC,GAAG,CAAC,OAAO,CAAC,GAAG,CAAC,GAAG,CAAC,CAAG,CAAC;iBACrE;qBAAM,IAAI,GAAG,KAAK,OAAO,EAAE;oBAC1B,IAAI,GAAG,WAAS,eAAe,CAAC,EAAE,WAAQ,CAAC;iBAC5C;gBAED,OAAO,CACL,oBAAC,oBAAoB,IACnB,GAAG,EAAE,GAAG,EACR,EAAE,EAAE,GAAG,EACP,IAAI,EAAE,IAAI,EACV,KAAK,EAAE,KAAK,EACZ,SAAS,EAAE,GAAG,CAAC,UAAU,CAAC,WAAW,CAAC,CAAC,CAAC,CAAC,SAAS,CAAC,CAAC,CAAC,GAAG,EACxD,QAAQ,EAAE,WAAW,KAAK,GAAG,EAC7B,IAAI,EAAE,KAAK,CAAC,GAAG,CAAC,EAChB,IAAI,EAAE,IAAI,EACV,WAAW,EAAE,KAAI,CAAC,eAAe,EACjC,aAAa,EAAE,GAAG,KAAK,OAAO,IAAI,eAAe,CAAC,EAAE,KAAK,KAAK,EAC9D,SAAS,EAAE,SAAS,EACpB,QAAQ,EAAE,QAAQ,GAClB,CACH,CAAC;YACJ,CAAC,CAAC,CACA,CACP,CAAC;IACJ,CAAC;IACH,2BAAC;AAAD,CAAC,AA3FD,CAA0C,KAAK,CAAC,aAAa,GA2F5D","sourcesContent":["import React from 'react';\nimport classnames from 'classnames';\n\nimport { CollapsedSectionItem } from '../Item/CollapsedSectionItem/CollapsedSectionItem';\nimport { Account, Tab } from '@src/types';\n\nimport Inbox from '@src/components/Icons/Inbox.svg';\nimport Archive from '@src/components/Icons/Archive.svg';\nimport Starred from '@src/components/Icons/Starred.svg';\nimport ReadLater from '@src/components/Icons/Clock.svg';\nimport Sent from '@src/components/Icons/Sent.svg';\nimport SendLater from '@src/components/Icons/SendLater.svg';\nimport FollowUp from '@src/components/Icons/FollowUp.svg';\nimport Draft from '@src/components/Icons/Draft.svg';\nimport Trash from '@src/components/Icons/Trash.svg';\nimport OutboxIcon from '@src/components/Icons/OutboxIcon.svg';\n\nimport AccountChooserPopoverInstance from '../MailHeader/AccountChooserPopover/AccountChooserPopover';\nimport AccountChooserPopover from '../MailHeader/AccountChooserPopover';\nimport './CollapsedMailSection.scss';\n\nconst ICONS = {\n  inbox: <Inbox width=\"16px\" height=\"14px\"/>,\n  archive: <Archive width=\"16px\" height=\"16px\" />,\n  trash: <Trash width=\"16px\" height=\"16px\" />,\n  starred: <Starred width=\"16px\" height=\"16px\" />,\n  later: <ReadLater width=\"16px\" height=\"16px\" />,\n  sent: <Sent width=\"16px\" height=\"14px\" />,\n  sendLater: <SendLater width=\"20px\" height=\"16px\" />,\n  followUp: <FollowUp width=\"16px\" height=\"15px\" />,\n  drafts: <Draft width=\"16px\" height=\"16px\" />,\n  outbox: <OutboxIcon width=\"16px\" height=\"14px\" />,\n};\n\nexport interface Props {\n  title: string;\n  accounts: Account[];\n  selectedTab: string;\n  tabs: Tab[];\n  selectedAccount: Account;\n  collapseTrash: (collapse: boolean) => void;\n  trashCollapsed: boolean;\n  collapsed: boolean;\n}\n\nexport class CollapsedMailSection extends React.PureComponent<Props> {\n  private chooser: AccountChooserPopoverInstance;\n\n  handleSelectAccount = () => {\n    this.chooser.toggle(0);\n  }\n\n  handleRef = (ref: any) => {\n    if (ref) {\n      this.chooser = ref.getWrappedInstance();\n    }\n  }\n\n  trashClick = (e: React.SyntheticEvent<HTMLElement>) => {\n    const { trashCollapsed, collapseTrash } = this.props;\n    e.preventDefault();\n    e.stopPropagation();\n\n    collapseTrash(!trashCollapsed);\n  }\n\n  handleMoreClick = (name: string, e: React.MouseEvent<HTMLElement>) => {\n    const target = e.currentTarget;\n\n    switch (name) {\n      case 'inbox': {\n        const boundingBox = target && target.getBoundingClientRect &&\n          target.getBoundingClientRect();\n\n        if (boundingBox) {\n          e.stopPropagation();\n          e.preventDefault();\n          this.chooser.toggle(boundingBox.bottom);\n        }\n        break;\n      }\n      default: break;\n    }\n  }\n\n  render() {\n    const {\n      tabs, selectedTab, selectedAccount, trashCollapsed, accounts,\n    } = this.props;\n    const conditionallyShow = ['later', 'starred', 'sendLater', 'followUp', 'outbox'];\n    const AccountChooserPopoverConnect = AccountChooserPopover as any;\n\n    return (\n      <div className={classnames(\n        'collapsed-mail-tab', { 'trash-collapsed': trashCollapsed },\n        )}\n      >\n        <AccountChooserPopoverConnect\n          onSelect={this.handleSelectAccount}\n          selectedAccount={selectedAccount}\n          accounts={accounts}\n          ref={this.handleRef}\n        />\n        {tabs\n          .filter(t => !t.hideCollapsed && conditionallyShow.indexOf(t.key) === -1 ||\n            (conditionallyShow.indexOf(t.key) > -1 && !!t.count))\n          .map((itemProps) => {\n            const { name, count, key, errorCopy, showMore } = itemProps;\n            let link = `/mail/${selectedAccount.id}/${key}`;\n\n            if (key.startsWith('accounts-')) {\n              link = `/mail/all/inbox/account-${key.slice(key.indexOf('-') + 1)}`;\n            } else if (key === 'inbox') {\n              link = `/mail/${selectedAccount.id}/inbox`;\n            }\n\n            return (\n              <CollapsedSectionItem\n                key={key}\n                id={key}\n                name={name}\n                count={count}\n                className={key.startsWith('accounts-') ? 'account' : key}\n                selected={selectedTab === key}\n                icon={ICONS[key]}\n                link={link}\n                onMoreClick={this.handleMoreClick}\n                highlightMore={key === 'inbox' && selectedAccount.id !== 'all'}\n                errorCopy={errorCopy}\n                showMore={showMore}\n              />\n            );\n          })}\n      </div>\n    );\n  }\n}\n"]}]}