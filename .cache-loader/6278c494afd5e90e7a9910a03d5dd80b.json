ACX,IAAI,EAAC,UAAU,EACf,YAAY;oBAEd,gCAAQ,OAAO,EAAE,IAAI,CAAC,UAAU,IAC7B,EAAE,IAAI,6BAAK,GAAG,EAAE,EAAE,CAAC,QAAQ,GAAI,CACzB,CACE,CACP,CACF,CACP,CAAC;IACJ,CAAC;IACH,wBAAC;AAAD,CAAC,AAhJD,CAAuC,KAAK,CAAC,aAAa,GAgJzD","sourcesContent":["import React from 'react';\nimport classnames from 'classnames';\nimport { Me, Team, TeamPlan } from '@src/types';\nimport SettingsMenu, { SettingName } from './components/SettingsMenu';\nimport Sent from '@src/components/Icons/Sent.svg';\nimport Analytics from '@src/components/Icons/Analytics.svg';\nimport Sequence from '@src/components/Icons/Sequence.svg';\n\nimport './LeftSidebarFooter.scss';\nimport browserHistory from '@src/lib/browserHistory';\nimport { NavbarTab } from '@src/types/leftSidebar';\nimport HoverTip from '@src/components/Base/HoverTip';\n\nexport interface Props {\n  me?: Me;\n  team?: Team;\n  selectedTab: NavbarTab;\n  collapsed?: boolean;\n}\n\nexport interface DispatchProps {\n  showCommandControl: () => void;\n  showKeyboardShortcut: () => void;\n}\n\nexport class LeftSidebarFooter extends React.PureComponent<Props & DispatchProps> {\n  private menu: SettingsMenu;\n\n  constructor() {\n    super();\n\n    this.state = {};\n  }\n\n  toggleMenu = (e: React.MouseEvent<HTMLElement>) => {\n    e.preventDefault();\n    this.menu.toggle();\n  }\n\n  handlePreferenceSelection = (key: string) => {\n    const { showCommandControl, showKeyboardShortcut } = this.props;\n\n    if (key === SettingName.SignOut) {\n      browserHistory.push('/settings/signout');\n      return;\n    }\n\n    if (key === SettingName.CommandCenter) {\n      showCommandControl();\n    }\n\n    if (key === SettingName.KeyboardShortcuts) {\n      showKeyboardShortcut();\n    }\n\n    this.menu.toggle();\n  }\n\n  handleRef = (ref: SettingsMenu) => {\n    this.menu = ref;\n  }\n\n  goToSequence = () => {\n    browserHistory.push('/sequences');\n  }\n\n  goToAnalytics = () => {\n    browserHistory.push('/analytics');\n  }\n\n  goToMail = () => {\n    browserHistory.push('/');\n  }\n\n  render() {\n    const { me, team, selectedTab, collapsed } = this.props;\n    const teamName = team && team.plan !== TeamPlan.Individual && team.name || undefined;\n\n    if (collapsed) {\n      return (\n        <div\n        className={classnames('left-sidebar-footer', { collapsed })}\n      >\n        <SettingsMenu\n          onSelect={this.handlePreferenceSelection}\n          ref={this.handleRef}\n          me={me && `${me.firstName} ${me.lastName}`}\n          team={teamName}\n        />\n        <div className=\"container\">\n          <HoverTip\n              top=\"-35px\"\n              left=\"10px\"\n              copy=\"Settings\"\n              anchorBottom\n            >\n            <button onClick={this.toggleMenu}>\n              {me && <img src={me.photoUrl} />}\n            </button>\n            </HoverTip>\n        </div>\n      </div>\n      );\n    }\n\n    return (\n      <div\n        className=\"left-sidebar-footer\"\n      >\n        <SettingsMenu\n          onSelect={this.handlePreferenceSelection}\n          ref={this.handleRef}\n          me={me && `${me.firstName} ${me.lastName}`}\n          team={teamName}\n        />\n        <div className=\"container\">\n          <HoverTip\n            copy=\"Mail\"\n            top=\"-35px\"\n            left=\"10px\"\n            anchorBottom\n          >\n            <button\n              className={classnames({ selected: selectedTab === NavbarTab.Mail })}\n              onClick={this.goToMail}\n            >\n              <Sent />\n            </button>\n          </HoverTip>\n          <HoverTip\n            top=\"-35px\"\n            left=\"10px\"\n            copy=\"Sequences\"\n            anchorBottom\n          >\n          <button\n            className={classnames({ selected: selectedTab === NavbarTab.Sequences })}\n            onClick={this.goToSequence}\n          >\n            <Sequence />\n          </button>\n          </HoverTip>\n          <HoverTip\n            top=\"-35px\"\n            left=\"10px\"\n            copy=\"Analytics\"\n            anchorBottom\n          >\n          <button\n            className={classnames({ selected: selectedTab === NavbarTab.Analytics })}\n            onClick={this.goToAnalytics}\n          >\n            <Analytics />\n          </button>\n          </HoverTip>\n          <HoverTip\n            top=\"-35px\"\n            left=\"10px\"\n            copy=\"Settings\"\n            anchorBottom\n          >\n          <button onClick={this.toggleMenu}>\n            {me && <img src={me.photoUrl} />}\n          </button>\n          </HoverTip>\n        </div>\n      </div>\n    );\n  }\n}\n"]}]}