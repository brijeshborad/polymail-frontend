tactGroupAPI {\n  list(id?: string): Observable<ContactGroup[]> {\n    return jsonRequest({\n      method: 'GET',\n      route: id ? `/v1/contactGroups?from=${id}` : '/v1/contactGroups',\n    })\n    .map((groups: ContactGroup[]) => groups.map(group => applyContactGroupDefaults(group)));\n  }\n\n  listByTeam(team: string, id?: string): Observable<ContactGroup[]> {\n    return jsonRequest({\n      method: 'GET',\n      route: id ? `/v1/contactGroups?team=${team}&from=${id}` : `/v1/contactGroups?team=${team}`,\n    })\n    .map((groups: ContactGroup[]) => groups.map(group => applyContactGroupDefaults(group)));\n  }\n}\n"]}]}