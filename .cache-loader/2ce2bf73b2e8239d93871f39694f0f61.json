MAAM,IAClB,UAAU,EAAE,CACT;QACN,6BAAK,SAAS,EAAC,MAAM,IAClB,WAAW,EAAE,CACV,CACC,CACV,CAAC;AACJ,CAAC,CAAC","sourcesContent":["import React from 'react';\n\nimport Popover, { PopoverPosition } from '@src/components/Popover/Popover';\nimport { RightSidebarTabOrder } from '../../RightSidebar';\nimport './TabsPopover.scss';\nimport { RightSidebarTab } from '@src/types/rightSidebar';\n\nimport Activity from '@src/components/Icons/Activity.svg';\nimport SilverHelp from '@src/components/Icons/SilverHelp.svg';\nimport Bio from '@src/components/Icons/Bio.svg';\nimport SalesforceLogo from '@src/components/Icons/SalesforceLogo.svg';\nimport HubspotLogo from '@src/components/Icons/Hubspot.svg';\nimport CalendarIcon from '@src/components/Icons/Calendar.svg';\nimport Hyperlink from '@src/components/Icons/Hyperlink.svg';\nimport classNames from 'classnames';\n\ninterface Props {\n  shownTabs: { [key: string]: RightSidebarTabOrder };\n  onTabSelect: (tab: RightSidebarTab) => void;\n  onHide?: () => void;\n  selectedTab?: RightSidebarTab;\n}\n\nexport default class TabsPopover extends React.PureComponent<Props> {\n  popover: Popover;\n\n  handleTabsPopoverRef = (ref: Popover) => {\n    this.popover = ref;\n  }\n\n  handleTabSelect = (tab: RightSidebarTab) => {\n    const { onTabSelect } = this.props;\n    onTabSelect(tab);\n    this.popover.hide();\n  }\n\n  render() {\n    const { shownTabs, onHide, selectedTab } = this.props;\n    return (\n      <Popover\n        className=\"rsb-tabs-popover\"\n        ref={this.handleTabsPopoverRef}\n        onHide={onHide}\n        position={PopoverPosition.BelowRight}\n        showOnMount\n      >\n        <div className=\"items\">\n          {Object.values(shownTabs).map(t => (\n            <TabItem\n              key={`rsb-tab-item-${t.tab}`}\n              tab={t.tab}\n              onTabSelect={this.handleTabSelect}\n              selected={selectedTab === t.tab}\n            />\n          ))}\n        </div>\n      </Popover>\n    );\n  }\n}\n\ninterface TabProps {\n  tab: RightSidebarTab;\n  selected?: boolean;\n  onTabSelect: (tab: RightSidebarTab) => void;\n}\n\nconst TabItem = (props: TabProps) => {\n  const { onTabSelect, tab, selected } = props;\n\n  const handleClick = () => {\n    onTabSelect(tab);\n  };\n\n  const titleForTab = (): string => {\n    switch (tab) {\n      case RightSidebarTab.Activity:\n        return 'Activity Feed';\n      case RightSidebarTab.Calendar:\n        return 'Calendar';\n      case RightSidebarTab.Contact:\n        return 'Contact Profile';\n      case RightSidebarTab.Hubspot:\n        return 'Hubspot';\n      case RightSidebarTab.KeyboardShortcut:\n        return 'Keyboard Shortcuts';\n      case RightSidebarTab.LinkExtractor:\n        return 'Link Extractor';\n      case RightSidebarTab.Onboarding:\n        return 'Onboarding';\n      case RightSidebarTab.Salesforce:\n        return 'Salesforce Integration';\n      default:\n        return '';\n    }\n  };\n\n  const iconForTab = () => {\n    switch (tab) {\n      case RightSidebarTab.Activity:\n        return <Activity className=\"icon\" width=\"16px\" height=\"15px\" />;\n      case RightSidebarTab.Calendar:\n        return <CalendarIcon className=\"icon\" width=\"15px\" height=\"15px\" />;\n      case RightSidebarTab.Contact:\n        return <Bio className=\"icon\" width=\"17px\" height=\"17px\" />;\n      case RightSidebarTab.Hubspot:\n        return <HubspotLogo className=\"icon\" width=\"23px\" height=\"16px\" />;\n      case RightSidebarTab.KeyboardShortcut:\n        return 'Keyboard Shortcuts';\n      case RightSidebarTab.LinkExtractor:\n        return <Hyperlink className=\"icon\" width=\"23px\" height=\"16px\" />;\n      case RightSidebarTab.Onboarding:\n        return <SilverHelp className=\"icon\" width=\"17px\" height=\"17px\" />;\n      case RightSidebarTab.Salesforce:\n        return <SalesforceLogo className=\"icon\" width=\"23px\" height=\"16px\" />;\n      default:\n        return null;\n    }\n  };\n\n  return (\n    <button\n      className={classNames('item', { selected })}\n      onClick={handleClick}\n    >\n      <div className=\"icon\">\n        {iconForTab()}\n      </div>\n      <div className=\"copy\">\n        {titleForTab()}\n      </div>\n    </button>\n  );\n};\n"]}]}