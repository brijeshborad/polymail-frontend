G,EAAE,oBAAoB,EAAE,GAAG,EAAC,EAAE,GAAG,CAC1C;gBACA,CAAC,MAAM,IAAI,CACV,6BAAK,GAAG,EAAE,qBAAqB,EAAE,GAAG,EAAC,EAAE,GAAG,CAC3C;gBACD,4BAAI,SAAS,EAAE,UAAU,CAAC,aAAa,EAAE,EAAE,IAAI,EAAE,cAAc,EAAE,CAAC,IAAG,WAAW,CAAM,CAClF,CACF,CACP,CAAC;IACJ,CAAC;IACH,oBAAC;AAAD,CAAC,AAxDD,CAA4B,KAAK,CAAC,aAAa,GAwD9C;AAED,eAAe,aAAa,CAAC","sourcesContent":["import React from 'react';\nimport classnames from 'classnames';\nimport './ThemeSwitcher.scss';\nimport PolymailLogoLightMode from '@src/routes/Login/Icons/PolymailLogo@3x.png';\nimport PolymailLogoDarkMode from '@src/routes/Login/Icons/PolymailLogoDarkMode@3x.png';\nimport { Resource, ThemeSetting } from '@src/types';\n\nexport interface Props {\n  theme: Resource<ThemeSetting>;\n}\n\nexport interface DispatchProps {\n  setTheme: (theme: ThemeSetting) => void;\n}\n\ninterface State {\n  fadeAnimations: boolean;\n}\n\nclass ThemeSwitcher extends React.PureComponent<Props & DispatchProps, State> {\n  constructor() {\n    super();\n    this.state = {\n      fadeAnimations: false,\n    };\n  }\n\n  componentDidMount() {\n    this.handleSwitch();\n  }\n\n  handleSwitch = () => {\n    const { setTheme, theme } = this.props;\n    const updates = setTimeout(\n      () => {\n        if (theme.data) {\n          setTheme(theme.data);\n        }\n      },\n      4000,\n    );\n\n    setTimeout(\n      () => {\n        this.setState({ fadeAnimations: true }, () => updates);\n      },\n      1500,\n    );\n  }\n\n  render() {\n    const { theme } = this.props;\n    const { fadeAnimations } = this.state;\n    const isDark = theme.data === ThemeSetting.Dark;\n    const description = isDark\n      ? 'Welcome to Dark Mode'\n      : 'Light Mode';\n\n    return (\n      <div\n        className=\"theme-switcher\"\n        key={theme.data}\n      >\n        <div className=\"main\">\n          {isDark && (\n            <img src={PolymailLogoDarkMode} alt=\"\" />\n          )}\n          {!isDark && (\n            <img src={PolymailLogoLightMode} alt=\"\" />\n          )}\n          <h4 className={classnames('description', { show: fadeAnimations })}>{description}</h4>\n        </div>\n      </div>\n    );\n  }\n}\n\nexport default ThemeSwitcher;\n"]}]}