BC,KAAI,CAAC,KAAK,GAAG;YACX,UAAU,EAAE,KAAK;SAClB,CAAC;;IACJ,CAAC;IAyBD,6BAAM,GAAN;QACQ,IAAA,eAKQ,EAJZ,oBAAO,EACP,8BAAY,EACZ,8BAAY,EACZ,sBACY,CAAC;QAEb,IAAA,kCAAU,CACG;QAEf,IAAM,SAAS,GAAG,QAAQ,CAAC,CAAC,CAAC,QAAQ,CAAC,CAAC,CAAC,OAAO,CAAC;QAEhD,OAAO,CACL,6BACE,SAAS,EAAC,eAAe;YAEzB,gCACE,SAAS,EAAC,SAAS,EACnB,OAAO,EAAE,IAAI,CAAC,WAAW;gBAEzB,6BAAK,SAAS,EAAC,MAAM;oBAClB,OAAO;wBACN,oBAAC,MAAM,OAAG;oBAEX,CAAC,OAAO;wBACP,oBAAC,KAAK,OAAG,CAEP;gBACN,6BAAK,SAAS,EAAC,MAAM,IAClB,SAAS,CACN,CACC;YACR,CAAC,YAAY,IAAI,YAAY,CAAC,IAAI,6BAAK,SAAS,EAAC,SAAS,GAAG;YAC7D,YAAY,IAAI,gCACf,SAAS,EAAC,mBAAmB,EAC7B,OAAO,EAAE,IAAI,CAAC,gBAAgB;gBAE9B,oBAAC,KAAK,IAAC,SAAS,EAAC,MAAM,EAAC,KAAK,EAAC,MAAM,EAAC,MAAM,EAAC,MAAM,GAAG,CAC9C;YACR,YAAY,IAAI,gCACf,SAAS,EAAC,mBAAmB,EAC7B,YAAY,EAAE,IAAI,CAAC,gBAAgB,EACnC,YAAY,EAAE,IAAI,CAAC,gBAAgB,EACnC,OAAO,EAAE,IAAI,CAAC,gBAAgB;gBAE9B,6BAAK,GAAG,EAAE,UAAU,CAAC,CAAC,CAAC,KAAK,CAAC,CAAC,CAAC,SAAS,GAAI,CACrC,CACL,CACP,CAAC;IACJ,CAAC;IACH,mBAAC;AAAD,CAAC,AAnFD,CAA2B,KAAK,CAAC,aAAa,GAmF7C;AAED,eAAe,YAAY,CAAC","sourcesContent":["import React from 'react';\nimport Share from '@src/components/Icons/Share.svg';\nimport Loader from '@src/components/Loader/Loader';\nimport Slack from '@src/components/Icons/Slack.svg';\nimport Asana from '@src/assets/Asana.png';\nimport AsanaGrey from '@src/assets/AsanaGrey.png';\n\nimport './BestowButton.scss';\nimport { IntegrationType } from '@src/types';\nimport { AmplitudeEvents, logAmplitudeEvent } from '@src/lib/amplitude-analytics';\n\nexport interface Props {\n  onShareClick: (integration?: IntegrationType) => void;\n  loading: boolean;\n  asanaEnabled: boolean;\n  slackEnabled: boolean;\n  isShared?: boolean;\n}\n\ninterface State {\n  asanaColor: boolean;\n}\n\nclass BestowButton extends React.PureComponent<Props, State> {\n  constructor() {\n    super();\n\n    this.state = {\n      asanaColor: false,\n    };\n  }\n\n  handleAsanaEnter = () => {\n    this.setState({ asanaColor: true });\n  }\n\n  handleAsanaLeave = () => {\n    this.setState({ asanaColor: false });\n  }\n\n  handleShare = () => {\n    const { onShareClick } = this.props;\n    logAmplitudeEvent(AmplitudeEvents.ClickShareEmailButton);\n    onShareClick();\n  }\n\n  handleAsanaShare = () => {\n    this.props.onShareClick(IntegrationType.Asana);\n  }\n\n  handleSlackShare = () => {\n    this.props.onShareClick(IntegrationType.Slack);\n    logAmplitudeEvent(AmplitudeEvents.ShareEmailViaSlack);\n  }\n\n  render() {\n    const {\n      loading,\n      asanaEnabled,\n      slackEnabled,\n      isShared,\n    } = this.props;\n    const {\n      asanaColor,\n    } = this.state;\n\n    const labelText = isShared ? 'Manage' : 'Share';\n\n    return (\n      <div\n        className=\"bestow-button\"\n      >\n        <button\n          className=\"primary\"\n          onClick={this.handleShare}\n        >\n          <div className=\"icon\">\n            {loading &&\n              <Loader />\n            }\n            {!loading &&\n              <Share />\n            }\n          </div>\n          <div className=\"copy\">\n            {labelText}\n          </div>\n        </button>\n        {(slackEnabled || asanaEnabled) && <div className=\"divider\" />}\n        {slackEnabled && <button\n          className=\"slack integration\"\n          onClick={this.handleSlackShare}\n        >\n          <Slack className=\"icon\" width=\"14px\" height=\"14px\" />\n        </button>}\n        {asanaEnabled && <button\n          className=\"asana integration\"\n          onMouseEnter={this.handleAsanaEnter}\n          onMouseLeave={this.handleAsanaLeave}\n          onClick={this.handleAsanaShare}\n        >\n          <img src={asanaColor ? Asana : AsanaGrey} />\n        </button>}\n      </div>\n    );\n  }\n}\n\nexport default BestowButton;\n"]}]}