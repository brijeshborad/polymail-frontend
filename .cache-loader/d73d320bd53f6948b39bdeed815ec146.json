 if (!draft || !me) {\n    return null;\n  }\n\n  const closeWindow = () => {\n    window.close();\n  };\n\n  document.title = draft.subject || '(No subject)';\n\n  return (\n    <div className=\"draft-page\">\n      <Composer\n        me={me}\n        accounts={accounts}\n        draft={draft}\n        hidePopout\n        onSend={closeWindow}\n        onDelete={closeWindow}\n        showFullHeight\n        hideActions\n      />\n      <CommandControl default={CommandControlType.Draft}/>\n    </div>\n  );\n};\n\nexport default Draft;\n"]}]}