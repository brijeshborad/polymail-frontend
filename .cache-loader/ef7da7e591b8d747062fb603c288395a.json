nfinite';\nimport { ContactAttachment } from '@src/types';\n\nimport File from './File';\n\nimport './FilesPane.scss';\n\ninterface Props {\n  files?: ContactAttachment[];\n  contactName: string;\n  listHeight: number;\n}\n\nconst CELL_HEIGHT = 70;\n\nconst FilesPane = (props: Props) => {\n  const isEmpty = !props.files || !props.files.length;\n  const FileConnect = File as any;\n\n  return (\n    <div className=\"files-pane\">\n      {isEmpty && <div className=\"empty\">\n        Nothing yet! Files shared between {props.contactName} and you will show up here.\n      </div>}\n      {!isEmpty && <Infinite\n        className=\"main\"\n        elementHeight={CELL_HEIGHT}\n        containerHeight={props.listHeight}\n      >\n        {props.files!.map((file: ContactAttachment, index: number) => (\n          <FileConnect file={file} key={index} cellHeight={CELL_HEIGHT} />\n        ))}\n      </Infinite>}\n    </div>\n  );\n};\n\nexport default FilesPane;\n"]}]}