ninterface Props {\n  className?: string;\n}\n\nexport interface DispatchProps {\n  dismissModal: () => void;\n}\n\nconst TeamJoined = (props: Props & DispatchProps & StateProps) => {\n  const { dismissModal, team } = props;\n  const handleClick = () => {\n    dismissModal();\n    return;\n  };\n\n  if (!team) {\n    return null;\n  }\n\n  const title = `Congratulations! You've joined ${team?.name}.`;\n  const description = 'You can now collaborate with your team on Polymail!';\n  return (\n    <Overlay\n      className={classnames('overlay', props.className)}\n    >\n      <div className=\"team-joined-main\">\n        <div className=\"header\">\n          <div className=\"title\">\n            {title}\n          </div>\n          <div className=\"description\">\n            {description}\n          </div>\n        </div>\n        <button\n          onClick={handleClick}\n          className=\"cta-button\"\n        >\n          Continue\n        </button>\n      </div>\n    </Overlay>\n  );\n};\n\nexport default TeamJoined;\n"]}]}