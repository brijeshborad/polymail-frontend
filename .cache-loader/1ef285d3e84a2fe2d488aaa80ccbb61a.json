,CAChB,CAAC;AACJ,CAAC,CAAC;AAEF,eAAe,YAAY,CAAC","sourcesContent":["import React from 'react';\nimport classnames from 'classnames';\nimport { animated } from 'react-spring';\nimport './OverviewCell.scss';\n\ninterface Props {\n  title: string;\n  description: string;\n  value: number;\n  styles?: any;\n  hasBackground?: boolean;\n  valueDescriptor?: string;\n  type: OverviewCellType;\n}\n\nexport enum OverviewCellType {\n  Card = 'card',\n  TextBlock = 'textBlock',\n}\n\nconst OverviewCell = (props: Props) => {\n  const {\n    title, value,\n    description, type,\n    styles, hasBackground,\n    valueDescriptor,\n  } = props;\n\n  if (!value) {\n    return null;\n  }\n\n  if (type === OverviewCellType.Card) {\n    return (\n      <animated.div\n        className=\"overview-cell-card\"\n        style={styles}\n      >\n        <div\n          className={classnames('title', { white: !!hasBackground })}\n        >\n          {title}\n        </div>\n        <animated.div className=\"container\">\n          <div className=\"stat-section\">\n            {value.toLocaleString()}\n            {valueDescriptor && (\n              <span className=\"value-descriptor\">&nbsp;&nbsp;{valueDescriptor}</span>\n            )}\n          </div>\n          <div className=\"description\">{description}</div>\n        </animated.div>\n      </animated.div>\n    );\n  }\n\n  return (\n    <animated.div\n      className=\"overview-cell-text-block\"\n      style={styles}\n    >\n      <div className=\"stat-section\">\n        {value.toLocaleString()}\n      </div>\n      <div className=\"title\">\n        {title}\n      </div>\n    </animated.div>\n  );\n};\n\nexport default OverviewCell;\n"]}]}