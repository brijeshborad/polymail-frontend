nimport Toggle from '@src/components/Form/Toggle';\n\nexport interface Props {\n  sequence: Sequence;\n  onCheckboxChange: (id: string) => void;\n  checked: boolean;\n}\n\nexport interface StateProps {\n  photoUrl: string;\n}\n\nexport interface DispatchProps {\n  updateSequence: (s: Sequence) => void;\n}\n\nconst SequenceCell = (props: Props & StateProps & DispatchProps) => {\n  const { sequence, checked, onCheckboxChange, photoUrl } = props;\n  const stages = sequence.stages;\n  const { analytics, recipients } = sequence;\n  const { active, completed, paused, bounced, failed } = recipients;\n  const { delivered, deliveredUnique, opened, replied, clicked } = analytics;\n  const handleToggle = (e: React.SyntheticEvent<HTMLInputElement>) => {\n    props.updateSequence({\n      ...sequence,\n      paused: !sequence.paused,\n    });\n  };\n  const handleCheck = (e: React.SyntheticEvent<HTMLInputElement>) => {\n    onCheckboxChange(sequence.id);\n  };\n\n  return (\n    <div className=\"sequence-cell\">\n      <Checkbox onChange={handleCheck} checked={checked} />\n      {!sequence.archived && <div className=\"toggle-container\">\n        <Toggle onChange={handleToggle} on={!sequence.paused} />\n      </div>}\n      <Link to={`/sequences/${sequence.id}`} className=\"link\">\n        <div className=\"sequence-info\">\n          <div className=\"name-container\">\n            <h4 className=\"name\">{sequence.name}</h4>\n            <span\n              className={classnames('tag', {\n                shared: !!sequence.team,\n                private: !sequence.team,\n              })}\n            >\n              {sequence.team ? 'Shared' : 'Private'}\n            </span>\n          </div>\n          <h5 className=\"info\">\n            {`${stages} Stage${stages > 1 ? 's' : ''}\n              ${active ? ` · ${active} Active` : ''}\n              ${completed ? `· ${completed} Completed` : ''}\n              ${paused ? ` · ${paused} Paused` : ''}\n              ${bounced ? ` · ${bounced} Bounced` : ''}`}\n            <span className=\"error\">{!!failed && `⚠️ ${failed} Failed`}</span>\n          </h5>\n        </div>\n        <div className=\"sequence-analytics\">\n          <div className=\"analytic delivered-container\">\n            <h4>{delivered}</h4>\n            <h5>Delivered</h5>\n          </div>\n          <div className=\"analytic opened-container\">\n            <h4>{Math.round((delivered ? (opened / delivered || 0) : 0) * 100)}%</h4>\n            <h5>Opened</h5>\n          </div>\n          <div className=\"analytic clicked-container\">\n            <h4>{Math.round((delivered ? (clicked / delivered || 0) : 0) * 100)}%</h4>\n            <h5>Clicked</h5>\n          </div>\n          <div className=\"analytic replied-container\">\n            <h4>\n              {Math.round((deliveredUnique ? (replied / deliveredUnique || 0) : 0) * 100)}%\n            </h4>\n            <h5>Replied</h5>\n          </div>\n        </div>\n        <div className=\"contact\">\n          <img src={photoUrl} />\n        </div>\n        <div className=\"date\">\n          {moment(sequence.created).format('MMM D')}\n        </div>\n      </Link>\n    </div>\n  );\n};\n\nexport default SequenceCell;\n"]}]}