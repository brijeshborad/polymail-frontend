AAI,CAAC,qBAAqB,EACzC,YAAY,EAAE,IAAI,CAAC,gBAAgB,GACnC,CACH,CACG,CACP,CAAC;IACJ,CAAC;IACH,iBAAC;AAAD,CAAC,AA7JD,CAAyB,KAAK,CAAC,aAAa,GA6J3C;AAED,eAAe,UAAU,CAAC","sourcesContent":["import React from 'react';\nimport {\n  Preferences, OnboardingType, Action, Features, Account, Onboarding as StateOnboarding,\n} from '@src/types';\nimport './Onboarding.scss';\nimport Legend from './Legend/Legend';\nimport OnboardingSectionDetail from './Legend/components/OnboardingSectionDetail';\nimport { OnboardingSection } from '@src/lib/onboarding';\n\nexport interface StateProps {\n  accounts: Account[];\n  features: Features;\n  preferences: Preferences;\n  onboarding: StateOnboarding;\n  currentStep?: OnboardingType;\n  allSteps: OnboardingType[];\n  isDraft?: boolean;\n  displayedSections: OnboardingSection[];\n  muteSounds?: boolean;\n}\n\ninterface State {\n  transitionAnimation: boolean;\n  activeStep?: OnboardingType;\n}\n\nexport interface DispatchProps {\n  updatePreferences: (preferences: Preferences) => void;\n  dispatchAction: (action: Action<any, any>) => void;\n}\n\nclass Onboarding extends React.PureComponent<StateProps & DispatchProps, State> {\n  constructor(props: StateProps & DispatchProps) {\n    super(props);\n\n    this.state = {\n      transitionAnimation: false,\n    };\n  }\n\n  componentDidMount() {\n    const { currentStep } = this.props;\n    if (currentStep) {\n      this.setState({ activeStep: currentStep });\n    }\n  }\n\n  componentDidUpdate(oldProps: StateProps) {\n    const { currentStep } = this.props;\n    if (currentStep !== oldProps.currentStep) {\n      this.setState({ activeStep: currentStep });\n\n    }\n  }\n\n  handleStepClick = (step: OnboardingType) => {\n    this.setState({ activeStep: step });\n  }\n\n  handleClick = (e: React.MouseEvent<HTMLElement>) => {\n    const threadList = document.getElementsByClassName('infinite-thread-list');\n    const element = threadList[0];\n    if (element && element instanceof HTMLElement) {\n      element.focus();\n    }\n    e.stopPropagation();\n    e.preventDefault();\n  }\n\n  handleClearActiveStep = () => {\n    this.setState({ activeStep: undefined });\n  }\n\n  handleManualNext = () => {\n    const { allSteps } = this.props;\n    const { activeStep } = this.state;\n    if (!activeStep) {\n      return;\n    }\n\n    const currentIndex = allSteps.indexOf(activeStep);\n    const nextItem = allSteps[currentIndex + 1];\n    if (nextItem) {\n      this.setState({ activeStep: nextItem });\n    } else {\n      this.setState({ activeStep: undefined });\n    }\n  }\n\n  renderLegendSection = () => {\n    const { onboarding, currentStep, displayedSections } = this.props;\n\n    let description = (\n      <div className=\"description\">\n        <span>\n          Get started by following the onboarding checklist. Once\n          you complete the checklist you'll receive\n        </span>\n        <span className=\"azure\">\n          &nbsp; $10 in free credit!\n        </span>\n      </div>\n    );\n\n    // Set description when credit should not be claimable\n    if (!onboarding.creditClaimable) {\n      description = (\n        <div className=\"description\">\n          <span>\n            Get started by completing the onboarding checklist!\n            <br />\n            <br />\n            Learn the basics on how to use Polymail to manage your emails quickly!\n          </span>\n        </div>\n      );\n    }\n\n    // tslint:disable-next-line:max-line-length\n    if (currentStep === OnboardingType.Complete) {\n      description = (\n        <div className=\"description\">\n          <span>\n            Congratulations! You're set up to use Polymail!\n            <br />\n            <br />\n            Keep a look out for email updates where we teach you more about the product!\n            <br />\n            <br />\n            Feel free to look back at previous steps if you ever want to\n            remember how to perform certain actions.\n          </span>\n        </div>\n      );\n    }\n\n    let title = 'Getting Started';\n\n    if (currentStep === OnboardingType.Complete) {\n      title = 'You\\'re All Done! ðŸ’Œ';\n    }\n\n    return (\n      <div className=\"main\">\n        <div className=\"title-section\">\n          <div className=\"title\">\n            {title}\n          </div>\n          {description}\n        </div>\n        <div className=\"legend-section\">\n          <Legend\n            onOnboardingStepClick={this.handleStepClick}\n            onboarding={onboarding}\n            currentStep={currentStep}\n            displayedSections={displayedSections}\n          />\n        </div>\n      </div>\n    );\n  }\n\n  render() {\n    const { currentStep, onboarding } = this.props;\n    const { activeStep } = this.state;\n    const completed = (!activeStep && currentStep === OnboardingType.Complete) ||\n      activeStep === OnboardingType.Complete;\n    const exploreModalShowing = !onboarding.exploreModal;\n    const showLegend = (!activeStep || completed) || exploreModalShowing;\n    return (\n      <div\n        className=\"onboarding\"\n        onClick={this.handleClick}\n      >\n        {showLegend && (\n          this.renderLegendSection()\n        )}\n        {!!activeStep && activeStep !== OnboardingType.Complete && !exploreModalShowing && (\n          <OnboardingSectionDetail\n            currentStep={currentStep}\n            activeStep={activeStep}\n            onLegendClick={this.handleClearActiveStep}\n            onManualNext={this.handleManualNext}\n          />\n        )}\n      </div>\n    );\n  }\n}\n\nexport default Onboarding;\n"]}]}