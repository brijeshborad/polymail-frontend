IAAE,UAAU,CAAC,KAAK,CAAC,CAAO;gBAClD,6BAAK,SAAS,EAAC,SAAS,IACrB,OAAO;qBACL,MAAM,CAAC,UAAC,CAAsB;oBAC7B,OAAA,CAAC,CAAC,IAAI,KAAK,YAAY,CAAC,qBAAqB,IAAI,CAAC,CAAC,CAAC,CAAC,OAAO;gBAA5D,CAA4D,CAAC;qBAC9D,GAAG,CAAC,UAAC,CAAsB,IAAK,OAAA,CAC/B,6BAAK,SAAS,EAAC,QAAQ,EAAC,GAAG,EAAE,CAAC,CAAC,IAAI;oBACjC,6BAAK,SAAS,EAAC,MAAM,IAAE,CAAC,CAAC,IAAI,CAAO;oBACpC,6BAAK,SAAS,EAAC,MAAM;wBAClB,CAAC,CAAC,CAAC,CAAC,OAAO,IAAI,oBAAC,cAAc,IAAC,SAAS,EAAE,OAAO,CAAC,aAAa,GAAI;wBACnE,CAAC,CAAC,CAAC,CAAC,OAAO,IAAI,oBAAC,cAAc,IAAC,SAAS,EAAE,OAAO,CAAC,IAAI,GAAI;wBAC1D,CAAC,CAAC,CAAC,CAAC,QAAQ,IAAI,oBAAC,cAAc,IAAC,SAAS,EAAE,OAAO,CAAC,KAAK,GAAI;wBAC5D,CAAC,CAAC,CAAC,CAAC,MAAM,IAAI,oBAAC,cAAc,IAAC,SAAS,EAAE,OAAO,CAAC,GAAG,GAAI;wBACxD,CAAC,CAAC,CAAC,CAAC,OAAO,IAAI,oBAAC,cAAc,IAAC,SAAS,EAAE,CAAC,CAAC,OAAO,GAAI;wBACvD,CAAC,CAAC,CAAC,CAAC,UAAU,IAAI,sCAAc;wBAChC,CAAC,CAAC,CAAC,CAAC,UAAU,IAAI,oBAAC,cAAc,IAAC,SAAS,EAAE,CAAC,CAAC,UAAU,GAAI,CAC1D,CACF,CACP,EAbgC,CAahC,CAAC,CACA,CACF,CACP,EAvBsF,CAuBtF,CAAC,CACE,CACF,CACP,CAAC;IACJ,CAAC;IACH,wBAAC;AAAD,CAAC,AA9GD,CAAgC,KAAK,CAAC,aAAa,GA8GlD;AAED,eAAe,iBAAiB,CAAC","sourcesContent":["import React from 'react';\nimport { Hotkeys, CommandControlType, Preferences, KeyCode } from '@src/types';\nimport Dropdown from '@src/components/Form/Dropdown';\nimport './KeyboardShortcuts.scss';\nimport { getHotkeySet, Hotkey, HotkeyCode, HotkeyAction } from '@src/lib/hotkeys';\nimport { map, capitalize } from 'lodash';\nimport KeyboardSymbol from '@src/components/Base/KeyboardSymbol/KeyboardSymbol';\nimport { IS_MAC } from '@src/lib/device';\n\nexport interface StateProps {\n  preferences: Preferences;\n}\n\ninterface Sections {\n  basics: Array<Hotkey | Hotkey & HotkeyCode>;\n  thread: Array<Hotkey | Hotkey & HotkeyCode>;\n  composing: Array<Hotkey | Hotkey & HotkeyCode>;\n  navigation: Array<Hotkey | Hotkey & HotkeyCode>;\n  system: Array<Hotkey | Hotkey & HotkeyCode>;\n}\n\ninterface State {\n  hotkeyType: Hotkeys;\n  sections: Sections;\n}\n\nexport interface DispatchProps {\n  updatePreferences: (preferences: Preferences) => void;\n}\n\nconst HOTKEY_VALUES = [Hotkeys.Default, Hotkeys.Gmail];\nclass KeyboardShortcuts extends React.PureComponent<StateProps & DispatchProps, State> {\n  constructor(props: StateProps & DispatchProps) {\n    super(props);\n\n    this.state = {\n      hotkeyType: props.preferences.hotkeys,\n      sections: this.getHotkeySections(props.preferences.hotkeys),\n    };\n  }\n\n  UNSAFE_componentWillReceiveProps(nextProps: StateProps & DispatchProps) {\n    if (nextProps !== this.props) {\n      this.setState({\n        hotkeyType: nextProps.preferences.hotkeys,\n        sections: this.getHotkeySections(nextProps.preferences.hotkeys),\n      });\n    }\n  }\n\n  handleHotkeysTypeSelect = (value: string, index: number, id?: string) => {\n    const { preferences, updatePreferences } = this.props;\n    const { hotkeyType } = this.state;\n\n    if (HOTKEY_VALUES[index] && HOTKEY_VALUES[index] !== hotkeyType) {\n      updatePreferences({\n        ...preferences,\n        hotkeys: HOTKEY_VALUES[index],\n      });\n    }\n  }\n\n  getHotkeySections = (hotkeyType: Hotkeys) => {\n    const hotkeys = getHotkeySet(hotkeyType);\n    const sections: Sections = {\n      basics: [],\n      thread: [],\n      composing: [],\n      navigation: [],\n      system: [],\n    };\n\n    hotkeys.forEach((h) => {\n      if (h.route) {\n        sections.navigation.push({\n          ...h,\n          name: h.name.match('Go to') ? h.name.slice('Go to'.length).trim() : h.name,\n        });\n      } else if (h.scope && h.scope === CommandControlType.Thread) {\n        sections.thread.push(h);\n      } else if (h.scope && h.scope === CommandControlType.Draft) {\n        sections.composing.push(h);\n      } else if (h.scope && h.scope === CommandControlType.System) {\n        sections.system.push(h);\n      } else if (!h.scope || [\n        CommandControlType.Global,\n        CommandControlType.Navigation,\n      ].indexOf(h.scope) > -1) {\n        sections.basics.push(h);\n      }\n    });\n\n    return sections;\n  }\n\n  render() {\n    const { hotkeyType, sections } = this.state;\n\n    return (\n      <div className=\"keyboard-shortcuts\">\n        <h5>Keyboard Shortcuts</h5>\n        <Dropdown\n          values={HOTKEY_VALUES.map(v => `${capitalize(v)} Shortcuts`)}\n          onSelect={this.handleHotkeysTypeSelect}\n          startIndex={HOTKEY_VALUES.indexOf(hotkeyType)}\n        />\n        <div className=\"keyboard-hotkey\">\n          Press\n          <span className=\"keyboard\">{IS_MAC ? '⌘' : 'ctrl'}</span>\n          <span className=\"keyboard\">/</span>\n          to toggle this panel\n        </div>\n        <div className=\"hotkeys-wrapper\">\n          {map(sections as any, (hotkeys: Array<Hotkey | Hotkey & HotkeyCode>, title: string) => (\n            <div className=\"section\" key={title}>\n              <div className=\"heading\">{capitalize(title)}</div>\n              <div className=\"hotkeys\">\n                {hotkeys\n                  .filter((h: Hotkey & HotkeyCode) =>\n                    h.name !== HotkeyAction.KeyboardShortcutGuide && !!h.keyCode)\n                  .map((h: Hotkey & HotkeyCode) => (\n                    <div className=\"hotkey\" key={h.name}>\n                      <div className=\"name\">{h.name}</div>\n                      <div className=\"code\">\n                        {!!h.metaKey && <KeyboardSymbol character={KeyCode.LeftWindowKey} />}\n                        {!!h.ctrlKey && <KeyboardSymbol character={KeyCode.Ctrl} />}\n                        {!!h.shiftKey && <KeyboardSymbol character={KeyCode.Shift} />}\n                        {!!h.altKey && <KeyboardSymbol character={KeyCode.Alt} />}\n                        {!!h.keyCode && <KeyboardSymbol character={h.keyCode} />}\n                        {!!h.altKeyCode && <span>/</span>}\n                        {!!h.altKeyCode && <KeyboardSymbol character={h.altKeyCode} />}\n                      </div>\n                    </div>\n                  ))}\n              </div>\n            </div>\n          ))}\n        </div>\n      </div>\n    );\n  }\n}\n\nexport default KeyboardShortcuts;\n"]}]}