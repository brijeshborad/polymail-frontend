AAlED,CAA+B,KAAK,CAAC,aAAa,GAkEjD;AAED,eAAe,gBAAgB,CAAC","sourcesContent":["import React from 'react';\nimport classnames from 'classnames';\n\nimport './SettingTextInput.scss';\n\ninterface Props {\n  className?: string;\n  name?: string;\n  title: string;\n  prefix?: string;\n  description: string;\n  placeholder: string;\n  autoComplete?: string;\n  value: string;\n  type: string;\n  autofocus?: boolean;\n  required?: boolean;\n  readOnly?: boolean;\n  onChange?: ((e: React.ChangeEvent<HTMLInputElement>) => void);\n  onFocus?: ((e: React.FocusEvent<HTMLInputElement>) => void);\n}\n\nclass SettingTextInput extends React.PureComponent<Props> {\n  input?: HTMLInputElement;\n\n  constructor() {\n    super();\n  }\n\n  componentDidMount() {\n    if (this.props.autofocus && this.input) {\n      this.input.focus();\n    }\n  }\n\n  handleRef = (ref: HTMLInputElement) => {\n    this.input = ref;\n  }\n\n  render() {\n    const {\n      title,\n      description,\n      placeholder,\n      prefix,\n      value,\n      onChange,\n      onFocus,\n      type,\n      autoComplete,\n      readOnly,\n      required,\n      name,\n      className,\n    } = this.props;\n\n    return (\n      <div className={classnames('setting-text-input', className)}>\n        <div className=\"title\">\n          {title}\n        </div>\n        <div className=\"description\">\n          {description}\n        </div>\n        <div className=\"input-area\">\n          {prefix &&\n            <div className=\"prefix\">\n              {prefix}\n            </div>\n          }\n          <input\n            dir=\"auto\"\n            ref={this.handleRef}\n            className=\"input\"\n            placeholder={placeholder}\n            value={value}\n            onChange={onChange}\n            onFocus={onFocus}\n            type={type}\n            name={name}\n            required={required}\n            readOnly={readOnly}\n            autoComplete={autoComplete || ''}\n          />\n        </div>\n      </div>\n    );\n  }\n}\n\nexport default SettingTextInput;\n"]}]}