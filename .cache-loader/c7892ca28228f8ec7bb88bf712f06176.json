sBAAgB,GAAG;YACjB,IAAI,KAAI,CAAC,KAAK,CAAC,WAAW,EAAE;gBAC1B,KAAI,CAAC,gBAAgB,EAAE,CAAC;gBAExB,UAAU,CAAC;oBACT,KAAI,CAAC,gBAAgB,EAAE,CAAC;oBAC1B,iCAAiC;gBACjC,CAAC,EAAE,IAAI,CAAC,CAAC;aACV;QACH,CAAC,CAAA;QArDC,KAAI,CAAC,KAAK,GAAG;YACX,OAAO,EAAE,KAAK;YACd,IAAI,EAAE,KAAK,CAAC,IAAI;YAChB,GAAG,EAAE,KAAK,CAAC,GAAG;YACd,MAAM,EAAE,KAAK,CAAC,MAAM;YACpB,KAAK,EAAE,KAAK,CAAC,KAAK;SACnB,CAAC;;IACJ,CAAC;IAgDD,yBAAM,GAAN;QACQ,IAAA,eASQ,EARZ,sBAAQ,EACR,cAAI,EACJ,8BAAY,EACZ,8BAAY,EACZ,0BAAU,EACV,sCAAgB,EAChB,wBAAS,EACT,wBACY,CAAC;QACT,IAAA,eAAkD,EAAhD,oBAAO,EAAE,YAAG,EAAE,cAAI,EAAE,kBAAM,EAAE,gBAAoB,CAAC;QACzD,IAAM,MAAM,GAAG,OAAO,IAAI,YAAY,IAAI,eAAe,CAAC,YAAY,CAAC,CAAC;QACxE,IAAM,kBAAkB,GAAG,MAAM,IAAI,MAAM,CAAC,IAAI;YAC9C,MAAM,CAAC,IAAI;iBACR,OAAO,CAAC,aAAa,EAAE,EAAE,CAAC;iBAC1B,IAAI,EAAE,CAAC;QAEZ,IAAM,WAAW,GAAG,kBAAkB,CAAC,CAAC,CAAC,IAAI,GAAG,kBAAkB,GAAG,GAAG,CAAC,CAAC,CAAC,EAAE,CAAC;QAC9E,IAAM,OAAO,GAAG,MAAM,IAAI,MAAM,CAAC,IAAI,IAAI,IAAI,CAAC;QAE9C,OAAO,CACL,6BACE,SAAS,EAAE,UAAU,CAAC,WAAW,EAAE,SAAS,CAAC,EAC7C,YAAY,EAAE,IAAI,CAAC,gBAAgB,EACnC,YAAY,EAAE,IAAI,CAAC,gBAAgB,EACnC,KAAK,EAAE,EAAE,QAAQ,EAAE,UAAU,EAAE,EAC/B,OAAO,EAAE,IAAI,CAAC,gBAAgB;YAE7B,QAAQ;YACR,OAAO;gBACN,oBAAC,OAAO,IACN,SAAS,EAAE,UAAU,CAAC,SAAS,EAAE;wBAC/B,GAAG,EAAE,CAAC,CAAC,GAAG;wBACV,MAAM,EAAE,CAAC,CAAC,MAAM;wBAChB,KAAK,EAAE,CAAC,CAAC,gBAAgB;wBACzB,SAAS,EAAE,CAAC,CAAC,SAAS;qBACvB,CAAC,EACF,GAAG,EAAE,GAAG,EACR,IAAI,EAAE,IAAI,EACV,MAAM,EAAE,MAAM,EACd,KAAK,EAAE,KAAK,EACZ,IAAI,EAAE,OAAO,GAAG,WAAW,EAC3B,KAAK,EAAE,GAAG,EACV,QAAQ,EAAE,IAAI,CAAC,KAAK,CAAC,QAAQ,EAC7B,YAAY,EAAE,YAAY,EAC1B,UAAU,EAAE,UAAU,GACtB,CAEA,CACP,CAAC;IACJ,CAAC;IACH,eAAC;AAAD,CAAC,AA/GD,CAAuB,KAAK,CAAC,aAAa,GA+GzC;AAED,eAAe,QAAQ,CAAC","sourcesContent":["import React from 'react';\nimport Tooltip from './Tooltip';\nimport classnames from 'classnames';\nimport { HotkeyAction, getHotkeyByName } from '@src/lib/hotkeys';\n\nexport interface Props {\n  copy?: string;\n  children: JSX.Element;\n  className?: string;\n  top?: string;\n  left?: string;\n  bottom?: string;\n  right?: string;\n  hotkeyAction?: HotkeyAction;\n  anchorBottom?: boolean;\n  fixedPositioning?: boolean;\n  hoverChild?: JSX.Element;\n  multiline?: boolean;\n  maxWidth?: string;\n  showOnClick?: boolean;\n}\n\nexport interface State {\n  showTip: boolean;\n  top?: string;\n  left?: string;\n  bottom?: string;\n  right?: string;\n}\n\nclass HoverTip extends React.PureComponent<Props, State> {\n  constructor(props: Props) {\n    super(props);\n\n    this.state = {\n      showTip: false,\n      left: props.left,\n      top: props.top,\n      bottom: props.bottom,\n      right: props.right,\n    };\n  }\n\n  getNewPosition = (mousePosition?: number, defaultPosition?: string) => {\n    const { fixedPositioning } = this.props;\n\n    if (!defaultPosition || !fixedPositioning || !mousePosition) {\n      return defaultPosition;\n    }\n\n    return `${parseInt(defaultPosition, 10) + mousePosition}px`;\n  }\n\n  handleMouseEnter = (e?: React.MouseEvent<HTMLDivElement>) => {\n    const {\n      left,\n      right,\n      top,\n      bottom,\n    } = this.props;\n    let rect;\n    if (e && e.currentTarget) {\n      rect = e.currentTarget.getBoundingClientRect();\n    }\n\n    this.setState({\n      showTip: true,\n      left: this.getNewPosition(rect && rect.left, left),\n      right: this.getNewPosition(rect && rect.right, right),\n      top: this.getNewPosition(rect && rect.top, top),\n      bottom: this.getNewPosition(rect && rect.bottom, bottom),\n    });\n  }\n\n  handleMouseLeave = () => {\n    this.setState({ showTip: false });\n  }\n\n  handleMouseClick = () => {\n    if (this.props.showOnClick) {\n      this.handleMouseEnter();\n\n      setTimeout(() => {\n        this.handleMouseLeave();\n      // tslint:disable-next-line:align\n      }, 3000);\n    }\n  }\n\n  render() {\n    const {\n      children,\n      copy,\n      hotkeyAction,\n      anchorBottom,\n      hoverChild,\n      fixedPositioning,\n      multiline,\n      className,\n    } = this.props;\n    const { showTip, top, left, bottom, right } = this.state;\n    const hotkey = showTip && hotkeyAction && getHotkeyByName(hotkeyAction);\n    const strippedHotkeyText = hotkey && hotkey.text &&\n      hotkey.text\n        .replace(/[\\(\\)\\s\\+]/g, '')\n        .trim();\n\n    const hotkeyLabel = strippedHotkeyText ? ' (' + strippedHotkeyText + ')' : '';\n    const endCopy = hotkey && hotkey.name || copy;\n\n    return (\n      <div\n        className={classnames('hover-tip', className)}\n        onMouseEnter={this.handleMouseEnter}\n        onMouseLeave={this.handleMouseLeave}\n        style={{ position: 'relative' }}\n        onClick={this.handleMouseClick}\n      >\n        {children}\n        {showTip &&\n          <Tooltip\n            className={classnames('tooltip', {\n              top: !!top,\n              bottom: !!bottom,\n              fixed: !!fixedPositioning,\n              multiline: !!multiline,\n            })}\n            top={top}\n            left={left}\n            bottom={bottom}\n            right={right}\n            copy={endCopy + hotkeyLabel}\n            delay={500}\n            maxWidth={this.props.maxWidth}\n            anchorBottom={anchorBottom}\n            hoverChild={hoverChild}\n          />\n        }\n      </div>\n    );\n  }\n}\n\nexport default HoverTip;\n"]}]}