t } from '@src/types/scheduling';\n\nexport interface Props {\n  startTime: string;\n  duration: number;\n  disabled?: boolean;\n  timeslot: TimeSlot;\n  truncateTimeHeight?: boolean;\n\n  showVerticalSeparator?: boolean;\n  onSelect?: (timeSlot: TimeSlot) => void;\n}\n\nconst SchedulingRow = (props: Props) => {\n  const { onSelect, truncateTimeHeight, disabled, showVerticalSeparator } = props;\n  const handleClick = () => {\n    if (onSelect) {\n      onSelect(props.timeslot);\n    }\n  };\n\n  if (truncateTimeHeight) {\n    return (\n      <div className=\"scheduling-row-empty\">\n        <div className={classnames('vertical-bar', { 'show-vertical': !!showVerticalSeparator })} />\n      </div>\n    );\n  }\n\n  if (disabled) {\n    return (\n      <div className=\"scheduling-row-blocked\">\n        <div className={classnames('empty-slot', { 'show-vertical': showVerticalSeparator })}>\n          <div className=\"circle\" />\n        </div>\n      </div>\n    );\n  }\n\n  return (\n    <div\n      className={classnames('scheduling-row', { disabled })}\n      onClick={handleClick}\n    >\n      <div className=\"start-time\">\n        {props.startTime}\n      </div>\n      <div className=\"duration\">\n        {`${props.duration} minutes`}\n      </div>\n    </div>\n  );\n};\n\nexport default SchedulingRow;\n"]}]}