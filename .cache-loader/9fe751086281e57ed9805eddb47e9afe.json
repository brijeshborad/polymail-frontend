CACP,CAAC;IACJ,CAAC;IACH,oBAAC;AAAD,CAAC,AA5HD,CAA4B,KAAK,CAAC,aAAa,GA4H9C;AAED,eAAe,aAAa,CAAC","sourcesContent":["import React from 'react';\n\nimport { SectionItem } from '../MailTab/components/Section/Item/SectionItem';\nimport PreferencesTabHeader from './PreferencesTabHeader';\nimport Confirm, { Props as ConfirmProps } from '@src/components/Base/Confirm';\nimport routes from '@src/routes/Settings/routes';\n\nimport { Route } from '@src/types';\n\nimport './PreferencesTab.scss';\nimport CustomScroll from 'react-custom-scroll';\nimport { Configuration } from '@src/lib/draft';\n\ninterface Props {\n  selectedTitle: string;\n  onClearCache: () => void;\n  createDraft: (configuration: Configuration, recipients: string[]) => void;\n  toggleContactSupportPopup: (collapsed: boolean) => void;\n  isMobile?: boolean;\n  toggleSidebar: () => void;\n}\n\n// const secondOptions = [\n//   'Invite Friends',\n//   'Updates',\n//   'Help Center',\n//   'Send Feedback',\n//   'Logout',\n// ];\n\ninterface State {\n  confirmProps?: ConfirmProps;\n}\n\n// TODO(SHIN): Abstract Section Item\nclass PreferenceTab extends React.PureComponent<Props, State> {\n  constructor() {\n    super();\n    this.state = {};\n  }\n\n  handleClearCacheClick = (e: React.MouseEvent<HTMLElement>) => {\n    const { onClearCache } = this.props;\n\n    const confirmProps: ConfirmProps = {\n      title: 'Clear Cache',\n      message: 'This will cause your account to fully re-sync. Are you sure you want to continue?',\n      confirmCopy: 'Yes',\n      onConfirm: () => {\n        onClearCache();\n        this.setState({ confirmProps: undefined });\n      },\n      onCancel: () => this.setState({ confirmProps: undefined }),\n    };\n\n    this.setState({ confirmProps });\n  }\n\n  handleHelpCenterClick = (e: React.MouseEvent<HTMLElement>) => {\n    window.open('https://help.polymail.io/', '_blank');\n  }\n\n  handleEmailUsClick = (e: React.MouseEvent<HTMLElement>) => {\n    this.props.toggleContactSupportPopup(false);\n    this.props.createDraft(Configuration.MailTo, ['team@polymail.io']);\n  }\n\n  handleSectionsElementClick = (e: React.MouseEvent<HTMLElement>) => {\n    const { toggleSidebar, isMobile } = this.props;\n    if (isMobile) {\n      toggleSidebar();\n    }\n  }\n\n  render() {\n    const { selectedTitle, isMobile, toggleSidebar } = this.props;\n    const { confirmProps } = this.state;\n\n    return (\n      <div className=\"preferences-tab\">\n        {confirmProps &&\n          <Confirm {...confirmProps} />\n        }\n        <PreferencesTabHeader isMobile={isMobile} toggleSidebar={toggleSidebar} />\n        <CustomScroll flex=\"1\">\n          <div className=\"main\">\n            <div className=\"sections\">\n              <div className=\"section\">\n                {routes\n                  .filter(r => !r.hide)\n                  .map((item: Route) => {\n                    const selected = selectedTitle === item.title;\n                    return <SectionItem\n                      key={item.title || item.pathname}\n                      name={item.title || item.pathname}\n                      count={0}\n                      selected={selected}\n                      link={item.pathname}\n                      mutedColor={!selected}\n                      onClick={this.handleSectionsElementClick}\n                    />;\n                  })\n                }\n                <br />\n                <SectionItem\n                  key=\"Refer Friends\"\n                  name=\"Refer Friends\"\n                  count={0}\n                  selected={selectedTitle === 'Refer Friends'}\n                  link=\"/settings/refer-friends\"\n                  mutedColor\n                  pillText=\"Get $10\"\n                  onClick={this.handleSectionsElementClick}\n                />\n                <br />\n                <SectionItem\n                  key=\"Help Center\"\n                  name=\"Help Center\"\n                  count={0}\n                  selected={false}\n                  onClick={this.handleHelpCenterClick}\n                  mutedColor\n                />\n                {!isMobile && (\n                  <>\n                  <SectionItem\n                    key=\"Email us\"\n                    name=\"Email us\"\n                    count={0}\n                    selected={false}\n                    onClick={this.handleEmailUsClick}\n                    mutedColor\n                  />\n                  <SectionItem\n                    key=\"Clear Cache\"\n                    name=\"Clear Cache\"\n                    count={0}\n                    selected={false}\n                    onClick={this.handleClearCacheClick}\n                    mutedColor\n                  />\n                  </>\n                )}\n                <SectionItem\n                  key=\"Sign Out\"\n                  name=\"Sign Out\"\n                  selected={selectedTitle === 'Sign Out'}\n                  count={0}\n                  link=\"/settings/signout\"\n                  mutedColor={selectedTitle !== 'Sign Out'}\n                  onClick={this.handleSectionsElementClick}\n                />\n              </div>\n            </div>\n          </div>\n        </CustomScroll>\n      </div>\n    );\n  }\n}\n\nexport default PreferenceTab;\n"]}]}