AAC,CAAC,EAAtC,CAAsC;IAChF,kBAAkB,EAAE,UAAC,KAA6B,IAAK,OAAA,QAAQ,CAAC,iBAAiB,CAAC,KAAK,CAAC,CAAC,EAAlC,CAAkC;CAC1F,CAAC,EAP4E,CAO5E,CAAC;AAEH,eAAe,OAAO,CAAmC,eAAe,EAAE,kBAAkB,CAAC,CAC3F,QAAQ,CACT,CAAC","sourcesContent":["import { connect } from 'react-redux';\n\nimport { StoreState, TimeBlock } from '@src/types';\nimport Calendar, { Props, StateProps, DispatchProps } from './Calendar';\nimport { Dispatch } from 'redux';\nimport { draftCalendarInviteAdd, selectDraft, selectDraftForThread } from '@src/ducks/drafts';\nimport {\n  calendarList, calendarHideToggle, CalendarListTrigger,\n  selectWritableCalendars, CalendarEventRangeMeta, calendarEventList,\n} from '@src/ducks/calendars';\n\nconst mapStateToProps = (state: StoreState): StateProps => {\n  const draft = state.selectedDraft\n    ? selectDraft(state, state.selectedDraft).data\n    : state.selectedThread && selectDraftForThread(state, state.selectedThread.id).data;\n  return {\n    selectedThread: state.selectedThread,\n    draft,\n    // NOTE(SHAHAN): Disabling account filtering, as this causes calendars to get filtered,\n    //  which is a behavior we don't want.\n    // account: state.mailbox.account,\n    calendars: selectWritableCalendars(state),\n    calendarEvents: state.calendarEvents.command,\n    readOnly: !draft,\n  };\n};\n\nconst mapDispatchToProps = (dispatch: Dispatch<StoreState>): DispatchProps => ({\n  addCalendarInviteToDraft: (tb: TimeBlock) => dispatch(draftCalendarInviteAdd(tb)),\n  listCalendars: (account?: string) => dispatch(\n    calendarList({ type: CalendarListTrigger.ClientIncludingEvents, account }),\n  ),\n  toggleCalendarHide: (calendar: string) => dispatch(calendarHideToggle(calendar)),\n  listCalendarEvents: (range: CalendarEventRangeMeta) => dispatch(calendarEventList(range)),\n});\n\nexport default connect<StateProps, DispatchProps, Props>(mapStateToProps, mapDispatchToProps)(\n  Calendar,\n);\n"]}]}