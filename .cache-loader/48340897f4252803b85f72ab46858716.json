act';\nimport ControlPopover from '../components/ControlPopover';\nimport {\n  CommandControlType, Preferences, Mailbox, Features, MailboxType,\n} from '@src/types';\n\nimport './Navigation.scss';\nimport { OnboardingPatchMeta } from '@src/ducks/me';\nimport { getHotkeyByName, hotkeysTypeahead, Hotkey, HotkeyCode } from '@src/lib/hotkeys';\nimport browserHistory from '@src/lib/browserHistory';\nimport KeyboardSymbol from '@src/components/Base/KeyboardSymbol/KeyboardSymbol';\n\nexport interface Props {\n  preferences: Preferences;\n  onHide: () => void;\n}\n\nexport interface DispatchProps {\n  updateOnboarding: (meta: OnboardingPatchMeta) => void;\n  hide: () => void;\n}\n\nexport interface StateProps {\n  mailbox?: Mailbox;\n  features: Features;\n  preferences: Preferences;\n}\n\ninterface State {\n  options: Array<{\n    title: string;\n    desc: string;\n    customDescRenderer: any;\n  }>;\n}\n\nclass Navigation extends React.PureComponent<StateProps & DispatchProps & Props, State> {\n  constructor() {\n    super();\n    this.state = {\n      options: [],\n    };\n  }\n\n  componentDidMount() {\n    this.setState({\n      options: hotkeysTypeahead('')\n        .filter(hotkey => this.filterResult(hotkey))\n        .filter(o => this.filterResult(o))\n        .map((option: Hotkey & HotkeyCode) => ({\n          title: option.name,\n          desc: option.text,\n          customDescRenderer: option.keyCode ? this.renderOptionDesc : undefined,\n        })),\n    });\n  }\n\n  filterResult = (hotkey: Hotkey): boolean => {\n    return hotkey.scope === CommandControlType.Navigation;\n  }\n\n  handleSelect = (option: any, index: number) => {\n    const { mailbox, hide } = this.props;\n    const hotkey = getHotkeyByName(option);\n    if (hotkey && hotkey.route) {\n      if (mailbox) {\n        browserHistory.push(hotkey.route(mailbox));\n      } else {\n        const newMailbox = { type: MailboxType.Any };\n        browserHistory.push(hotkey.route(newMailbox));\n      }\n      hide();\n    }\n  }\n\n  handleInputChange = (partial: string) => {\n    const options = hotkeysTypeahead(partial)\n      .filter(hotkey => this.filterResult(hotkey))\n      .filter(o => this.filterResult(o))\n      .map((option: Hotkey & HotkeyCode) => ({\n        title: option.name,\n        desc: option.text,\n        customDescRenderer: option.keyCode ? this.renderOptionDesc : undefined,\n      }));\n\n    this.setState({ options });\n  }\n\n  renderOptionDesc = (desc: string) => {\n    return desc.split('+').map((c, i) => (\n      <KeyboardSymbol\n        key={`${c}-${i}`}\n        character={c}\n      />\n    ));\n  }\n\n  renderFooter = () => {\n    return (\n      <div className=\"control-footer\">\n        <div>\n          <KeyboardSymbol\n            character=\"⬆\"\n          />\n          <KeyboardSymbol\n            character=\"⬇\"\n          />\n          <span>to move</span>\n        </div>\n        <div>\n          <KeyboardSymbol\n            character=\"↵\"\n          />\n          <span>to select</span>\n        </div>\n        <div>\n          <KeyboardSymbol\n            character=\"g\"\n          />\n          <span>open again</span>\n        </div>\n      </div>\n    );\n  }\n\n  render() {\n    const { options } = this.state;\n\n    return (\n      <ControlPopover\n        className=\"navigation-popover\"\n        type={CommandControlType.Navigation}\n        heading=\"Navigate to\"\n        options={options}\n        placeholder=\"Inbox, Archive, Settings...\"\n        onHide={this.props.onHide}\n        onInputChange={this.handleInputChange}\n        onSelect={this.handleSelect}\n        renderFooter={this.renderFooter}\n        showOnMount\n      />\n    );\n  }\n}\n\nexport default Navigation;\n"]}]}