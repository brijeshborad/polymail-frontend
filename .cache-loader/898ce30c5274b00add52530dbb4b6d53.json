AAC,CAC3C,CACG,CACF,CACF,CACP,CAAC;AACJ,CAAC,CAAC;AAEF,eAAe,SAAS,CAAC","sourcesContent":["import React from 'react';\nimport classnames from 'classnames';\n\nimport './AwardCell.scss';\n\ninterface Props {\n  awardType?: AwardCellType;\n  awardValue?: number;\n}\n\nexport enum AwardCellType {\n  MessageSent = 'messageSent',\n  LaterSent = 'laterSent',\n  FollowUp = 'followUp',\n  Scheduling = 'scheduling',\n  MessageTemplates = 'messageTemplates',\n  Blocked = 'blockedEmails',\n  ThreadRead = 'threadRead',\n  ReadLater = 'readLater',\n  SharedConversations = 'sharedConversations',\n}\n\nconst imageForAwardType = (type: AwardCellType): string|null => {\n  switch (type) {\n    case AwardCellType.MessageSent:\n      return 'https://mailer-assets.polymail.io/2019/12-16-end-of-year-report-v2/mailbox.png';\n    case AwardCellType.LaterSent:\n      return 'https://mailer-assets.polymail.io/2019/12-16-end-of-year-report-v2/bird.png';\n    case AwardCellType.FollowUp:\n      return 'https://mailer-assets.polymail.io/2019/12-16-end-of-year-report-v2/basketball-player.png';\n    case AwardCellType.Scheduling:\n      return 'https://mailer-assets.polymail.io/2019/12-16-end-of-year-report-v2/calendar.png';\n    case AwardCellType.MessageTemplates:\n      return 'https://mailer-assets.polymail.io/2019/12-16-end-of-year-report-v2/keyboard.png';\n    case AwardCellType.Blocked:\n      return 'https://mailer-assets.polymail.io/2019/12-16-end-of-year-report-v2/dustpan.png';\n    case AwardCellType.ThreadRead:\n      return 'https://mailer-assets.polymail.io/2019/12-16-end-of-year-report-v2/popularity.png';\n    case AwardCellType.ReadLater:\n      return 'https://mailer-assets.polymail.io/2019/12-16-end-of-year-report-v2/alarm.png';\n    case AwardCellType.SharedConversations:\n      return 'https://mailer-assets.polymail.io/2019/12-16-end-of-year-report-v2/chatting.png';\n    default: return null;\n  }\n};\n\nconst titleForAward = (type: AwardCellType): string|null => {\n  switch (type) {\n    case AwardCellType.MessageSent:\n      return 'Email Guru';\n    case AwardCellType.LaterSent:\n      return 'The Early Bird';\n    case AwardCellType.FollowUp:\n      return 'Never Dropped the Ball';\n    case AwardCellType.Scheduling:\n      return 'Calendar Official';\n    case AwardCellType.MessageTemplates:\n      return 'Types at 500 WPM!';\n    case AwardCellType.Blocked:\n      return 'The Janitor';\n    case AwardCellType.ThreadRead:\n      return 'In Demand';\n    case AwardCellType.ReadLater:\n      return 'You Snooze You Win';\n    case AwardCellType.SharedConversations:\n      return 'Sharing is Caring';\n    default: return null;\n  }\n};\n\nconst descriptionForAward = (type: AwardCellType, value: number): string|null => {\n  switch (type) {\n    case AwardCellType.MessageSent:\n      return `You sent more messages than ${(value * 100).toFixed(0)}% of Polymail users`;\n    case AwardCellType.LaterSent:\n      return `You scheduled more emails than ${(value * 100).toFixed(0)}% of Polymail users`;\n    case AwardCellType.FollowUp:\n      return `You used more follow-ups than ${(value * 100).toFixed(0)}% of Polymail users`;\n    case AwardCellType.Scheduling:\n      return `You scheduled more messages than ${(value * 100).toFixed(0)}% of Polymail users`;\n    case AwardCellType.MessageTemplates:\n      return `You used more email templates than ${(value * 100).toFixed(0)}% of Polymail users`;\n    case AwardCellType.Blocked:\n      return `You blocked more emails than ${(value * 100).toFixed(0)}% of Polymail users`;\n    case AwardCellType.ThreadRead:\n      return `You read more emails than ${(value * 100).toFixed(0)}% of Polymail users`;\n    case AwardCellType.ReadLater:\n      return `You snoozed more emails than ${(value * 100).toFixed(0)}% of Polymail users`;\n    case AwardCellType.SharedConversations:\n      return `You shared more emails than ${(value * 100).toFixed(0)}% of Polymail users`;\n    default: return null;\n  }\n};\n\nconst AwardCell = (props: Props) => {\n  const {\n    awardType,\n    awardValue,\n  } = props;\n  if (!awardType) {\n    return null;\n  }\n\n  const awardImage = imageForAwardType(awardType);\n  if (!awardImage) {\n    return null;\n  }\n\n  return (\n    <div\n      className={classnames('award-cell')}\n    >\n      <div className=\"avatar\">\n        <img src={awardImage} alt=\"\" className=\"award-avatar\"/>\n      </div>\n      <div className=\"sender-details\">\n        <div className=\"name\">\n          {titleForAward(awardType)}\n        </div>\n        <div className=\"description\">\n          {awardValue && (\n            descriptionForAward(awardType, awardValue)\n          )}\n        </div>\n      </div>\n    </div>\n  );\n};\n\nexport default AwardCell;\n"]}]}