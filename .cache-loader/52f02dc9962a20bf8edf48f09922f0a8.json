 '@src/types';\nimport Hubspot, { StateProps, DispatchProps } from './Hubspot';\nimport {\n  integrationList, integrationHubspotLogin, integrationHubspotLogout, selectHubspotIntegration,\n} from '@src/ducks/integrations';\nimport { selectTeam } from '@src/ducks/team';\nimport { withRouter, RouteComponentProps } from 'react-router';\n\nconst mapStateToProps = (state: StoreState, ownProps: RouteComponentProps<any>): StateProps => {\n  const search = new URLSearchParams(ownProps.location.search);\n\n  return {\n    hubspotIntegration: selectHubspotIntegration(state),\n    team: selectTeam(state),\n    refreshIntegrations: !!search.get('integrationRefresh'),\n  };\n};\n\nconst mapDispatchToProps = (dispatch: Dispatch<StoreState>): DispatchProps => ({\n  loginHubspot: (teamId: string) => dispatch(integrationHubspotLogin(teamId)),\n  logoutHubspot: () => dispatch(integrationHubspotLogout(null)),\n  listIntegrations: () => dispatch(integrationList(null)),\n});\n\nexport default withRouter(\n  connect<StateProps, DispatchProps>(mapStateToProps, mapDispatchToProps)(Hubspot),\n);\n"]}]}