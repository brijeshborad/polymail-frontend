wBACF,oBAAC,KAAK,IACJ,IAAI,EAAE,YAAY,EAClB,SAAS,EAAE,SAAS,EACpB,GAAG,EAAE,YAAY,EACjB,KAAK,SACL,CACK;oBACT,oBAAC,KAAK,OAAG;oBACT,oBAAC,cAAc,IAAC,OAAO,EAAE,kBAAkB,CAAC,MAAM,GAAI,CAClD,CACF;YACN,oBAAC,YAAY,IAAC,IAAI,EAAE,IAAI,CAAC,OAAO,GAAI,CAChC,CACP,CAAC;IACJ,CAAC;IACH,uBAAC;AAAD,CAAC,AApND,CAA+B,KAAK,CAAC,aAAa,GAoNjD;AAED,IAAM,SAAS,GAAG,cAAM,OAAA,CAAC,6BAAK,SAAS,EAAC,QAAQ,GAAG,CAAC,EAA5B,CAA4B,CAAC;AAErD,eAAe,gBAAgB,CAAC","sourcesContent":["import React from 'react';\nimport MessageTemplateChooser from './routes/MessageTemplateChooser';\nimport { Switch, Route, RouteComponentProps } from 'react-router';\nimport routes from './routes';\nimport {\n  Route as RouteType,\n  Preferences,\n  Me,\n  Mailbox,\n  LeftSidebarCount,\n  MailCount,\n  Action,\n  CommandControlType,\n  Features,\n  Onboarding,\n  OnboardingType,\n  MailboxType,\n} from '@src/types';\nimport './MessageTemplates.scss';\nimport Resizable from 're-resizable';\nimport NavigationHeader from '../Settings/components/NavigationHeader';\nimport { RightSidebarTab, Page } from '@src/types/rightSidebar';\nimport { getHotkey, HotkeyAction } from '@src/lib/hotkeys';\nimport { commandControlShow, commandControlHide } from '@src/ducks/commandControl';\nimport LeftSidebar from '@src/components/LeftSidebar';\nimport Toast from '@src/components/Base/Toast';\nimport CommandControl from '@src/components/CommandControl';\nimport UpgradePrompt from '@src/components/Base/UpgradePrompt/UpgradePrompt';\nimport classnames from 'classnames';\nimport RightSidebar from '@src/components/RightSidebar';\nimport browserHistory from '@src/lib/browserHistory';\nimport { OnboardingPatchMeta } from '@src/ducks/me';\nimport { getOnboardingActions } from '@src/lib/OnboardingDispatcher';\nimport ContactSupport from '@src/components/ContactSupport';\nimport { AmplitudeEvents, logAmplitudeEvent } from '@src/lib/amplitude-analytics';\n\nconst handleStyles = {\n  width: '8px',\n  height: '8px',\n  borderRadius: '2px',\n  backgroundColor: '#00aaff',\n};\n\nexport interface StateProps {\n  preferences: Preferences;\n  accounts: Account[];\n  me?: Me;\n  selectedMailbox: Mailbox;\n  mailCount: MailCount;\n  leftSidebarCount: LeftSidebarCount;\n  rightSidebarTab: RightSidebarTab;\n  features: Features;\n  onboarding?: Onboarding;\n}\n\nexport interface DispatchProps {\n  dispatchAction: (action: Action<any, any>) => void;\n  collapseRightSidebar: (collapsed: boolean) => void;\n  updateOnboarding: (onboarding: OnboardingPatchMeta) => void;\n}\n\nclass MessageTemplates extends React.PureComponent<\n  StateProps & RouteComponentProps<any> & DispatchProps, {}\n  > {\n\n  timeout: NodeJS.Timeout;\n  ref: HTMLDivElement;\n\n  componentDidMount() {\n    const {\n      collapseRightSidebar,\n      onboarding,\n      updateOnboarding,\n      dispatchAction }\n    = this.props;\n\n    collapseRightSidebar(true);\n\n    if (onboarding && !onboarding.templatesTabGem) {\n      updateOnboarding({\n        skipped: false,\n        type: { templatesTabGem: true },\n      });\n    }\n\n    if (onboarding && !onboarding.steps?.saveAsTemplate) {\n      updateOnboarding({\n        skipped: false,\n        type: {\n          steps: { [OnboardingType.SaveAsTemplate]: true },\n        },\n      });\n    }\n\n    getOnboardingActions().next(OnboardingType.MessageTemplates);\n\n    // Hide Command Control if showing\n    dispatchAction(commandControlHide(undefined));\n\n    if (this.ref && document.activeElement === document.body) {\n      this.ref.focus();\n    }\n\n    logAmplitudeEvent(AmplitudeEvents.ViewMessageTemplates);\n  }\n\n  handleRef = (ref: HTMLDivElement) => {\n    this.ref = ref;\n  }\n\n  handleKeyDown = (e: React.KeyboardEvent<HTMLDivElement>) => {\n    const {\n      collapseRightSidebar,\n      rightSidebarTab,\n      dispatchAction,\n      features,\n    } = this.props;\n    let handled = false;\n\n    // Do not handle keyboard shortcuts when message templates is not available\n    if (!features.messageTemplates) {\n      return;\n    }\n\n    const activeElement = document.activeElement;\n    const ignoreInput = activeElement &&\n      ['INPUT', 'TEXTAREA'].indexOf(activeElement.tagName) !== -1;\n\n    if (!!ignoreInput) {\n      return;\n    }\n\n    const hotkey = getHotkey(e);\n    switch (hotkey && hotkey.name) {\n      // Navigation\n      case HotkeyAction.NavigationCenter:\n        this.props.dispatchAction(commandControlShow(CommandControlType.Navigation));\n        handled = true;\n        break;\n      case HotkeyAction.NavigateInbox:\n      case HotkeyAction.NavigateStarred:\n      case HotkeyAction.NavigateReadLater:\n      case HotkeyAction.NavigateArchive:\n      case HotkeyAction.NavigateTrash:\n      case HotkeyAction.NavigateFollowUp:\n      case HotkeyAction.NavigateSent:\n      case HotkeyAction.NavigateSendLater:\n      case HotkeyAction.NavigateDraft:\n        if (hotkey && hotkey.route) {\n          const newMailbox = { type: MailboxType.Any };\n          browserHistory.push(hotkey.route(newMailbox));\n          handled = true;\n        }\n        break;\n      case HotkeyAction.KeyboardShortcutGuide:\n        if (rightSidebarTab && hotkey) {\n          if (rightSidebarTab === RightSidebarTab.KeyboardShortcut) {\n            dispatchAction(hotkey.action(rightSidebarTab));\n            collapseRightSidebar(true);\n            handled = true;\n          } else {\n            dispatchAction(hotkey.action(rightSidebarTab));\n            collapseRightSidebar(false);\n            handled = true;\n          }\n        }\n        break;\n      case HotkeyAction.ToggleCommandControl:\n        dispatchAction(commandControlShow(CommandControlType.Thread));\n        handled = true;\n        break;\n      default:\n        break;\n    }\n\n    if (handled) {\n      e.preventDefault();\n      e.stopPropagation();\n    }\n  }\n\n  render() {\n    const { me, selectedMailbox, mailCount, leftSidebarCount, features } = this.props;\n    if (me === undefined) {\n      return <div>Loading</div>;\n    }\n\n    const LeftSidebarConnect = LeftSidebar as any;\n\n    return (\n      <div\n        className=\"app-templates\"\n        tabIndex={-1}\n        onKeyDown={this.handleKeyDown}\n        ref={this.handleRef}\n      >\n        <LeftSidebarConnect\n          accounts={[]}\n          lists={[]}\n          preferences={false}\n          selectedTitle=\"templates\"\n          selectedMailbox={selectedMailbox}\n          mailCount={mailCount}\n          leftSidebarCount={leftSidebarCount}\n        />\n        {!features.messageTemplates &&\n          <div className=\"upgrade-container\">\n            <UpgradePrompt\n              title=\"Message Templates\"\n              description={'To create and use customizable message ' +\n                'templates, upgrade to a paid plan.'}\n            />\n          </div>\n        }\n        <div className=\"message-templates\">\n          <NavigationHeader\n            title=\"Message Templates\"\n          />\n          <div\n            className={classnames('main', {\n              'should-upgrade': !features.messageTemplates,\n            })}\n          >\n            <ContactSupport />\n            <Resizable\n              handleStyles={{\n                bottomRight: handleStyles,\n                bottomLeft: handleStyles,\n                topRight: handleStyles,\n                topLeft: handleStyles,\n              }}\n              enable={{\n                top: false,\n                right: true,\n                bottom: false,\n                left: false,\n                topRight: false,\n                bottomRight: false,\n                bottomLeft: false,\n                topLeft: false,\n              }}\n              minWidth={100}\n              defaultSize={{\n                width: 300,\n                height: 'auto',\n              }}\n            >\n              <MessageTemplateChooser />\n            </Resizable>\n            <Switch>\n              {routes.map((route: RouteType) => (\n                <Route\n                  path={route.pathname}\n                  component={route.component}\n                  key={route.pathname}\n                  exact\n                />\n              ))}\n              <Route\n                path={'/templates'}\n                component={FillerDiv}\n                key={'/templates'}\n                exact\n              />\n            </Switch>\n            <Toast />\n            <CommandControl default={CommandControlType.System} />\n          </div>\n        </div>\n        <RightSidebar page={Page.Threads} />\n      </div>\n    );\n  }\n}\n\nconst FillerDiv = () => (<div className=\"filler\" />);\n\nexport default MessageTemplates;\n"]}]}