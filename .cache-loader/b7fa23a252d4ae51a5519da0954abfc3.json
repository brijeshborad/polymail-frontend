,IAAI,EAAE,IAAI,EACV,QAAQ,EAAE,QAAQ,EAClB,QAAQ,EAAE,QAAQ,EAClB,YAAY,EAAE,YAAY,IAAI,EAAE,GAChC,CACE,CACP,CAAC;IACJ,CAAC;IACH,gBAAC;AAAD,CAAC,AA3ED,CAAwB,KAAK,CAAC,aAAa,GA2E1C;AAED,eAAe,SAAS,CAAC","sourcesContent":["import React from 'react';\nimport classnames from 'classnames';\n\nimport './TextInput.scss';\n\ninterface Props {\n  id?: string;\n  name?: string;\n  description: string;\n  placeholder: string;\n  autoComplete?: string;\n  value: string;\n  type: string;\n  autofocus?: boolean;\n  required?: boolean;\n  readOnly?: boolean;\n  onChange?: ((e: React.ChangeEvent<HTMLInputElement>) => void);\n  onFocus?: ((e: React.FocusEvent<HTMLInputElement>) => void);\n}\n\nclass TextInput extends React.PureComponent<Props> {\n  input?: HTMLInputElement;\n  timeout: NodeJS.Timer;\n\n  constructor() {\n    super();\n  }\n\n  componentDidMount() {\n    if (this.props.autofocus) {\n      this.focus();\n    }\n  }\n\n  componentWillUnmount() {\n    if (this.timeout) {\n      clearTimeout(this.timeout);\n    }\n  }\n\n  handleRef = (ref: HTMLInputElement) => {\n    this.input = ref;\n  }\n\n  focus = () => {\n    this.timeout = global.setTimeout(\n        () => {\n          if (this.input) {\n            this.input.focus();\n          }\n        },\n        0,\n    );\n  }\n\n  render() {\n    const {\n      description,\n      placeholder,\n      value,\n      onChange,\n      onFocus,\n      type,\n      id,\n      autoComplete,\n      readOnly,\n      required,\n      name,\n    } = this.props;\n\n    return (\n      <div className=\"text-input\">\n        {description &&\n          <div className=\"description\">\n            {description}\n          </div>\n        }\n        <input\n          dir=\"auto\"\n          ref={this.handleRef}\n          className={classnames('input', { 'read-only': !!readOnly })}\n          placeholder={placeholder}\n          value={value}\n          onChange={onChange}\n          onFocus={onFocus}\n          type={type}\n          id={id}\n          name={name}\n          required={required}\n          readOnly={readOnly}\n          autoComplete={autoComplete || ''}\n        />\n      </div>\n    );\n  }\n}\n\nexport default TextInput;\n"]}]}