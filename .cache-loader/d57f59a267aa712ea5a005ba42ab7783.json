AS,YAAY,WAAW,EAAE;YACpC,SAAS,CAAC,KAAK,EAAE,CAAC;SACnB;IACH,CAAC;IA6BD,gCAAM,GAAN;QACQ,IAAA,eAAuD,EAArD,gBAAK,EAAE,oBAAO,EAAE,8BAAY,EAAE,sBAAuB,CAAC;QAE9D,OAAO,CACL,oBAAC,OAAO,IACN,GAAG,EAAE,IAAI,CAAC,gBAAgB,EAC1B,SAAS,EAAC,0BAA0B,EACpC,MAAM,EAAE,IAAI,CAAC,YAAY,EACzB,MAAM,EAAE,IAAI,CAAC,gBAAgB,EAC7B,OAAO,QACP,UAAU;YAEV,6BAAK,SAAS,EAAC,SAAS;gBACtB,6BAAK,SAAS,EAAC,OAAO;oBACnB,KAAK;oBACN,gCAAQ,OAAO,EAAE,QAAQ;wBACvB,oBAAC,KAAK,IAAC,KAAK,EAAC,MAAM,EAAC,MAAM,EAAC,MAAM,GAAG,CAC7B,CACL;gBACN,8BACE,SAAS,EAAC,MAAM,EAChB,QAAQ,EAAE,IAAI,CAAC,YAAY;oBAE3B,6BAAK,SAAS,EAAC,SAAS,IACrB,OAAO,CACJ;oBACN,6BAAK,SAAS,EAAC,OAAO;wBACpB,+BACE,WAAW,EAAC,UAAU,EACtB,GAAG,EAAE,IAAI,CAAC,cAAc,EACxB,IAAI,EAAC,UAAU,GACf,CACE;oBACN,6BAAK,SAAS,EAAC,gBAAgB;wBAC7B,gCACE,SAAS,EAAC,gBAAgB,EAC1B,OAAO,EAAE,IAAI,CAAC,cAAc,EAC5B,IAAI,EAAC,QAAQ,IAEZ,YAAY,IAAI,UAAU,CACpB,CACL,CACD,CACH,CACE,CACX,CAAC;IACJ,CAAC;IACH,sBAAC;AAAD,CAAC,AAjGD,CAA8B,KAAK,CAAC,aAAa,GAiGhD;AAED,eAAe,eAAe,CAAC","sourcesContent":["import React from 'react';\nimport ReactDOM from 'react-dom';\n\nimport './PasswordConfirm.scss';\nimport Popover from '@src/components/Popover/Popover';\nimport Close from '@src/components/Icons/Close.svg';\n\nexport interface Props {\n  title: string;\n  message: string;\n  continueCopy?: string;\n  onContinue: (password: string) => void;\n  onCancel?: () => void;\n}\n\nclass PasswordConfirm extends React.PureComponent<Props> {\n  popover: Popover;\n  input: HTMLInputElement;\n\n  handlePopoverRef = (ref: Popover) => {\n    this.popover = ref;\n  }\n\n  handleInputRef = (ref: HTMLInputElement) => {\n    this.input = ref;\n  }\n\n  componentDidMount() {\n    const component = ReactDOM.findDOMNode(this);\n    if (this.popover) {\n      this.popover.show();\n    }\n\n    if (component instanceof HTMLElement) {\n      component.focus();\n    }\n  }\n\n  handleInputFocus = () => {\n    if (this.input instanceof HTMLInputElement) {\n      this.input.focus();\n    }\n  }\n\n  handleCancel = () => {\n    const { onCancel } = this.props;\n    if (onCancel) {\n      onCancel();\n    }\n  }\n\n  handleSubmit = (e: React.FormEvent<any>): boolean => {\n    e.preventDefault();\n    e.stopPropagation();\n    this.handleContinue();\n    return false;\n  }\n\n  handleContinue = () => {\n    const { onContinue } = this.props;\n    if (onContinue) {\n      onContinue(this.input.value);\n    }\n  }\n\n  render() {\n    const { title, message, continueCopy, onCancel } = this.props;\n\n    return (\n      <Popover\n        ref={this.handlePopoverRef}\n        className=\"password-confirm-popover\"\n        onHide={this.handleCancel}\n        onShow={this.handleInputFocus}\n        overlay\n        persistent\n      >\n        <div className=\"confirm\">\n          <div className=\"title\">\n            {title}\n            <button onClick={onCancel}>\n              <Close width=\"10px\" height=\"10px\" />\n            </button>\n          </div>\n          <form\n            className=\"body\"\n            onSubmit={this.handleSubmit}\n          >\n            <div className=\"message\">\n              {message}\n            </div>\n            <div className=\"input\">\n              <input\n                placeholder=\"password\"\n                ref={this.handleInputRef}\n                type=\"password\"\n              />\n            </div>\n            <div className=\"action-buttons\">\n              <button\n                className=\"confirm-button\"\n                onClick={this.handleContinue}\n                type=\"submit\"\n              >\n                {continueCopy || 'Continue'}\n              </button>\n            </div>\n          </form>\n        </div>\n      </Popover>\n    );\n  }\n}\n\nexport default PasswordConfirm;\n"]}]}