t from 'react';\nimport classnames from 'classnames';\n\nimport './Overlay.scss';\n\ninterface Props {\n  onSkip?: () => void;\n  className?: string;\n  children?: JSX.Element;\n  hide?: () => void;\n  contactSupport?: boolean;\n}\n\nclass Overlay extends React.PureComponent<Props, {}> {\n\n  constructor() {\n    super();\n  }\n\n  handleClick = (e: React.MouseEvent<HTMLElement>) => {\n    if (!this.props.className?.startsWith('onboarding')) {\n      return;\n    }\n\n    const threadList = document.getElementsByClassName('infinite-thread-list');\n    const element = threadList[0];\n    if (element && element instanceof HTMLElement) {\n      element.focus();\n    }\n    e.stopPropagation();\n    e.preventDefault();\n  }\n\n  handleDraftDelete = (e: React.MouseEvent<HTMLElement>) => {\n    if (!this.props.contactSupport) return;\n    if (this.props.hide) {\n      this.props.hide();\n    }\n    e.stopPropagation();\n    e.preventDefault();\n  }\n\n  render() {\n    const { className, children } = this.props;\n    return (\n      <div\n        className={classnames('overlay', className)}\n        onClick={this.handleClick}\n      >\n        {children}\n      <div className=\"overlay-background\" onClick={this.handleDraftDelete} />\n      </div>\n    );\n  }\n}\n\nexport default Overlay;\n"]}]}