// tslint:disable-next-line:max-line-length\nimport BillingStatusBanner from '@src/routes/Settings/routes/Billing/components/BillingStatusBanner';\nimport {\n  BillingCycle, BillingSubscription, BillingInvoice, BillingCard,\n} from '@src/types/billing';\nimport browserHistory from '@src/lib/browserHistory';\nimport { formatCurrencyCents } from '@src/lib/utils';\n\nimport './PlanDetail.scss';\n\nexport interface Props {\n  subscription: BillingSubscription;\n  nextInvoice?: BillingInvoice;\n  card?: BillingCard;\n  balance: number;\n  isMobile?: boolean;\n  collapsedMobile?: boolean;\n  isLoadingCoupon: boolean;\n\n  collapseLsb?: (collapse: boolean) => void;\n}\n\nclass PlanDetail extends React.PureComponent<Props> {\n  handleCancelSubscription = () => {\n    browserHistory.push('/settings/billing/cancel');\n  }\n\n  handleChangePlan = () => {\n    browserHistory.push('/settings/billing/upgrade');\n  }\n\n  handleUpdateCard = (e: React.MouseEvent<HTMLAnchorElement>) => {\n    e.preventDefault();\n    e.stopPropagation();\n    browserHistory.push('/settings/billing/update');\n  }\n\n  handleSeatsUpdate = () => {\n    const billingCycle = this.props.subscription.billingCycle;\n    const tier = this.props.subscription.tier;\n    browserHistory.push(`/settings/billing/upgrade/${tier}/${billingCycle}`);\n  }\n\n  render() {\n    const {\n      subscription, nextInvoice, balance, card, isMobile, collapsedMobile, collapseLsb,\n    } = this.props;\n    const planExpiring = subscription.planExpires;\n    const monthlyCost = subscription.billingCycle === BillingCycle.Annual ?\n      subscription.cost / 12 : subscription.cost;\n\n    return (\n      <div className=\"settings-panel preferences-team\">\n        <NavigationHeader\n          title=\"Plans & Billing\"\n          isMobile={isMobile}\n          collapsedMobile={collapsedMobile}\n          collapseLsb={collapseLsb} />\n        <div className=\"plan-detail\">\n          <BillingStatusBanner\n            isAdmin\n            subscription={subscription}\n            trialExpires={subscription.trialExpires}\n            card={card}\n          />\n          <BillingPlanHeader\n            tier={subscription.tier}\n            cycle={subscription.billingCycle}\n            monthlyCost={monthlyCost}\n            seats={subscription.seats}\n            promoCode={subscription.coupon}\n            promoCodePercent={subscription.couponPercent}\n            seatsUpdateButton\n            onSeatsUpdate={this.handleSeatsUpdate}\n            isLoadingCoupon={this.props.isLoadingCoupon}\n            isMobile={isMobile || false}\n          />\n          <div className=\"plan-information\">\n            {nextInvoice && !planExpiring && <div className=\"renewal-info\">\n              Your plan will renew on{' '}\n              <strong>{moment(nextInvoice.date).format('MMMM D, YYYY')}</strong>.\n            </div>}\n            {planExpiring && <div className=\"renewal-info\">\n              Your plan ends on{' '}\n              <strong>{moment(subscription.planExpires).format('MMMM D, YYYY')}</strong>.\n            </div>}\n            {balance < 0 && <div className=\"account-credit\">\n              You have <strong>{formatCurrencyCents(-1 * balance)}{' '}\n              in account credit</strong>.\n            </div>}\n            {nextInvoice && card &&\n              <div className=\"next-invoice\">\n                <strong>{formatCurrencyCents(nextInvoice.amount)}</strong>{' '}\n                will be billed to your{' '}\n                <strong>{card.brand} card ending in {card.lastFour}</strong>.{' '}\n                <a className=\"billing-update\" onClick={this.handleUpdateCard}>\n                  Update payment method.\n                </a>\n              </div>\n            }\n            <div className=\"action-buttons\">\n              <button className=\"change-plan\" onClick={this.handleChangePlan}>\n                Change Plan\n              </button>\n              <button className=\"cancel-subscription\" onClick={this.handleCancelSubscription}>\n                Cancel Subscription\n              </button>\n            </div>\n          </div>\n        </div>\n      </div>\n    );\n  }\n}\n\nexport default PlanDetail;\n"]}]}