t from 'react';\nimport RightArrow from '@src/components/Icons/RightArrow.svg';\n\nimport './NavigationHeader.scss';\nimport MenuToggler from '@src/components/Base/MenuToggler';\n\nexport interface Props {\n  title: string;\n  contextButton?: JSX.Element;\n  onBack?: () => void;\n  onSave?: () => void;\n\n  isMobile?: boolean;\n  collapsedMobile?: boolean;\n  collapseLsb?: (collapse: boolean) => void;\n}\n\nconst NavigationHeader = (props: Props) => {\n  const { title, contextButton, onBack, onSave, isMobile, collapsedMobile, collapseLsb } = props;\n\n  const handleOnBack = () => {\n    onBack!();\n  };\n\n  const handleOnSave = () => {\n    onSave!();\n  };\n\n  const handleMenuTogglerTouch = () => {\n\n    if (collapseLsb) {\n      collapseLsb(!collapsedMobile);\n    }\n  };\n  const showContextButton = !onSave && contextButton;\n\n  return (\n    <div className=\"navigation-header-container\">\n      <div className=\"navigation-header\">\n        <div className=\"content\">\n          {\n            onBack &&\n              <button onClick={handleOnBack} className=\"back\">\n                <RightArrow />\n              </button>\n          }\n          <div className=\"title\">\n          {isMobile && collapsedMobile && !showContextButton &&\n            <span>\n              <MenuToggler\n                onClick={handleMenuTogglerTouch}\n              />\n            </span>\n            }\n            {!showContextButton && (\n              <span>{title}</span>\n            )}\n          </div>\n          {showContextButton && (contextButton)}\n          {\n            props.onSave &&\n            <button onClick={handleOnSave} className=\"save\">\n              Save\n            </button>\n          }\n        </div>\n      </div>\n    </div>\n  );\n};\n\nexport default NavigationHeader;\n"]}]}