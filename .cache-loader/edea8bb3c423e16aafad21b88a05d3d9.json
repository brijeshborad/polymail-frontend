IACpC,GAAG,EACN,EAJ4B,CAI5B,CAAC","sourcesContent":["import {\n  Preferences,\n  LeftSidebarCount,\n  DockCount,\n  UnsubscribeBehavior,\n  DisplayMode,\n  MarkReadOn,\n  Swipe,\n  Hotkeys,\n  Me,\n  Partial,\n  MePlan,\n  Onboarding,\n  TwoFactorAuthentication,\n  TwoFactorAuthenticationStatus,\n  TwoFactorAuthenticationType,\n  BlockContactActionType,\n  MeStatus,\n} from '@src/types';\nimport { Token, TokenType } from '@src/api/auth';\n\nexport const createMe = (me?: Partial<Me>): Me => {\n  return {\n    id: 'mock-me',\n    firstName: 'Bowie',\n    lastName: 'Foo',\n    middleName: '',\n    email: 'bowiefoo@polymail.io',\n    photoUrl: 'https://google.com',\n    plan: MePlan.Free,\n    slug: 'test-user-slug',\n    team: 'dogeteam',\n    referralUrl: 'ply.ml/code',\n    intercomIdentity: 'identity',\n    created: new Date('1970-01-01T00:00:00.000Z'),\n    status: MeStatus.Ok,\n    ...me,\n  };\n};\n\nexport const createTfa = (tfa?: Partial<TwoFactorAuthentication>): TwoFactorAuthentication => {\n  return {\n    status: TwoFactorAuthenticationStatus.Verified,\n    type: TwoFactorAuthenticationType.Phone,\n    phone: '+1(234)555-5555',\n\n    ...tfa,\n  };\n};\n\nexport const createPreferences = (preferences?: Partial<Preferences>): Preferences => {\n  return {\n    showSocialList: false,\n    showUpdatesList: false,\n    showPromotionsList: false,\n\n    disableLinkPreviews: false,\n    disableTrackByDefault: false,\n    sendAndArchive: false,\n\n    leftSidebarCount: LeftSidebarCount.All,\n    dockCount: DockCount.All,\n    hotkeys: Hotkeys.Default,\n    unsubscribeBehavior: UnsubscribeBehavior.Archive,\n    blockContactAction: BlockContactActionType.Trash,\n    defaultAccount: '',\n    displayMode: DisplayMode.Compact,\n    markReadOn: MarkReadOn.Archive,\n    blockTrackingPixels: false,\n\n    swipeShortRight: Swipe.Archive,\n    swipeLongRight: Swipe.Inbox,\n    swipeLongLeft: Swipe.Later,\n    swipeShortLeft: Swipe.List,\n\n    laterToday: 10800,\n    laterMorning: 28800,\n    laterEvening: 68400,\n\n    linkTracking: false,\n    cloudAttachments: false,\n    followUp: 0,\n\n    composeFontSize: 0,\n    composeFontColor: '\\\\',\n    composeFontFamily: '\\\\',\n\n    syncEmailsToCrm: false,\n    sentFromPolymail: false,\n    cancelOnReply: false,\n\n    ...preferences,\n  };\n};\n\nexport const createOnboarding = (shouldHide: boolean = false): Onboarding => ({\n  exploreModal: !shouldHide,\n  addSequenceRecipients: !shouldHide,\n  editSequenceStage: !shouldHide,\n  createAdditionalStage: !shouldHide,\n  activateSequence: !shouldHide,\n  sendMessage: !shouldHide,\n  createDraft: !shouldHide,\n  sequences: !shouldHide,\n  scheduling: !shouldHide,\n  downloadNativeApps: !shouldHide,\n  commandCenter: !shouldHide,\n  threadDetailTracking: !shouldHide,\n  inviteTeamMembers: !shouldHide,\n  createComment: !shouldHide,\n  mentionsTab: !shouldHide,\n  meetingTimesTabGem: !shouldHide,\n  sharedThreadsTabGem: !shouldHide,\n  templatesTabGem: !shouldHide,\n  unsubscriberGem: !shouldHide,\n  contactSupportPopup: !shouldHide,\n\n  // RSB Draft\n  sendLater: !shouldHide,\n  followUpReminder: !shouldHide,\n  saveAsTemplate: !shouldHide,\n  messageTemplates: !shouldHide,\n\n  // Thread Extra\n  shareThread: !shouldHide,\n  comment: !shouldHide,\n  activityFeed: !shouldHide,\n  claimedCredit: !shouldHide,\n  creditClaimable: !shouldHide,\n\n  steps: {\n    nextThread: !shouldHide,\n    previousThread: !shouldHide,\n    archive: !shouldHide,\n    undo: !shouldHide,\n    readLater: !shouldHide,\n    block: !shouldHide,\n    commandLater: !shouldHide,\n\n    sendLater: !shouldHide,\n    followUpReminder: !shouldHide,\n    saveAsTemplate: !shouldHide,\n    messageTemplates: !shouldHide,\n\n    shareThread: !shouldHide,\n    comment: !shouldHide,\n    scheduling: !shouldHide,\n  },\n});\n\nexport const createToken = (token: Partial<Token>): Token => ({\n  token: 'token',\n  type: TokenType.Limited,\n  ...token,\n});\n\nexport const createTwoFactorAuthentication = (\n  tfa: Partial<TwoFactorAuthentication>,\n): TwoFactorAuthentication => ({\n  status: TwoFactorAuthenticationStatus.Verified,\n  type: TwoFactorAuthenticationType.Phone,\n  ...tfa,\n});\n"]}]}