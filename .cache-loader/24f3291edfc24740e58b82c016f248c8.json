jB,CACP;AATmE,CASnE,CAAC;AAEF,eAAe,kBAAkB,CAAC","sourcesContent":["import React from 'react';\n\nimport { getFieldValue } from './TasksTable';\n\nimport { SObject } from '@src/types/salesforce';\n\nimport './OpportunitiesTable.scss';\n\nfunction cells(opportunities: SObject[]) {\n  return opportunities.map(item => (\n    <div key={item.href} className=\"salesforce-opportunities-table-cell\">\n      <div className=\"name\">\n        <a href={item.url} target=\"_blank\" rel=\"noopener noreferrer\">\n          {getFieldValue('Name', item.fields)}\n        </a>\n      </div>\n      <div className=\"amount\">\n        {getFieldValue('Amount', item.fields)}\n      </div>\n      <div className=\"field\">\n        <span>Stage: </span>{getFieldValue('StageName', item.fields)}\n      </div>\n      <div className=\"field\">\n        <span>Close Date: </span>{getFieldValue('CloseDate', item.fields)}\n      </div>\n    </div>\n  ));\n}\n\ninterface Props {\n  opportunities: SObject[];\n}\n\nconst OpportunitiesTable: React.SFC<Props> = ({ opportunities }) => (\n  <div className=\"salesforce-opportunities-table\">\n    {opportunities.length === 0 ? (\n      <div className=\"empty\">\n        No Opportunities\n      </div>\n    ) : null}\n    {cells(opportunities)}\n  </div>\n);\n\nexport default OpportunitiesTable;\n"]}]}