AAC;AAEF,eAAe,SAAS,CAAC","sourcesContent":["import React from 'react';\nimport classnames from 'classnames';\n\nimport Alert from '@src/components/Icons/Alert.svg';\n\nimport './AliasItem.scss';\n\ninterface Props {\n  name: string;\n  email: string;\n  id: string;\n  onClick: (id: string) => void;\n  onReauthenticate?: (id: string) => void;\n  reauthenticate?: boolean;\n}\n\nconst AliasItem = (props: Props) => {\n  const onClick = (e: React.MouseEvent<HTMLDivElement>) => {\n    e.preventDefault();\n    e.stopPropagation();\n    props.onClick(props.id);\n  };\n\n  const onReauthenticate = (e: React.MouseEvent<HTMLButtonElement>) => {\n    e.preventDefault();\n    e.stopPropagation();\n\n    if (props.onReauthenticate) {\n      props.onReauthenticate(props.id);\n    }\n  };\n\n  return (\n    <div\n      className={classnames('alias-item', {\n        reauthenticate: props.reauthenticate,\n      })}\n      onClick={onClick}\n    >\n      {props.reauthenticate && <div className=\"alert\">\n        <button onClick={onReauthenticate}>\n          <Alert width=\"20px\" height=\"20px\" />\n        </button>\n      </div>}\n      <div className=\"main\">\n        <div className=\"name\">\n          {props.name}\n        </div>\n        <div className=\"email\">\n          {props.email}\n        </div>\n      </div>\n      {props.reauthenticate && <div className=\"reauthenticate\">\n        <button onClick={onReauthenticate}>Reauthenticate</button>\n      </div>}\n    </div>\n  );\n};\n\nexport default AliasItem;\n"]}]}