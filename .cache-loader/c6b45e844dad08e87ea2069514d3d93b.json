C,GAAG,GAAG;qBACzC;oBACA,OAAO,CAAC,KAAK,KAAK,WAAW,CAAC,aAAa,IAAI,OAAO,CAAC,KAAK,CACtD,CAAC;YAEZ,CAAC,CAAC,CACM,CACX,CAAC;IACJ,CAAC;IACH,mBAAC;AAAD,CAAC,AAnGD,CAA2B,KAAK,CAAC,aAAa,GAmG7C;AAED,gFAAgF;AAChF,2BAA2B;AAC3B,kDAAkD;AAClD,kCAAkC;AAClC,SAAS;AACT,uCAAuC;AACvC,YAAY;AACZ,yCAAyC;AACzC,iCAAiC;AACjC,uBAAuB;AACvB,0BAA0B;AAC1B,MAAM;AACN,sBAAsB;AACtB,eAAe;AACf,KAAK;AACL,cAAc;AACd,YAAY;AACZ,yCAAyC;AACzC,+BAA+B;AAC/B,uBAAuB;AACvB,wBAAwB;AACxB,MAAM;AACN,kBAAkB;AAClB,cAAc;AACd,KAAK;AAEL,eAAe,YAAY,CAAC","sourcesContent":["import React from 'react';\nimport classnames from 'classnames';\nimport { Link } from 'react-router-dom';\n\nimport Popover from '@src/components/Popover/Popover';\n\nimport './SettingsMenu.scss';\nimport KeyboardSymbol from '@src/components/Base/KeyboardSymbol/KeyboardSymbol';\nimport { AmplitudeEvents, logAmplitudeEvent } from '@src/lib/amplitude-analytics';\n\ninterface Props {\n  onSelect: (key: string) => void;\n  me?: string;\n  team?: string;\n  settings?: Setting[];\n  selectedKey?: string;\n  onHide?: () => void;\n  showOnMount?: boolean;\n}\n\nexport interface Setting {\n  title: string;\n  key?: string;\n  link?: string;\n  newWindow?: boolean;\n  icon?: JSX.Element;\n}\n\nexport const enum SettingName {\n  CommandCenter = 'Command Center',\n  Settings = 'Settings',\n  KeyboardShortcuts = 'Keyboard Shortcuts',\n  HelpCenter= 'Help Center',\n  EmailUs= 'Email us',\n  SignOut = 'Sign Out',\n  KeyboardShorctus = 'Keyboard Shortcuts',\n}\n\n// TODO(SHIN): Re-enable when team settings is available\n// const teamSettings: Setting[] = [\n//   {\n//     title: 'Add team members',\n//   },\n//   {\n//     title: 'Manage Team',\n//   },\n// ];\n\n// TODO(SHIN): Beta - re-enable when options are available\nconst userSettings: Setting[] = [\n  {\n    title: SettingName.CommandCenter,\n  },\n  {\n    title: SettingName.KeyboardShortcuts,\n  },\n  {\n    title: SettingName.Settings,\n    link: '/settings',\n  },\n  {\n    title: SettingName.SignOut,\n  },\n];\n\nclass SettingsMenu extends React.PureComponent<Props> {\n  private popover: Popover;\n\n  componentDidMount() {\n    if (this.props.showOnMount) {\n      this.popover.show();\n    }\n  }\n\n  toggle = () => {\n    this.popover.toggle();\n  }\n\n  handleSettingSelect = (e: React.SyntheticEvent<HTMLButtonElement>) => {\n    logAmplitudeEvent(AmplitudeEvents.ViewCommandCenter);\n    const { onSelect } = this.props;\n    const title = e.currentTarget.dataset && e.currentTarget.dataset.title;\n    const key = e.currentTarget.dataset && e.currentTarget.dataset.key;\n\n    if (onSelect && title) {\n      onSelect(key || title);\n    }\n  }\n\n  handleNewWindow = (e: React.SyntheticEvent<HTMLButtonElement>) => {\n    const link = e.currentTarget.dataset && e.currentTarget.dataset.link;\n    window.open(link, '_blank');\n  }\n\n  handleRef = (ref: Popover) => {\n    this.popover = ref;\n  }\n\n  render() {\n\n    // TODO(SHIN): Beta - re-enable when team settings are introduced\n    const {\n      me,\n      settings,\n      // team,\n      selectedKey,\n      onHide,\n     } = this.props;\n\n    return (\n      <Popover\n        ref={this.handleRef}\n        className=\"settings-menu\"\n        onHide={onHide}\n      >\n        <div className=\"header\">\n          <div className=\"title\">{me || 'YOU'}</div>\n          <div className=\"separator\" />\n        </div>\n        {(settings || userSettings).map((setting) => {\n          if (setting.newWindow) {\n            return <button\n              onClick={this.handleNewWindow}\n              data-link={setting.link}\n              className=\"cell\"\n              key={setting.title}\n            >\n              {setting.title}\n            </button>;\n          }\n          if (setting.link) {\n            return <Link\n              to={setting.link}\n              key={setting.title}\n              className=\"link\"\n            >\n              <div className=\"cell\">\n                {setting.title}\n              </div>\n            </Link>;\n          }\n\n          return <button\n            onClick={this.handleSettingSelect}\n            data-title={setting.title}\n            data-key={setting.key}\n            className={classnames('cell', {\n              selected: selectedKey && selectedKey === setting.key || false,\n            })}\n            key={setting.title}\n          >\n            {setting.icon}\n            {setting.title === SettingName.CommandCenter && [\n              <span key=\"title\" className=\"title\">{setting.title}</span>,\n              <KeyboardSymbol character=\"⌘\" key=\"⌘\" />,\n              <KeyboardSymbol character=\"J\" key=\"j\" />,\n            ]}\n            {setting.title !== SettingName.CommandCenter && setting.title}\n          </button>;\n\n        })}\n      </Popover>\n    );\n  }\n}\n\n// TODO(SHIN): Beta - place in end of Popover when team functionality is enabled\n// <div className=\"header\">\n//   <div className=\"title\">{team || 'Team'}</div>\n//   <div className=\"separator\" />\n// </div>\n// {team && teamSettings.map(setting =>\n//   <button\n//     onClick={this.handleSettingSelect}\n//     data-title={setting.title}\n//     className=\"cell\"\n//     key={setting.title}\n//   >\n//     {setting.title}\n//   </button>,\n// )}\n// {!team && (\n//   <button\n//     onClick={this.handleSettingSelect}\n//     data-title=\"Create team\"\n//     className=\"cell\"\n//     key=\"Create team\"\n//   >\n//     Create team\n//   </button>\n// )}\n\nexport default SettingsMenu;\n"]}]}