React from 'react';\n\nimport logo from '@src/routes/Login/Icons/PolymailLogo@3x.png';\nimport RightArrow from '@src/components/Icons/RightArrow.svg';\n\nimport './SignUpHeader.scss';\nimport { Me } from '@src/types';\nimport browserHistory from '@src/lib/browserHistory';\nimport ViewingPill from '@src/components/ThreadDetail/components/ViewingPill';\n\ninterface Props {\n  me?: Me;\n  onButtonPress: () => void;\n}\n\nclass SignUpHeader extends React.PureComponent<Props> {\n  handleAppRedirect = () => {\n    browserHistory.push('/');\n  }\n\n  render() {\n    const { onButtonPress, me } = this.props;\n    const loggedIn = !!me;\n    return (\n      <div className=\"sign-up-header\">\n        <div className=\"logo\">\n          {loggedIn &&\n            <button onClick={this.handleAppRedirect} className=\"back-button\">\n              <RightArrow className=\"back-icon\" />\n              Go to Polymail\n            </button>\n          }\n          {!loggedIn &&\n            <a href=\"https://polymail.io\"><img src={logo} className=\"logo-image\" /></a>\n          }\n        </div>\n        {loggedIn && <ViewingPill />}\n        {!me &&\n          <div className=\"action-buttons\">\n            <button className=\"sign-in\" onClick={onButtonPress}>\n              Sign In\n          </button>\n            {/* <button className=\"sign-up\" onClick={onButtonPress}>\n            Sign Up\n          </button> */}\n          </div>\n        }\n      </div>\n    );\n  }\n}\n\nexport default SignUpHeader;\n"]}]}