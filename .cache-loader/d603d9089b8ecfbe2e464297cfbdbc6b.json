AAC,UAAC,OAAO,EAAE,KAAK,IAAK,OAAA,CAC/B,gCACE,SAAS,EAAE,UAAU,CACnB,+BAA+B,EAC/B,EAAE,QAAQ,EAAE,aAAa,KAAK,KAAK,EAAE,CACtC,EACD,GAAG,EAAE,OAAO,CAAC,EAAE,gBACH,KAAK,EACjB,OAAO,EAAE,WAAW,EACpB,YAAY,EAAE,gBAAgB;YAE7B,YAAY,IAAI,8BAAM,SAAS,EAAC,UAAU,YAAa;YACvD,OAAO,CAAC,QAAQ,IAAI,6BAAK,GAAG,EAAE,OAAO,CAAC,QAAQ,GAAI;YACnD,6BAAK,SAAS,EAAC,MAAM;gBACnB,8BAAM,SAAS,EAAC,SAAS,IAAE,OAAO,CAAC,QAAQ,CAAQ;gBACnD,8BAAM,SAAS,EAAC,aAAa,IAAE,OAAO,CAAC,KAAK,CAAQ,CAChD,CACC,CACV,EAlBgC,CAkBhC,CAAC,CACE,CACF,CACP,CAAC;AACJ,CAAC,CAAC;AAEF,eAAe,cAAc,CAAC","sourcesContent":["import React from 'react';\nimport classnames from 'classnames';\nimport { Contact } from '@src/types';\nimport './ContactsSearch.scss';\n\nexport interface Props {\n  showModifier: boolean;\n  onSuggestionMouseEnter: (component: string, index: number) => void;\n  onSelect: (index: number) => void;\n  options: Contact[];\n  selectedIndex?: number;\n}\n\nconst ContactsSearch: React.SFC<Props> = ({\n  showModifier, onSelect, options, selectedIndex, onSuggestionMouseEnter,\n}) => {\n  const handleClick = (e: React.SyntheticEvent<HTMLButtonElement>) => {\n    const index = e.currentTarget.dataset.index;\n\n    if (index) {\n      onSelect(parseInt(index, 10));\n    }\n  };\n  const handleMouseEnter = (e: React.SyntheticEvent<HTMLButtonElement>) => {\n    const index = e.currentTarget.dataset.index;\n\n    if (index) {\n      onSuggestionMouseEnter('contact', parseInt(index, 10));\n    }\n  };\n\n  if (!options.length) {\n    return <div />;\n  }\n\n  return (\n    <div className=\"contacts-search suggestion-category\">\n      <div className=\"heading\">\n        <span>Contacts</span>\n        <span className=\"separator\" />\n      </div>\n      <div className=\"suggestions\">\n        {options.map((contact, index) => (\n          <button\n            className={classnames(\n              'contact-suggestion suggestion',\n              { selected: selectedIndex === index },\n            )}\n            key={contact.id}\n            data-index={index}\n            onClick={handleClick}\n            onMouseEnter={handleMouseEnter}\n          >\n            {showModifier && <span className=\"modifier\">with:</span>}\n            {contact.photoUrl && <img src={contact.photoUrl} />}\n            <div className=\"info\">\n              <span className=\"heading\">{contact.fullName}</span>\n              <span className=\"description\">{contact.email}</span>\n            </div>\n          </button>\n        ))}\n      </div>\n    </div>\n  );\n};\n\nexport default ContactsSearch;\n"]}]}