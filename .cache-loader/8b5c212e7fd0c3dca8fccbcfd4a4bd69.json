.scss';\n\ninterface SchedulingColumnScrollProps {\n  moveForward: boolean;\n  disabled?: boolean;\n  onClick?: (moveForward: boolean) => void;\n  height: number;\n}\n\nconst SchedulingColumnScroll = (props: SchedulingColumnScrollProps) => {\n  const handleClick = () => {\n    if (props.onClick) {\n      props.onClick(props.moveForward);\n    }\n  };\n\n  const columnHeight = props.height;\n  return (\n    <div\n      key={`${props.moveForward}`}\n      className=\"scheduling-column scroll\"\n      style={{\n        height: `${columnHeight}px`,\n      }}\n    >\n      <div className=\"header\">\n        <button\n          onClick={handleClick}\n          className={\n            classnames('arrow', { flipped: !props.moveForward, disabled: props.disabled },\n          )}\n          disabled={props.disabled}\n        >\n          {props.moveForward\n            ? <RightArrow />\n            : <RightArrow />\n          }\n        </button>\n      </div>\n      <div className=\"rows\" />\n    </div>\n  );\n};\n\nexport default SchedulingColumnScroll;\n"]}]}