./FollowUp';\nimport { draftUpdate, selectDraft, selectDraftForThread } from '@src/ducks/drafts';\nimport { selectRsbOnboarding, onboardingPatch } from '@src/ducks/me';\n\nconst mapStateToProps = (state: StoreState, ownProps: Props): any => {\n  return {\n    preferences: state.preferences,\n    draft: state.selectedDraft\n      ? selectDraft(state, state.selectedDraft).data\n      : state.selectedThread && selectDraftForThread(state, state.selectedThread.id).data,\n    rsbOnboarding: selectRsbOnboarding(state),\n  };\n};\n\nconst mapDispatchToProps = (dispatch: Dispatch<StoreState>): DispatchProps => ({\n  updateDraft: (draft: Draft) => dispatch(draftUpdate(draft)),\n  updateOnboarding: meta => dispatch(onboardingPatch(meta)),\n});\n\nexport default connect(mapStateToProps, mapDispatchToProps)(FollowUp);\n"]}]}