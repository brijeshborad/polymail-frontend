port Archive from '@src/components/Icons/Archive.svg';\nimport ShareIcon from './Icons/ShareIcon.svg';\nimport './SharedThreadsOnboarding.scss';\n\ninterface Props {\n  id: number;\n}\n\nconst description = 'Share conversations from your inbox to other '\n  + 'members on your team with one click';\nconst features = [\n  'Create secure links to allow access',\n  'Comment on conversations to add context',\n  'Tie conversations to projects and tasks',\n  'Manage permission for shared conversations',\n];\n\nconst SharedThreadsOnboarding = (props: Props) => (\n  <div className=\"shared-threads-onboarding\">\n      <div className=\"main\">\n        <div className=\"icon\">\n          <ShareIcon />\n        </div>\n        <div className=\"title\">\n          Share Conversations with Polymail\n        </div>\n        <div className=\"description\">\n          {description}\n        </div>\n        <div className=\"features\">\n          {features.map(feature =>\n            (\n              <div\n                key={feature}\n                className=\"feature\"\n              >\n                <div className=\"icon\">\n                  <Archive />\n                </div>\n                <div className=\"copy\">\n                  {feature}\n                </div>\n              </div>\n            ))}\n        </div>\n        <div className=\"action-button\">\n          <a\n            className=\"enable-button\"\n            href=\"https://help.polymail.io/articles/2156127-shared-conversations\"\n            target=\"_blank\"\n          >\n            Learn More\n          </a>\n        </div>\n      </div>\n  </div>\n);\n\nexport default SharedThreadsOnboarding;\n"]}]}