C7C,CAAC;aACF,CAAC;YAEF,IAAI,CAAC,QAAQ,CAAC,EAAE,YAAY,cAAA,EAAE,CAAC,CAAC;SACjC;IACH,CAAC;IAgFD,yCAAM,GAAN;QACQ,IAAA,eAA2D,EAAzD,kCAAc,EAAE,sBAAQ,EAAE,sCAA+B,CAAC;QAClE,IAAM,QAAQ,GAAG,MAAM,CAAC,EAAE,CAAC,KAAK,EAAE,CAAC;QACnC,IAAM,YAAY,GAAG,QAAQ,IAAI,MAAM,CAAC,EAAE,CAAC,QAAQ,CAAC,CAAC,MAAM,CAAC,GAAG,CAAC,CAAC;QACjE,IAAM,aAAa,GAAG,MAAM,CAAC,QAAQ,CAAC,SAAS,CAAC,CAAC,MAAM,CAAC,MAAM,CAAC,CAAC;QAChE,IAAM,WAAW,GAAG,MAAM,CAAC,QAAQ,CAAC,OAAO,CAAC,CAAC,MAAM,CAAC,QAAQ,CAAC,CAAC;QAExD,IAAA,eAA4C,EAA1C,sBAAQ,EAAE,wCAAgC,CAAC;QACnD,OAAO,CACL,6BAAK,SAAS,EAAC,4BAA4B;YACxC,IAAI,CAAC,KAAK,CAAC,YAAY;gBACtB,oBAAC,OAAO,eACF,IAAI,CAAC,KAAK,CAAC,YAAY,EAC3B;YAEJ,6BAAK,SAAS,EAAC,QAAQ;gBACrB,6BAAK,SAAS,EAAC,OAAO;;oBACE,gBAAgB,CAAC,aAAa,CAChD,CACF;YACN,6BAAK,SAAS,EAAC,MAAM;gBACnB,gCACE,SAAS,EAAC,aAAa,EACvB,OAAO,EAAE,IAAI,CAAC,KAAK,CAAC,QAAQ;oBAE5B,oBAAC,UAAU,OAAG,CACP;gBACT,6BAAK,SAAS,EAAC,OAAO,IACnB,gBAAgB,CAAC,SAAS,CACvB;gBACN,6BAAK,SAAS,EAAC,YAAY;oBACxB,aAAa;;oBAAK,WAAW,CAC1B;gBACN,6BAAK,SAAS,EAAC,MAAM,IACnB,MAAM,CAAC,QAAQ,CAAC,SAAS,CAAC,CAAC,MAAM,CAAC,wBAAwB,CAAC,CACvD;gBACN,6BAAK,SAAS,EAAC,WAAW;oBACvB,YAAY;;oBAAK,QAAQ,IAAI,QAAQ,CAAC,OAAO,CAAC,GAAG,EAAE,GAAG,CAAC,CACpD;gBACH,CAAC,CAAC,gBAAgB,CAAC,QAAQ;oBAC1B,6BAAK,SAAS,EAAC,UAAU;wBACvB,6BAAK,SAAS,EAAC,MAAM;4BACnB,oBAAC,QAAQ,OAAG,CACR;wBACN,6BAAK,SAAS,EAAC,MAAM,IAClB,gBAAgB,CAAC,QAAQ,CACtB,CACF;gBAEV,8BACE,QAAQ,EAAE,IAAI,CAAC,YAAY;oBAE3B,oBAAC,SAAS,IACR,IAAI,EAAC,MAAM,EACX,WAAW,EAAC,WAAW,EACvB,WAAW,EAAC,iBAAiB,EAC7B,KAAK,EAAE,QAAQ,CAAC,IAAI,EACpB,QAAQ,EAAE,IAAI,CAAC,gBAAgB,EAC/B,IAAI,EAAC,OAAO,EACZ,SAAS,SACT;oBACF,oBAAC,SAAS,IACR,IAAI,EAAC,OAAO,EACZ,WAAW,EAAC,oBAAoB,EAChC,WAAW,EAAC,iBAAiB,EAC7B,KAAK,EAAE,QAAQ,CAAC,KAAK,EACrB,QAAQ,EAAE,IAAI,CAAC,iBAAiB,EAChC,IAAI,EAAC,OAAO,GACZ;oBACF,oBAAC,SAAS,IACR,IAAI,EAAC,UAAU,EACf,IAAI,EAAC,MAAM,EACX,WAAW,EAAC,oCAAoC,EAChD,WAAW,EAAC,oCAAoC,EAChD,KAAK,EAAE,iBAAiB,IAAI,EAAE,EAC9B,QAAQ,EAAE,IAAI,CAAC,uBAAuB,GACtC;oBACF,gCACE,SAAS,EAAE,UAAU,CAAC,eAAe,EAAE,EAAE,OAAO,EAAE,CAAC,CAAC,cAAc,EAAE,CAAC,EACrE,QAAQ,EAAE,CAAC,CAAC,cAAc,IAEzB,CAAC,CAAC,cAAc;wBACf,CAAC,CAAC,oBAAC,MAAM,OAAE;wBACX,CAAC,CAAC,iBAAiB,CAEd,CACJ,CACH,CACF,CACP,CAAC;IACJ,CAAC;IACH,+BAAC;AAAD,CAAC,AAzMD,CAAuC,KAAK,CAAC,aAAa,GAyMzD;AAED,eAAe,wBAAwB,CAAC","sourcesContent":["import React from 'react';\nimport moment from 'moment-timezone';\nimport classnames from 'classnames';\n\nimport Location from '@src/components/Icons/Location.svg';\nimport { Attendee, AttendeeStatus } from '@src/types';\nimport { SchedulingPublic, TimeSlot } from '@src/types/scheduling';\nimport TextInput from '@src/components/Form/TextInput';\nimport RightArrow from '@src/components/Icons/RightArrow.svg';\n\nimport './CalendarSchedulingDetail.scss';\nimport { CalendarEventCreateMeta } from '@src/ducks/scheduling';\nimport Confirm, { Props as ConfirmProps } from '@src/components/Base/Confirm';\nimport Loader from '@src/components/Loader/Loader';\nimport { parseAddress } from '@src/lib/address';\nimport { uniqBy } from 'lodash';\nimport { parseAttendee } from '@src/lib/scheduling';\nimport { isEmail } from '@src/lib/search';\nimport { AmplitudeEvents, logAmplitudeEvent } from '@src/lib/amplitude-analytics';\n\nexport interface Props {\n  userSlug: string;\n  slug: string;\n  schedulingPublic: SchedulingPublic;\n  timeSlot: TimeSlot;\n  creationError?: Error;\n\n  pendingRequest?: boolean;\n  onCancel?: () => void;\n}\n\nexport interface DispatchProps {\n  createEvent: (meta: CalendarEventCreateMeta) => void;\n}\n\nexport interface State {\n  attendee: Attendee;\n  otherParticipants?: string;\n  confirmProps?: ConfirmProps;\n}\n\nclass CalendarSchedulingDetail extends React.PureComponent<Props & DispatchProps, State> {\n  constructor() {\n    super();\n\n    this.state = {\n      attendee: {\n        name: '',\n        email: '',\n        status: AttendeeStatus.NeedsAction,\n        optional: false,\n        organizer: false,\n      },\n    };\n  }\n\n  componentDidUpdate(oldProps: Props) {\n    // Handle showing error messages\n    if (!oldProps.creationError && this.props.creationError) {\n      const confirmProps: ConfirmProps = {\n        title: 'We ran into an issue scheduling your meeting',\n        message: 'Please try a different time slot or try again later.'\n          + `\\n\\nError: ${this.props.creationError.message}`,\n        confirmCopy: 'Ok',\n        onConfirm: () => {\n          this.setState({ confirmProps: undefined });\n        },\n      };\n\n      this.setState({ confirmProps });\n    }\n  }\n\n  handleNameChange = (e: React.ChangeEvent<HTMLInputElement>) => {\n    this.setState({\n      attendee: {\n        ...this.state.attendee,\n        name: e.currentTarget.value,\n      },\n    });\n  }\n\n  handleEmailChange = (e: React.ChangeEvent<HTMLInputElement>) => {\n    this.setState({\n      attendee: {\n        ...this.state.attendee,\n        email: e.currentTarget.value,\n      },\n    });\n  }\n\n  handleOtherParticipants = (e: React.ChangeEvent<HTMLInputElement>) => {\n    this.setState({\n      otherParticipants: e.currentTarget.value,\n    });\n  }\n\n  handleCreate = (e: React.FormEvent<HTMLFormElement>) => {\n    logAmplitudeEvent(AmplitudeEvents.BookCalendarMeeeting);\n\n    const { userSlug, slug, createEvent } = this.props;\n    const { otherParticipants } = this.state;\n    const timeslot = this.props.timeSlot;\n\n    // Custom handling of form event\n    e.stopPropagation();\n    e.preventDefault();\n\n    // Add All Attendees\n    let attendees = [this.state.attendee];\n\n    // Parse address and make sure attendees are unique by email\n    if (otherParticipants && otherParticipants.length) {\n      const transformed = otherParticipants\n        .split(',')\n        .map(parseAddress)\n        .map(parseAttendee);\n\n      // Validator for email check\n      const validated = transformed.filter(p => isEmail(p.email));\n      if (validated.length !== transformed.length) {\n        const confirmProps: ConfirmProps = {\n          title: 'Invalid Email in Invited Participants',\n          message: 'Please make sure the email addresses you\\'ve entered are valid and try again.',\n          confirmCopy: 'Ok',\n          onConfirm: () => {\n            this.setState({ confirmProps: undefined });\n          },\n        };\n\n        this.setState({ confirmProps });\n        return;\n      }\n\n      // Concat attendees\n      attendees = attendees\n        .concat(transformed);\n\n      attendees = uniqBy(attendees, 'email');\n    }\n\n    // Create event\n    createEvent({\n      slug,\n      userSlug,\n      start: timeslot.startTime,\n      end: timeslot.endTime,\n      attendees,\n    });\n  }\n\n  render() {\n    const { pendingRequest, timeSlot, schedulingPublic } = this.props;\n    const timeZone = moment.tz.guess();\n    const abbreviation = timeZone && moment.tz(timeZone).format('z');\n    const startTimeCopy = moment(timeSlot.startTime).format('h:mm');\n    const endTimeCopy = moment(timeSlot.endTime).format('h:mm a');\n\n    const { attendee, otherParticipants } = this.state;\n    return (\n      <div className=\"calendar-scheduling-detail\">\n        {this.state.confirmProps &&\n          <Confirm\n            {...this.state.confirmProps}\n          />\n        }\n        <div className=\"header\">\n          <div className=\"title\">\n            Confirm meeting with {schedulingPublic.organizerName}\n          </div>\n        </div>\n        <div className=\"main\">\n          <button\n            className=\"back-button\"\n            onClick={this.props.onCancel}\n          >\n            <RightArrow />\n          </button>\n          <div className=\"title\">\n            {schedulingPublic.eventName}\n          </div>\n          <div className=\"event-time\">\n            {startTimeCopy} - {endTimeCopy}\n          </div>\n          <div className=\"date\">\n           {moment(timeSlot.startTime).format('dddd[,] MMMM D[,] YYYY')}\n          </div>\n          <div className=\"time-zone\">\n            {abbreviation} - {timeZone && timeZone.replace('_', ' ')}\n          </div>\n            {!!schedulingPublic.location &&\n              <div className=\"location\">\n                <div className=\"icon\">\n                  <Location />\n                </div>\n                <div className=\"text\">\n                  {schedulingPublic.location}\n                </div>\n              </div>\n            }\n          <form\n            onSubmit={this.handleCreate}\n          >\n            <TextInput\n              name=\"name\"\n              description=\"Your Name\"\n              placeholder=\"enter your name\"\n              value={attendee.name}\n              onChange={this.handleNameChange}\n              type=\"input\"\n              autofocus\n            />\n            <TextInput\n              name=\"email\"\n              description=\"Your email address\"\n              placeholder=\"name@domain.com\"\n              value={attendee.email}\n              onChange={this.handleEmailChange}\n              type=\"email\"\n            />\n            <TextInput\n              name=\"invitees\"\n              type=\"text\"\n              description=\"Invite others to join this meeting\"\n              placeholder=\"name1@domain.com, name2@domain.com\"\n              value={otherParticipants || ''}\n              onChange={this.handleOtherParticipants}\n            />\n            <button\n              className={classnames('submit-button', { pending: !!pendingRequest })}\n              disabled={!!pendingRequest}\n            >\n              {!!pendingRequest\n                ? <Loader/>\n                : 'Confirm Meeting'\n              }\n            </button>\n          </form>\n        </div>\n      </div>\n    );\n  }\n}\n\nexport default CalendarSchedulingDetail;\n"]}]}