zCD,CAA4B,KAAK,CAAC,aAAa,GAyC9C;AAED,eAAe,aAAa,CAAC","sourcesContent":["import React from 'react';\nimport classnames from 'classnames';\nimport Popover from '@src/components/Popover/Popover';\n\nimport './FilterPopover.scss';\n\ninterface Props {\n  filters: string[];\n  onFilterSelect: (key: string) => void;\n  selectedFilter: string;\n}\n\nclass FilterPopover extends React.PureComponent<Props> {\n  private popover: Popover;\n\n  toggle = () => {\n    this.popover.toggle();\n  }\n\n  show = () => {\n    this.popover.show();\n  }\n\n  handleFilterSelect = (e: any) => {\n    this.props.onFilterSelect(e.currentTarget.id);\n    this.popover.hide();\n  }\n\n  handleRef = (ref: Popover) => {\n    this.popover = ref;\n  }\n\n  render() {\n    const { filters, selectedFilter } = this.props;\n\n    return (\n      <Popover\n        className=\"filter-popover\"\n        ref={this.handleRef}\n      >\n        {filters.map((filter: string) => {\n          return <button\n            key={filter}\n            className={classnames('filter', { selected: selectedFilter === filter })}\n            id={filter}\n            onClick={this.handleFilterSelect}\n          >\n            {filter}\n          </button>;\n        })}\n      </Popover>\n    );\n  }\n}\n\nexport default FilterPopover;\n"]}]}