eturn {\n    invalidAccount: selectInvalidAccount(state),\n    permissions: state.permissions,\n    closedBanners: state.closedBanners,\n    upgradePrompts: selectUpgradePrompt(state),\n    appUpdateReady: state.appUpdateReady,\n    webappUpdateReady: state.webappUpdateReady,\n    isOnline: state.isOnline,\n    isDefaultClient: state.isDefaultClient,\n    statusTypeState: state.me.data && state.me.data.statusNew || MeStatusNew.None,\n    isDisabled: state.me.data && isDisabledState(state.me.data.statusNew),\n    isMobile: state.isMobile,\n    showReferralRedemption: !!state.me.data && !!state.me.data.referrer &&\n      !state.me.data.referralRedeemed,\n    rsbOnboardingComplete: selectRsbOnboarding(state) === OnboardingType.Complete,\n    rightSidebarTab: state.rightSidebar.tab,\n    yearlySummary: state.yearlySummaries && state.yearlySummaries[2020]?.data,\n    currentPath: browserHistory.location.pathname,\n  };\n};\n\nconst mapDispatchToProps = (dispatch: Dispatch<StoreState>): DispatchProps => ({\n  registerMailToProtocol: () => dispatch(permissionsMailtoRegister(null)),\n  requestNotificationPermission: () => dispatch(permissionsNotifications(null)),\n  closeBanner: type => dispatch(permissionsBannerClose(type)),\n  updateAccountPassword: (account, newPassword) =>\n    dispatch(accountUpdatePassword(account, newPassword)),\n  reauthorizeAccount: accountId => dispatch(accountReauthorize(accountId)),\n  setAppUpdateReady: ready => dispatch(appUpdateReady(ready)),\n  setWebappUpdateReady: ready => dispatch(webappUpdateReady(ready)),\n  bannerSet: meta => dispatch(bannerSet(meta)),\n});\n\nexport default connect<StateProps, DispatchProps>(\n  mapStateToProps, mapDispatchToProps,\n)(Banner);\n"]}]}