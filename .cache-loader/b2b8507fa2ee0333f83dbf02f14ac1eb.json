;gBACtC,CAAC,MAAM,CAAC,IAAI,CAAC,MAAM,CAAC,YAAY,CAAC,CAAC,IAAI,EAAE,CAAC;qBACvC,MAAM,CAAC,UAAA,CAAC,IAAI,OAAA,CAAC,CAAC,QAAQ;oBACrB,MAAM,CAAC,CAAC,CAAC,OAAO,CAAC,CAAC,MAAM,CAAC,YAAY,CAAC,KAAK,IAAI,CAAC,MAAM,CAAC,YAAY,CAAC,EADzD,CACyD,CAAC;qBACtE,GAAG,CAAC,UAAC,KAAK;oBACT,IAAI,MAAM,CAAC;oBACX,IAAI,CAAC,CAAC;oBACN,IAAI,CAAC,CAAC;oBACN,IAAI,CAAC,CAAC;oBACN,IAAI,KAAK,CAAC,QAAQ,CAAC,eAAe,CAAC,KAAK,CAAC,IAAI,CAAC,EAAE;wBAC9C,MAAM,GAAG,QAAQ,CAAC,KAAK,CAAC,QAAQ,CAAC,eAAe,CAAC,KAAK,CAAC,CAAC,CAAC,EAAE,EAAE,CAAC,CAAC;wBAC/D,4BAA4B;wBAC5B,CAAC,GAAG,CAAC,MAAM,IAAI,EAAE,CAAC,GAAG,GAAG,CAAC;wBACzB,CAAC,GAAG,CAAC,MAAM,IAAI,CAAC,CAAC,GAAG,GAAG,CAAC;wBACxB,CAAC,GAAG,MAAM,GAAG,GAAG,CAAC;wBACjB,2BAA2B;qBAC5B;oBAED,OAAO,CACL,6BACE,GAAG,EAAE,KAAK,CAAC,EAAE,EACb,SAAS,EAAC,eAAe,EACzB,KAAK,EAAE;4BACL,eAAe,EAAE,MAAM,KAAK,SAAS,CAAC,CAAC;gCACrC,UAAQ,CAAC,CAAC,EAAE,CAAC,EAAE,CAAC,CAAC,CAAC,IAAI,CAAC,GAAG,CAAC,WAAQ,CAAC,CAAC,CAAC,KAAK,CAAC,QAAQ,CAAC,eAAe;4BACtE,KAAK,EAAE,KAAK,CAAC,QAAQ,CAAC,eAAe;yBACtC,IAEA,KAAK,CAAC,IAAI,CACP,CACP,CAAC;gBACJ,CAAC,CAAC,CAEA,CACP,EA7CsB,CA6CtB,CAAC,CACE,CACF,CACP,CAAC;IACJ,CAAC;IACH,qBAAC;AAAD,CAAC,AAnID,CAA6B,KAAK,CAAC,aAAa,GAmI/C;AAED,eAAe,cAAc,CAAC","sourcesContent":["import React from 'react';\nimport classnames from 'classnames';\nimport moment, { Moment } from 'moment';\nimport Popover from '@src/components/Popover/Popover';\nimport MonthView from './MonthView/MonthView';\nimport MonthViewIcon from '@src/components/Icons/MonthView.svg';\nimport Arrow from '@src/components/Icons/Arrow.svg';\n\nimport './CalendarHeader.scss';\nimport { CalendarEvent } from '@src/types';\n\ninterface Props {\n  startDate: Moment;\n  endDate: Moment;\n  onStartDateChange: (newStartDate: Moment) => void;\n  columnWidth: number;\n  dateRange: Moment[];\n  events: { [date: string]: CalendarEvent[] };\n  singleDay?: boolean;\n  onOpenCalendar?: () => void;\n}\n\nclass CalendarHeader extends React.PureComponent<Props> {\n  private popover: Popover;\n\n  componentDidMount() {\n    this.popover.hide();\n  }\n\n  handleLeftArrowClick = () => {\n    const { onStartDateChange, startDate, singleDay } = this.props;\n    const daysToChange = !!singleDay ? 1 : 5;\n    onStartDateChange(startDate.clone().add(-daysToChange, 'days'));\n  }\n\n  handleTodayClick = () => {\n    const { onStartDateChange } = this.props;\n    onStartDateChange(moment().startOf('day'));\n  }\n\n  handleRightArrowClick = () => {\n    const{ onStartDateChange, startDate, singleDay } = this.props;\n    const daysToChange = !!singleDay ? 1 : 5;\n    onStartDateChange(startDate.clone().add(daysToChange, 'days'));\n  }\n\n  showPopover = () => {\n    this.popover.show();\n  }\n\n  handleRef = (ref: Popover) => {\n    this.popover = ref;\n  }\n\n  handleMonthViewDateClick = (newStartDate: Moment) => {\n    const { onStartDateChange } = this.props;\n    onStartDateChange(newStartDate);\n  }\n\n  render() {\n    const {\n      startDate, endDate, columnWidth,\n      dateRange, events, singleDay,\n    } = this.props;\n    const startDateString = startDate.format('MMM D');\n    const endDateString = endDate.format('MMM') === startDate.format('MMM') ?\n      endDate.format('D') : endDate.format('MMM D');\n\n    const finalDateString = singleDay\n      ? startDateString\n      : `${startDateString} - ${endDateString}`;\n    return (\n      <div className=\"calendar-header\">\n        <div className=\"top-bar\">\n          <div className=\"dates\">\n            {finalDateString}\n          </div>\n          <button className=\"left-arrow\" onClick={this.handleLeftArrowClick}>\n            <Arrow className=\"icon\" />\n          </button>\n          <button\n            className=\"today\"\n            onClick={this.handleTodayClick}\n          >\n            Today\n          </button>\n          <button className=\"right-arrow\" onClick={this.handleRightArrowClick}>\n            <Arrow className=\"icon\" />\n          </button>\n          <div className=\"month\">\n            <button className=\"month-button\" onClick={this.showPopover}>\n              <MonthViewIcon className=\"icon\" />\n            </button>\n            <Popover className=\"month-view-popover\" ref={this.handleRef}>\n              <MonthView\n                startDate={startDate}\n                endDate={singleDay ? startDate : endDate }\n                onDateClick={this.handleMonthViewDateClick}\n              />\n            </Popover>\n          </div>\n        </div>\n        <div className=\"date-heading\">\n          {dateRange.map(date => (\n            <div\n              key={date.format('MM/DD/YYYY')}\n              className={classnames('heading', {\n                today: date.diff(moment().startOf('day'), 'days') === 0,\n              })}\n              style={{\n                width: columnWidth,\n              }}\n            >\n              <h5 className=\"weekday\">{moment.weekdaysShort(date.weekday())}</h5>\n              <h4 className=\"date\">{date.date()}</h4>\n              {(events[date.format('MM-DD-YYYY')] || [])\n                .filter(e => e.isAllDay &&\n                  moment(e.endTime).format('MM-DD-YYYY') === date.format('MM-DD-YYYY'))\n                .map((event) => {\n                  let bigInt;\n                  let r;\n                  let b;\n                  let g;\n                  if (event.calendar.backgroundColor.match(/^#/)) {\n                    bigInt = parseInt(event.calendar.backgroundColor.slice(1), 16);\n                    // tslint:disable:no-bitwise\n                    r = (bigInt >> 16) & 255;\n                    g = (bigInt >> 8) & 255;\n                    b = bigInt & 255;\n                    // tslint:enable:no-bitwise\n                  }\n\n                  return (\n                    <div\n                      key={event.id}\n                      className=\"all-day-event\"\n                      style={{\n                        backgroundColor: bigInt !== undefined ?\n                          `rgba(${[r, g, b].join(',')}, .15)` : event.calendar.backgroundColor,\n                        color: event.calendar.backgroundColor,\n                      }}\n                    >\n                      {event.name}\n                    </div>\n                  );\n                })\n              }\n            </div>\n          ))}\n        </div>\n      </div>\n    );\n  }\n}\n\nexport default CalendarHeader;\n"]}]}