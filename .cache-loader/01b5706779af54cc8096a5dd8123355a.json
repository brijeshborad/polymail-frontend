AM,EACN,kCACY,CAAC;QACf,OAAO,CACL,oBAAC,OAAO,IACN,SAAS,EAAE,UAAU,CAAC,gBAAgB,EAAE,SAAS,EAAE,cAAc,CAAC,EAClE,KAAK,EAAE;gBACL,GAAG,KAAA;gBACH,IAAI,MAAA;gBACJ,KAAK,OAAA;gBACL,MAAM,QAAA;aACP,EACD,GAAG,EAAE,IAAI,CAAC,SAAS,EACnB,QAAQ,QACR,YAAY,QACZ,UAAU;YAEV,6BAAK,SAAS,EAAC,OAAO,IACnB,KAAK,CACF;YACL,OAAO;gBACN,6BAAK,SAAS,EAAC,SAAS,IACrB,OAAO,CACJ;YAEP,MAAM;gBACL,gCACE,SAAS,EAAC,aAAa,EACvB,OAAO,EAAE,IAAI,CAAC,WAAW,WAGlB,CAEH,CACX,CAAC;IACJ,CAAC;IACH,oBAAC;AAAD,CAAC,AAzED,CAA4B,KAAK,CAAC,aAAa,GAyE9C;AAED,eAAe,aAAa,CAAC","sourcesContent":["import React from 'react';\nimport classnames from 'classnames';\nimport Popover from '@src/components/Popover/Popover';\nimport './OnboardingTip.scss';\n\nexport interface Props {\n  className?: string;\n  top?: string;\n  left?: string;\n  right?: string;\n  bottom?: string;\n  title: string;\n  delay?: number;\n  message?: string|JSX.Element;\n  ref?: (ref: Popover) => void;\n  onNext?: () => void;\n  anchorPosition?: AnchorPosition;\n}\n\nexport enum AnchorPosition {\n  Top= 'anchor-top',\n  Left= 'anchor-left',\n  Right= 'anchor-right',\n  Bottom= 'anchor-bottom',\n}\n\nclass OnboardingTip extends React.PureComponent<Props> {\n  popover: Popover;\n  timeout: NodeJS.Timer;\n\n  componentDidMount() {\n    const { delay } = this.props;\n    this.timeout = global.setTimeout(() => this.popover.show(), delay || 1500);\n  }\n\n  componentWillUnmount() {\n    if (this.timeout) {\n      clearTimeout(this.timeout);\n    }\n  }\n\n  handleClick = (e: React.MouseEvent<HTMLElement>) => {\n    const { onNext } =  this.props;\n    if (onNext) {\n      onNext();\n      e.stopPropagation();\n      e.preventDefault();\n    }\n  }\n\n  handleRef = (ref: Popover) => {\n    this.popover = ref;\n  }\n\n  render() {\n    const {\n      top,\n      left,\n      title,\n      right,\n      message,\n      bottom,\n      className,\n      onNext,\n      anchorPosition,\n    } = this.props;\n    return (\n      <Popover\n        className={classnames('onboarding-tip', className, anchorPosition)}\n        style={{\n          top,\n          left,\n          right,\n          bottom,\n        }}\n        ref={this.handleRef}\n        fadesOut\n        autoFocusOff\n        persistent\n      >\n        <div className=\"title\">\n          {title}\n        </div>\n        {message &&\n          <div className=\"message\">\n            {message}\n          </div>\n        }\n        {onNext &&\n          <button\n            className=\"next-button\"\n            onClick={this.handleClick}\n          >\n            Next\n          </button>\n        }\n      </Popover>\n    );\n  }\n}\n\nexport default OnboardingTip;\n"]}]}