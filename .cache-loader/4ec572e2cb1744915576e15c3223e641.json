\n  iOS = 'ios',\n  Windows = 'windows',\n}\n\nexport const enum LogDevice {\n  Desktop = 'desktop',\n  Mobile = 'mobile',\n  Tablet = 'tablet',\n}\n\nexport interface Log {\n  level: LogLevel;\n  message: string;\n  properties?: {\n    [name: string]: any;\n  };\n  created: Date;\n  os: LogOs;\n  device: LogDevice;\n}\n\nexport class LogsClient implements LogsAPI {\n  recordLog(log: Log) {\n    return blankRequest({\n      method: 'POST',\n      route: '/v1/logs',\n      body: log,\n    });\n  }\n}\n\nexport function mockLogsClient(): LogsAPI {\n  return {\n    recordLog: (log: Log) => Observable.of(null),\n  };\n}\n"]}]}