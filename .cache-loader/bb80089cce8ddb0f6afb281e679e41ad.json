B,IAE/B,MAAM,CAAC,KAAK,CACN,CAAC;QACZ,CAAC,CAAC,CACM,CACX,CAAC;IACJ,CAAC;IACH,oBAAC;AAAD,CAAC,AA1CD,CAA4B,KAAK,CAAC,aAAa,GA0C9C;AAED,eAAe,aAAa,CAAC","sourcesContent":["import React from 'react';\nimport classnames from 'classnames';\n\nimport Popover from '@src/components/Popover/Popover';\n\nimport './ActivityFeedFilterPopover.scss';\nimport { ActivityType } from '@src/types';\n\ninterface Props {\n  filters: Array<{ title: string, type?: ActivityType }>;\n  onFilterSelect: (filter: string) => void;\n  selectedFilter: string;\n}\n\nclass FilterPopover extends React.PureComponent<Props> {\n  private popover: Popover;\n\n  show = () => {\n    this.popover.show();\n  }\n\n  toggle = () => {\n    this.popover.toggle();\n  }\n\n  handleFilterSelect = (e: any) => {\n    this.props.onFilterSelect(e.target.dataset.type);\n  }\n\n  handleRef = (ref: Popover) => {\n    this.popover = ref;\n  }\n\n  render() {\n    const { filters, selectedFilter } = this.props;\n\n    return (\n      <Popover\n        className=\"activity-feed-filter-popover\"\n        ref={this.handleRef}\n      >\n        {filters.map((filter) => {\n          return <button\n            key={filter.title}\n            className={classnames('filter', {\n              selected: selectedFilter === filter.title,\n            })}\n            data-type={filter.type}\n            onClick={this.handleFilterSelect}\n          >\n            {filter.title}\n          </button>;\n        })}\n      </Popover>\n    );\n  }\n}\n\nexport default FilterPopover;\n"]}]}