AC,IAAI,8BAAM,SAAS,EAAC,QAAQ,IACnD,KAAK,CAAC,MAAM,CACR,EACJ,UAAO,KAAK,IAAI,KAAK,CAAC,MAAM,KAAK,CAAC,CAAC,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC,GAAG,CAAE,CACzC,CACL;YACN,6BAAK,SAAS,EAAC,MAAM;gBAClB,WAAW,KAAK,kBAAkB,CAAC,YAAY,IAAI,oBAAC,iBAAiB,IACpE,WAAW,EAAE,OAAO,CAAC,SAAS,EAC9B,aAAa,EAAE,aAAa,EAC5B,UAAU,EAAE,UAAU,GACtB;gBACD,KAAK,IAAI,WAAW,KAAK,kBAAkB,CAAC,IAAI,IAAI,oBAAC,SAAS,IAC7D,KAAK,EAAE,KAAK,EACZ,UAAU,EAAE,UAAU,GACtB;gBACD,WAAW,KAAK,kBAAkB,CAAC,IAAI,IAAI,oBAAC,SAAS,IACpD,KAAK,EAAE,KAAK,EACZ,WAAW,EAAE,OAAO,CAAC,SAAS,EAC9B,UAAU,EAAE,UAAU,GACtB,CACE,CACF,CACP,CAAC;IACJ,CAAC;IACH,0BAAC;AAAD,CAAC,AA3HD,CAAkC,KAAK,CAAC,aAAa,GA2HpD;AAED,eAAe,mBAAmB,CAAC","sourcesContent":["import React from 'react';\nimport classnames from 'classnames';\n\nimport {\n  Thread, ContactNote, ContactAttachment, Contact,\n} from '@src/types';\nimport {\n  RightSidebarSubTab, RightSidebarTab,\n} from '@src/types/rightSidebar';\n\nimport ConversationsPane from './components/ConversationsPane/ConversationsPane';\nimport NotesPane from './components/NotesPane/NotesPane';\nimport FilesPane from './components/FilesPane/FilesPane';\n\nimport './ContactInteractions.scss';\nimport { RIGHT_SIDEBAR_TOP_BAR_HEIGHT } from '@src/components/RightSidebar/constants';\n\nexport interface Props {\n  conversations?: Thread[];\n  notes?: ContactNote[];\n  files?: ContactAttachment[];\n  contact: Contact;\n  selectedTab: RightSidebarSubTab;\n}\n\ninterface State {\n  listHeight: number;\n}\n\nexport interface DispatchProps {\n  selectRightSidebarTab: (tab: RightSidebarTab, subTab: RightSidebarSubTab) => void;\n}\n\nconst TABS_HEIGHT = 44;\n\nclass ContactInteractions extends React.PureComponent<Props & DispatchProps, State> {\n  constructor() {\n    super();\n\n    this.state = {\n      listHeight: 100,\n    };\n  }\n\n  componentDidMount() {\n    window.addEventListener('resize', this.handleResize);\n    this.handleResize();\n  }\n\n  componentWillUnmount() {\n    window.removeEventListener('resize', this.handleResize);\n  }\n\n  componentDidUpdate(prevProps: Props, prevState: State) {\n    this.handleResize();\n  }\n\n  handleResize = () => {\n    const rsb = window.document.getElementsByClassName('right-sidebar');\n    let rsbHeight = 0;\n\n    if (rsb && rsb[0]) {\n      rsbHeight = rsb[0].clientHeight;\n    }\n\n    if (rsbHeight <= 0) {\n      return;\n    }\n\n    const info = rsb[0].getElementsByClassName('contact-desc');\n    let contactInfoHeight = 0;\n\n    if (info && info[0]) {\n      contactInfoHeight = info[0].clientHeight;\n    }\n\n    const companyInfo = rsb[0].getElementsByClassName('company-info');\n    let companyInfoHeight = 0;\n    if (companyInfo && companyInfo[0]) {\n      companyInfoHeight = companyInfo[0].clientHeight;\n    }\n\n    const listHeight = rsbHeight - contactInfoHeight - companyInfoHeight\n       - TABS_HEIGHT - RIGHT_SIDEBAR_TOP_BAR_HEIGHT;\n    if (listHeight !== this.state.listHeight) {\n      this.setState({ listHeight });\n    }\n  }\n\n  handleSelectTab = (e: React.SyntheticEvent<HTMLButtonElement>) => {\n    const { selectRightSidebarTab } = this.props;\n\n    selectRightSidebarTab(RightSidebarTab.Contact, e.currentTarget.id as RightSidebarSubTab);\n  }\n\n  render() {\n    const { listHeight } = this.state;\n    const { conversations, notes, files, contact, selectedTab } = this.props;\n\n    return (\n      <div className=\"contact-interactions\">\n        <div className=\"tabs\">\n          <button\n            id={RightSidebarSubTab.Conversation}\n            className={classnames({\n              selected: selectedTab === RightSidebarSubTab.Conversation,\n            })}\n            onClick={this.handleSelectTab}\n          >\n            {conversations && conversations.length > 0 && <span className=\"number\">\n              {conversations.length}\n            </span>}\n            {`Conversation${conversations && conversations.length === 1 ? '' : 's'}`}\n          </button>\n          {false && <button\n            id={RightSidebarSubTab.Note}\n            className={classnames({\n              selected: selectedTab === RightSidebarSubTab.Note,\n            })}\n            onClick={this.handleSelectTab}\n          >\n          {notes && notes!.length > 0 && <span className=\"number\">\n            {notes!.length}\n          </span>}\n            {`Note${notes && notes!.length === 1 ? '' : 's'}`}\n          </button>}\n          <button\n            id={RightSidebarSubTab.File}\n            className={classnames({\n              selected: selectedTab === RightSidebarSubTab.File,\n            })}\n            onClick={this.handleSelectTab}\n          >\n          {files && files.length > 0 && <span className=\"number\">\n            {files.length}\n          </span>}\n            {`File${files && files.length === 1 ? '' : 's'}`}\n          </button>\n        </div>\n        <div className=\"main\">\n          {selectedTab === RightSidebarSubTab.Conversation && <ConversationsPane\n            contactName={contact.givenName}\n            conversations={conversations}\n            listHeight={listHeight}\n          />}\n          {false && selectedTab === RightSidebarSubTab.Note && <NotesPane\n            notes={notes}\n            listHeight={listHeight}\n          />}\n          {selectedTab === RightSidebarSubTab.File && <FilesPane\n            files={files}\n            contactName={contact.givenName}\n            listHeight={listHeight}\n          />}\n        </div>\n      </div>\n    );\n  }\n}\n\nexport default ContactInteractions;\n"]}]}