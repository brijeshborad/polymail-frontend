tent":["import React from 'react';\nimport classnames from 'classnames';\n\nimport { AccountType } from '@src/types';\nimport HoverTip from '@src/components/Base/HoverTip';\n\ninterface ProviderItemProps {\n  disabled?: boolean;\n  onClick: (provider: string) => void;\n  provider: {\n    type: string;\n    logo: string;\n    width: string;\n    accountType: AccountType;\n  };\n}\n\nconst ProviderItem = (props: ProviderItemProps) => {\n  const { disabled } = props;\n  const onClick = (e: React.MouseEvent<HTMLButtonElement>) => {\n    if (disabled) {\n      return;\n    }\n\n    props.onClick(props.provider.type);\n  };\n\n  if (disabled) {\n    return (\n      <HoverTip\n        className=\"provider-cell\"\n        copy=\"Signing up with Yahoo Accounts is currently disabled\"\n        top=\"-40px\"\n        left=\"20px\"\n        anchorBottom\n        multiline\n      >\n        <button\n          className={classnames('main', { disabled })}\n          key={props.provider.type}\n          onClick={onClick}\n          disabled\n        >\n          <img width={props.provider.width} src={props.provider.logo} />\n        </button>\n      </HoverTip>\n    );\n  }\n\n  return (\n    <button\n      className={classnames('provider-cell', { disabled })}\n      key={props.provider.type}\n      onClick={onClick}\n      disabled={disabled}\n    >\n      <img width={props.provider.width} src={props.provider.logo} />\n    </button>\n  );\n};\n\nexport default ProviderItem;\n"]}]}