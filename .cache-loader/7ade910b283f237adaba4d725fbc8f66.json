AI,KAAK,MAAM,IAAI,KAAK,CAAC,IAAI,KAAK,OAAO,EAAE;gBAC7E,OAAO;aACR;YACD;YACE,6DAA6D;YAC7D,KAAK,CAAC,IAAI,KAAK,YAAY,EAC3B;gBACA,OAAO;aACR;YAED,MAAM,CAAC,IAAI,CAAC,CACV,oBAAC,KAAK,IACJ,GAAG,EAAE,KAAK,CAAC,IAAI,EACf,IAAI,EAAE,KAAK,CAAC,IAAI,EAChB,KAAK,EAAE,KAAK,EACZ,QAAQ,EAAE,KAAI,CAAC,KAAK,CAAC,QAAQ,EAC7B,QAAQ,SACR,CACH,CAAC,CAAC;QACL,CAAC,CAAC,CAAC;QAEH,OAAO,CACL,6BAAK,SAAS,EAAC,MAAM;YACnB;gBACE,mCAAQ,MAAM,CAAS,CACjB,CACJ,CACP,CAAC;IACJ,CAAC;IAED,wBAAM,GAAN;QACE,OAAO,CACL,6BAAK,SAAS,EAAC,oBAAoB;YACjC,oBAAC,KAAK,IAAC,KAAK,EAAE,IAAI,CAAC,KAAK,CAAC,KAAK,GAAI;YAClC,oBAAC,MAAM,IACL,IAAI,EAAE,IAAI,CAAC,IAAI,EAAE,EACjB,KAAK,EAAE,IAAI,CAAC,KAAK,EAAE,EACnB,KAAK,EAAE,IAAI,CAAC,KAAK,EAAE,EACnB,IAAI,EAAE,IAAI,CAAC,KAAK,CAAC,IAAI,CAAC,GAAG,GACzB;YACD,IAAI,CAAC,KAAK,CAAC,MAAM,CAAC,CAAC;gBAClB,6BAAK,SAAS,EAAC,iBAAiB;oBAC9B,gCAAQ,OAAO,EAAE,IAAI,CAAC,KAAK,CAAC,aAAa,IACtC,IAAI,CAAC,KAAK,CAAC,MAAM,CACX,CACL,CAAC,CAAC,CAAC,IAAI;YAEd,IAAI,CAAC,YAAY,EAAE,CAChB,CACP,CAAC;IACJ,CAAC;IACH,cAAC;AAAD,CAAC,AA1ED,CAAsB,KAAK,CAAC,aAAa,GA0ExC;AAED,eAAe,OAAO,CAAC","sourcesContent":["import React from 'react';\n\nimport Title from './Title';\nimport Header from './Header';\nimport Field from './Field';\nimport { SObject, SObjectFieldType } from '@src/types/salesforce';\n\nimport './Section.scss';\nimport { getSalesforceField } from '@src/lib/models/salesforce';\n\nexport interface Props {\n  title: string;\n  data: SObject;\n  button?: string;\n  onButtonClick?: (e: React.SyntheticEvent<HTMLElement>) => void;\n  onChange: (fieldName: string, value: any) => void;\n}\n\nclass Section extends React.PureComponent<Props> {\n  /* eslint-disable no-restricted-syntax */\n  name() {\n    return getSalesforceField(this.props.data.fields, 'Name');\n  }\n\n  email() {\n    return getSalesforceField(this.props.data.fields, 'Email');\n  }\n\n  phone() {\n    return getSalesforceField(this.props.data.fields, 'Phone');\n  }\n  /* eslint-enable */\n\n  renderFields() {\n    const fields: JSX.Element[] = [];\n\n    this.props.data.fields.forEach((field) => {\n      if ((field.type === 'reference' && field.name !== 'OwnerId')\n       || field.type === SObjectFieldType.Id) {\n        return;\n      }\n      if (field.name === 'Email' || field.name === 'Name' || field.name === 'Phone') {\n        return;\n      }\n      if (\n        // field.name === 'FirstName' || field.name === 'LastName' ||\n        field.name === 'Salutation'\n      ) {\n        return;\n      }\n\n      fields.push((\n        <Field\n          key={field.name}\n          name={field.name}\n          field={field}\n          onChange={this.props.onChange}\n          tabbable\n        />\n      ));\n    });\n\n    return (\n      <div className=\"data\">\n        <table>\n          <tbody>{fields}</tbody>\n        </table>\n      </div>\n    );\n  }\n\n  render() {\n    return (\n      <div className=\"salesforce-section\">\n        <Title title={this.props.title} />\n        <Header\n          name={this.name()}\n          email={this.email()}\n          phone={this.phone()}\n          href={this.props.data.url}\n        />\n        {this.props.button ?\n          <div className=\"convert-contact\">\n            <button onClick={this.props.onButtonClick}>\n              {this.props.button}\n            </button>\n          </div> : null\n        }\n        {this.renderFields()}\n      </div>\n    );\n  }\n}\n\nexport default Section;\n"]}]}