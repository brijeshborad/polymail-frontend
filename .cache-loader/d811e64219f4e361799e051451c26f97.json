AC;IACH,oBAAC;AAAD,CAAC,AA1DD,CAA4B,KAAK,CAAC,aAAa,GA0D9C;AAED,eAAe,aAAa,CAAC","sourcesContent":["import React from 'react';\nimport Popover from '@src/components/Popover/Popover';\nimport logo from '@src/routes/Login/Icons/PolymailLogo@3x.png';\n\nimport './SignUpPopover.scss';\nimport browserHistory from '@src/lib/browserHistory';\n\ninterface Props {\n  onHide: () => void;\n}\n\nclass SignUpPopover extends React.PureComponent<Props> {\n  popover: Popover;\n\n  componentDidMount() {\n    this.popover.show();\n  }\n\n  handleRef = (ref: Popover) => {\n    this.popover = ref;\n  }\n\n  handleSignInClick = () => {\n    browserHistory.push('/login');\n  }\n\n  render() {\n    const { onHide } = this.props;\n\n    return (\n      <Popover\n        className=\"sign-up-popover\"\n        ref={this.handleRef}\n        overlay\n        persistent\n      >\n        <div className=\"main\">\n          <div className=\"heading\">\n            Sign In\n          </div>\n          <div className=\"input-container\">\n            <div className=\"logo-container\">\n              <img src={logo} className=\"logo\" />\n            </div>\n            <div className=\"instructions\">\n              Sign in to comment on this conversation.\n            </div>\n            {/* <div className=\"email\">\n              <div className=\"label\">\n                Email Address\n              </div>\n              <input type=\"text\" className=\"email-input\" placeholder=\"name@company.com\" />\n            </div> */}\n            <div className=\"actions\">\n              <button\n                className=\"sign-up-button\"\n                onClick={this.handleSignInClick}\n              >\n                Sign In\n              </button>\n              <button className=\"cancel-button\" onClick={onHide}>\n                Cancel\n              </button>\n            </div>\n          </div>\n        </div>\n      </Popover>\n    );\n  }\n}\n\nexport default SignUpPopover;\n"]}]}