B,CAAA;QArOC,KAAI,CAAC,KAAK,GAAG;YACX,YAAY,EAAE,KAAK;YACnB,KAAK,EAAE,EAAE;YACT,QAAQ,EAAE,EAAE;YAEZ,YAAY,EAAE,EAAE;YAChB,YAAY,EAAE,EAAE;YAChB,QAAQ,EAAE,EAAE;YACZ,QAAQ,EAAE,GAAG;YACb,eAAe,EAAE,KAAK;YAEtB,YAAY,EAAE,EAAE;YAChB,YAAY,EAAE,EAAE;YAChB,QAAQ,EAAE,EAAE;YACZ,QAAQ,EAAE,GAAG;YACb,YAAY,EAAE,KAAK;YACnB,eAAe,EAAE,KAAK;SACvB,CAAC;;IACJ,CAAC;IAqND,wBAAM,GAAN;QACE,OAAO,CACL,6BAAK,SAAS,EAAC,UAAU;YACtB,IAAI,CAAC,KAAK,CAAC,UAAU,IAAI,oBAAC,gBAAgB,IACzC,KAAK,EAAC,UAAU,EAChB,MAAM,EAAE,IAAI,CAAC,eAAe,GAC5B;YACF,6BAAK,SAAS,EAAC,QAAQ;gBACrB,uCAAa,CACT;YACN,8BAAM,QAAQ,EAAE,IAAI,CAAC,YAAY;gBAC/B,6BAAK,SAAS,EAAC,OAAO;oBACpB,oBAAC,SAAS,IACR,IAAI,EAAC,OAAO,EACZ,WAAW,EAAC,OAAO,EACnB,WAAW,EAAC,OAAO,EACnB,KAAK,EAAE,IAAI,CAAC,KAAK,CAAC,KAAK,EACvB,IAAI,EAAC,OAAO,EACZ,QAAQ,EAAE,IAAI,CAAC,YAAY,EAC3B,SAAS,QACT,QAAQ,SACR,CACE;gBACL,IAAI,CAAC,KAAK,CAAC,YAAY,CAAC,CAAC,CAAC,IAAI,CAAC,kBAAkB,EAAE,CAAC,CAAC,CAAC,6BAAK,SAAS,EAAC,OAAO;oBAC3E,oBAAC,SAAS,IACR,IAAI,EAAC,UAAU,EACf,WAAW,EAAC,UAAU,EACtB,WAAW,EAAC,UAAU,EACtB,KAAK,EAAE,IAAI,CAAC,KAAK,CAAC,QAAQ,EAC1B,IAAI,EAAC,UAAU,EACf,QAAQ,EAAE,IAAI,CAAC,YAAY,EAC3B,QAAQ,SACR,CACE;gBACN,6BAAK,SAAS,EAAC,QAAQ;oBACrB,oCAAS,IAAI,CAAC,KAAK,CAAC,YAAY,CAAC,CAAC,CAAC,aAAa,CAAC,CAAC,CAAC,YAAY,CAAU,CACrE,CACD,CACH,CACP,CAAC;IACJ,CAAC;IACH,cAAC;AAAD,CAAC,AApRD,CAAqC,KAAK,CAAC,aAAa,GAoRvD","sourcesContent":["import React from 'react';\n\nimport TextInput from '@src/components/Form/TextInput';\nimport NavigationHeader from '@src/routes/Settings/components/NavigationHeader';\n\nimport browserHistory from '@src/lib/browserHistory';\n\nimport Toggle from '@src/components/Form/Toggle';\nimport { Partial, Account, AccountType } from '@src/types';\n\nimport './AddIMAP.scss';\n\nexport interface Props {\n  showHeader: boolean;\n}\n\nexport interface State {\n  detailedForm: boolean;\n\n  email: string;\n  password: string;\n\n  imapUsername: string;\n  imapPassword: string;\n  imapHost: string;\n  imapPort: number;\n  imapInsecureSsl: boolean;\n\n  smtpUsername: string;\n  smtpPassword: string;\n  smtpHost: string;\n  smtpPort: number;\n  smtpStarttls: boolean;\n  smtpInsecureSsl: boolean;\n}\n\nexport interface DispatchProps {\n  addAccount(account: Partial<Account>): void;\n}\n\nexport default class AddIMAP extends React.PureComponent<Props & DispatchProps, State> {\n  constructor() {\n    super();\n\n    this.state = {\n      detailedForm: false,\n      email: '',\n      password: '',\n\n      imapUsername: '',\n      imapPassword: '',\n      imapHost: '',\n      imapPort: 993,\n      imapInsecureSsl: false,\n\n      smtpUsername: '',\n      smtpPassword: '',\n      smtpHost: '',\n      smtpPort: 465,\n      smtpStarttls: false,\n      smtpInsecureSsl: false,\n    };\n  }\n\n  handleChange = (e: React.ChangeEvent<HTMLInputElement>) => {\n    this.setState({\n      [e.currentTarget.name]: e.currentTarget.value,\n    } as any);\n  }\n\n  handleBackPress = () => {\n    browserHistory.goBack();\n  }\n\n  handleSubmit = (e: React.FormEvent<HTMLElement>) => {\n    e.preventDefault();\n    e.stopPropagation();\n\n    if (!this.state.detailedForm) {\n      const { email, password } = this.state;\n      this.setState({\n        detailedForm: true,\n        imapUsername: email,\n        imapPassword: password,\n        smtpUsername: email,\n        smtpPassword: password,\n      });\n    } else {\n      this.props.addAccount({\n        type: AccountType.IMAP,\n        email: this.state.email,\n        imap: {\n          username: this.state.imapUsername,\n          password: this.state.imapPassword,\n          host: this.state.imapHost,\n          port: parseInt(this.state.imapPort.toString(), 10),\n          ssl: true,\n          insecureSsl: this.state.imapInsecureSsl,\n          idle: true,\n          folders: {\n            inbox: 'INBOX',\n            sent: 'Sent Messages',\n            archive: 'Archive',\n            trash: 'Deleted Messages',\n            spam: 'Spam',\n            readLater: 'Read Later',\n          },\n        },\n        smtp: {\n          username: this.state.smtpUsername,\n          password: this.state.smtpPassword,\n          host: this.state.smtpHost,\n          port: parseInt(this.state.smtpPort.toString(), 10),\n          ssl: true,\n          starttls: this.state.smtpStarttls,\n          insecureSsl: this.state.smtpInsecureSsl,\n        },\n      });\n    }\n  }\n\n  handleToggle = (e: React.SyntheticEvent<HTMLElement>) => {\n    e.preventDefault();\n    e.stopPropagation();\n\n    if (e.currentTarget.id === 'imapInsecureSsl') {\n      this.setState({ imapInsecureSsl: !this.state.imapInsecureSsl });\n    }\n\n    if (e.currentTarget.id === 'smtpInsecureSsl') {\n      this.setState({ smtpInsecureSsl: !this.state.smtpInsecureSsl });\n    }\n\n    if (e.currentTarget.id === 'smtpStarttls') {\n      const starttls = !this.state.smtpStarttls;\n      let port = this.state.smtpPort;\n\n      if (port === 465 && starttls) {\n        port = 587;\n      } else if (port === 587 && !starttls) {\n        port = 465;\n      }\n\n      this.setState({\n        smtpStarttls: starttls,\n        smtpPort: port,\n      });\n    }\n  }\n\n  renderDetailedForm = () => (\n    <div>\n      <div className=\"field\">\n        <h4>IMAP Server</h4>\n      </div>\n      <div className=\"field\">\n        <TextInput\n          name=\"imapUsername\"\n          description=\"Username\"\n          placeholder=\"Username\"\n          value={this.state.imapUsername}\n          type=\"text\"\n          onChange={this.handleChange}\n        />\n        <TextInput\n          name=\"imapPassword\"\n          description=\"Password\"\n          placeholder=\"Password\"\n          value={this.state.imapPassword}\n          type=\"password\"\n          onChange={this.handleChange}\n        />\n        <div className=\"host\">\n          <TextInput\n            name=\"imapHost\"\n            description=\"Host\"\n            placeholder=\"Host\"\n            value={this.state.imapHost}\n            type=\"text\"\n            onChange={this.handleChange}\n          />\n          <TextInput\n            name=\"imapPort\"\n            description=\"Port\"\n            placeholder=\"Port\"\n            value={this.state.imapPort.toString()}\n            type=\"number\"\n            onChange={this.handleChange}\n          />\n        </div>\n        <div className=\"ssl\">\n          <p>\n            <Toggle on={true} onChange={this.handleToggle} id=\"imapSsl\" />\n            Enable SSL\n          </p>\n        </div>\n        <div className=\"ssl\">\n          <p>\n            <Toggle\n              on={this.state.imapInsecureSsl}\n              onChange={this.handleToggle}\n              id=\"imapInsecureSsl\"\n            />\n            Allow Insecure SSL\n          </p>\n        </div>\n      </div>\n      <div className=\"field\">\n        <h4>SMTP Server</h4>\n      </div>\n      <div className=\"field\">\n        <TextInput\n          name=\"smtpUsername\"\n          description=\"Username\"\n          placeholder=\"Username\"\n          value={this.state.smtpUsername}\n          type=\"text\"\n          onChange={this.handleChange}\n        />\n        <TextInput\n          name=\"smtpPassword\"\n          description=\"Password\"\n          placeholder=\"Password\"\n          value={this.state.smtpPassword}\n          type=\"password\"\n          onChange={this.handleChange}\n        />\n        <div className=\"host\">\n          <TextInput\n            name=\"smtpHost\"\n            description=\"Host\"\n            placeholder=\"Host\"\n            value={this.state.smtpHost}\n            type=\"text\"\n            onChange={this.handleChange}\n          />\n          <TextInput\n            name=\"smtpPort\"\n            description=\"Port\"\n            placeholder=\"Port\"\n            value={this.state.smtpPort.toString()}\n            type=\"number\"\n            onChange={this.handleChange}\n          />\n        </div>\n        <div className=\"ssl\">\n          <p>\n            <Toggle on={true} onChange={this.handleToggle} id=\"smtpSsl\" />\n            Enable SSL\n          </p>\n        </div>\n        <div className=\"ssl\">\n          <p>\n            <Toggle\n              on={this.state.smtpStarttls}\n              onChange={this.handleToggle}\n              id=\"smtpStarttls\"\n            />\n            STARTTLS\n          </p>\n        </div>\n        <div className=\"ssl\">\n          <p>\n            <Toggle\n              on={this.state.smtpInsecureSsl}\n              onChange={this.handleToggle}\n              id=\"smtpInsecureSsl\"\n            />\n            Allow Insecure SSL\n          </p>\n        </div>\n      </div>\n    </div>\n  )\n\n  render() {\n    return (\n      <div className=\"add-imap\">\n        {this.props.showHeader && <NavigationHeader\n          title=\"Add IMAP\"\n          onBack={this.handleBackPress}\n        />}\n        <div className=\"header\">\n          <h3>IMAP</h3>\n        </div>\n        <form onSubmit={this.handleSubmit}>\n          <div className=\"field\">\n            <TextInput\n              name=\"email\"\n              description=\"Email\"\n              placeholder=\"Email\"\n              value={this.state.email}\n              type=\"email\"\n              onChange={this.handleChange}\n              autofocus\n              required\n            />\n          </div>\n          {this.state.detailedForm ? this.renderDetailedForm() : <div className=\"field\">\n            <TextInput\n              name=\"password\"\n              description=\"Password\"\n              placeholder=\"Password\"\n              value={this.state.password}\n              type=\"password\"\n              onChange={this.handleChange}\n              required\n            />\n          </div>}\n          <div className=\"submit\">\n            <button>{this.state.detailedForm ? 'Add Account' : 'Continue ›'}</button>\n          </div>\n        </form>\n      </div>\n    );\n  }\n}\n"]}]}