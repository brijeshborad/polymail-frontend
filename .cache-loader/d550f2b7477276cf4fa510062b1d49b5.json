c/ducks/contactSupport';\n\nconst mapStateToProps = (state: StoreState): StateProps => {\n  const me = state.me.data;\n\n  return {\n    me,\n    accounts: state.accounts,\n    popupCollapsed: state.contactSupportPopup.collapsed,\n    draft: state.selectedDraft\n      ? selectDraft(state, state.selectedDraft).data\n      : state.selectedThread && selectDraftForThread(state, state.selectedThread.id).data,\n  };\n};\n\nconst mapDispatchToProps = (dispatch: Dispatch<StoreState>): DispatchProps => ({\n  toggleContactSupportPopup: (collapsed: boolean) => dispatch(contactSupportToggle(collapsed)),\n});\n\nexport default connect(mapStateToProps, mapDispatchToProps)(ContactSupport);\n"]}]}