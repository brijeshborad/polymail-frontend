andControlType } from '@src/types';\nimport browserHistory from '@src/lib/browserHistory';\n\ninterface Props {\n  type: CommandControlType;\n  onClick: () => void;\n}\n\nexport const ManageButton = (props: Props) => {\n  const { type, onClick } = props;\n\n  const handleManage = () => {\n    switch (type) {\n      case CommandControlType.Templates:\n        browserHistory.push('/templates');\n        break;\n      case CommandControlType.SchedulingPages:\n        browserHistory.push('/calendar');\n        break;\n      default: return;\n    }\n    onClick();\n  };\n\n  // Copy\n  let title = 'Manage';\n  switch (type) {\n    case CommandControlType.Templates:\n      title = 'Manage Templates';\n      break;\n    case CommandControlType.SchedulingPages:\n      title = 'Manage Availabilities';\n      break;\n    default: break;\n  }\n\n  return (\n    <div className=\"control-popover-manage-button\">\n      <button\n        className=\"manage-button\"\n        onClick={handleManage}\n      >\n        {title}\n      </button>\n    </div>\n  );\n};\n\nexport default ManageButton;\n"]}]}