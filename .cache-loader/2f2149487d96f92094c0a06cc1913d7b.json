CvD,CACH;gBACA,CAAC,gBAAgB,IAAI,SAAS,IAAI,6BAAK,SAAS,EAAC,OAAO;oBACvD,8BAAM,SAAS,EAAE,UAAU,CAAC,mBAAmB,IAAI,SAAS,CAAC,IAC1D,CAAC,mBAAmB,CAAC,CAAC,CAAC,SAAS,CAAC,CAAC,CAAC,mBAAmB,CAClD;oBACL,SAAS;oBACV,gBAAgB,IAAI,OAAK,gBAAgB,gBAAa;oBACtD,CAAC,cAAc,IAAI,mBAAmB,CAAC,IAAI,cAAc,IAAI;wBAC5D,gCAAQ,OAAO,EAAE,IAAI,CAAC,iBAAiB,kBAAsB,CACzD,CACF;gBACL,gBAAgB;oBACjB;wBACE,6BAAK,SAAS,EAAC,OAAO;4BACnB,YAAY,CAAC,CAAC,CAAC,IAAI,CAAC,CAAC,CAAC,4CAAoB;4BAC3C,+BACE,GAAG,EAAC,MAAM,EACV,IAAI,EAAC,OAAO,EACZ,KAAK,EAAE,IAAI,CAAC,KAAK,CAAC,WAAW,EAC7B,QAAQ,EAAE,IAAI,CAAC,uBAAuB,EACtC,WAAW,EAAC,kBAAkB,EAC9B,SAAS,SACT,CACE;wBACL,CAAC,eAAe,IAAI,CAAC,eAAe;4BACnC,CAAC,CAAC,6BAAK,SAAS,EAAC,OAAO,GAAG;4BAC3B,CAAC,CAAC,6BAAK,SAAS,EAAC,eAAe;gCAC7B,eAAe,IAAI,oBAAC,MAAM,IAAC,SAAS,EAAC,QAAQ,GAAG;gCAChD,eAAe,IAAI,qDAA6B,CAC7C,CAEP,CAEC,CACF,CACP,CAAC;IACJ,CAAC;IACH,wBAAC;AAAD,CAAC,AA/LD,CAAgC,KAAK,CAAC,aAAa,GA+LlD;AAED,eAAe,iBAAiB,CAAC","sourcesContent":["import React, { ChangeEvent } from 'react';\nimport classnames from 'classnames';\n\nimport { BillingTier, BillingCycle } from '@src/types/billing';\nimport CostLabel from '@src/routes/Settings/routes/Billing/components/CostLabel';\nimport { debounce, formatCurrencyCents } from '@src/lib/utils';\n\nimport './BillingPlanHeader.scss';\nimport { titleForTier } from '@src/lib/models/billing';\nimport Loader from '@src/components/Loader/Loader';\n\nexport interface Props {\n  tier: BillingTier;\n  cycle: BillingCycle;\n  monthlyCost: number;\n  seats: number;\n  promoCode?: string;\n  promoCodePercent?: number;\n\n  seatsUpdateButton?: boolean;\n  seatsEditable?: boolean;\n  couponEditable?: boolean;\n  isLoadingCoupon: boolean;\n  initialParamsCoupon?: string;\n  hidePlanData?: boolean;\n\n  isMobile: boolean;\n\n  onSeatsChange?(seats: number): void;\n  onSeatsUpdate?(): void;\n  onCouponRemove?(): void;\n  onCouponChange?(coupon: string): void;\n}\n\ninterface State {\n  seats: string;\n  couponInput: string;\n  isSettingCoupon: boolean | null;\n  hasReceivedFetchStartFlag: boolean;\n}\n\nclass BillingPlanHeader extends React.PureComponent<Props, State> {\n  constructor(props: Props) {\n    super(props);\n    this.state = {\n      seats: props.seats.toString(),\n      isSettingCoupon: null,\n      couponInput: props.promoCode || '',\n      hasReceivedFetchStartFlag: false,\n    };\n  }\n\n  componentDidMount() {\n    // insert any coupon stored via params\n    if (this.props.initialParamsCoupon) {\n      this.handleCouponInputChange(this.props.initialParamsCoupon);\n    }\n  }\n\n  UNSAFE_componentWillReceiveProps(nextProps: Props) {\n    // HACK: since the component is re-mounted before the fetch starts, we need to\n    // wait for the fetch to start before we can figure out whether the coupon\n    // has been validated or not.\n    if (this.state.isSettingCoupon && nextProps.isLoadingCoupon) {\n      this.setState({ hasReceivedFetchStartFlag: true });\n    }\n    if (this.state.hasReceivedFetchStartFlag && !nextProps.isLoadingCoupon) {\n      this.setState({ isSettingCoupon: false, hasReceivedFetchStartFlag: false });\n    }\n\n    if (nextProps.seats !== this.props.seats && nextProps.seats.toString() !== this.state.seats) {\n      this.setState({ seats: nextProps.seats.toString() });\n    }\n  }\n\n  handleSeatsChange = (e: ChangeEvent<HTMLInputElement>) => {\n    e.preventDefault();\n    e.stopPropagation();\n    const newValue = parseInt(e.target.value, 10);\n    if (!isNaN(newValue) && newValue > 0) {\n      if (this.props.onSeatsChange) {\n        this.props.onSeatsChange(newValue);\n      }\n      this.setState({ seats: newValue.toString() });\n    } else if (e.target.value === '') {\n      this.setState({ seats: '' });\n    }\n  }\n\n  handleSeatsUpdate = () => {\n    if (this.props.onSeatsUpdate) {\n      this.props.onSeatsUpdate();\n    }\n  }\n\n  handleSeatsBlur = () => {\n    // reset seats if nothing specified\n    const seats = parseInt(this.state.seats, 10);\n    if (isNaN(seats)) {\n      this.setState({ seats: this.props.seats.toString() });\n    }\n  }\n\n  handlePromoChange = (coupon: string) => {\n    if (this.props.onCouponChange) {\n      // is resetting form to initial state - don't show anything\n      if (coupon.length === 0) {\n        this.setState({ isSettingCoupon: false });\n        return;\n      }\n      this.props.onCouponChange(coupon.toUpperCase());\n    }\n  }\n\n  handlePromoRemove = (e: React.MouseEvent<HTMLButtonElement>) => {\n    if (this.props.onCouponRemove) {\n      e.preventDefault();\n      e.stopPropagation();\n      this.props.onCouponRemove();\n      this.setState({ couponInput: '', isSettingCoupon: false });\n    }\n  }\n\n  handlePromoFetch = debounce((coupon: string) => this.handlePromoChange(coupon), 1500);\n\n  handleCouponInputChange = (input: ChangeEvent<HTMLInputElement>|string) => {\n    if (input && typeof input !== 'string') {\n      this.setState({ couponInput: input.target.value, isSettingCoupon: true });\n      this.handlePromoFetch(input.target.value);\n    } else {\n      this.setState({ couponInput: input, isSettingCoupon: true });\n      this.handlePromoFetch(input);\n    }\n  }\n\n  render() {\n    const {\n      monthlyCost, tier, seats, cycle, promoCodePercent, seatsUpdateButton, seatsEditable,\n      couponEditable, onCouponRemove, promoCode, initialParamsCoupon, isMobile, hidePlanData,\n    } = this.props;\n    const { isSettingCoupon, couponInput, hasReceivedFetchStartFlag } = this.state;\n\n    // show feedback logic for coupon fetching\n    const showCouponError =\n      promoCode\n      && couponInput.length > 0\n      && !promoCodePercent\n      && !isSettingCoupon;\n\n    const costPerUser = monthlyCost * (cycle === BillingCycle.Annual ? 12 : 1);\n    const cost = costPerUser * seats;\n    const discountedCost = promoCodePercent && cost * (1 - (promoCodePercent / 100)) || cost;\n    const isCouponEditable = couponEditable && !promoCodePercent;\n    const paramsCouponInvalid =\n      initialParamsCoupon\n      && promoCode\n      && !isSettingCoupon\n      && !hasReceivedFetchStartFlag\n      && !promoCodePercent;\n\n    return (\n      <div\n        className=\"billing-plan-header\"\n      >\n        {hidePlanData ? null : (\n          <div className=\"plan-overview\">\n            <div className=\"title\">\n              Polymail {titleForTier(tier)}\n            </div>\n            <div className={classnames('description', { editable: seatsEditable })}>\n              {!seatsEditable && seats}\n              {seatsEditable && <input\n                dir=\"auto\"\n                value={this.state.seats}\n                onChange={this.handleSeatsChange}\n                onBlur={this.handleSeatsBlur}\n                type=\"number\"\n              />}\n              {' '}users x {formatCurrencyCents(discountedCost)} per{' '}\n              {cycle === BillingCycle.Annual ? 'year' : 'month'}{' '}\n              {promoCodePercent && `(normally ${formatCurrencyCents(cost)})`}\n              {seatsUpdateButton &&\n                <button className=\"update-seats\" onClick={this.handleSeatsUpdate}>\n                  Update Seats\n                </button>\n              }\n            </div>\n          </div>\n        )}\n        <div className={classnames('cost-overview', isMobile && 'mobile')}>\n          {hidePlanData ? null : (\n            <CostLabel\n              cost={discountedCost}\n              timePeriod={cycle === BillingCycle.Annual ? 'yr' : 'mo'}\n            />\n          )}\n          {!isCouponEditable && promoCode && <div className=\"promo\">\n            <span className={classnames(paramsCouponInvalid && 'invalid')}>\n              {!paramsCouponInvalid ? 'promo: ' : 'promo not found: '}\n            </span>\n             {promoCode}\n            {promoCodePercent && ` (${promoCodePercent}% off 1 yr)`}\n            {(couponEditable || initialParamsCoupon) && onCouponRemove && <div>\n              <button onClick={this.handlePromoRemove}>remove code</button>\n            </div>}\n          </div>}\n          {isCouponEditable &&\n          <>\n            <div className=\"promo\">\n              {hidePlanData ? null : <span>promo: </span>}\n              <input\n                dir=\"auto\"\n                name=\"promo\"\n                value={this.state.couponInput}\n                onChange={this.handleCouponInputChange}\n                placeholder=\"Enter promo code\"\n                autoFocus\n              />\n            </div>\n            {!showCouponError && !isSettingCoupon\n              ? <div className=\"empty\" />\n              : <div className=\"coupon-loader\">\n                {isSettingCoupon && <Loader className=\"coupon\" />}\n                {showCouponError && <span>Coupon not found</span>}\n              </div>\n            }\n          </>\n          }\n        </div>\n      </div>\n    );\n  }\n}\n\nexport default BillingPlanHeader;\n"]}]}