SAAS,EAAC,WAAW;YACxB,gCAAQ,OAAO,EAAE,IAAI,CAAC,QAAQ,aAAmB;YACjD,gCAAM;YACN,gCAAQ,OAAO,EAAE,IAAI,CAAC,QAAQ,aAAmB;YACjD,gCAAQ,OAAO,EAAE,IAAI,CAAC,QAAQ,IAAG,IAAI,CAAC,KAAK,CAAC,SAAS,CAAC,CAAC,CAAC,QAAQ,CAAC,CAAC,CAAC,QAAQ,CAAU;YACrF,gCAAQ,OAAO,EAAE,IAAI,CAAC,KAAK,aAAmB,CAC1C,CACP,CAAC;IACJ,CAAC;IACH,sBAAC;AAAD,CAAC,AAvDD,CAA8B,KAAK,CAAC,aAAa,GAuDhD;AAMD,MAAM,CAAC,IAAM,uBAAuB,GAAqB,UAAC,KAAK;IAC7D,IAAI,iBAAiB,EAAE;QACrB,OAAO,CACL,6BAAK,SAAS,EAAC,kBAAkB;YAC/B,oBAAC,eAAe,OAAG;YACnB,6BAAK,SAAS,EAAC,KAAK,IAAE,KAAK,CAAC,QAAQ,CAAO,CACvC,CACP,CAAC;KACH;IACD,OAAO,KAAK,CAAC,QAAQ,CAAC;AACxB,CAAC,CAAC","sourcesContent":["import React from 'react';\nimport { isElectronWindows } from '@src/lib/electron';\n\ninterface WindowsTitleBarState {\n  maximized: boolean;\n}\n\nclass WindowsTitleBar extends React.PureComponent<{}, WindowsTitleBarState> {\n  constructor() {\n    super();\n\n    this.state = {\n      maximized: window.PMElectronBridge?.isMaximized ?\n        window.PMElectronBridge.isMaximized() : false,\n    };\n  }\n\n  componentDidMount() {\n    if (window.PMElectronBridge?.setWindowMaximizedHandler) {\n      window.PMElectronBridge.setWindowMaximizedHandler((maximized) => {\n        this.setState({\n          maximized,\n        });\n      });\n    }\n  }\n\n  minimize = () => {\n    if (window.PMElectronBridge?.minimizeWindow) {\n      window.PMElectronBridge?.minimizeWindow();\n    }\n  }\n\n  maximize = () => {\n    if (window.PMElectronBridge?.maximizeWindow) {\n      window.PMElectronBridge?.maximizeWindow();\n    }\n  }\n\n  close = () => {\n    if (window.PMElectronBridge?.closeWindow) {\n      window.PMElectronBridge?.closeWindow();\n    }\n  }\n\n  showMenu = (e: React.MouseEvent<HTMLButtonElement>) => {\n    if (window.PMElectronBridge?.showMenu) {\n      window.PMElectronBridge?.showMenu(e.clientX, e.clientY);\n    }\n  }\n\n  render() {\n    return (\n      <div className=\"title-bar\">\n        <button onClick={this.showMenu}>&#xE700;</button>\n        <div/>\n        <button onClick={this.minimize}>&#xE921;</button>\n        <button onClick={this.maximize}>{this.state.maximized ? '\\uE923' : '\\uE922'}</button>\n        <button onClick={this.close}>&#xE8BB;</button>\n      </div>\n    );\n  }\n}\n\ninterface Props {\n  children: any;\n}\n\nexport const WindowsTitleBarProvider: React.SFC<Props> = (props) => {\n  if (isElectronWindows) {\n    return (\n      <div className=\"electron-windows\">\n        <WindowsTitleBar />\n        <div className=\"app\">{props.children}</div>\n      </div>\n    );\n  }\n  return props.children;\n};\n"]}]}