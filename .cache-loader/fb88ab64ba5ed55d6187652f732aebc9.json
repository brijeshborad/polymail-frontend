AE,2BAA2B,CAAC,KAAK,CAAC,EACjD,UAAU,EAAE,KAAK,CAAC,QAAQ,CAAC,UAAU,EACrC,cAAc,EAAE,CAAC,KAAK,CAAC,WAAW,CAAC,mBAAmB,IACtD;AACJ,CAAC,CAAC;AAEF,IAAM,kBAAkB,GAAG,UAAC,QAAuB,IAAoB,OAAA,CAAC;IACtE,WAAW,EAAE,UAAC,EAAU,IAAK,OAAA,QAAQ,CAAC,aAAa,CAAC,EAAE,CAAC,CAAC,EAA3B,CAA2B;IACxD,OAAO,EAAE,cAAM,OAAA,QAAQ,CAAC,SAAS,CAAC,EAAE,CAAC,CAAC,EAAvB,CAAuB;IACtC,cAAc,EAAE,UAAC,MAAwB,IAAK,OAAA,QAAQ,CAAC,MAAM,CAAC,EAAhB,CAAgB;IAC9D,cAAc,EAAE,UAAA,QAAQ,IAAI,OAAA,QAAQ,CAAC,cAAc,CAAC,QAAQ,CAAC,CAAC,EAAlC,CAAkC;IAC9D,cAAc,EAAE,UAAA,QAAQ,IAAI,OAAA,QAAQ,CAAC,cAAc,CAAC,QAAQ,CAAC,CAAC,EAAlC,CAAkC;IAC9D,cAAc,EAAE,UAAA,QAAQ,IAAI,OAAA,QAAQ,CAAC,cAAc,CAAC,QAAQ,CAAC,CAAC,EAAlC,CAAkC;IAC9D,aAAa,EAAE,UAAA,IAAI,IAAI,OAAA,QAAQ,CAAC,qBAAqB,CAAC,IAAI,CAAC,CAAC,EAArC,CAAqC;IAC5D,gBAAgB,EAAE,UAAA,IAAI,IAAI,OAAA,QAAQ,CAAC,wBAAwB,CAAC,IAAI,CAAC,CAAC,EAAxC,CAAwC;IAClE,aAAa,EAAE,UAAA,IAAI,IAAI,OAAA,QAAQ,CAAC,aAAa,CAAC,IAAI,CAAC,CAAC,EAA7B,CAA6B;IACpD,YAAY,EAAE,UAAA,IAAI,IAAI,OAAA,QAAQ,CAAC,YAAY,CAAC,IAAI,CAAC,CAAC,EAA5B,CAA4B;IAClD,QAAQ,EAAE,UAAA,KAAK,IAAI,OAAA,QAAQ,CAAC,QAAQ,CAAC,KAAK,CAAC,CAAC,EAAzB,CAAyB;CAC7C,CAAC,EAZqE,CAYrE,CAAC;AAEH,eAAe,UAAU,CACvB,OAAO,CAAuB,eAAe,EAAE,kBAAkB,CAAC,CAAC,qBAAqB,CAAC,CAC1F,CAAC","sourcesContent":["import { connect, Dispatch } from 'react-redux';\nimport { RouteComponentProps } from 'react-router-dom';\nimport { withRouter } from 'react-router';\nimport { StoreState, Action } from '@src/types';\nimport MessageTemplateDetail, { Props, DispatchProps } from './MessageTemplateDetail';\nimport {\n  templateFetch,\n  selectTemplate,\n  templateCreate,\n  templateUpdate,\n  templateDelete,\n  templateAttachmentAdd,\n  templateAttachmentRemove,\n  templateShare,\n} from '@src/ducks/templates';\nimport {\n  selectAttachmentMetasForResource,\n} from '@src/ducks/drafts';\nimport { teamFetch, selectTeam } from '@src/ducks/team';\nimport { selectCalendarSchedulingUrl } from '@src/ducks/scheduling';\nimport { hotkeyUpdate } from '@src/ducks/hotkeys';\nimport { toastSet } from '@src/ducks/toasts';\n\nconst mapStateToProps = (state: StoreState, ownProps: Props & RouteComponentProps<any>):\n  Props & RouteComponentProps<any> => {\n\n  if (!ownProps.match) {\n    return ownProps;\n  }\n  const template = selectTemplate(state, ownProps.match.params.templateId);\n\n  return {\n    ...ownProps,\n    me: state.me.data,\n    template,\n    team: selectTeam(state),\n    teamMembers: state.teamMembers,\n    attachmentMetas: template && selectAttachmentMetasForResource(state, template.id) || [],\n    cloudAttachments: state.preferences.cloudAttachments && state.features.cloudAttachments,\n    features: state.features,\n    schedulingUrl: selectCalendarSchedulingUrl(state),\n    noBranding: state.features.noBranding,\n    useLinkPreview: !state.preferences.disableLinkPreviews,\n  };\n};\n\nconst mapDispatchToProps = (dispatch: Dispatch<any>): DispatchProps => ({\n  getTemplate: (id: string) => dispatch(templateFetch(id)),\n  getTeam: () => dispatch(teamFetch('')),\n  dispatchAction: (action: Action<any, any>) => dispatch(action),\n  createTemplate: template => dispatch(templateCreate(template)),\n  updateTemplate: template => dispatch(templateUpdate(template)),\n  deleteTemplate: template => dispatch(templateDelete(template)),\n  addAttachment: meta => dispatch(templateAttachmentAdd(meta)),\n  removeAttachment: meta => dispatch(templateAttachmentRemove(meta)),\n  shareTemplate: meta => dispatch(templateShare(meta)),\n  hotkeyUpdate: meta => dispatch(hotkeyUpdate(meta)),\n  setToast: error => dispatch(toastSet(error)),\n});\n\nexport default withRouter(\n  connect<Props, DispatchProps>(mapStateToProps, mapDispatchToProps)(MessageTemplateDetail),\n);\n"]}]}