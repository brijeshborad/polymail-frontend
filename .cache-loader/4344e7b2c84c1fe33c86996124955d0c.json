teAsanaTask = 'Create Asana Task from an Email',\n  ClickAsanaTask = 'Click on Task Created from an Email',\n  ClickBillingPlan = 'Click Any Billing Plan',\n  CommentOnEmail = 'Comment on Email',\n  ComposeNewEmail = 'Compose New Email',\n  CompleteUserProfile = 'Complete User Profile',\n  CopyZoomUrl = 'Copy Zoom URL',\n  CreatePassword = 'Create Password From OAuth',\n  ConnectAnEmailAccount = 'Connect An Email Account',\n  DownloadNativeApp = 'Download Native App',\n  LoadHubspotSidebar = 'Loaded Hubspot Sidebar',\n  MentionTeamOnEmail = 'Team Member Mention In Comment',\n  MakeDefault = 'Make Default Email',\n  ReferFriend = 'Refer a Friend',\n  OpenApp = 'Opened App',\n  SaveMessageTemplate = 'Template Created',\n  SendCalendarInvite = 'Calendar Invite Created',\n  SendCampaign = 'Schedule or Send a Campaign',\n  SendEmail = 'Send Email',\n  Subscribe = 'Subscribe - Click Pay Now',\n  SendEmailLater = 'Schedule Email for Delivery',\n  SendInviteToTeamMember = 'Send Invite to Team Member',\n  SelectUpgradePlan = 'Select Plan to Upgrade',\n  PlanSelected = 'Plan Selected', // choose plan when signing up\n  SetFollowUpReminder = 'Set Follow Up Reminder',\n  ShareCalendarMeetingTime = 'Shared Calendar Meeting Times',\n  ShareEmailViaSlack = 'Share Email to Slack',\n  ShareTemplateWithTeam = 'Template Shared',\n  StartNewSequence = 'Start New Sequence',\n  SignUpStepOne = 'Completed Sign Up Step One',\n  TourStepOne = 'Tour Step One',\n  TourStepOneSend = 'Learn Step One - Sending Onboarding Test Email',\n  TourStepTwo = 'Learn Step Two - Set Send Later',\n  TourStepThree = 'Learn Step Three - Set Follow Up',\n  TourStepFour = 'Learn Step Four - Set a Template',\n  TourStepFive = 'Learn Step Five - Create a Template',\n  TourStepSix = 'Learn Step Six - Move Up or Down in Inbox',\n  TourStepSeven = 'Learn Step Seven - Move To Archive to Keyboard Shortcut',\n  TourStepEight = 'Learn Step Eight - Undo with Keyboard Shortcut',\n  TourStepNine = 'Learn Step Nine - Read Later',\n  TourStepTen = 'Learn Step Ten - Share Conversation with Command Center',\n  TourStepEleven = 'Learn Step Eleven - Save a Comment',\n  TourStepTwelve = 'Learn Step Twelve - Share Meeting Times',\n  TourStepThirteen = 'Learn Step Thirteen - Click Download Desktop Client Button',\n  TourStepFourteen = 'Learn Step Fourteen - Click Claim Credit Button',\n  TurnOnDesktopNotifications = 'Turn On Desktop Notifications',\n  Unsubscribe = 'Click Unsubscribe Link',\n  UpgradePlan = 'Select Plan to Upgrade',\n  UnleashExperiment = 'Unleash Experiment',\n  ViewActivityFeed = 'Activity Feed View',\n  ViewCalendarMeetingTimes = 'View Calendar Meeting Times',\n  ViewCommandCenter = 'View Command Center',\n  ViewTeamMentions = 'View Team Mentions',\n  ViewTemplates = 'View Templates',\n  ViewSharedByMe = 'View Shared By Me',\n  ViewMessageTemplates = 'View Message Templates',\n  ViewMeetingTimes = 'View Meeting Times',\n  ViewAnalytics = 'View Team Analytics',\n  ViewInvitePeople = 'View Invite People',\n  ViewPlans = 'View Plans',\n  ViewUnsubscriber = 'View Unsubscriber',\n  ViewReferFriends = 'View Refer Friends',\n}\n\ntype EventDetails = {\n  skipped?: boolean;\n  details?: string | { [key: string]: any };\n};\n\nconst configureAmplitudeUser = (user: UserData | undefined) => {\n  const key = process.env.AMPLITUDE_API_KEY;\n  if (key) {\n    amplitude.getInstance().init(key);\n  }\n\n  if (user) {\n    amplitude.getInstance().setUserId(user.email);\n\n    const identify = new amplitude\n      .Identify()\n      .set('created', user.created)\n      .set('firstName', user.firstName)\n      .set('id', user.id)\n      .set('lastName', user.lastName)\n      .set('plan', user.plan)\n      .set('team', user.team);\n\n    amplitude.getInstance().identify(identify);\n    amplitude.getInstance().logEvent(AmplitudeEvents.OpenApp);\n\n  } else {\n    amplitude.getInstance().setUserId(null);\n  }\n};\n\nconst logAmplitudeEvent = (event: AmplitudeEvents, details: EventDetails = {}) => {\n  // Only log events if Amplitude is configured.\n  if (process.env.AMPLITUDE_API_KEY) {\n    amplitude.getInstance().logEvent(event, details);\n  }\n};\n\nexport { logAmplitudeEvent, AmplitudeEvents, configureAmplitudeUser };\n"]}]}