AI,EAAE,IAAI,CAAC,OAAO;KACnB,CAAC,CAAC,EAH2C,CAG3C;IACH,YAAY,EAAE,UAAA,YAAY,IAAI,OAAA,QAAQ,CAAC,YAAY,CAAC,YAAY,CAAC,CAAC,EAApC,CAAoC;CACnE,CAAC,EAX4E,CAW5E,CAAC;AAEH,eAAe,OAAO,CAAC,eAAe,EAAE,kBAAkB,CAAC,CAAC,OAAO,CAAC,CAAC","sourcesContent":["import { connect } from 'react-redux';\nimport { Dispatch } from 'redux';\n\nimport { StoreState, Action, Partial, OnboardingType } from '@src/types';\nimport Threads, { Props, DispatchProps } from './Threads';\nimport { threadCommitsSubmit, selectedIdsUpdate } from '@src/ducks/threads';\nimport { selectUnsubscribeLink } from '@src/ducks/messageBodies';\nimport { selectDraft, selectDraftForThread, selectSendingDrafts } from '@src/ducks/drafts';\nimport { contactSelect } from '@src/ducks/contacts';\nimport { selectMailboxCounts } from '@src/ducks/mailCounts';\nimport { selectShareLink } from '@src/ducks/shareLinks';\nimport { selectAccounts } from '@src/ducks/accounts';\nimport { selectUserInitialSyncFinished, selectOnboardingShow, selectRsbOnboarding } from '@src/ducks/me';\nimport { selectUpgradePrompt } from '@src/ducks/upgradePrompt';\nimport { permissionsBannerClose } from '@src/ducks/permissions';\nimport { rightSidebarCollapse } from '@src/ducks/rightSidebar';\nimport { hotkeyUpdate } from '@src/ducks/hotkeys';\nimport { Page } from '@src/types/rightSidebar';\nimport { selectSharedThreads } from '@src/ducks/sharedThreads';\nimport { selectCalendarSchedulingUrl } from '@src/ducks/scheduling';\nimport { SharedThreadFilterMode } from '@src/types/sharedThreads';\nimport { selectBlockFilter } from '@src/ducks/filters';\nimport { last } from 'lodash';\n\nconst mapStateToProps = (state: StoreState, ownProps: Props): Partial<Props> => ({\n  accounts: selectAccounts(state),\n  draft: state.selectedDraft\n    ? selectDraft(state, state.selectedDraft).data\n    : state.selectedThread && selectDraftForThread(state, state.selectedThread.id).data,\n  sendingDraft: last(selectSendingDrafts(state)),\n  contactGroups: state.contactGroups,\n  threads: state.mailboxThreads,\n  selectedMailbox: state.mailbox,\n  selectedThread: state.selectedThread,\n  sessionId: state.sessionId,\n  selectedContact: state.selectedContact,\n  commandControlType: state.commandControlType,\n  selectedIds: state.selectedIds,\n  mailCount: selectMailboxCounts(state),\n  unsubscribeLink: state.selectedThread && selectUnsubscribeLink(state, state.selectedThread.id),\n  preferences: state.preferences,\n  me: state.me,\n  shareLink: selectShareLink(state),\n  initialSyncing: !selectUserInitialSyncFinished(state),\n  upgradePrompts: selectUpgradePrompt(state),\n  closedBanners: state.closedBanners,\n  rightSidebarCollapsed: state.rightSidebar.collapsed.threads,\n  rightSidebarTab: state.rightSidebar.tab,\n  selectedAccount: state.leftSidebar.selectedAccount,\n  hotkeyActions: state.hotkeyActions,\n\n  showCommandCenterOnboarding: false,\n  showCompleteOnboarding: false,\n  showCreateDraftOnboarding: false,\n  showInviteTeamOnboarding: false,\n  showMentionsTabOnboarding: selectOnboardingShow(state, OnboardingType.MentionsTab) &&\n    !!selectSharedThreads(state, { mode: SharedThreadFilterMode.Unread }).length,\n  showExploreModal: !state.onboarding.exploreModal,\n  schedulingUrl: selectCalendarSchedulingUrl(state),\n  blockFilter: state.selectedThread && selectBlockFilter(state, state.selectedThread.id),\n  features: state.features,\n  rsbOnboardingType: selectRsbOnboarding(state),\n  showTeamInviteJoinedPrompt: state.showTeamInviteAcceptedModal,\n});\n\nconst mapDispatchToProps = (dispatch: Dispatch<StoreState>): DispatchProps => ({\n  dispatchAction: (action: Action<any, any>) => dispatch(action),\n  selectContact: (email: string) => dispatch(contactSelect(email)),\n  submitCommitMeta: commit => dispatch(threadCommitsSubmit(commit)),\n  updateSelections: selections => dispatch(selectedIdsUpdate(selections)),\n  closeBanner: banner => dispatch(permissionsBannerClose(banner)),\n  collapseRightSidebar: (collapsed: boolean) => dispatch(rightSidebarCollapse({\n    collapsed,\n    page: Page.Threads,\n  })),\n  updateHotkey: hotkeyAction => dispatch(hotkeyUpdate(hotkeyAction)),\n});\n\nexport default connect(mapStateToProps, mapDispatchToProps)(Threads);\n"]}]}