K,EAAE,EAAE,IAAI,KAAK,CAAC,MAAM,CAAC,EAAE,CAAC;4BAC7B,QAAQ,EAAE,EAAE;gCACV,EAAE,CAAC,aAAa,CAAC,UAAU,CAAC,OAAO,CAAC,MAAM,CAAC,CAAC;gCAC5C,EAAE,CAAC,cAAc,CAAC,QAAQ,CAAC,OAAO,CAAC,MAAM,CAAC,CAAC;4BAC7C,KAAK,EAAE,EAAE,IAAI,UAAU,CAAC,OAAO,CAAC,MAAM,CAAC,CAAC,MAAM,CAAC,EAAE,CAAC;4BAClD,GAAG,EAAE,EAAE,IAAI,QAAQ,CAAC,OAAO,CAAC,MAAM,CAAC,CAAC,MAAM,CAAC,EAAE,CAAC;yBAC/C,CAAC,eACS,EAAE,IAAI,EAAE,CAAC,MAAM,CAAC,YAAY,CAAC,EACxC,GAAG,EAAE,CAAC,EACN,OAAO,EAAE,KAAI,CAAC,eAAe;wBAE7B,6BACE,SAAS,EAAE,UAAU,CAAC;gCACpB,KAAK,EAAE,EAAE,IAAI,KAAK,CAAC,MAAM,CAAC,EAAE,CAAC;6BAC9B,CAAC,IAED,EAAE,CAAC,CAAC,CAAC,EAAE,CAAC,IAAI,EAAE,CAAC,CAAC,CAAC,SAAS,CACvB,CACC,CACV,CAAC;gBACJ,CAAC,CAAC,CACE,CACF,CACF,CACP,CAAC;IACJ,CAAC;IACH,gBAAC;AAAD,CAAC,AArHD,CAAwB,KAAK,CAAC,aAAa,GAqH1C;AAED,eAAe,SAAS,CAAC","sourcesContent":["import React from 'react';\nimport classnames from 'classnames';\nimport moment, { Moment } from 'moment';\nimport Arrow from '@src/components/Icons/Arrow.svg';\n\nimport './MonthView.scss';\n\ninterface Props {\n  startDate: Moment;\n  endDate: Moment;\n  onDateClick: (newStartDate: Moment) => void;\n}\n\ninterface State {\n  month: Moment;\n}\n\nclass MonthView extends React.PureComponent<Props, State> {\n  private weekdays: string[];\n  private dateRange: Array<Moment|null>;\n\n  constructor(props: Props) {\n    super(props);\n\n    this.weekdays = moment.weekdays();\n\n    const month = props.startDate.clone().startOf('month');\n    this.dateRange = this.generateDateRange(month);\n\n    this.state = {\n      month,\n    };\n  }\n\n  generateDateRange = (month: Moment) => {\n    const dateRange = [];\n    const monthDate = month.clone().startOf('month');\n    const nextMonth = monthDate.clone().add(1, 'month').startOf('month');\n\n    for (let i = 0; i < monthDate.weekday(); i += 1) {\n      dateRange.push(null);\n    }\n\n    while (monthDate < nextMonth) {\n      dateRange.push(monthDate.clone());\n      monthDate.add(1, 'day');\n    }\n\n    return dateRange;\n  }\n\n  handleDateClick = (e: React.SyntheticEvent<HTMLElement>) => {\n    const { onDateClick } = this.props;\n    const target = e.currentTarget as HTMLElement;\n    const date = target.dataset && target.dataset.date;\n    const dateMoment = moment(date);\n\n    if (dateMoment.isValid()) {\n      onDateClick(dateMoment);\n    }\n  }\n\n  previousMonth = () => {\n    const { month } = this.state;\n    const previousMonth = month.clone().subtract(1, 'month');\n    this.dateRange = this.generateDateRange(previousMonth);\n\n    this.setState({ month: previousMonth });\n  }\n\n  nextMonth = () => {\n    const { month } = this.state;\n    const nextMonth = month.clone().add(1, 'month');\n    this.dateRange = this.generateDateRange(nextMonth);\n\n    this.setState({ month: nextMonth });\n  }\n\n  render() {\n    const { startDate, endDate } = this.props;\n    const { month } = this.state;\n    const startClone = startDate.clone();\n    const endClone = endDate.clone();\n    const today = moment().startOf('day');\n\n    return (\n      <div className=\"month-view\">\n        <div className=\"month\">\n          <button onClick={this.previousMonth}>\n            <Arrow className=\"icon left-arrow\" />\n          </button>\n          <span className=\"label\">{month.format('MMM YYYY')}</span>\n          <button onClick={this.nextMonth}>\n            <Arrow className=\"icon right-arrow\" />\n          </button>\n        </div>\n        <div className=\"dates\">\n          <div className=\"weekdays date-container\">\n            {this.weekdays.map((w, i) => (\n              <div className=\"weekday date\" key={i}>{w[0].toUpperCase()}</div>\n            ))}\n          </div>\n          <div className=\"month-dates date-container\">\n            {this.dateRange.map((dr, i) => {\n              return (\n                <button\n                  className={classnames('date', {\n                    past: dr && today.isAfter(dr),\n                    today: dr && today.isSame(dr),\n                    selected: dr &&\n                      dr.isSameOrAfter(startClone.startOf('date')) &&\n                      dr.isSameOrBefore(endClone.startOf('date')),\n                    start: dr && startClone.startOf('date').isSame(dr),\n                    end: dr && endClone.startOf('date').isSame(dr),\n                  })}\n                  data-date={dr && dr.format('YYYY-MM-DD')}\n                  key={i}\n                  onClick={this.handleDateClick}\n                >\n                  <div\n                    className={classnames({\n                      today: dr && today.isSame(dr),\n                    })}\n                  >\n                    {dr ? dr.date() : undefined}\n                  </div>\n                </button>\n              );\n            })}\n          </div>\n        </div>\n      </div>\n    );\n  }\n}\n\nexport default MonthView;\n"]}]}