tachments: [],\n    isAllDay: false,\n    calendar: createCalendar({}),\n    ...partial,\n  };\n};\n\nexport const createEvent = (partial?: Partial<EventType>) => {\n  return {\n    name: 'name',\n    notes: 'this is notes',\n    recipients: [],\n    location: 'this is a location',\n    ...partial,\n  };\n};\n\nexport const createTimeBlock = (partial?: Partial<TimeBlock>) => {\n  return {\n    startTime: moment(new Date('2017-09-27T05:20:31.216Z')),\n    endTime: moment(new Date('2017-09-27T06:20:31.216Z')),\n    event: createEvent(),\n    ...partial,\n  };\n};\n\nexport const convertContactToEventRecipient = (contact: Contact): EventRecipient => {\n  return {\n    type: EventRecipientType.Contact,\n    displayName: contact.fullName,\n    identifier: contact.id,\n  };\n};\n"]}]}