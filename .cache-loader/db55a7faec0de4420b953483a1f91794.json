ErrorPage = (props: Props) => {\n  const loggedIn = !!props.email;\n  const onHelpClick = () => {\n    if (!loggedIn) {\n      browserHistory.push('/login');\n      return;\n    }\n\n    window.open('http://help.polymail.io/features/shared-threads', '_blank');\n  };\n\n  const helpButtonCopy = !loggedIn\n    ? 'Sign in'\n    : 'Learn why';\n\n  return (\n    <div className=\"shared-thread-error-page\">\n      <div className=\"main\">\n        <div className=\"emoji\">\n          ðŸ¤”\n      </div>\n        <h3>It looks like you don't have access to this conversation.</h3>\n        <button\n          className=\"help-button\"\n          onClick={onHelpClick}\n        >\n          {helpButtonCopy}\n        </button>\n        {props.email &&\n          <div className=\"description\">\n            You're currently signed in as {props.email}\n          </div>\n        }\n        {!props.email &&\n          <div className=\"description\">\n            Try signing in to view this conversation.\n          </div>\n        }\n      </div>\n    </div>\n  );\n};\n\nexport default SharedThreadErrorPage;\n"]}]}