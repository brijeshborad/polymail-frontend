H,4BAAC;AAAD,CAAC,AAjCD,CAAoC,KAAK,CAAC,aAAa,GAiCtD;AAED,eAAe,qBAAqB,CAAC","sourcesContent":["import React from 'react';\nimport Popover from '@src/components/Popover/Popover';\nimport browserHistory from '@src/lib/browserHistory';\nimport './TeamDisclosurePopover.scss';\n\ninterface State {\n  top: number;\n}\n\nclass TeamDisclosurePopover extends React.PureComponent<{}, State> {\n  popover: Popover;\n  constructor() {\n    super();\n\n    this.state = { top: 0 };\n  }\n\n  handleRef = (ref: Popover) => {\n    this.popover = ref;\n  }\n\n  show = (top: number) => {\n    this.setState({ top }, this.popover.show);\n  }\n\n  goToSettings = () => {\n    browserHistory.push('/settings/team');\n  }\n\n  render() {\n    const { top } = this.state;\n\n    return (\n      <Popover\n        ref={this.handleRef}\n        className=\"team-disclosure-popover\"\n        style={{ top }}\n      >\n        <button className=\"settings\" onClick={this.goToSettings}>Team Settings</button>\n      </Popover>\n    );\n  }\n}\n\nexport default TeamDisclosurePopover;\n"]}]}