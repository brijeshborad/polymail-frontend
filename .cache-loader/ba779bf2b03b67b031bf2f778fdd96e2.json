YAAY,KAAK,IAAI,CAAC,KAAK,CAAC,KAAK,CAAC,CAAC;IAChE,CAAC;IA+CD,4BAAM,GAAN;QACQ,IAAA,eAA0E,EAAxE,8BAAY,EAAE,kBAAM,EAAE,kCAAc,EAAE,kBAAM,EAAE,4BAA0B,CAAC;QAEjF,OAAO,CACL,6BACE,SAAS,EAAC,cAAc,EACxB,SAAS,EAAE,IAAI,CAAC,aAAa,EAC7B,KAAK,EAAE;gBACL,MAAM,QAAA;aACP;YAED,6BAAK,SAAS,EAAC,OAAO;gBACnB,cAAc,CAAC,CAAC,CAAC,oBAAC,OAAO,IAAC,KAAK,EAAC,MAAM,EAAC,MAAM,EAAC,MAAM,GAAG,CAAC,CAAC,CAAC,oBAAC,MAAM,OAAG;gBACrE,+BACE,GAAG,EAAC,MAAM,EACV,GAAG,EAAE,IAAI,CAAC,SAAS,EACnB,GAAG,EAAC,OAAO,EACX,IAAI,EAAC,QAAQ,EACb,WAAW,EAAE,WAAW,IAAI,QAAQ,EACpC,YAAY,EAAE,YAAY,EAC1B,OAAO,EAAE,IAAI,CAAC,aAAa,EAC3B,MAAM,EAAE,MAAM,EACd,QAAQ,EAAE,IAAI,CAAC,iBAAiB,GAChC,CACE,CACF,CACP,CAAC;IACJ,CAAC;IACH,kBAAC;AAAD,CAAC,AAjKD,CAA0B,KAAK,CAAC,SAAS,GAiKxC;AAED,eAAe,WAAW,CAAC","sourcesContent":["import React from 'react';\nimport { debounce } from 'lodash';\n\nimport { KeyCode } from '@src/types';\nimport Search from '@src/components/Icons/Search.svg';\nimport Spinner from '@src/components/Icons/Spinner.svg';\nimport './SearchInput.scss';\n\ninterface Props {\n  search?: string;\n  height?: number;\n  loadingResults?: boolean;\n  defaultValue?: string;\n  searchOnKeyDown?: boolean;\n  shouldFocus?: boolean;\n  placeholder?: string;\n\n  onSearch: (query: string, complete: boolean) => void;\n  onFocus?: () => void;\n  onBlur?: () => void;\n  onKeyDown?: (e: React.KeyboardEvent<HTMLElement>) => void;\n  onClear?: () => void;\n}\n\nclass SearchInput extends React.Component<Props> {\n  private input: HTMLInputElement;\n  private timeout: NodeJS.Timer;\n  private focusDebounce: any;\n\n  constructor(props: Props) {\n    super();\n\n    // Create focus debounce\n    if (props.onFocus) {\n      this.focusDebounce = debounce(\n        props.onFocus,\n        150,\n        { trailing: true },\n      );\n    }\n  }\n\n  componentDidMount() {\n    if (this.input) {\n      this.input.addEventListener('search', this.handleSubmit);\n    }\n  }\n\n  componentWillUnmount() {\n    if (this.timeout) {\n      clearTimeout(this.timeout);\n    }\n\n    if (this.input) {\n      this.input.removeEventListener('search', this.handleSubmit);\n    }\n\n    if (this.focusDebounce) {\n      this.focusDebounce.cancel();\n    }\n  }\n\n  focus(force: boolean = false) {\n    if (force && this.input) {\n      this.input.focus();\n      return;\n    }\n\n    if (document.activeElement === this.input) {\n      this.timeout = global.setTimeout(\n        () => {\n          this.input.focus();\n        },\n        0,\n      );\n    }\n  }\n\n  blur(force: boolean = false) {\n    if (force && this.input) {\n      this.input.blur();\n      return;\n    }\n\n    if (document.activeElement === this.input) {\n      this.timeout = global.setTimeout(\n        () => {\n          this.input.blur();\n        },\n        0,\n      );\n    }\n  }\n\n  UNSAFE_componentWillReceiveProps(nextProps: Props) {\n    if (nextProps.shouldFocus) {\n      this.focus();\n    }\n\n    const nextValue = nextProps.defaultValue || '';\n    if (this.input && nextValue !== this.props.defaultValue) {\n      this.input.value = nextValue;\n    }\n  }\n\n  shouldComponentUpdate(nextProps: Props) {\n    return nextProps.onSearch !== this.props.onSearch || nextProps.onFocus !== this.props.onFocus ||\n      nextProps.height !== this.props.height ||\n      nextProps.loadingResults !== this.props.loadingResults ||\n      (this.input && nextProps.defaultValue !== this.input.value);\n  }\n\n  handleInputChange = () => {\n    const { onSearch, searchOnKeyDown } = this.props;\n\n    onSearch(this.input.value, searchOnKeyDown || false);\n  }\n\n  handleKeyDown = (e: React.KeyboardEvent<HTMLElement>) => {\n    const { onKeyDown } = this.props;\n\n    // Do not do special handling for ForwardSlash\n    if (e.keyCode === KeyCode.ForwardSlash) {\n      e.stopPropagation();\n      return;\n    }\n\n    if (onKeyDown) {\n      onKeyDown(e);\n    } else if (KeyCode.Enter === e.keyCode) {\n      this.props.onSearch(this.input.value, true);\n    }\n  }\n\n  handleRef = (ref: HTMLInputElement) => {\n    this.input = ref;\n  }\n\n  handleSubmit = () => {\n    const { onClear, onBlur } = this.props;\n    if (onClear && onBlur && this.input.value === '') {\n\n      // Immediately cancel focus behavior, clear and set onBlur\n      if (this.focusDebounce) {\n        this.focusDebounce.cancel();\n      }\n\n      onClear();\n      this.timeout = global.setTimeout(\n        () => {\n          onBlur();\n        },\n        0,\n      );\n    }\n  }\n\n  render() {\n    const { defaultValue, height, loadingResults, onBlur, placeholder } = this.props;\n\n    return (\n      <div\n        className=\"search-input\"\n        onKeyDown={this.handleKeyDown}\n        style={{\n          height,\n        }}\n      >\n        <div className=\"inner\">\n          {loadingResults ? <Spinner width=\"14px\" height=\"14px\" /> : <Search />}\n          <input\n            dir=\"auto\"\n            ref={this.handleRef}\n            key=\"input\"\n            type=\"search\"\n            placeholder={placeholder || 'Search'}\n            defaultValue={defaultValue}\n            onFocus={this.focusDebounce}\n            onBlur={onBlur}\n            onChange={this.handleInputChange}\n          />\n        </div>\n      </div>\n    );\n  }\n}\n\nexport default SearchInput;\n"]}]}