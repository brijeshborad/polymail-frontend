 (type) {\n    // failed payment\n    case MeStatusNew.Unpaid:\n      return (\n        <div className={classnames('disabled-user banner', isMobile && 'mobile')}>\n          <span>Your last subscription payment failed.</span>\n          <span>\n            <Link to=\"/settings/billing/update\">\n              Click here\n            </Link>\n          </span>\n          <span>to add a new payment method</span>\n        </div>\n      );\n\n    // canceled\n    case MeStatusNew.Canceled:\n      return (\n        <div className={classnames('disabled-user banner light', isMobile && 'mobile')}>\n          <span>Welcome back. </span>\n          <span>\n            <Link to=\"/settings/billing\">\n              Click here\n            </Link>\n          </span>\n          <span>to select a plan to continue using Polymail.</span>\n        </div>\n      );\n\n    default:\n      return null;\n  }\n};\n\nexport default Disabled;\n"]}]}