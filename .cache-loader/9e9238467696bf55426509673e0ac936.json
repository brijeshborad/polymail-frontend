act';\nimport Popover from '@src/components/Popover/Popover';\nimport logo from '@src/routes/Login/Icons/PolymailLogo@3x.png';\nimport './CommentPopover.scss';\n\ninterface Props {\n  onHide: () => void;\n}\n\n// TODO(SHIN): Implement in SharedThreadsV2 when request to join is implemented API side\nclass CommentPopover extends React.PureComponent<Props> {\n  popover: Popover;\n\n  componentDidMount() {\n    this.popover.show();\n  }\n\n  handleRef = (ref: Popover) => {\n    this.popover = ref;\n  }\n\n  render() {\n    const { onHide } = this.props;\n\n    return (\n      <Popover\n        className=\"comment-popover\"\n        ref={this.handleRef}\n        overlay\n        persistent\n      >\n        <div className=\"main\">\n          <div className=\"heading\">\n            Join Awesome Company on Polymail\n          </div>\n          <div className=\"container\">\n            <div className=\"logo-container\">\n              <img src={logo} className=\"logo\" />\n            </div>\n            <div className=\"instructions\">\n              Join Sarah and Awesome Company on Polymail to comment on this conversation.\n            </div>\n            <div className=\"actions\">\n              <button className=\"request-button\">\n                Request to Join\n              </button>\n              <button className=\"cancel-button\" onClick={onHide}>\n                Cancel\n              </button>\n            </div>\n          </div>\n        </div>\n      </Popover>\n    );\n  }\n}\n\nexport default CommentPopover;\n"]}]}