,CAAC,aAAa,IAAI,CAC9B,gCACE,SAAS,EAAC,eAAe,EACzB,OAAO,EAAE,IAAI,CAAC,YAAY,cAGnB,CACV;oBACA,CAAC,QAAQ,IAAI,CAAC,aAAa,IAAI,CAC9B,6BACE,SAAS,EAAC,eAAe;;wBAGzB,gCAAQ,SAAS,EAAC,cAAc,EAAC,OAAO,EAAE,IAAI,CAAC,WAAW,aAAiB,CACvE,CACP;oBACA,QAAQ,IAAI,CAAC,aAAa,IAAI,EAAE,IAAI,CACnC,6BACE,SAAS,EAAC,kBAAkB;;wBAEf,+BAAI,EAAE,aAAF,EAAE,uBAAF,EAAE,CAAE,KAAK,CAAK;wBAAA,+BAAK;;wBACpC,gCAAQ,SAAS,EAAC,iBAAiB,EAAC,OAAO,EAAE,IAAI,CAAC,aAAa,eAAmB,CAC9E,CACP,CACG,CACF,CACF,CACP,CAAC;IACJ,CAAC;IACH,kBAAC;AAAD,CAAC,AAnQD,CAA0B,KAAK,CAAC,aAAa,GAmQ5C;AAED,eAAe,WAAW,CAAC","sourcesContent":["import React from 'react';\nimport classnames from 'classnames';\n\nimport { Me, Resource, ResourceStatus, TeamInviteStatus } from '@src/types';\nimport PolymailLogo from '@src/routes/Login/Icons/PolymailLogo@3x.png';\nimport { TeamInviteValidateResponse } from '@src/api/team';\nimport { TeamInviteValidateMeta } from '@src/ducks/teamMembers';\nimport { RouteComponentProps } from 'react-router-dom';\nimport Confirm, { Props as ConfirmProps } from '@src/components/Base/Confirm';\nimport browserHistory from '@src/lib/browserHistory';\nimport CenteredLoader from '@src/components/Loader/CenteredLoader';\nimport Loader from '@src/components/Loader/Loader';\nimport Toast from '@src/components/Base/Toast';\n\nimport './TeamInvites.scss';\n\nexport interface StateProps {\n  loggedIn?: boolean;\n  me?: Me;\n  invite: Resource<TeamInviteValidateResponse>;\n  toast?: Error;\n}\n\nexport interface DispatchProps {\n  acceptInvite: (invite: TeamInviteValidateMeta) => void;\n  validateInvite: (invite: TeamInviteValidateMeta) => void;\n  signout: () => void;\n}\n\nexport interface RouteProps {\n  nonce: string;\n  payload: string;\n}\n\ninterface State {\n  confirmProps?: ConfirmProps;\n  disableAcceptButton?: boolean;\n}\n\nclass TeamInvites extends React.PureComponent<\n  StateProps & RouteComponentProps<RouteProps> & DispatchProps, State\n> {\n\n  constructor(props: StateProps & RouteComponentProps<RouteProps>) {\n    super();\n    this.state = {};\n  }\n\n  componentDidMount() {\n    const { invite, validateInvite } = this.props;\n\n    // Trigger invite validation\n    if (invite.status === ResourceStatus.Pending) {\n      validateInvite({\n        nonce: this.props.match.params.nonce,\n        payload: this.props.match.params.payload,\n      });\n    }\n  }\n\n  componentDidUpdate(oldProps: StateProps) {\n    const { disableAcceptButton } = this.state;\n    const { toast } = this.props;\n\n    // Handle resetting button state when toast appears\n    if (!!toast && disableAcceptButton) {\n      this.setState({ disableAcceptButton: undefined });\n    }\n  }\n\n  handleLogin = (e: React.MouseEvent<any>) => {\n    const { nonce, payload } = this.props.match.params;\n    e.stopPropagation();\n    e.preventDefault();\n    browserHistory.push(`/login/team/${nonce}/${payload}`);\n  }\n\n  handleSignout = (e: React.MouseEvent<any>) => {\n    const { signout } = this.props;\n    e.stopPropagation();\n    e.preventDefault();\n    signout();\n  }\n\n  handleSignup = (e: React.MouseEvent<any>) => {\n    const { nonce, payload } = this.props.match.params;\n    e.stopPropagation();\n    e.preventDefault();\n    browserHistory.push(`/login/signup/team/${nonce}/${payload}`);\n  }\n\n  handleLogoClick = () => {\n    window.location.href = window.location.origin;\n  }\n\n  handleExpiredClick = (e: React.MouseEvent<any>) => {\n    e.stopPropagation();\n    e.preventDefault();\n    window.location.href = window.location.origin;\n  }\n\n  handleAccept = () => {\n    const { nonce, payload } = this.props.match.params;\n    const { acceptInvite } = this.props;\n\n    const confirmProps: ConfirmProps = {\n      title: 'Accept this invitation?',\n      message: 'Please confirm that you would like to join this team. Once you join the team, your billing and features will be managed by the team\\'s owner.',\n      onConfirm: () => {\n        this.setState(\n          {\n            confirmProps: undefined,\n            disableAcceptButton: true,\n          },\n          () => { // Handle accepting the invitation\n            acceptInvite({\n              nonce, payload,\n            });\n          });\n      },\n      onCancel: () => {\n        this.setState({\n          confirmProps: undefined,\n        });\n      },\n    };\n\n    this.setState({\n      confirmProps,\n    });\n  }\n\n  renderLoading = () => {\n    return (\n      <div className=\"team-invites-app\">\n        <div className=\"header\">\n          <img onClick={this.handleLogoClick} src={PolymailLogo} alt=\"\" />\n        </div>\n        <div className=\"container\">\n          <CenteredLoader />\n        </div>\n      </div>\n    );\n  }\n\n  // TODO(SHIN): Get the tier that the team is on?\n  render() {\n    const { me, invite, loggedIn } = this.props;\n    const { confirmProps, disableAcceptButton } = this.state;\n\n    // tslint:disable-next-line:max-line-length\n    let description = (\n      <span className=\"info\">\n        By joining <b>{invite.data?.teamName}&nbsp;</b>\n        you'll automatically be upgraded to their plan which includes:\n      </span>\n    );\n\n    const features = [\n      'Consolidated billing for your team',\n      'Comments and @mentions to share conversations with your teammates',\n      'Access to the team\\'s shared templates and sequences',\n      'All team features and shared resources under the team\\'s plan',\n    ];\n\n    // Set title\n    let title = `You've been invited to join ${invite.data?.teamName || 'a team'} on Polymail!`;\n    const inviteExpired = invite.data?.status === TeamInviteStatus.Expired ||\n      invite.status === ResourceStatus.Failure;\n    const inviteAccepted = invite.data?.status === TeamInviteStatus.Accepted;\n\n    // Alternative display for invite failed\n    if (inviteExpired) {\n      title = 'Your invitation to this team has expired';\n      description = (\n        <span className=\"info\">\n          {`Please request a new invitation from the team's administrator`}\n        </span>\n      );\n    }\n\n    // Alternative display for invite accepted\n    if (inviteAccepted) {\n      title = 'This invitation has already been accepted.';\n      description = (\n        <span className=\"info\">\n          {`Please request another invitation from your administrator if you're having trouble joining the team`}\n        </span>\n      );\n    }\n\n    // Handle loading\n    if (invite.status === ResourceStatus.Loading) {\n      return this.renderLoading();\n    }\n\n    return (\n      <div className=\"team-invites-app\">\n        <Toast />\n        {confirmProps && <Confirm {...confirmProps} />}\n        <div className=\"header\">\n          <img onClick={this.handleLogoClick} src={PolymailLogo} alt=\"\" />\n        </div>\n        <div className=\"container\">\n          <div className=\"title\">\n            {title}\n          </div>\n          {invite.data?.inviter && (\n          <div className=\"inviter-preview-section\">\n            <div className=\"invited-by\">\n              INVITED BY\n            </div>\n            <div className=\"inviter-preview\">\n              <div className=\"avatar\">\n                {invite.data?.inviter?.photoUrl && (\n                  <img src={invite.data?.inviter?.photoUrl} alt=\"\" />\n                )}\n              </div>\n              <div className=\"sender-details\">\n                <div className=\"name\">\n                  {invite.data?.inviter?.firstName + ' ' + invite.data?.inviter?.lastName}\n                </div>\n                <div className=\"email\">\n                  {invite.data?.inviter?.email}\n                </div>\n              </div>\n            </div>\n          </div>\n          )}\n          <div className={classnames('description', {\n            expired: inviteExpired,\n            accepted: inviteAccepted,\n          })}>\n            {description}\n          </div>\n          {!inviteExpired && (\n          <div className=\"features\">\n            <ul className=\"features-list\">\n              {features.map(f => (\n                <li\n                  key={f}\n                >\n                  {f}\n                </li>\n              ))}\n            </ul>\n          </div>\n          )}\n          <div className=\"action-section\">\n            {inviteExpired && (\n              <button\n                onClick={this.handleExpiredClick}\n                className=\"expired-button\"\n              >\n                Continue\n              </button>\n            )}\n            {loggedIn && !inviteExpired && (\n              <button\n                className=\"accept-button\"\n                onClick={this.handleAccept}\n                disabled={disableAcceptButton}\n              >\n                {!!disableAcceptButton\n                  ? <Loader />\n                  : 'Join Team'\n                }\n              </button>\n            )}\n            {!loggedIn && !inviteExpired && (\n              <button\n                className=\"signup-button\"\n                onClick={this.handleSignup}\n              >\n                Sign up\n              </button>\n            )}\n            {!loggedIn && !inviteExpired && (\n              <div\n                className=\"login-section\"\n              >\n                Already have an account?&nbsp;\n                <button className=\"login-button\" onClick={this.handleLogin}>Log in</button>\n              </div>\n            )}\n            {loggedIn && !inviteExpired && me && (\n              <div\n                className=\"sign-out-section\"\n              >\n                Logged in as <b>{me?.email}</b><br/>Not you?&nbsp;\n                <button className=\"sign-out-button\" onClick={this.handleSignout}>Sign Out</button>\n              </div>\n            )}\n          </div>\n        </div>\n      </div>\n    );\n  }\n}\n\nexport default TeamInvites;\n"]}]}