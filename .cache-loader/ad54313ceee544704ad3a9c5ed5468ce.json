s/NavigationHeader';\nimport './CloseAccount.scss';\n\nexport interface DispatchProps {\n  deleteMe: () => void;\n}\n\nexport interface State {\n  emailSent: boolean;\n}\n\nclass CloseAccount extends React.PureComponent<DispatchProps, State> {\n  constructor() {\n    super();\n\n    this.state = { emailSent: false };\n  }\n\n  handleDelete = () => {\n    const { deleteMe } = this.props;\n\n    deleteMe();\n    this.setState({ emailSent: true });\n  }\n\n  render() {\n    const { emailSent } = this.state;\n\n    return (\n      <div className=\"settings-panel close-account\">\n        <NavigationHeader\n          title=\"Close Account\"\n        />\n        <div className=\"main\">\n          <h3>Close your Polymail account</h3>\n          <div className=\"desc\">\n            Closing your account is irreversible, and will permanently delete your:\n          </div>\n          <ul>\n            <li>Email tracking history</li>\n            <li>Tracked attachments sent from Polymail</li>\n            <li>Email share links</li>\n            <li>Follow up reminders</li>\n            <li>Message templates</li>\n            <li>Polymail Calendar page</li>\n            <li>Sequences</li>\n            <li>Analytics</li>\n          </ul>\n          <div className=\"desc\">\n            If youâ€™d still like to proceed with closing your account, click the button below.\n            {' '}You will be sent a confirmation email with instructions on how to close your\n            {' '}account.\n          </div>\n          {!emailSent && <button onClick={this.handleDelete}>\n            Close Polymail Account\n          </button>}\n          {emailSent && <div className=\"email-sent\">\n            Email Sent!\n          </div>}\n        </div>\n      </div>\n    );\n  }\n}\n\nexport default CloseAccount;\n"]}]}