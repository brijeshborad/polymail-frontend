AAS,EAAC,uBAAuB;wBACpC,6BAAK,SAAS,EAAC,OAAO;;4BAC6B,+CAAuB,CACpE;wBACN,oBAAC,QAAQ,IACP,MAAM,EAAE,gBAAgB,EACxB,QAAQ,EAAE,IAAI,CAAC,oBAAoB,EACnC,EAAE,EAAC,qBAAqB,EACxB,UAAU,EAAE,gBAAgB,CAAC,CAAC;gCAC5B,gBAAgB,CAAC,OAAO,CAAC,gBAAgB,CAAC,CAAC,CAAC,CAAC,SAAS,GACxD,CACE,CACF,CACF;YACN,6BAAK,SAAS,EAAC,QAAQ;gBACrB,gCAAQ,SAAS,EAAC,iBAAiB,EAAC,OAAO,EAAE,IAAI,CAAC,gBAAgB,eAEzD,CACL;YACN,gCAAQ,SAAS,EAAC,cAAc,EAAC,OAAO,EAAE,OAAO;gBAC/C,oBAAC,KAAK,IAAC,SAAS,EAAC,MAAM,GAAG,CACnB,CACL,CACP,CAAC;IACJ,CAAC;IACH,uBAAC;AAAD,CAAC,AA7ND,CAA+B,KAAK,CAAC,aAAa,GA6NjD;AAED,eAAe,gBAAgB,CAAC","sourcesContent":["import React from 'react';\nimport Dropdown from '@src/components/Form/Dropdown';\nimport Close from '@src/components/Icons/Close.svg';\n\nimport './CSVHeaderMapping.scss';\nimport Checkbox from '@src/components/Form/Checkbox/Checkbox';\nimport { SequenceRecipientHeadersAddMeta } from '@src/ducks/sequenceRecipients';\nimport { Resource } from '@src/types';\nimport Loader from '@src/components/Loader/Loader';\n\nexport interface Props {\n  defaultEmailHeader?: string;\n  defaultGivenNameHeader?: string;\n  defaultFamilyNameHeader?: string;\n  headers: Resource<string[]>;\n  onClose: () => void;\n  addHeaders: (meta: SequenceRecipientHeadersAddMeta) => void;\n}\n\ninterface State {\n  emailHeader?: string;\n  givenNameHeader?: string;\n  familyNameHeader?: string;\n  headersToAdd: {\n    [header: string]: boolean;\n  };\n  showError: boolean;\n}\n\nclass CSVHeaderMapping extends React.PureComponent<Props, State> {\n  constructor(props: Props) {\n    super();\n\n    const headersToAdd = {};\n\n    if (props.defaultEmailHeader) {\n      headersToAdd[props.defaultEmailHeader] = true;\n    }\n\n    if (props.defaultGivenNameHeader) {\n      headersToAdd[props.defaultGivenNameHeader] = true;\n    }\n\n    if (props.defaultFamilyNameHeader) {\n      headersToAdd[props.defaultFamilyNameHeader] = true;\n    }\n\n    this.state = {\n      emailHeader: props.defaultEmailHeader,\n      givenNameHeader: props.defaultGivenNameHeader,\n      familyNameHeader: props.defaultFamilyNameHeader,\n      headersToAdd,\n      showError: false,\n    };\n  }\n\n  UNSAFE_componentWillReceiveProps(nextProps: Props) {\n    if (this.props.defaultEmailHeader !== nextProps.defaultEmailHeader ||\n      this.props.defaultGivenNameHeader !== nextProps.defaultGivenNameHeader ||\n      this.props.defaultFamilyNameHeader !== nextProps.defaultFamilyNameHeader) {\n      const { headersToAdd } = this.state;\n      if (nextProps.defaultEmailHeader) {\n        headersToAdd[nextProps.defaultEmailHeader] = true;\n      }\n\n      if (nextProps.defaultGivenNameHeader) {\n        headersToAdd[nextProps.defaultGivenNameHeader] = true;\n      }\n\n      if (nextProps.defaultFamilyNameHeader) {\n        headersToAdd[nextProps.defaultFamilyNameHeader] = true;\n      }\n\n      this.setState({\n        emailHeader: nextProps.defaultEmailHeader,\n        givenNameHeader: nextProps.defaultGivenNameHeader,\n        familyNameHeader: nextProps.defaultFamilyNameHeader,\n        headersToAdd,\n      });\n    }\n  }\n\n  handleDropdownSelect = (value: string, index: number, id?: string) => {\n    const { headersToAdd } = this.state;\n\n    if (id === 'email-dropdown') {\n      this.setState({\n        emailHeader: value,\n        headersToAdd: { ...headersToAdd, [value]: true },\n        showError: false,\n      });\n    } else if (id === 'familyName-dropdown') {\n      this.setState({\n        familyNameHeader: value ? value : undefined,\n        headersToAdd: value ? { ...headersToAdd, [value]: true } : headersToAdd,\n      });\n    } else if (id === 'givenName-dropdown') {\n      this.setState({\n        givenNameHeader: value ? value : undefined,\n        headersToAdd: value ? { ...headersToAdd, [value]: true } : headersToAdd,\n      });\n    }\n  }\n\n  handleAddHeaders = () => {\n    const { addHeaders } = this.props;\n    const { headersToAdd, emailHeader, familyNameHeader, givenNameHeader } = this.state;\n\n    if (!emailHeader) {\n      this.setState({\n        showError: true,\n      });\n      return;\n    }\n\n    if (emailHeader) {\n      delete headersToAdd[emailHeader];\n    }\n\n    if (familyNameHeader) {\n      delete headersToAdd[familyNameHeader];\n    }\n\n    if (givenNameHeader) {\n      delete headersToAdd[givenNameHeader];\n    }\n\n    addHeaders({\n      email: emailHeader,\n      familyName: familyNameHeader,\n      givenName: givenNameHeader,\n      headers: Object.keys(headersToAdd),\n    });\n  }\n\n  handleCheckboxChange = (e: React.SyntheticEvent<HTMLInputElement>) => {\n    const target = e.target as HTMLInputElement;\n    const name = target.name;\n    const { headersToAdd } = this.state;\n    const newHeadersToAdd = { ...headersToAdd };\n\n    if (name) {\n      if (newHeadersToAdd[name]) {\n        delete newHeadersToAdd[name];\n      } else {\n        newHeadersToAdd[name] = true;\n      }\n      this.setState({\n        headersToAdd: newHeadersToAdd,\n      });\n    }\n  }\n\n  render() {\n    const {\n      headers,\n      onClose,\n    } = this.props;\n    const {\n      emailHeader,\n      givenNameHeader,\n      familyNameHeader,\n      headersToAdd,\n      showError,\n    } = this.state;\n\n    if (!headers.data) {\n      return (\n        <div className=\"csv-header-mapping loading\">\n          <Loader />\n        </div>\n      );\n    }\n\n    const emailHeaderValues = headers.data\n      .filter(h => (h !== givenNameHeader && h !== familyNameHeader));\n    const givenNameValues = [''].concat(headers.data\n      .filter(h => (h !== emailHeader && h !== familyNameHeader)));\n    const familyNameValues = [''].concat(headers.data\n      .filter(h => (h !== emailHeader && h !== givenNameHeader)));\n\n    return (\n      <div className=\"csv-header-mapping\">\n        <h1>Which fields should be imported as variables?</h1>\n        <h5>Variables can be used to personalize your messages to each recipient.</h5>\n        <div className=\"main\">\n          <div className=\"checkbox-container\">\n            {headers.data.map(h => (\n              <div className=\"header\" key={h}>\n                <Checkbox\n                  inputName={h}\n                  onChange={this.handleCheckboxChange}\n                  checked={headersToAdd[h]}\n                />\n                <span>{h}</span>\n              </div>\n            ))}\n          </div>\n          <div className=\"header-container\">\n            <div className=\"email-header row\">\n              <div className=\"value\">\n                Which field contains your recipients’ email? <span>(required)</span>\n              </div>\n              <div className=\"dropdown-row\">\n                <Dropdown\n                  values={emailHeaderValues}\n                  onSelect={this.handleDropdownSelect}\n                  id=\"email-dropdown\"\n                  startIndex={emailHeader ? emailHeaderValues.indexOf(emailHeader) : undefined}\n                />\n                {showError &&\n                  <span className=\"error\">Please specify the corresponding email field</span>}\n              </div>\n            </div>\n            <div className=\"givenName-header row\">\n              <div className=\"value\">\n                Which field contains your recipients’ first name? <span>(optional)</span>\n              </div>\n              <Dropdown\n                values={givenNameValues}\n                onSelect={this.handleDropdownSelect}\n                id=\"givenName-dropdown\"\n                startIndex={givenNameHeader ? givenNameValues.indexOf(givenNameHeader) : undefined}\n              />\n            </div>\n            <div className=\"familyName-header row\">\n              <div className=\"value\">\n                Which field contains your recipients’ last name? <span>(optional)</span>\n              </div>\n              <Dropdown\n                values={familyNameValues}\n                onSelect={this.handleDropdownSelect}\n                id=\"familyName-dropdown\"\n                startIndex={familyNameHeader ?\n                  familyNameValues.indexOf(familyNameHeader) : undefined}\n              />\n            </div>\n          </div>\n        </div>\n        <div className=\"footer\">\n          <button className=\"continue-button\" onClick={this.handleAddHeaders}>\n            Continue\n          </button>\n        </div>\n        <button className=\"close-button\" onClick={onClose}>\n          <Close className=\"icon\" />\n        </button>\n      </div>\n    );\n  }\n}\n\nexport default CSVHeaderMapping;\n"]}]}